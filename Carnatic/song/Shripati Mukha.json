\u0c26\u0c30\u0c3f \u0c2d\u0c41\u0c35\u0c28\u0c47\u0c36\u0c4d\u0c35\u0c30\u0c3f <br/> \u0c36\u0c3e\u0c2e\u0c4d\u0c2d\u0c35\u0c3f \u0c2e\u0c39\u0c3e \u0c24\u0c4d\u0c30\u0c3f\u0c2a\u0c41\u0c30 \u0c38\u0c41\u0c28\u0c4d\u0c26\u0c30\u0c3f <br/> \u0c39\u0c3f\u0c2e \u0c17\u0c3f\u0c30\u0c3f \u0c15\u0c41\u0c2e\u0c3e\u0c30\u0c3f \u0c15\u0c35\u0c3f \u0c15\u0c41\u0c32 <br/> \u0c15\u0c3e\u0c2e\u0c26\u0c47 \u0c15\u0c3e\u0c28\u0c4d\u0c15\u0c4d\u0c37\u0c3f\u0c24 \u0c2b\u0c32 \u0c26\u0c3e\u0c2f\u0c3f\u0c15\u0c47 \u0c36\u0c4d\u0c30\u0c40-\u0c2a\u0c24\u0c3f"}]}, {"L": "Kannada", "text": [{"M": "\u0caa\u0cb2\u0ccd\u0cb2\u0cb5\u0cbf", "T": "\u0cb6\u0ccd\u0cb0\u0cc0-\u0caa\u0ca4\u0cbf \u0cae\u0cc1\u0c96 \u0cb5\u0cbf\u0cb0\u0c9a\u0cbf\u0ca4 \u0caa\u0cc2\u0c9c\u0ccd\u0caf\u0cc7 <br/> \u0cb6\u0ccd\u0cb0\u0cc0 \u0caa\u0cbe\u0cb0\u0ccd\u0cb5\u0ca4\u0cbf \u0cae\u0cbe\u0cae\u0ccd \u0caa\u0cbe\u0cb9\u0cbf \u0ca6\u0cc7\u0cb5\u0cbf"}, {"M": "\u0c85\u0ca8\u0cc1\u0caa\u0cb2\u0ccd\u0cb2\u0cb5\u0cbf", "T": "\u0ca8\u0cc0\u0caa \u0cb5\u0ca8 \u0ca8\u0cbf\u0cb2\u0caf\u0cc7 \u0ca8\u0cbf\u0cb0\u0cbe\u0cae\u0caf\u0cc7 <br/> \u0ca8\u0cbf\u0c9f\u0cbf\u0cb2 \u0ca8\u0caf\u0ca8 \u0c9c\u0cbe\u0caf\u0cc7 \u0cae\u0cae \u0cb9\u0cc3\u0ca6\u0caf <br/> \u0ca4\u0cbe\u0caa \u0cb9\u0cbe\u0cb0\u0cbf\u0ca3\u0cbf \u0ca8\u0cb5 \u0cb0\u0ca4\u0ccd\u0ca8\u0cbe\u0cb2\u0caf\u0cc7 <br/> \u0ca4\u0cbe\u0caa\u0cb8 \u0cb5\u0cb0 \u0ca8\u0cbe\u0cb0\u0ca6 \u0cae\u0cc1\u0ca6\u0cbf\u0ca4\u0cc7 \u0ca6\u0cc7\u0cb5\u0cbf \u0cb6\u0ccd\u0cb0\u0cc0-\u0caa\u0ca4\u0cbf"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 1", "T": "\u0ca4\u0cb0\u0cc1\u0ca3\u0cbf \u0cb2\u0ca4\u0cbe \u0caa\u0cb2\u0ccd\u0cb2\u0cb5 \u0cae\u0cc3\u0ca6\u0cc1 \u0c9a\u0cb0\u0ca3\u0cc7 <br/> \u0ca4\u0caa\u0ca8 \u0cb5\u0cbf\u0ca7\u0cc1 \u0cb5\u0cbf\u0cb2\u0ccb\u0c9a\u0ca8\u0cc7 <br/> \u0c85\u0cb0\u0cc1\u0ca3 \u0c95\u0ccb\u0c9f\u0cbf \u0cb8\u0cae \u0c95\u0cbe\u0ca8\u0ccd\u0ca4\u0cbf \u0caf\u0cc1\u0ca4 <br/> \u0cb6\u0cb0\u0cc0\u0cb0\u0cc7 \u0c95\u0cb2 \u0ca7\u0cc3\u0ca4 \u0c95\u0cb2\u0cbe\u0caa\u0cc7 <br/> \u0cb8\u0cc1\u0cb0\u0cc1\u0c9a\u0cbf\u0cb0 \u0cae\u0ca3\u0cbf \u0c95\u0ca3\u0ccd\u0ca0 \u0cb2\u0cb8\u0ca8\u0ccd-\u0cae\u0ca3\u0cbf \u0cb9\u0cbe\u0cb0\u0cc7 <br/> \u0cb8\u0cc1\u0c97\u0cc1\u0ca3-\u0cb6\u0cc0\u0cb2\u0cc7 \u0cb8\u0ca4\u0ca4\u0cae\u0ccd \u0cb8\u0cae\u0cc1\u0ca6\u0cae\u0ccd <br/> \u0c95\u0cb0\u0cc1\u0ca3\u0caf\u0cbe \u0c85\u0cb5 \u0ca6\u0cc0\u0ca8\u0cae\u0ccd \u0caa\u0cb0 \u0ca6\u0cc7\u0cb5\u0ca4\u0cc7 <br/> \u0c95\u0cbe\u0cae \u0c95\u0ccb\u0c9f\u0cbf \u0caa\u0cc0\u0ca0 \u0c97\u0ca4\u0cc7 \u0cb2\u0cb2\u0cbf\u0ca4\u0cc7 \u0cb6\u0ccd\u0cb0\u0cc0-\u0caa\u0ca4\u0cbf"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 2", "T": "\u0c95\u0cb0\u0cbf \u0cae\u0cc1\u0c96 \u0c95\u0cbe\u0cb0\u0ccd\u0ca4\u0cbf\u0c95\u0cc7\u0caf \u0c9c\u0ca8\u0ca8\u0cbf <br/> \u0cb8\u0ccd\u0cb5\u0cb0 \u0caa\u0cbe\u0cb2\u0cbf\u0ca8\u0cbf \u0caa\u0cbe\u0cb5\u0ca8\u0cbf <br/> \u0cb9\u0cb0\u0cbf \u0cb8\u0cb9\u0ccb\u0ca6\u0cb0\u0cbf \u0cb5\u0cbf\u0ca6\u0cb3\u0cbf\u0ca4 <br/> \u0ca6\u0cc8\u0ca4\u0ccd\u0caf\u0cbe\u0cb0\u0cbf \u0c97\u0ca3\u0cc7 \u0cb8\u0ca6\u0cbe \u0caa\u0cc2\u0cb0\u0ccd\u0ca3\u0cc7 <br/> \u0caa\u0cb0\u0cae\u0cc7\u0cb6 \u0cb5\u0cbf\u0ca8\u0cc1\u0ca4\u0cc7 \u0cb6\u0ccd\u0cb0\u0cbf\u0ca4 \u0c9c\u0ca8 \u0caa\u0cbe\u0cb2\u0cbf\u0ca4\u0cc7 <br/> \u0caa\u0ccd\u0cb0\u0cc0\u0ca4\u0cbf\u0cb0\u0cbf\u0cb9 \u0cb5\u0cb8\u0ca4\u0cc1 \u0cb5\u0cbf\u0cae\u0cb2\u0cc7 <br/> \u0caa\u0cc1\u0cb0 \u0cb9\u0cb0 \u0caa\u0ccd\u0cb0\u0cbf\u0caf\u0cc7 \u0cb6\u0cb6\u0cbf \u0ca8\u0cbf\u0cad\u0cbe\u0ca8\u0ca8\u0cc7 <br/> \u0caa\u0cc2\u0cb0\u0ccd\u0ca3 \u0c95\u0cbe\u0cae\u0cc7 \u0cb8\u0cbe\u0cae \u0c97\u0cbe\u0ca8 \u0cb2\u0ccb\u0cb2\u0cc7 \u0cb6\u0ccd\u0cb0\u0cc0-\u0caa\u0ca4\u0cbf"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 3", "T": "\u0cb6\u0ccd\u0caf\u0cbe\u0cae\u0cb3\u0cbe\u0c99\u0ccd\u0c97\u0cbf \u0cae\u0ca8\u0ccd\u0c9c\u0cc1\u0cb3 \u0cb5\u0cbe\u0ca3\u0cbf <br/> \u0cb8\u0c95\u0cb2 \u0cad\u0caf \u0ca8\u0cbf\u0cb5\u0cbe\u0cb0\u0cbf\u0ca3\u0cbf <br/> \u0cb9\u0cc7 \u0cae\u0cb9\u0cc7\u0cb6\u0ccd\u0cb5\u0cb0\u0cbf \u0cae\u0ca7\u0cc1\u0caa \u0cb8\u0ca6\u0cc3\u0cb6 <br/> \u0cb5\u0cc7\u0ca3\u0cbf \u0c95\u0cbe\u0cae\u0cc7\u0cb6\u0ccd\u0cb5\u0cb0\u0cbf \u0c97\u0ccc\u0cb0\u0cbf <br/> \u0cb6\u0ccd\u0caf\u0cbe\u0cae \u0c95\u0cc3\u0cb7\u0ccd\u0ca3 \u0cb8\u0ccb\u0ca6\u0cb0\u0cbf \u0cad\u0cc1\u0cb5\u0ca8\u0cc7\u0cb6\u0ccd\u0cb5\u0cb0\u0cbf <br/> \u0cb6\u0cbe\u0cae\u0ccd\u0cad\u0cb5\u0cbf \u0cae\u0cb9\u0cbe \u0ca4\u0ccd\u0cb0\u0cbf\u0caa\u0cc1\u0cb0 \u0cb8\u0cc1\u0ca8\u0ccd\u0ca6\u0cb0\u0cbf <br/> \u0cb9\u0cbf\u0cae \u0c97\u0cbf\u0cb0\u0cbf \u0c95\u0cc1\u0cae\u0cbe\u0cb0\u0cbf \u0c95\u0cb5\u0cbf \u0c95\u0cc1\u0cb2 <br/> \u0c95\u0cbe\u0cae\u0ca6\u0cc7 \u0c95\u0cbe\u0ca8\u0ccd\u0c95\u0ccd\u0cb7\u0cbf\u0ca4 \u0cab\u0cb2 \u0ca6\u0cbe\u0caf\u0cbf\u0c95\u0cc7 \u0cb6\u0ccd\u0cb0\u0cc0-\u0caa\u0ca4\u0cbf"}]}, {"L": "Malayalam", "text": [{"M": "\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d36\u0d4d\u0d30\u0d40-\u0d2a\u0d24\u0d3f \u0d2e\u0d41\u0d16 \u0d35\u0d3f\u0d30\u0d1a\u0d3f\u0d24 \u0d2a\u0d42\u0d1c\u0d4d\u0d2f\u0d47 <br/> \u0d36\u0d4d\u0d30\u0d40 \u0d2a\u0d3e\u0d30\u0d4d\u0d35\u0d24\u0d3f \u0d2e\u0d3e\u0d2e\u0d4d \u0d2a\u0d3e\u0d39\u0d3f \u0d26\u0d47\u0d35\u0d3f"}, {"M": "\u0d05\u0d28\u0d41\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d28\u0d40\u0d2a \u0d35\u0d28 \u0d28\u0d3f\u0d32\u0d2f\u0d47 \u0d28\u0d3f\u0d30\u0d3e\u0d2e\u0d2f\u0d47 <br/> \u0d28\u0d3f\u0d1f\u0d3f\u0d32 \u0d28\u0d2f\u0d28 \u0d1c\u0d3e\u0d2f\u0d47 \u0d2e\u0d2e \u0d39\u0d43\u0d26\u0d2f <br/> \u0d24\u0d3e\u0d2a \u0d39\u0d3e\u0d30\u0d3f\u0d23\u0d3f \u0d28\u0d35 \u0d30\u0d24\u0d4d\u0d28\u0d3e\u0d32\u0d2f\u0d47 <br/> \u0d24\u0d3e\u0d2a\u0d38 \u0d35\u0d30 \u0d28\u0d3e\u0d30\u0d26 \u0d2e\u0d41\u0d26\u0d3f\u0d24\u0d47 \u0d26\u0d47\u0d35\u0d3f \u0d36\u0d4d\u0d30\u0d40-\u0d2a\u0d24\u0d3f"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 1", "T": "\u0d24\u0d30\u0d41\u0d23\u0d3f \u0d32\u0d24\u0d3e \u0d2a\u0d32\u0d4d\u0d32\u0d35 \u0d2e\u0d43\u0d26\u0d41 \u0d1a\u0d30\u0d23\u0d47 <br/> \u0d24\u0d2a\u0d28 \u0d35\u0d3f\u0d27\u0d41 \u0d35\u0d3f\u0d32\u0d4b\u0d1a\u0d28\u0d47 <br/> \u0d05\u0d30\u0d41\u0d23 \u0d15\u0d4b\u0d1f\u0d3f \u0d38\u0d2e \u0d15\u0d3e\u0d28\u0d4d\u0d24\u0d3f \u0d2f\u0d41\u0d24 <br/> \u0d36\u0d30\u0d40\u0d30\u0d47 \u0d15\u0d32 \u0d27\u0d43\u0d24 \u0d15\u0d32\u0d3e\u0d2a\u0d47 <br/> \u0d38\u0d41\u0d30\u0d41\u0d1a\u0d3f\u0d30 \u0d2e\u0d23\u0d3f \u0d15\u0d23\u0d4d\u0d20 \u0d32\u0d38\u0d28\u0d4d-\u0d2e\u0d23\u0d3f \u0d39\u0d3e\u0d30\u0d47 <br/> \u0d38\u0d41\u0d17\u0d41\u0d23-\u0d36\u0d40\u0d32\u0d47 \u0d38\u0d24\u0d24\u0d2e\u0d4d \u0d38\u0d2e\u0d41\u0d26\u0d2e\u0d4d <br/> \u0d15\u0d30\u0d41\u0d23\u0d2f\u0d3e \u0d05\u0d35 \u0d26\u0d40\u0d28\u0d2e\u0d4d \u0d2a\u0d30 \u0d26\u0d47\u0d35\u0d24\u0d47 <br/> \u0d15\u0d3e\u0d2e \u0d15\u0d4b\u0d1f\u0d3f \u0d2a\u0d40\u0d20 \u0d17\u0d24\u0d47 \u0d32\u0d32\u0d3f\u0d24\u0d47 \u0d36\u0d4d\u0d30\u0d40-\u0d2a\u0d24\u0d3f"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 2", "T": "\u0d15\u0d30\u0d3f \u0d2e\u0d41\u0d16 \u0d15\u0d3e\u0d30\u0d4d\u0d24\u0d3f\u0d15\u0d47\u0d2f \u0d1c\u0d28\u0d28\u0d3f <br/> \u0d38\u0d4d\u0d35\u0d30 \u0d2a\u0d3e\u0d32\u0d3f\u0d28\u0d3f \u0d2a\u0d3e\u0d35\u0d28\u0d3f <br/> \u0d39\u0d30\u0d3f \u0d38\u0d39\u0d4b\u0d26\u0d30\u0d3f \u0d35\u0d3f\u0d26\u0d33\u0d3f\u0d24 <br/> \u0d26\u0d48\u0d24\u0d4d\u0d2f\u0d3e\u0d30\u0d3f \u0d17\u0d23\u0d47 \u0d38\u0d26\u0d3e \u0d2a\u0d42\u0d30\u0d4d\u0d23\u0d47 <br/> \u0d2a\u0d30\u0d2e\u0d47\u0d36 \u0d35\u0d3f\u0d28\u0d41\u0d24\u0d47 \u0d36\u0d4d\u0d30\u0d3f\u0d24 \u0d1c\u0d28 \u0d2a\u0d3e\u0d32\u0d3f\u0d24\u0d47 <br/> \u0d2a\u0d4d\u0d30\u0d40\u0d24\u0d3f\u0d30\u0d3f\u0d39 \u0d35\u0d38\u0d24\u0d41 \u0d35\u0d3f\u0d2e\u0d32\u0d47 <br/> \u0d2a\u0d41\u0d30 \u0d39\u0d30 \u0d2a\u0d4d\u0d30\u0d3f\u0d2f\u0d47 \u0d36\u0d36\u0d3f \u0d28\u0d3f\u0d2d\u0d3e\u0d28\u0d28\u0d47 <br/> \u0d2a\u0d42\u0d30\u0d4d\u0d23 \u0d15\u0d3e\u0d2e\u0d47 \u0d38\u0d3e\u0d2e \u0d17\u0d3e\u0d28 \u0d32\u0d4b\u0d32\u0d47 \u0d36\u0d4d\u0d30\u0d40-\u0d2a\u0d24\u0d3f"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 3", "T": "\u0d36\u0d4d\u0d2f\u0d3e\u0