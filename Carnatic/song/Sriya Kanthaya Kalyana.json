/> \u0bb7\u0ba4 \u0b9a\u0bae\u0bcd\u0bae\u0bca\u0bb9\u0ba9\u0baf\u0bb8\u0bcd\u0ba4\u0bc1 V\u0b8e\u0b99\u0bcd\u0b95\u0ba4\u0bc6\u0b9a\u0baf \u0bae\u0bcd\u0b85\u0b99\u0bcd\u0b95\u0bb2\u0bae\u0bcd <br/> . \u0ba3\u0bbf\u0ba4\u0bcd\u0baf\u0baf \u0ba8\u0bbf\u0bb0\u0bb5\u0ba4\u0bcd\u0baf\u0baf \u0bb7\u0ba4\u0bcd\u0baf\u0ba9\u0ba8\u0bcd\u0ba4 \u0b9a\u0bbf\u0ba4\u0ba4\u0bcd\u0bae\u0ba9\u0bc6 <br/> \u0bb7\u0bb0\u0bcd\u0bb5\u0ba8\u0bcd\u0ba4\u0bb0\u0ba4\u0bcd \u0bae\u0ba9\u0bc6 \u0bb7\u0bcd\u0bb0\u0bbf\u0bae\u0ba4\u0bcd V\u0b8e\u0b99\u0bcd\u0b95\u0ba4\u0bc6\u0b9a\u0bb5 \u0bae\u0bcd\u0b85\u0b99\u0bcd\u0b95\u0bb2\u0bae\u0bcd <br/> . \u0bb7\u0bcd\u0bb5\u0ba4 \u0bb8\u0bcd\u0b9a\u0bb0\u0bcd\u0bb5\u0bb5\u0bbf\u0ba4\u0bc6 \u0b9a\u0bb0\u0bcd\u0bb5\u0b9a\u0b95\u0bcd\u0ba4\u0baf\u0bc6 \u0bb7\u0bb0\u0bcd\u0bb5\u0b9a\u0bc6\u0b9a\u0bbf\u0ba9\u0bc6 <br/> \u0bb7\u0bc1\u0bb2\u0baa\u0baf \u0bb7\u0bc1\u0b9a\u0bc0\u0bb2\u0baf V\u0b8e\u0b99\u0bcd\u0b95\u0ba4\u0bc6\u0b9a\u0baf \u0bae\u0bcd\u0b85\u0b99\u0bcd\u0b95\u0bb2\u0bae\u0bcd <br/> . P\u0b85\u0bb0\u0bb8\u0bcd\u0bae\u0bc8 B\u0bb0\u0bb9\u0bcd\u0bae\u0ba9\u0bc6 P\u0b8a\u0bb0\u0bcd\u0ba9 \u0b83\u0b85\u0bae\u0baf P\u0b85\u0bb0\u0bae\u0ba4\u0bcd\u0bae\u0ba9\u0bc6 <br/> P\u0bb0\u0baf\u0bc1\u0b9e\u0bcd\u0b9c\u0bc6 P\u0b85\u0bb0\u0ba4\u0ba4\u0bcd\u0bb5\u0baf V\u0b8e\u0b99\u0bcd\u0b95\u0ba4\u0bc6\u0b9a\u0baf \u0bae\u0bcd\u0b85\u0b99\u0bcd\u0b95\u0bb2\u0bae\u0bcd <br/> . \u0b86\u0b95\u0bb2\u0ba4\u0ba4\u0bcd\u0bb5 \u0bae\u0bb8\u0bcd\u0bb0\u0ba8\u0bcd\u0ba4 \u0bae\u0ba4\u0bcd\u0bae\u0ba9\u0bae\u0ba9\u0bc1-\u0baa\u0bb8\u0bcd\u0baf\u0ba4\u0bae\u0bcd <br/> \u0b86\u0ba4\u0bcd\u0bb0\u0bbf\u0baa\u0bcd\u0ba4\u0bcd\u0baf \u0bae\u0bcd\u0bb0\u0bbf\u0ba4 \u0bb0\u0bc1\u0baa\u0baf V\u0b8e\u0b99\u0bcd\u0b95\u0ba4\u0bc6\u0b9a\u0baf \u0bae\u0bcd\u0b85\u0b99\u0bcd\u0b95\u0bb2\u0bae\u0bcd <br/> . P\u0bb0\u0baf\u0bb9\u0bcd \u0bb8\u0bcd\u0bb5\u0b9a\u0bb0\u0ba9\u0bca\u0bae\u0bcd P\u0b89\u0bae\u0bcd\u0b9a\u0bae\u0bcd \u0bb7\u0bb0\u0ba9\u0bcd\u0baf\u0ba4\u0bcd\u0bb5\u0bc6\u0ba9 P\u0b85\u0ba9\u0bbf\u0ba9 <br/> \u0b83\u0bb0\u0bbf\u0baa\u0baf \u0ba4\u0bbf\u0b9a\u0ba4\u0bc6 \u0bb7\u0bcd\u0bb0\u0bbf\u0bae\u0ba4\u0bcd- V\u0b8e\u0b99\u0bcd\u0b95\u0ba4\u0bc6\u0b9a\u0bb5 \u0bae\u0bcd\u0b85\u0b99\u0bcd\u0b95\u0bb2\u0bae\u0bcd <br/> . \u0b9f\u0baf \u0bae\u0bcd\u0bb0\u0bbf\u0ba4 \u0ba4\u0bb0\u0b99\u0bcd\u0b95\u0bbf\u0ba9\u0bcd\u0baf-\u0bb7\u0bcd\u0ba4\u0bb0\u0b99\u0bcd\u0b95\u0bc8\u0bb0\u0bbf\u0bb5 \u0bb7\u0bc0\u0ba4\u0bb2\u0bc8\u0bb9\u0bcd <br/> \u0b86\u0baa\u0b95\u0bc8 \u0bb8\u0bcd\u0b9a\u0bbf\u0b9e\u0bcd\u0b9a\u0bb2\u0bcd\u0bb9\u0bc6 \u0bb5\u0bbf\u0bb8\u0bcd\u0bb5\u0bae\u0bcd V\u0b8e\u0b99\u0bcd\u0b95\u0ba4\u0bc6\u0b9a\u0bb5 \u0bae\u0bcd\u0b85\u0ba9\u0bcd\u0ba9\u0bb2\u0bae\u0bcd <br/> . \u0bb7\u0bcd\u0bb0\u0b95\u0bcd\u0baa\u0bc1\u0b9a\u0bae\u0bcd\u0baa\u0bb0 \u0bb9\u0bc6\u0ba4\u0bc0\u0ba9\u0bae\u0bcd \u0bb7\u0bc1\u0b9a\u0bae\u0bb5 \u0bb9\u0bae\u0bc1\u0bb0\u0bcd\u0ba4\u0baf\u0bc6 <br/> \u0bb7\u0bb0\u0bcd\u0bb5\u0bb0\u0bcd\u0ba4\u0bbf \u0bb7\u0bae\u0ba9\u0baf\u0bb8\u0bcd\u0ba4\u0bc1 V\u0b8e\u0b99\u0bcd\u0b95\u0ba4\u0bc6\u0b9a\u0baf \u0bae\u0bcd\u0b85\u0b99\u0bcd\u0b95\u0bb2\u0bae\u0bcd <br/> . \u0bb6\u0bcd\u0bb0\u0bc0 V\u0b90\u0b95\u0bc1\u0ba8\u0bcd\u0ba4 \u0bb5\u0bbf\u0bb0\u0b95\u0bcd\u0ba4\u0baf \u0bb7\u0bcd\u0bb5\u0bae\u0bbf P\u0b89\u0bb6\u0bcd\u0b95\u0bb0\u0bbf\u0ba9\u0bc0\u0ba4\u0ba4\u0bc6 <br/> \u0bb0\u0bae\u0baf \u0bb0\u0bae\u0bae\u0ba9\u0baf V\u0b8e\u0b99\u0bcd\u0b95\u0ba4\u0bc6\u0b9a\u0baf \u0bae\u0bcd\u0b85\u0b99\u0bcd\u0b95\u0bb2\u0bae\u0bcd <br/> . \u0bb7\u0bcd\u0bb0\u0bbf\u0bae\u0ba4\u0bcd \u0b9a\u0bc1\u0ba8\u0bcd\u0ba4\u0bb0 \u0b9c\u0bae\u0ba4\u0bcd\u0bb0\u0bc1 \u0bae\u0bc1\u0ba9\u0bbf \u0bae\u0ba9\u0b9a\u0bb5\u0b9a\u0bbf\u0ba9\u0bc6 <br/> \u0bb7\u0bb0\u0bcd\u0bb5\u0bb2\u0bca\u0b95 \u0ba3\u0bbf\u0bb5\u0b9a\u0bb5 \u0bb7\u0bcd\u0bb0\u0bbf\u0ba9\u0bbf\u0bb5\u0b9a\u0baf \u0bae\u0bcd\u0b85\u0b99\u0bcd\u0b95\u0bb2\u0bae\u0bcd <br/> . \u0bae\u0bcd\u0b85\u0b99\u0bcd\u0b95\u0bb2\u0b9a\u0b9a\u0ba9 \u0baa\u0bb0\u0bc8\u0bb9\u0bcd \u0bae\u0ba4\u0b9a\u0bb0\u0bcd\u0baf P\u0b89\u0bb0\u0bca\u0b95\u0bae\u0bc8\u0bb9\u0bcd <br/> \u0bb7\u0bb0\u0bcd\u0bb5\u0bc8\u0bb8\u0bcd\u0b9a P\u0b89\u0bb0\u0bcd\u0bb5\u0bc8 \u0bb0\u0b9a\u0bb0\u0bcd\u0baf\u0bc8\u0bb9\u0bcd \u0bb7\u0ba4\u0bcd\u0b95\u0bcd\u0bb0\u0bbf\u0ba4\u0baf\u0bb8\u0bcd\u0ba4\u0bc1 \u0bae\u0bcd\u0b85\u0b99\u0bcd\u0b95\u0bb2\u0bae\u0bcd"}]}, {"L": "Telugu", "text": [{"M": "\u0c2a\u0c32\u0c4d\u0c32\u0c35\u0c3f", "T": "\u0c37\u0c4d\u0c30\u0c3f\u0c2f\u0c39\u0c4d \u0c15\u0c28\u0c4d\u0c25\u0c2f \u0c15\u0c32\u0c4d\u0c2f\u0c28 \u0c28\u0c3f\u0c27\u0c2f\u0c46 \u0c28\u0c3f\u0c27\u0c2f\u0c46\u0c30\u0c4d\u0c25\u0c3f\u0c28\u0c2e\u0c4d <br/> \u0c36\u0c4d\u0c30\u0c40 V\u0c0e\u0c19\u0c4d\u0c15\u0c24 \u0c28\u0c3f\u0c35\u0c38\u0c2f \u0c37\u0c4d\u0c30\u0c3f\u0c28\u0c3f\u0c35\u0c38\u0c2f \u0c02\u0c05\u0c19\u0c4d\u0c17\u0c32\u0c2e\u0c4d <br/> . \u0c33\u0c15\u0c4d\u0c36\u0c4d\u0c2e\u0c3f \u0c38\u0c35\u0c3f\u0c2d\u0c4d\u0c30 \u0c2e\u0c32\u0c4a\u0c15-\u0c38\u0c41\u0c2d\u0c4d\u0c30\u0c41 \u0c35\u0c3f\u0c2d\u0c4d\u0c30\u0c2e \u0c1b\u0c15\u0c4d\u0c36\u0c41\u0c36\u0c46 <br/> C\u0c39\u0c15\u0c4d\u0c36\u0c41\u0c36\u0c46 \u0c37\u0c30\u0c4d\u0c35\u0c32\u0c4a\u0c17\u0c4d\u0c05\u0c28\u0c2e\u0c4d V\u0c0e\u0c19\u0c4d\u0c15\u0c24\u0c46\u0c36\u0c2f \u0c02\u0c05\u0c19\u0c4d\u0c17\u0c32\u0c2e\u0c4d <br/> . \u0c36\u0c4d\u0c30\u0c40 V\u0c0e\u0c19\u0c4d\u0c15\u0c24\u0c26\u0c4d\u0c30\u0c3f \u0c37\u0c4d\u0c30\u0c3f\u0c19\u0c4d\u0c17\u0c17\u0c4d\u0c30-\u0c02\u0c05\u0c19\u0c4d\u0c17\u0c32\u0c39 \u0c2d\u0c30\u0c28\u0c19\u0c4d\u0c18\u0c4d\u0c30\u0c2f\u0c46 <br/> \u0c02\u0c05\u0c19\u0c4d\u0c17\u0c32\u0c28\u0c2e\u0c4d \u0c28\u0c3f\u0c35\u0c38\u0c2f V\u0c0e\u0c19\u0c4d\u0c15\u0c24\u0c46\u0c36\u0c2f \u0c02\u0c05\u0c19\u0c4d\u0c17\u0c32\u0c2e\u0c4d <br/> . \u0c37\u0c30\u0c4d\u0c35\u0c35\u0c2f\u0c35 \u0c38\u0c4c\u0c28\u0c4d\u0c26\u0c30\u0c4d\u0c2f \u0c37\u0c2e\u0c4d\u0c2a\u0c27 \u0c37\u0c30\u0c4d\u0c35\u0c1b\u0c46\u0c25\u0c38\u0c2e\u0c4d <br/> \u0c37\u0c26 \u0c38\u0c2e\u0c4d\u0c2e\u0c4a\u0c39\u0c28\u0c2f\u0c38\u0c4d\u0c25\u0c41 V\u0c0e\u0c19\u0c4d\u0c15\u0c24\u0c46\u0c36\u0c2f \u0c02\u0c05\u0c19\u0c4d\u0c17\u0c32\u0c2e\u0c4d <br/> . \u0c23\u0c3f\u0c25\u0c4d\u0c2f\u0c2f \u0c28\u0c3f\u0c30\u0c35\u0c27\u0c4d\u0c2f\u0c2f \u0c37\u0c25\u0c4d\u0c2f\u0c28\u0c28\u0c4d\u0c26 \u0c1b\u0c3f\u0c27\u0c25\u0c4d\u0c2e\u0c28\u0c46 <br/> \u0c37\u0c30\u0c4d\u0c35\u0c28\u0c4d\u0c25\u0c30\u0c25\u0c4d \u0c2e\u0c28\u0c46 \u0c37\u0c4d\u0c30\u0c3f\u0c2e\u0c26\u0c4d V\u0c0e\u0c19\u0c4d\u0c15\u0c24\u0c46\u0c36\u0c35 \u0c02\u0c05\u0c19\u0c4d\u0c17\u0c32\u0c2e\u0c4d <br/> . \u0c37\u0c4d\u0c35\u0c25 \u0c38\u0c4d\u0c38\u0c30\u0c4d\u0c35\u0c35\u0c3f\u0c26\u0c46 \u0c38\u0c30\u0c4d\u0c35\u0c38\u0c15\u0c4d\u0c25\u0c2f\u0c46 \u0c37\u0c30\u0c4d\u0c35\u0c38\u0c46\u0c36\u0c3f\u0c28\u0c46 <br/> \u0c37\u0c41\u0c32\u0c2d\u0c2f \u0c37\u0c41\u0c38\u0c40\u0c32\u0c2f V\u0c0e\u0c19\u0c4d\u0c15\u0c24\u0c46\u0c36\u0c2f \u0c02\u0c05\u0c19\u0c4d\u0c17\u0c32\u0c2e\u0c4d <br/> . P\u0c05\u0c30\u0c38\u0c4d\u0c2e\u0c48 B\u0c30\u0c39\u0c4d\u0c2e\u0c28\u0c46 P\u0c0a\u0c30\u0c4d\u0c28 \u0c17\u0c4d\u0c05\u0c2e\u0c2f P\u0c05\u0c30\u0c2e\u0c25\u0c4d\u0c2e\u0c28\u0c46 <br/> P\u0c30\u0c2f\u0c41\u0c1e\u0c4d\u0c1c\u0c46 P\u0c05\u0c30\u0c25\u0c25\u0c4d\u0c35\u0c2f V\u0c0e\u0c19\u0c4d\u0c15\u0c24\u0c46\u0c36\u0c2f \u0c02\u0c05\u0c19\u0c4d\u0c17\u0c32\u0c2e\u0c4d <br/> . \u0c06\u0c15\u0c32\u0c25\u0c25\u0c4d\u0c35 \u0c2e\u0c38\u0c4d\u0c30\u0c28\u0c4d\u0c25 \u0c2e\u0c25\u0c4d\u0c2e\u0c28\u0c2e\u0c28\u0c41-\u0c2a\u0c38\u0c4d\u0c2f\u0c25\u0c2e\u0c4d <br/> \u0c06\u0c25\u0c4d\u0c30\u0c3f\u0c2a\u0c4d\u0c25\u0c4d\u0c2f \u0c2e\u0c4d\u0c30\u0c3f\u0c25 \u0c30\u0c41\u0c2a\u0c2f V\u0c0e\u0c19\u0c4d\u0c15\u0c24\u0c46\u0c36\u0c2f \u0c02\u0c05\u0c19\u0c4d\u0c17\u0c32\u0c2e\u0c4d <br/> . P\u0c30\u0c2f\u0c39\u0c4d \u0c38\u0c4d\u0c35\u0c1b\u0c30\u0c28\u0c4a\u0c2e\u0c4d P\u0c09\u0c2e\u0c4d\u0c38\u0c2e\u0c4d \u0c37\u0c30\u0c28\u0c4d\u0c2f\u0c25\u0c4d\u0c35\u0c46\u0c28 P\u0c05\u0c28\u0c3f\u0c28 <br/> \u0c17\u0c4d\u0c30\u0c3f\u0c2a\u0c2f \u0c27\u0c3f\u0c38\u0c25\u0c46 \u0c37\u0c4d\u0c30\u0c3f\u0c2e\u0c26\u0c4d- V\u0c0e\u0c19\u0c4d\u0c15\u0c24\u0c46\u0c36\u0c35 \u0c02\u0c05\u0c19\u0c4d\u0c17\u0c32\u0c2e\u0c4d <br/> . \u0c22\u0c2f \u0c2e\u0c4d\u0c30\u0c3f\u0c25 \u0c25\u0c30\u0c19\u0c4d\u0c17\u0c3f\u0c28\u0c4d\u0c2f-\u0c37\u0c4d\u0c25\u0c30\u0c19\u0c4d\u0c17\u0c48\u0c30\u0c3f\u0c35 \u0c37\u0c40\u0c25\u0c32\u0c48\u0c39\u0c4d <br/> \u0c06\u0c2a\u0c17\u0c48 \u0c38\u0c4d\u0c38\u0c3f\u0c1e\u0c4d\u0c1b\u0c32\u0c4d\u0c39\u0c46 \u0c35\u0c3f\u0c38\u0c4d\u0c35\u0c2e\u0c4d V\u0c0e\u0c19\u0c4d\u0c15\u0c24\u0c46\u0c36\u0c35 \u0c02\u0c05\u0c28\u0c4d\u0c28\u0c32\u0c2e\u0c4d <br/> . \u0c37\u0c4d\u0c30\u0c17\u0c4d\u0c2d\u0c41\u0c36\u0c2e\u0c4d\u0c2c\u0c30 \u0c39\u0c46\u0c25\u0c40\u0c28\u0c2e\u0c4d \u0c37\u0c41\u0c36\u0c2e\u0c35 \u0c39\u0c2e\u0c41\u0c30\u0c4d\u0c25\u0c2f\u0c46 <br/> \u0c37\u0c30\u0c4d\u0c35\u0c30\u0c4d\u0c25\u0c3f \u0c37\u0c2e\u0c28\u0c2f\u0c38\u0c4d\u0c25\u0c41 V\u0c0e\u0c19\u0c4d\u0c15\u0c24\u0c46\u0c36\u0c2f \u0c02\u0c05\u0c19\u0c4d\u0c17\u0c32\u0c2e\u0c4d <br/> . \u0c36\u0c4d\u0c30\u0c40 V\u0c10\u0c15\u0c41\u0c28\u0c4d\u0c24 \u0c35\u0c3f\u0c30\u0c15\u0c4d\u0c25\u0c2f \u0c37\u0c4d\u0c35\u0c2e\u0c3f P\u0c09\u0c36\u0c4d\u0c15\u0c30\u0c3f\u0c28\u0c40\u0c25\u0c24\u0c46 <br/> \u0c30\u0c2e\u0c2f \u0c30\u0c2e\u0c2e\u0c28\u0c2f V\u0c0e\u0c19\u0c4d\u0c15\u0c24\u0c46\u0c36\u0c2f \u0c02\u0c05\u0c19\u0c4d\u0c17\u0c32\u0c2e\u0c4d <br/> . \u0c37\u0c4d\u0c30\u0c3f\u0c2e\u0c26\u0c4d \u0c38\u0c41\u0c28\u0c4d\u0c26\u0c30 \u0c1c\u0c2e\u0c25\u0c4d\u0c30\u0c41 \u0c2e\u0c41\u0c28\u0c3f \u0c2e\u0c28\u0c38\u0c35\u0c38\u0c3f\u0c28\u0c46 <br/> \u0c37\u0c30\u0c4d\u0c35\u0c32\u0c4a\u0c15 \u0c23\u0c3f\u0c35\u0c38\u0c35 \u0c37\u0c4d\u0c30\u0c3f\u0c28\u0c3f\u0c35\u0c38\u0c2f \u0c02\u0c05\u0c19\u0c4d\u0c17\u0c32\u0c2e\u0c4d <br/> . \u0c02\u0c05\u0c19\u0c4d\u0c17\u0c32\u0c38\u0c38\u0c28 \u0c2a\u0c30\u0c48\u0c39\u0c4d \u0c2e\u0c26\u0c1b\u0c30\u0c4d\u0c2f P\u0c09\u0c30\u0c4a\u0c17\u0c2e\u0c48\u0c39\u0c4d <br/> \u0c37\u0c30\u0c4d\u0c35\u0c48\u0c38\u0c4d\u0c1b P\u0c09\u0c30\u0c4d\u0c35\u0c48 \u0c30\u0c1b\u0c30\u0c4d\u0c2f\u0c48\u0c39\u0c4d \u0c37\u0c25\u0c4d\u0c15\u0c4d\u0c30\u0c3f\u0c25\u0c2f\u0c38\u0c4d\u0c25\u0c41 \u0c02\u0c05\u0c19\u0c4d\u0c17\u0c32\u0c2e\u0c4d"}]}, {"L": "Kannada", "text": [{"M": "\u0caa\u0cb2\u0ccd\u0cb2\u0cb5\u0cbf", "T": "\u0cb7\u0ccd\u0cb0\u0cbf\u0caf\u0cb9\u0ccd \u0c95\u0ca8\u0ccd\u0ca5\u0caf \u0c95\u0cb2\u0ccd\u0caf\u0ca8 \u0ca8\u0cbf\u0ca7\u0caf\u0cc6 \u0ca8\u0cbf\u0ca7\u0caf\u0cc6\u0cb0\u0ccd\u0ca5\u0cbf\u0ca8\u0cae\u0ccd <br/> \u0cb6\u0ccd\u0cb0\u0cc0 V\u0c8e\u0c99\u0ccd\u0c95\u0ca4 \u0ca8\u0cbf\u0cb5\u0cb8\u0caf \u0cb7\u0ccd\u0cb0\u0cbf\u0ca8\u0cbf\u0cb5\u0cb8\u0caf \u0c82\u0c85\u0c99\u0ccd\u0c97\u0cb2\u0cae\u0ccd <br/> . \u0cb3\u0c95\u0ccd\u0cb6\u0ccd\u0cae\u0cbf \u0cb8\u0cb5\u0cbf\u0cad\u0ccd\u0cb0 \u0cae\u0cb2\u0cca\u0c95-\u0cb8\u0cc1\u0cad\u0ccd\u0cb0\u0cc1 \u0cb5\u0cbf\u0cad\u0ccd\u0cb0\u0cae \u0c9b\u0c95\u0ccd\u0cb6\u0cc1\u0cb6\u0cc6 <br/> C\u0cb9\u0c95\u0ccd\u0cb6\u0cc1\u0cb6\u0cc6 \u0cb7\u0cb0\u0ccd\u0cb5\u0cb2\u0cca\u0c97\u0ccd\u0c85\u0ca8\u0cae\u0ccd V\u0c8e\u0c99\u0ccd\u0c95\u0ca4\u0cc6\u0cb6\u0caf \u0c82\u0c85\u0c99\u0ccd\u0c97\u0cb2\u0cae\u0ccd <br/> . \u0cb6\u0ccd\u0cb0\u0cc0 V\u0c8e\u0c99\u0ccd\u0c95\u0ca4\u0ca6\u0ccd\u0cb0\u0cbf \u0cb7\u0ccd\u0cb0\u0cbf\u0c99\u0ccd\u0c97\u0c97\u0ccd\u0cb0-\u0c82\u0c85\u0c99\u0ccd\u0c97\u0cb2\u0cb9 \u0cad\u0cb0\u0ca8\u0c99\u0ccd\u0c98\u0ccd\u0cb0\u0caf\u0cc6 <br/> \u0c82\u0c85\u0c99\u0ccd\u0c97\u0cb2\u0ca8\u0cae\u0ccd \u0ca8\u0cbf\u0cb5\u0cb8\u0caf V\u0c8e\u0c99\u0ccd\u0c95\u0ca4\u0cc6\u0cb6\u0caf \u0c82\u0c85\u0c99\u0ccd\u0c97\u0cb2\u0cae\u0ccd <br/> . \u0cb7\u0cb0\u0ccd\u0cb5\u0cb5\u0caf\u0cb5 \u0cb8\u0ccc\u0ca8\u0ccd\u0ca6\u0cb0\u0ccd\u0caf \u0cb7\u0cae\u0ccd\u0caa\u0ca7 \u0cb7\u0cb0\u0ccd\u0cb5\u0c9b\u0cc6\u0ca5\u0cb8\u0cae\u0ccd <br/> \u0cb7\u0ca6 \u0cb8\u0cae\u0ccd\u0cae\u0cca\u0cb9\u0ca8\u0caf\u0cb8\u0ccd\u0ca5\u0cc1 V\u0c8e\u0c99\u0ccd\u0c95\u0ca4\u0cc6\u0cb6\u0caf \u0c82\u0c85\u0c99\u0ccd\u0c97\u0cb2\u0cae\u0ccd <br/> . \u0ca3\u0cbf\u0ca5\u0ccd\u0caf\u0caf \u0ca8\u0cbf\u0cb0\u0cb5\u0ca7\u0ccd\u0caf\u0caf \u0cb7\u0ca5\u0ccd\u0caf\u0ca8\u0ca8\u0ccd\u0ca6 \u0c9b\u0cbf\u0ca7\u0ca5\u0ccd\u0cae\u0ca8\u0cc6 <br/> \u0cb7\u0cb0\u0ccd\u0cb5\u0ca8\u0ccd\u0ca5\u0cb0\u0ca5\u0ccd \u0cae\u0ca8\u0cc6 \u0cb7\u0ccd\u0cb0\u0cbf\u0cae\u0ca6\u0ccd V\u0c8e\u0c99\u0ccd\u0c95\u0ca4\u0cc6\u0cb6\u0cb5 \u0c82\u0c85\u0c99\u0ccd\u0c97\u0cb2\u0cae\u0ccd <br/> . \u0cb7\u0ccd\u0cb5\u0ca5 \u0cb8\u0ccd\u0cb8\u0cb0\u0ccd\u0cb5\u0cb5\u0cbf\u0ca6\u0cc6 \u0cb8\u0cb0\u0ccd\u0cb5\u0cb8\u0c95\u0ccd\u0ca5\u0caf\u0cc6 \u0cb7\u0cb0\u0ccd\u0cb5\u0cb8\u0cc6\u0cb6\u0cbf\u0ca8\u0cc6 <br/> \u0cb7\u0cc1\u0cb2\u0cad\u0caf \u0cb7\u0cc1\u0cb8\u0cc0\u0cb2\u0caf V\u0c8e\u0c99\u0ccd\u0c95\u0ca4\u0cc6\u0cb6\u0caf \u0c82\u0c85\u0c99\u0ccd\u0c97\u0cb2\u0cae\u0ccd <br/> . P\u0c85\u0cb0\u0cb8\u0ccd\u0cae\u0cc8 B\u0cb0\u0cb9\u0ccd\u0cae\u0ca8\u0cc6 P\u0c8a\u0cb0\u0ccd\u0ca8 \u0c97\u0ccd\u0c85\u0cae\u0caf P\u0c85\u0cb0\u0cae\u0ca5\u0ccd\u0cae\u0ca8\u0cc6 <br/> P\u0cb0\u0caf\u0cc1\u0c9e\u0ccd\u0c9c\u0cc6 P\u0c85\u0cb0\u0ca5\u0ca5\u0ccd\u0cb5\u0caf V\u0c8e\u0c99\u0ccd\u0c95\u0ca4\u0cc6\u0cb6\u0caf \u0c82\u0c85\u0c99\u0ccd\u0c97\u0cb2\u0cae\u0ccd <br/> . \u0c86\u0c95\u0cb2\u0ca5\u0ca5\u0ccd\u0cb5 \u0cae\u0cb8\u0ccd\u0cb0\u0ca8\u0ccd\u0ca5 \u0cae\u0ca5\u0ccd\u0cae\u0ca8\u0cae\u0ca8\u0cc1-\u0caa\u0cb8\u0ccd\u0caf\u0ca5\u0cae\u0ccd <br/> \u0c86\u0ca5\u0ccd\u0cb0\u0cbf\u0caa\u0ccd\u0ca5\u0ccd\u0caf \u0cae\u0ccd\u0cb0\u0cbf\u0ca5 \u0cb0\u0cc1\u0caa\u0caf V\u0c8e\u0c99\u0ccd\u0c95\u0ca4\u0cc6\u0cb6\u0caf \u0c82\u0c85\u0c99\u0ccd\u0c97\u0cb2\u0cae\u0ccd <br/> . P\u0cb0\u0caf\u0cb9\u0ccd \u0cb8\u0ccd\u0cb5\u0c9b\u0cb0\u0ca8\u0cca\u0cae\u0ccd P\u0c89\u0cae\u0ccd\u0cb8\u0cae\u0ccd \u0cb7\u0cb0\u0ca8\u0ccd\u0caf\u0ca5\u0ccd\u0cb5\u0cc6\u0ca8 P\u0c85\u0ca8\u0cbf\u0ca8 <br/> \u0c97\u0ccd\u0cb0\u0cbf\u0caa\u0caf \u0ca7\u0cbf\u0cb8\u0ca5\u0cc6 \u0cb7\u0ccd\u0cb0\u0cbf\u0cae\u0ca6\u0ccd- V\u0c8e\u0c99\u0ccd\u0c95\u0ca4\u0cc6\u0cb6\u0cb5 \u0c82\u0c85\u0c99\u0ccd\u0c97\u0cb2\u0cae\u0ccd <br/> . \u0ca2\u0caf \u0cae\u0ccd\u0cb0\u0cbf\u0ca5 \u0ca5\u0cb0\u0c99\u0ccd\u0c97\u0cbf\u0ca8\u0ccd\u0caf-\u0cb7\u0ccd\u0ca5\u0cb0\u0c99\u0ccd\u0c97\u0cc8\u0cb0\u0cbf\u0cb5 \u0cb7\u0cc0\u0ca5\u0cb2\u0cc8\u0cb9\u0ccd <br/> \u0c86\u0caa\u0c97\u0cc8 \u0cb8\u0ccd\u0cb8\u0cbf\u0c9e\u0ccd\u0c9b\u0cb2\u0ccd\u0cb9\u0cc6 \u0cb5\u0cbf\u0cb8\u0ccd\u0cb5\u0cae\u0ccd V\u0c8e\u0c99\u0ccd\u0c95\u0ca4\u0cc6\u0cb6\u0cb5 \u0c82\u0c85\u0ca8\u0ccd\u0ca8\u0cb2\u0cae\u0ccd <br/> . \u0cb7\u0ccd\u0cb0\u0c97\u0ccd\u0cad\u0cc1\u0cb6\u0cae\u0ccd\u0cac\u0cb0 \u0cb9\u0cc6\u0ca5\u0cc0\u0ca8\u0cae\u0ccd \u0cb7\u0cc1\u0cb6\u0cae\u0cb5 \u0cb9\u0cae\u0cc1\u0cb0\u0ccd\u0ca5\u0caf\u0cc6 <br/> \u0cb7\u0cb0\u0ccd\u0cb5\u0cb0\u0ccd\u0ca5\u0cbf \u0cb7\u0cae\u0ca8\u0caf\u0cb8\u0ccd\u0ca5\u0cc1 V\u0c8e\u0c99\u0ccd\u0c95\u0ca4\u0cc6\u0cb6\u0caf \u0c82\u0c85\u0c99\u0ccd\u0c97\u0cb2\u0cae\u0ccd <br/> . \u0cb6\u0ccd\u0cb0\u0cc0 V\u0c90\u0c95\u0cc1\u0ca8\u0ccd\u0ca4 \u0cb5\u0cbf\u0cb0\u0c95\u0ccd\u0ca5\u0caf \u0cb7\u0ccd\u0cb5\u0cae\u0cbf P\u0c89\u0cb6\u0ccd\u0c95\u0cb0\u0cbf\u0ca8\u0cc0\u0ca5\u0ca4\u0cc6 <br/> \u0cb0\u0cae\u0caf \u0cb0\u0cae\u0cae\u0ca8\u0caf V\u0c8e\u0c99\u0ccd\u0c95\u0ca4\u0cc6\u0cb6\u0caf \u0c82\u0c85\u0c99\u0ccd\u0c97\u0cb2\u0cae\u0ccd <br/> . \u0cb7\u0ccd\u0cb0\u0cbf\u0cae\u0ca6\u0ccd \u0cb8\u0cc1\u0ca8\u0ccd\u0ca6\u0cb0 \u0c9c\u0cae\u0ca5\u0ccd\u0cb0\u0cc1 \u0cae\u0cc1\u0ca8\u0cbf \u0cae\u0ca8\u0cb8\u0cb5\u0cb8\u0cbf\u0ca8\u0cc6 <br/> \u0cb7\u0cb0\u0ccd\u0cb5\u0cb2\u0cca\u0c95 \u0ca3\u0cbf\u0cb5\u0cb8\u0cb5 \u0cb7\u0ccd\u0cb0\u0cbf\u0ca8\u0cbf\u0cb5\u0cb8\u0caf \u0c82\u0c85\u0c99\u0ccd\u0c97\u0cb2\u0cae\u0ccd <br/> . \u0c82\u0c85\u0c99\u0ccd\u0c97\u0cb2\u0cb8\u0cb8\u0ca8 \u0caa\u0cb0\u0cc8\u0cb9\u0ccd \u0cae\u0ca6\u0c9b\u0cb0\u0ccd\u0caf P\u0c89\u0cb0\u0cca\u0c97\u0cae\u0cc8\u0cb9\u0ccd <br/> \u0cb7\u0cb0\u0ccd\u0cb5\u0cc8\u0cb8\u0ccd\u0c9b P\u0c89\u0cb0\u0ccd\u0cb5\u0cc8 \u0cb0\u0c9b\u0cb0\u0ccd\u0caf\u0cc8\u0cb9\u0ccd \u0cb7\u0ca5\u0ccd\u0c95\u0ccd\u0cb0\u0cbf\u0ca5\u0caf\u0cb8\u0cc