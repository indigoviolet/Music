u0c28\u0c41\u0c24\u0c47 \u0c36\u0c41\u0c15 \u0c2a\u0c3e\u0c23\u0c3f \u0c35\u0c30 \u0c36\u0c47\u0c37 <br/> \u0c35\u0c47\u0c23\u0c3f \u0c32\u0c32\u0c3f\u0c24\u0c47 \u0c15\u0c32\u0c4d\u0c2f\u0c3e\u0c23\u0c3f \u0c38\u0c3e\u0c2e\u0c4d\u0c2c \u0c36\u0c3f\u0c35\u0c41\u0c28\u0c3f <br/> \u0c30\u0c3e\u0c23\u0c3f \u0c2e\u0c3e\u0c27\u0c41\u0c30\u0c4d\u0c2f \u0c35\u0c3e\u0c23\u0c3f \u0c28\u0c2e\u0c4d\u0c2e\u0c3f\u0c24\u0c3f <br/> \u0c2a\u0c42\u0c2c\u0c4b\u0c23\u0c3f \u0c24\u0c32\u0c4d\u0c32\u0c3f \u0c24\u0c4d\u0c30\u0c3f\u0c2a\u0c41\u0c30 (\u0c38\u0c41\u0c28\u0c4d\u0c26\u0c30\u0c3f)"}, {"M": "\u0c1a\u0c30\u0c23\u0c2e\u0c4d 3", "T": "\u0c35\u0c3e\u0c30\u0c4d(\u0c08)\u0c36 \u0c38\u0c4d\u0c24\u0c41\u0c24 \u0c17\u0c2e\u0c4d\u0c2d\u0c40\u0c30\u0c47 \u0c06\u0c26\u0c3f \u0c2a\u0c41\u0c30 <br/> \u0c35\u0c3f\u0c39\u0c3e\u0c30\u0c3f \u0c26\u0c40\u0c28 \u0c1c\u0c28\u0c4d(\u0c06)\u0c27\u0c3e\u0c30\u0c3f \u0c28\u0c17 \u0c30\u0c3e\u0c1c <br/> \u0c15\u0c41\u0c2e\u0c3e\u0c30\u0c3f \u0c26\u0c41\u0c37\u0c4d\u0c15\u0c30\u0c4d\u0c2e \u0c35\u0c3f\u0c26\u0c3e\u0c30\u0c3f \u0c24\u0c4d\u0c2f\u0c3e\u0c17\u0c30\u0c3e\u0c1c\u0c41 <br/> \u0c15\u0c4b\u0c30\u0c3f(\u0c2f\u0c4d)\u0c09\u0c28\u0c4d\u0c28 \u0c24\u0c4d\u0c30\u0c3f\u0c2a\u0c41\u0c30 (\u0c38\u0c41\u0c28\u0c4d\u0c26\u0c30\u0c3f)"}]}, {"L": "Kannada", "text": [{"M": "\u0caa\u0cb2\u0ccd\u0cb2\u0cb5\u0cbf", "T": "\u0cb8\u0cc1\u0ca8\u0ccd\u0ca6\u0cb0\u0cbf \u0ca8\u0ca8\u0ccd\u0ca8\u0ccd(\u0c87)\u0ca8\u0ccd\u0ca6\u0cb0\u0cbf\u0cb2\u0ccb <br/> \u0c9c\u0cc2\u0c9a\u0cbf \u0cac\u0ccd\u0cb0\u0ccb\u0cb5\u0ccd(\u0c85\u0cb5\u0ccd)\u0c85\u0cae\u0ccd\u0cae \u0ca4\u0ccd\u0cb0\u0cbf\u0caa\u0cc1\u0cb0 (\u0cb8\u0cc1\u0ca8\u0ccd\u0ca6\u0cb0\u0cbf)"}, {"M": "\u0c85\u0ca8\u0cc1\u0caa\u0cb2\u0ccd\u0cb2\u0cb5\u0cbf", "T": "\u0cb8\u0ca8\u0ccd\u0ca6\u0ca1\u0cbf(\u0caf\u0ccd)\u0c85\u0ca8\u0cbf \u0c87\u0ca8\u0ccd\u0ca6\u0cc1 \u0cae\u0cc1\u0c96\u0cbf <br/> \u0c9c\u0cbe\u0cb2\u0cae\u0cc1 \u0cb5\u0ca6\u0ccd\u0ca6\u0ccd(\u0c85)\u0cae\u0ccd\u0cae \u0ca4\u0ccd\u0cb0\u0cbf\u0caa\u0cc1\u0cb0 (\u0cb8\u0cc1\u0ca8\u0ccd\u0ca6\u0cb0\u0cbf)"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 1", "T": "\u0cac\u0cbe\u0cb2\u0cc7 \u0caa\u0cbe\u0cb2\u0cbf\u0ca4 \u0cb8\u0cc1\u0cb0 \u0c9c\u0cbe\u0cb2\u0cc7 \u0c97\u0cae\u0ca8 \u0c9c\u0cbf\u0ca4 <br/> \u0cae\u0cb0\u0cbe\u0cb2\u0cc7 \u0cb8\u0ccd\u0cb5\u0c95\u0cc3\u0ca4\u0ccd(\u0c86)\u0c96\u0cbf\u0cb2 \u0cb2\u0cc0\u0cb2\u0cc7 \u0ca4\u0cbf\u0cb2\u0c95\u0ccd(\u0c86)\u0ca8\u0ccd\u0c95\u0cbf\u0ca4 <br/> \u0cab\u0cbe\u0cb2\u0cc7 \u0ca8\u0cc0 \u0cad\u0c95\u0ccd\u0ca4\u0cbf\u0caf\u0cc1 \u0cae\u0cc7\u0cb2\u0cc7 \u0ca8\u0cc0 \u0ca6\u0caf <br/> \u0cb0\u0cbe\u0ca6\u0ccd(\u0c8f)\u0cb2\u0cc7 \u0ca4\u0cb2\u0ccd\u0cb2\u0cbf \u0ca4\u0ccd\u0cb0\u0cbf\u0caa\u0cc1\u0cb0 (\u0cb8\u0cc1\u0ca8\u0ccd\u0ca6\u0cb0\u0cbf)"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 2", "T": "\u0cb5\u0cbe\u0ca3\u0cbf \u0cb5\u0cbf\u0ca8\u0cc1\u0ca4\u0cc7 \u0cb6\u0cc1\u0c95 \u0caa\u0cbe\u0ca3\u0cbf \u0cb5\u0cb0 \u0cb6\u0cc7\u0cb7 <br/> \u0cb5\u0cc7\u0ca3\u0cbf \u0cb2\u0cb2\u0cbf\u0ca4\u0cc7 \u0c95\u0cb2\u0ccd\u0caf\u0cbe\u0ca3\u0cbf \u0cb8\u0cbe\u0cae\u0ccd\u0cac \u0cb6\u0cbf\u0cb5\u0cc1\u0ca8\u0cbf <br/> \u0cb0\u0cbe\u0ca3\u0cbf \u0cae\u0cbe\u0ca7\u0cc1\u0cb0\u0ccd\u0caf \u0cb5\u0cbe\u0ca3\u0cbf \u0ca8\u0cae\u0ccd\u0cae\u0cbf\u0ca4\u0cbf <br/> \u0caa\u0cc2\u0cac\u0ccb\u0ca3\u0cbf \u0ca4\u0cb2\u0ccd\u0cb2\u0cbf \u0ca4\u0ccd\u0cb0\u0cbf\u0caa\u0cc1\u0cb0 (\u0cb8\u0cc1\u0ca8\u0ccd\u0ca6\u0cb0\u0cbf)"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 3", "T": "\u0cb5\u0cbe\u0cb0\u0ccd(\u0c88)\u0cb6 \u0cb8\u0ccd\u0ca4\u0cc1\u0ca4 \u0c97\u0cae\u0ccd\u0cad\u0cc0\u0cb0\u0cc7 \u0c86\u0ca6\u0cbf \u0caa\u0cc1\u0cb0 <br/> \u0cb5\u0cbf\u0cb9\u0cbe\u0cb0\u0cbf \u0ca6\u0cc0\u0ca8 \u0c9c\u0ca8\u0ccd(\u0c86)\u0ca7\u0cbe\u0cb0\u0cbf \u0ca8\u0c97 \u0cb0\u0cbe\u0c9c <br/> \u0c95\u0cc1\u0cae\u0cbe\u0cb0\u0cbf \u0ca6\u0cc1\u0cb7\u0ccd\u0c95\u0cb0\u0ccd\u0cae \u0cb5\u0cbf\u0ca6\u0cbe\u0cb0\u0cbf \u0ca4\u0ccd\u0caf\u0cbe\u0c97\u0cb0\u0cbe\u0c9c\u0cc1 <br/> \u0c95\u0ccb\u0cb0\u0cbf(\u0caf\u0ccd)\u0c89\u0ca8\u0ccd\u0ca8 \u0ca4\u0ccd\u0cb0\u0cbf\u0caa\u0cc1\u0cb0 (\u0cb8\u0cc1\u0ca8\u0ccd\u0ca6\u0cb0\u0cbf)"}]}, {"L": "Malayalam", "text": [{"M": "\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d38\u0d41\u0d28\u0d4d\u0d26\u0d30\u0d3f \u0d28\u0d28\u0d4d\u0d28\u0d4d(\u0d07)\u0d28\u0d4d\u0d26\u0d30\u0d3f\u0d32\u0d4b <br/> \u0d1c\u0d42\u0d1a\u0d3f \u0d2c\u0d4d\u0d30\u0d4b\u0d35\u0d4d(\u0d05\u0d35\u0d4d)\u0d05\u0d2e\u0d4d\u0d2e \u0d24\u0d4d\u0d30\u0d3f\u0d2a\u0d41\u0d30 (\u0d38\u0d41\u0d28\u0d4d\u0d26\u0d30\u0d3f)"}, {"M": "\u0d05\u0d28\u0d41\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d38\u0d28\u0d4d\u0d26\u0d21\u0d3f(\u0d2f\u0d4d)\u0d05\u0d28\u0d3f \u0d07\u0d28\u0d4d\u0d26\u0d41 \u0d2e\u0d41\u0d16\u0d3f <br/> \u0d1c\u0d3e\u0d32\u0d2e\u0d41 \u0d35\u0d26\u0d4d\u0d26\u0d4d(\u0d05)\u0d2e\u0d4d\u0d2e \u0d24\u0d4d\u0d30\u0d3f\u0d2a\u0d41\u0d30 (\u0d38\u0d41\u0d28\u0d4d\u0d26\u0d30\u0d3f)"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 1", "T": "\u0d2c\u0d3e\u0d32\u0d47 \u0d2a\u0d3e\u0d32\u0d3f\u0d24 \u0d38\u0d41\u0d30 \u0d1c\u0d3e\u0d32\u0d47 \u0d17\u0d2e\u0d28 \u0d1c\u0d3f\u0d24 <br/> \u0d2e\u0d30\u0d3e\u0d32\u0d47 \u0d38\u0d4d\u0d35\u0d15\u0d43\u0d24\u0d4d(\u0d06)\u0d16\u0d3f\u0d32 \u0d32\u0d40\u0d32\u0d47 \u0d24\u0d3f\u0d32\u0d15\u0d4d(\u0d06)\u0d28\u0d4d\u0d15\u0d3f\u0d24 <br/> \u0d2b\u0d3e\u0d32\u0d47 \u0d28\u0d40 \u0d2d\u0d15\u0d4d\u0d24\u0d3f\u0d2f\u0d41 \u0d2e\u0d47\u0d32\u0d47 \u0d28\u0d40 \u0d26\u0d2f <br/> \u0d30\u0d3e\u0d26\u0d4d(\u0d0f)\u0d32\u0d47 \u0d24\u0d32\u0d4d\u0d32\u0d3f \u0d24\u0d4d\u0d30\u0d3f\u0d2a\u0d41\u0d30 (\u0d38\u0d41\u0d28\u0d4d\u0d26\u0d30\u0d3f)"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 2", "T": "\u0d35\u0d3e\u0d23\u0d3f \u0d35\u0d3f\u0d28\u0d41\u0d24\u0d47 \u0d36\u0d41\u0d15 \u0d2a\u0d3e\u0d23\u0d3f \u0d35\u0d30 \u0d36\u0d47\u0d37 <br/> \u0d35\u0d47\u0d23\u0d3f \u0d32\u0d32\u0d3f\u0d24\u0d47 \u0d15\u0d32\u0d4d\u0d2f\u0d3e\u0d23\u0d3f \u0d38\u0d3e\u0d2e\u0d4d\u0d2c \u0d36\u0d3f\u0d35\u0d41\u0d28\u0d3f <br/> \u0d30\u0d3e\u0d23\u0d3f \u0d2e\u0d3e\u0d27