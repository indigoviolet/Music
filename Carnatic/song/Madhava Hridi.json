kE madhukara (rAdhE) <br/> rAdhE rasayuta rAsha vilAsE hari smaraNa sukhavara prasAdE manOmudita lIlA vinOdE <br/> hariNA upa kUhita sanghrahItam api Sastra jagana rucira kanakavasanE mrdu vacanE (mAdhava)"}]}, {"L": "Tamil", "text": [{"M": "\u0baa\u0bb2\u0bcd\u0bb2\u0bb5\u0bbf", "T": "\u0bae\u0bbe\u0ba4\u0bb5 \u0bb9\u0bcd\u0bb0\u0bcd\u0ba4\u0bbf \u0b95\u0bc7\u0bb2\u0bbf\u0ba9\u0bbf \u0bae\u0ba4\u0bc1\u0bb0\u0bbf\u0baa\u0bc1 \u0b9a\u0bae\u0ba4\u0ba9 \u0bb5\u0ba4\u0ba9 \u0bae\u0ba4\u0bc1\u0baa\u0bc7 \u0b9c\u0baf"}, {"M": "\u0b85\u0ba9\u0bc1\u0baa\u0bb2\u0bcd\u0bb2\u0bb5\u0bbf", "T": "\u0bb5\u0bc0\u0ba4\u0bcb\u0baa\u0bae\u0bbe\u0ba9 \u0bb5\u0bc7\u0ba3\u0bc1\u0b95\u0bbe\u0ba9 \u0ba8\u0bbe\u0ba4 \u0b9a\u0bc1\u0bb2\u0baf \u0bb0\u0b9a\u0bbf\u0b95\u0bc7 \u0bb0\u0b9a\u0bbe\u0bb2\u0baf\u0bc7 <br/> \u0ba8\u0bbe\u0ba9\u0bbe\u0bb5\u0bbf\u0ba4 \u0baa\u0bc1\u0bb7\u0bcd\u0baa\u0bbf\u0ba4\u0bbe\u0b95\u0bcd\u0bb0 \u0b9a\u0bc1\u0b95\u0ba8\u0bcd\u0ba4 \u0bb2\u0ba4\u0bbe \u0ba8\u0bbf\u0b95\u0bc1\u0b9e\u0bcd\u0b9c \u0bae\u0ba8\u0bcd\u0ba4\u0bbf\u0bb0 \u0b9a\u0ba4\u0ba9\u0bc7"}, {"M": "\u0b9a\u0bb0\u0ba3\u0bae\u0bcd 1", "T": "\u0bb0\u0bbe\u0ba4\u0bc7 \u0bb0\u0b9a\u0baf\u0bc1\u0ba4 \u0bb0\u0bbe\u0b9a\u0bb5\u0bbf\u0bb2\u0bbe\u0b9a\u0bc7"}, {"M": "\u0b9a\u0bb0\u0ba3\u0bae\u0bcd 2", "T": "\u0bb6\u0bcd\u0bb0\u0bc0 \u0bb9\u0bb0\u0bbf \u0baa\u0bcd\u0bb0\u0bc7\u0bae \u0b85\u0b95\u0ba3\u0bcd\u0b9f \u0bae\u0ba3\u0bcd\u0b9f\u0bb2 \u0b9a\u0bbe\u0bae\u0bcd\u0bb0\u0bbe\u0b9c\u0bcd\u0baf\u0bbe \u0b85\u0ba4\u0baa\u0ba4\u0bc7 (\u0bb0\u0bbe\u0ba4\u0bc7)"}, {"M": "\u0b9a\u0bb0\u0ba3\u0bae\u0bcd 3", "T": "\u0b9a\u0baa\u0bcd\u0ba4 \u0bb5\u0bbf\u0bae\u0bcd\u0b9a\u0ba4\u0bbf \u0bae\u0bc1\u0b95\u0bcd\u0ba4\u0bbe \u0bae\u0bbe\u0bb2\u0bbf\u0b95 \u0b9a\u0bcb\u0baa\u0bbf\u0ba4 \u0b95\u0ba8\u0bcd\u0ba4\u0bb0\u0bc7 (\u0bb0\u0bbe\u0ba4\u0bc7)"}, {"M": "\u0b9a\u0bb0\u0ba3\u0bae\u0bcd 4", "T": "\u0ba8\u0bbf\u0ba8\u0bcd\u0ba4\u0bbf\u0ba4 \u0b9a\u0bbe\u0bb0\u0b9a \u0bb0\u0bbf\u0baa\u0bc1 \u0b95\u0bbf\u0bb0\u0ba3 \u0ba4\u0bb5\u0bb3 \u0bb0\u0ba4\u0ba9 \u0bb5\u0bbf\u0b95\u0b9a\u0bbf\u0ba4\u0bcb\u0ba4\u0bcd-\u0b9c\u0bcd\u0bb5\u0bb2\u0baf\u0bc1\u0ba4 \u0bae\u0ba9\u0b9a\u0bbf\u0b9c"}, {"M": "\u0b9a\u0bb0\u0ba3\u0bae\u0bcd 5", "T": "\u0ba8\u0b95\u0ba4\u0bb0 \u0b95\u0bcb\u0baa \u0bb5\u0ba4\u0bc2\u0b9c\u0ba9 \u0ba8\u0b9f\u0ba9\u0bbe\u0ba4\u0bcd\u0baa\u0bc1\u0ba4\u0b95\u0bae\u0bcd \u0baa\u0bcd\u0bb0\u0bb9\u0bbe\u0bb0 \u0b9a\u0bae\u0bbe\u0ba9 (\u0bb0\u0bbe\u0ba4\u0bc7) <br/> \u0b9a\u0bbe\u0bae\u0bc0\u0b95\u0bb0 \u0b9a\u0bb0\u0b9a\u0bbf\u0b9c \u0b95\u0bb0 \u0ba4\u0bb2 \u0bae\u0bcd\u0bb0\u0bcd\u0ba4\u0bc1 \u0ba4\u0bbe\u0bb3 \u0b95\u0bb2 \u0b95\u0bb2 \u0bb0\u0bb5\u0bae\u0ba3\u0bc0 \u0bb5\u0bb2\u0baf\u0bc7"}, {"M": "\u0b9a\u0bb0\u0ba3\u0bae\u0bcd 6", "T": "\u0b95\u0bb0\u0ba4\u0bb2 \u0b95\u0bae\u0bb2\u0bc7 \u0bb0\u0ba4\u0bbf \u0b9a\u0bae\u0baf\u0bc7 \u0b9c\u0bbf\u0ba4 \u0bae\u0bbe\u0ba4\u0bb5 \u0bae\u0ba3\u0bbf\u0bae\u0baf \u0b95\u0bc1\u0ba3\u0bcd\u0b9f\u0bb2 \u0b95\u0bc7\u0bb2\u0bbf\u0ba4 <br/> \u0b9a\u0bc1-\u0b95\u0bb0\u0bcd\u0ba3\u0bbf\u0b95\u0bc7 \u0baa\u0bcd\u0bb0\u0baa\u0bc0\u0ba4 \u0ba4\u0ba4\u0bcd \u0b9a\u0bc1-\u0baa\u0bbe\u0bb7\u0bbf\u0ba4 \u0bb6\u0bcd\u0bb0\u0bc1\u0ba4\u0bbf \u0baf\u0bc1\u0b95\u0bb3\u0bc7 \u0b9a\u0bb0\u0b9a\u0bb0 \u0b9a\u0bb0\u0b9a\u0ba9\u0bc7 (\u0bb0\u0bbe\u0ba4\u0bc7)"}, {"M": "\u0b9a\u0bb0\u0ba3\u0bae\u0bcd 7", "T": "\u0b9a\u0bae\u0ba4\u0bbf\u0b95 \u0ba8\u0bb5 \u0ba8\u0bb5 \u0bb5\u0bcd\u0bb0\u0b9c\u0ba4 \u0b95\u0bc1\u0ba3\u0bc0\u0b9c\u0ba9 \u0b95\u0bb2\u0bbe \u0b9a\u0bb2 \u0ba8\u0b9f\u0ba9 \u0b95\u0bcb\u0bb2\u0bbe\u0bb9\u0bb2 \u0b9a\u0bae\u0baf\u0bc7 <br/> \u0b95\u0bcd\u0bb0\u0bcd\u0ba4 \u0bb0\u0bc0\u0bb7\u0bbf\u0ba4 \u0bae\u0bbe\u0ba4\u0bb5 \u0b9a\u0bb9\u0bbf\u0ba4\u0bc7 \u0bae\u0bc1\u0ba9\u0bbf \u0bae\u0ba9\u0b9a\u0bbe\u0bae\u0baa\u0bbf \u0b95\u0bb2\u0bbf\u0ba4 \u0ba4\u0ba9\u0bcd\u0ba9\u0b9f\u0ba9 <br/> \u0ba8\u0bbf\u0bb0\u0bb5\u0ba4\u0bbf \u0b9a\u0bc1\u0b95\u0bbe\u0ba9\u0ba8\u0bcd\u0ba4\u0ba4 \u0ba8\u0bbf\u0bae\u0b95\u0bcd\u0ba9 \u0bb9\u0bcd\u0bb0\u0bcd\u0ba4\u0baf\u0bc7 \u0b9a\u0ba4\u0baf\u0bc7 \u0b85\u0ba4\u0bbf \u0b85\u0ba4\u0bcd\u0baa\u0bc1\u0ba4\u0bbe\u0ba9\u0b95 <br/> \u0b95\u0bc7\u0bb3\u0bc0 \u0bb5\u0bbf\u0bb2\u0bbe\u0b9a \u0b9a\u0ba4\u0bc1\u0bb0\u0bc7 \u0baa\u0bbe\u0bb5\u0bbf\u0ba4 \u0ba4\u0bcd\u0bb0\u0bbf\u0baa\u0bc1\u0bb5\u0ba9 \u0bae\u0ba4\u0bc1\u0bb0\u0b9a \u0bb0\u0b9a\u0bbf\u0b95\u0bc7 \u0bae\u0ba4\u0bc1\u0b95\u0bb0 (\u0bb0\u0bbe\u0ba4\u0bc7) <br/> \u0bb0\u0bbe\u0ba4\u0bc7 \u0bb0\u0b9a\u0baf\u0bc1\u0ba4 \u0bb0\u0bbe\u0bb7 \u0bb5\u0bbf\u0bb2\u0bbe\u0b9a\u0bc7 \u0bb9\u0bb0\u0bbf \u0bb8\u0bcd\u0bae\u0bb0\u0ba3 \u0b9a\u0bc1\u0b95\u0bb5\u0bb0 \u0baa\u0bcd\u0bb0\u0b9a\u0bbe\u0ba4\u0bc7 \u0bae\u0ba9\u0bcb\u0bae\u0bc1\u0ba4\u0bbf\u0ba4 \u0bb2\u0bc0\u0bb2\u0bbe \u0bb5\u0bbf\u0ba9\u0bcb\u0ba4\u0bc7 <br/> \u0bb9\u0bb0\u0bbf\u0ba3\u0bbe \u0b89\u0baa \u0b95\u0bc2\u0bb9\u0bbf\u0ba4 \u0b9a\u0b99\u0bcd\u0b95\u0bcd\u0bb0\u0bb9\u0bc0\u0ba4\u0bae\u0bcd \u0b85\u0baa\u0bbf \u0b9a\u0bb8\u0bcd\u0ba4\u0bcd\u0bb0 \u0b9c\u0b95\u0ba9 \u0bb0\u0bc1\u0b9a\u0bbf\u0bb0 \u0b95\u0ba9\u0b95\u0bb5\u0b9a\u0ba9\u0bc7 \u0bae\u0bcd\u0bb0\u0bcd\u0ba4\u0bc1 \u0bb5\u0b9a\u0ba9\u0bc7 (\u0bae\u0bbe\u0ba4\u0bb5)"}]}, {"L": "Telugu", "text": [{"M": "\u0c2a\u0c32\u0c4d\u0c32\u0c35\u0c3f", "T": "\u0c2e\u0c3e\u0c27\u0c35 \u0c39\u0c4d\u0c30\u0c4d\u0c26\u0c3f \u0c17\u0c47\u0c32\u0c3f\u0c28\u0c3f \u0c2e\u0c27\u0c41\u0c30\u0c3f\u0c2a\u0c41 \u0c38\u0c2e\u0c26\u0c28 \u0c35\u0c26\u0c28 \u0c2e\u0c27\u0c41\u0c2a\u0c47 \u0c1c\u0c2f"}, {"M": "\u0c05\u0c28\u0c41\u0c2a\u0c32\u0c4d\u0c32\u0c35\u0c3f", "T": "\u0c35\u0c40\u0c24\u0c4b\u0c2a\u0c2e\u0c3e\u0c28 \u0c35\u0c47\u0c23\u0c41\u0c17\u0c3e\u0c28 \u0c28\u0c3e\u0c26 \u0c38\u0c41\u0c32\u0c2f \u0c30\u0c38\u0c3f\u0c15\u0c47 \u0c30\u0c38\u0c3e\u0c32\u0c2f\u0c47 <br/> \u0c28\u0c3e\u0c28\u0c3e\u0c35\u0c3f\u0c27 \u0c2a\u0c41\u0c37\u0c4d\u0c2a\u0c3f\u0c24\u0c3e\u0c17\u0c4d\u0c30 \u0c38\u0c41\u0c17\u0c28\u0c4d\u0c27 \u0c32\u0c24\u0c3e \u0c28\u0c3f\u0c15\u0c41\u0c1e\u0c4d\u0c1c \u0c2e\u0c28\u0c4d\u0c26\u0c3f\u0c30 \u0c38\u0c26\u0c28\u0c47"}, {"M": "\u0c1a\u0c30\u0c23\u0c2e\u0c4d 1", "T": "\u0c30\u0c3e\u0c27\u0c47 \u0c30\u0c38\u0c2f\u0c41\u0c24 \u0c30\u0c3e\u0c38\u0c35\u0c3f\u0c32\u0c3e\u0c38\u0c47"}, {"M": "\u0c1a\u0c30\u0c23\u0c2e\u0c4d 2", "T": "\u0c36\u0c4d\u0c30\u0c40 \u0c39\u0c30\u0c3f \u0c2a\u0c4d\u0c30\u0c47\u0c2e \u0c05\u0c16\u0c23\u0c4d\u0c21 \u0c2e\u0c23\u0c4d\u0c21\u0c32 \u0c38\u0c3e\u0c2e\u0c4d\u0c30\u0c3e\u0c1c\u0c4d\u0c2f\u0c3e \u0c05\u0c24\u0c2a\u0c24\u0c47 (\u0c30\u0c3e\u0c27\u0c47)"}, {"M": "\u0c1a\u0c30\u0c23\u0c2e\u0c4d 3", "T": "\u0c38\u0c2a\u0c4d\u0c24 \u0c35\u0c3f\u0c2e\u0c4d\u0c36\u0c24\u0c3f \u0c2e\u0c41\u0c15\u0c4d\u0c24\u0c3e \u0c2e\u0c3e\u0c32\u0c3f\u0c15 \u0c36\u0c4b\u0c2d\u0c3f\u0c24 \u0c15\u0c28\u0c4d\u0c26\u0c30\u0c47 (\u0c30\u0c3e\u0c27\u0c47)"}, {"M": "\u0c1a\u0c30\u0c23\u0c2e\u0c4d 4", "T": "\u0c28\u0c3f\u0c28\u0c4d\u0c26\u0c3f\u0c24 \u0c38\u0c3e\u0c30\u0c38 \u0c30\u0c3f\u0c2a\u0c41 \u0c15\u0c3f\u0c30\u0c23 \u0c27\u0c35\u0c33 \u0c30\u0c26\u0c28 \u0c35\u0c3f\u0c15\u0c38\u0c3f\u0c24\u0c4b\u0c26\u0c4d-\u0c1c\u0c4d\u0c35\u0c32\u0c2f\u0c41\u0c24 \u0c2e\u0c28\u0c38\u0c3f\u0c1c"}, {"M": "\u0c1a\u0c30\u0c23\u0c2e\u0c4d 5", "T": "\u0c28\u0c17\u0c27\u0c30 \u0c17\u0c4b\u0c2a \u0c35\u0c27\u0c42\u0c1c\u0c28 \u0c28\u0c1f\u0c28\u0c3e\u0c26\u0c4d\u0c2d\u0c41\u0c24\u0c15\u0c2e\u0c4d \u0c2a\u0c4d\u0c30\u0c39\u0c3e\u0c30 \u0c38\u0c2e\u0c3e\u0c28 (\u0c30\u0c3e\u0c27\u0c47) <br/> \u0c36\u0c3e\u0c2e\u0c40\u0c15\u0c30 \u0c38\u0c30\u0c38\u0c3f\u0c1c \u0c15\u0c30 \u0c24\u0c32 \u0c2e\u0c4d\u0c30\u0c4d\u0c26\u0c41 \u0c24\u0c3e\u0c33 \u0c16\u0c32 \u0c16\u0c32 \u0c30\u0c35\u0c2e\u0c23\u0c40 \u0c35\u0c32\u0c2f\u0c47"}, {"M": "\u0c1a\u0c30\u0c23\u0c2e\u0c4d 6", "T": "\u0c15\u0c30\u0c24\u0c32 \u0c15\u0c2e\u0c32\u0c47 \u0c30\u0c24\u0c3f \u0c38\u0c2e\u0c2f\u0c47 \u0c1c\u0c3f\u0c24 \u0c2e\u0c3e\u0c27\u0c35 \u0c2e\u0c23\u0c3f\u0c2e\u0c2f \u0c15\u0c41\u0c23\u0c4d\u0c21\u0c32 \u0c16\u0c47\u0c32\u0c3f\u0c24 <br/> \u0c38\u0c41-\u0c15\u0c30\u0c4d\u0c23\u0c3f\u0c15\u0c47 \u0c2a\u0c4d\u0c30\u0c2d\u0c40\u0c24 \u0c24\u0c24\u0c4d \u0c38\u0c41-\u0c2d\u0c3e\u0c37\u0c3f\u0c24 \u0c36\u0c4d\u0c30\u0c41\u0c24\u0c3f \u0c2f\u0c41\u0c17\u0c33\u0c47 \u0c38\u0c30\u0c38\u0c30 \u0c38\u0c30\u0c38\u0c28\u0c47 (\u0c30\u0c3e\u0c27\u0c47)"}, {"M": "\u0c1a\u0c30\u0c23\u0c2e\u0c4d 7", "T": "\u0c38\u0c2e\u0c27\u0c3f\u0c15 \u0c28\u0c35 \u0c28\u0c35 \u0c35\u0c4d\u0c30\u0c1c\u0c24 \u0c17\u0c41\u0c23\u0c40\u0c1c\u0c28 \u0c15\u0c32\u0c3e \u0c1a\u0c32 \u0c28\u0c1f\u0c28 \u0c15\u0c4b\u0c32\u0c3e\u0c39\u0c32 \u0c38\u0c2e\u0c2f\u0c47 <br/> \u0c15\u0c4d\u0c30\u0c4d\u0c24 \u0c30\u0c40\u0c37\u0c3f\u0c24 \u0c2e\u0c3e\u0c27\u0c35 \u0c38\u0c39\u0c3f\u0c24\u0c47 \u0c2e\u0c41\u0c28\u0c3f \u0c2e\u0c28\u0c38\u0c3e\u0c2e\u0c2a\u0c3f \u0c15\u0c32\u0c3f\u0c24 \u0c24\u0c28\u0c4d\u0c28\u0c1f\u0c28 <br/> \u0c28\u0c3f\u0c30\u0c35\u0c27\u0c3f \u0c38\u0c41\u0c16\u0c3e\u0c28\u0c28\u0c4d\u0c26\u0c24 \u0c28\u0c3f\u0c2e\u0c18\u0c4d\u0c28 \u0c39\u0c4d\u0c30\u0c4d\u0c26\u0c2f\u0c47 \u0c38\u0c26\u0c2f\u0c47 \u0c05\u0c24\u0c3f \u0c05\u0c26\u0c4d\u0c2d\u0c41\u0c24\u0c3e\u0c28\u0c15 <br/> \u0c15\u0c47\u0c33\u0c40 \u0c35\u0c3f\u0c32\u0c3e\u0c38 \u0c1a\u0c24\u0c41\u0c30\u0c47 \u0c2d\u0c3e\u0c35\u0c3f\u0c24 \u0c24\u0c4d\u0c30\u0c3f\u0c2d\u0c41\u0c35\u0c28 \u0c2e\u0c27\u0c41\u0c30\u0c38 \u0c30\u0c38\u0c3f\u0c15\u0c47 \u0c2e\u0c27\u0c41\u0c15\u0c30 (\u0c30\u0c3e\u0c27\u0c47) <br/> \u0c30\u0c3e\u0c27\u0c47 \u0c30\u0c38\u0c2f\u0c41\u0c24 \u0c30\u0c3e\u0c37 \u0c35\u0c3f\u0c32\u0c3e\u0c38\u0c47 \u0c39\u0c30\u0c3f \u0c38\u0c4d\u0c2e\u0c30\u0c23 \u0c38\u0c41\u0c16\u0c35\u0c30 \u0c2a\u0c4d\u0c30\u0c38\u0c3e\u0c26\u0c47 \u0c2e\u0c28\u0c4b\u0c2e\u0c41\u0c26\u0c3f\u0c24 \u0c32\u0c40\u0c32\u0c3e \u0c35\u0c3f\u0c28\u0c4b\u0c26\u0c47 <br/> \u0c39\u0c30\u0c3f\u0c23\u0c3e \u0c09\u0c2a \u0c15\u0c42\u0c39\u0c3f\u0c24 \u0c38\u0c19\u0c4d\u0c18\u0c4d\u0c30\u0c39\u0c40\u0c24\u0c2e\u0c4d \u0c05\u0c2a\u0c3f \u0c36\u0c38\u0c4d\u0c24\u0c4d\u0c30 \u0c1c\u0c17\u0c28 \u0c30\u0c41\u0c1a\u0c3f\u0c30 \u0c15\u0c28\u0c15\u0c35\u0c38\u0c28\u0c47 \u0c2e\u0c4d\u0c30\u0c4d\u0c26\u0c41 \u0c35\u0c1a\u0c28\u0c47 (\u0c2e\u0c3e\u0c27\u0c35)"}]}, {"L": "Kannada", "text": [{"M": "\u0caa\u0cb2\u0ccd\u0cb2\u0cb5\u0cbf", "T": "\u0cae\u0cbe\u0ca7\u0cb5 \u0cb9\u0ccd\u0cb0\u0ccd\u0ca6\u0cbf \u0c97\u0cc7\u0cb2\u0cbf\u0ca8\u0cbf \u0cae\u0ca7\u0cc1\u0cb0\u0cbf\u0caa\u0cc1 \u0cb8\u0cae\u0ca6\u0ca8 \u0cb5\u0ca6\u0ca8 \u0cae\u0ca7\u0cc1\u0caa\u0cc7 \u0c9c\u0caf"}, {"M": "\u0c85\u0ca8\u0cc1\u0caa\u0cb2\u0ccd\u0cb2\u0cb5\u0cbf", "T": "\u0cb5\u0cc0\u0ca4\u0ccb\u0caa\u0cae\u0cbe\u0ca8 \u0cb5\u0cc7\u0ca3\u0cc1\u0c97\u0cbe\u0ca8 \u0ca8\u0cbe\u0ca6 \u0cb8\u0cc1\u0cb2\u0caf \u0cb0\u0cb8\u0cbf\u0c95\u0cc7 \u0cb0\u0cb8\u0cbe\u0cb2\u0caf\u0cc7 <br/> \u0ca8\u0cbe\u0ca8\u0cbe\u0cb5\u0cbf\u0ca7 \u0caa\u0cc1\u0cb7\u0ccd\u0caa\u0cbf\u0ca4\u0cbe\u0c97\u0ccd\u0cb0 \u0cb8\u0cc1\u0c97\u0ca8\u0ccd\u0ca7 \u0cb2\u0ca4\u0cbe \u0ca8\u0cbf\u0c95\u0cc1\u0c9e\u0ccd\u0c9c \u0cae\u0ca8\u0ccd\u0ca6\u0cbf\u0cb0 \u0cb8\u0ca6\u0ca8\u0cc7"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 1", "T": "\u0cb0\u0cbe\u0ca7\u0cc7 \u0cb0\u0cb8\u0caf\u0cc1\u0ca4 \u0cb0\u0cbe\u0cb8\u0cb5\u0cbf\u0cb2\u0cbe\u0cb8\u0cc7"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 2", "T": "\u0cb6\u0ccd\u0cb0\u0cc0 \u0cb9\u0cb0\u0cbf \u0caa\u0ccd\u0cb0\u0cc7\u0cae \u0c85\u0c96\u0ca3\u0ccd\u0ca1 \u0cae\u0ca3\u0ccd\u0ca1\u0cb2 \u0cb8\u0cbe\u0cae\u0ccd\u0cb0\u0cbe\u0c9c\u0ccd\u0caf\u0cbe \u0c85\u0ca4\u0caa\u0ca4\u0cc7 (\u0cb0\u0cbe\u0ca7\u0cc7)"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 3", "T": "\u0cb8\u0caa\u0ccd\u0ca4 \u0cb5\u0cbf\u0cae\u0ccd\u0cb6\u0ca4\u0cbf \u0cae\u0cc1\u0c95\u0ccd\u0ca4\u0cbe \u0cae\u0cbe\u0cb2\u0cbf\u0c95 \u0cb6\u0ccb\u0cad\u0cbf\u0ca4 \u0c95\u0ca8\u0ccd\u0ca6\u0cb0\u0cc7 (\u0cb0\u0cbe\u0ca7\u0cc7)"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 4", "T": "\u0ca8\u0cbf\u0ca8\u0ccd\u0ca6\u0cbf\u0ca4 \u0cb8\u0cbe\u0cb0\u0cb8 \u0cb0\u0cbf\u0caa\u0cc1 \u0c95\u0cbf\u0cb0\u0ca3 \u0ca7\u0cb5\u0cb3 \u0cb0\u0ca6\u0ca8 \u0cb5\u0cbf\u0c95\u0cb8\u0cbf\u0ca4\u0ccb\u0ca6\u0ccd-\u0c9c\u0ccd\u0cb5\u0cb2\u0caf\u0cc1\u0ca4 \u0cae\u0ca8\u0cb8\u0cbf\u0c9c"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 5", "T": "\u0ca8\u0c97\u0ca7\u0cb0 \u0c97\u0ccb\u0caa \u0cb5\u0ca7\u0cc2\u0c9c\u0ca8 \u0ca8\u0c9f\u0ca8\u0cbe\u0ca6\u0ccd\u0cad\u0cc1\u0ca4\u0c95\u0cae\u0ccd \u0caa\u0ccd\u0cb0\u0cb9\u0cbe\u0cb0 \u0cb8\u0cae\u0cbe\u0ca8 (\u0cb0\u0cbe\u0ca7\u0cc7) <br/> \u0cb6\u0cbe\u0cae\u0cc0\u0c95\u0cb0 \u0cb8\u0cb0\u0cb8\u0cbf\u0c9c \u0c95\u0cb0 \u0ca4\u0cb2 \u0cae\u0ccd\u0cb0\u0ccd\u0ca6\u0cc1 \u0ca4\u0cbe\u0cb3 \u0c96\u0cb2 \u0c96\u0cb2 \u0cb0\u0cb5\u0cae\u0ca3\u0cc0 \u0cb5\u0cb2\u0caf\u0cc7"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 6", "T": "\u0c95\u0cb0\u0ca4\u0cb2 \u0c95\u0cae\u0cb2\u0cc7 \u0cb0\u0ca4\u0cbf \u0cb8\u0cae\u0caf\u0cc7 \u0c9c\u0cbf\u0ca4 \u0cae\u0cbe\u0ca7\u0cb5 \u0cae\u0ca3\u0cbf\u0cae\u0caf \u0c95\u0cc1\u0ca3\u0ccd\u0ca1\u0cb2 \u0c96\u0cc7\u0cb2\u0cbf\u0ca4 <br/> \u0cb8\u0cc1-\u0c95\u0cb0\u0ccd\u0ca3\u0cbf\u0c95\u0cc7 \u0caa\u0ccd\u0cb0\u0cad\u0cc0\u0ca4 \u0ca4\u0ca4\u0ccd \u0cb8\u0cc1-\u0cad\u0cbe\u0cb7\u0cbf\u0ca4 \u0cb6\u0ccd\u0cb0\u0cc1\u0ca4\u0cbf \u0caf\u0cc1\u0c97\u0cb3\u0cc7 \u0cb8\u0cb0\u0cb8\u0cb0 \u0cb8\u0cb0\u0cb8\u0ca8\u0cc7 (\u0cb0\u0cbe\u0ca7\u0cc7)"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 7", "T": "\u0cb8\u0cae\u0ca7\u0cbf\u0c95 \u0ca8\u0cb5 \u0ca8\u0cb5 \u0cb5\u0ccd\u0cb0\u0c9c\u0ca4 \u0c97\u0cc1\u0ca3\u0cc0\u0c9c\u0ca8 \u0c95\u0cb2\u0cbe \u0c9a\u0cb2 \u0ca8\u0c9f\u0ca8 \u0c95\u0ccb\u0cb2\u0cbe\u0cb9\u0cb2 \u0cb8\u0cae\u0caf\u0cc7 <br/> \u0c95\u0ccd\u0cb0\u0ccd\u0ca4 \u0cb0\u0cc0\u0cb7\u0cbf\u0ca4 \u0cae\u0cbe\u0ca7\u0cb5 \u0cb8\u0cb9\u0cbf\u0ca4\u0cc7 \u0cae\u0cc1\u0ca8\u0cbf \u0cae\u0ca8\u0cb8\u0cbe\u0cae\u0caa\u0cbf \u0c95\u0cb2\u0cbf\u0ca4 \u0ca4\u0ca8\u0ccd\u0ca8\u0c9f\u0ca8 <br/> \u0ca8\u0cbf\u0cb0\u0cb5\u0ca7\u0cbf \u0cb8\u0cc1\u0c96\u0cbe\u0ca8\u0ca8\u0ccd\u0ca6\u0ca4 \u0ca8\u0cbf\u0cae\u0c98\u0ccd\u0ca8 \u0cb9\u0ccd\u0cb0\u0ccd\u0ca6\u0caf\u0cc7 \u0cb8\u0ca6\u0caf\u0cc7 \u0c85\u0ca4\u0cbf \u0c85\u0ca6\u0ccd\u0cad\u0cc1\u0ca4\u0cbe\u0ca8\u0c95 <br/> \u0c95\u0cc7\u0cb3\u0cc0 \u0cb5\u0cbf\u0cb2\u0cbe\u0cb8 \u0c9a\u0ca4\u0cc1\u0cb0\u0cc7 \u0cad\u0cbe\u0cb5\u0cbf\u0ca4 \u0ca4\u0ccd\u0cb0\u0cbf\u0cad\u0cc1\u0cb5\u0ca8 \u0cae\u0ca7\u0cc1\u0cb0\u0cb8 \u0cb0\u0cb8\u0cbf\u0c95\u0cc7 \u0cae\u0ca7\u0cc1\u0c95\u0cb0 (\u0cb0\u0cbe\u0ca7\u0cc7) <br/> \u0cb0\u0cbe\u0ca7\u0cc7 \u0cb0\u0cb8\u0caf\u0cc1\u0ca4 \u0cb0\u0cbe\u0cb7 \u0cb5\u0cbf\u0cb2\u0cbe\u0cb8\u0cc7 \u0cb9\u0cb0\u0cbf \u0cb8\u0ccd\u0cae\u0cb0\u0ca3 \u0cb8\u0cc1\u0c96\u0cb5\u0cb0 \u0caa\u0ccd\u0cb0\u0cb8\u0cbe\u0ca6\u0cc7 \u0cae\u0ca8\u0ccb\u0cae\u0cc1\u0ca6\u0cbf\u0ca4 \u0cb2\u0cc0\u0cb2\u0cbe \u0cb5\u0cbf\u0ca8\u0ccb\u0ca6\u0cc7 <br/> \u0cb9\u0cb0\u0cbf\u0ca3\u0cbe \u0c89\u0caa \u0c95\u0cc2\u0cb9\u0cbf\u0ca4 \u0cb8\u0c99\u0ccd\u0c98\u0ccd\u0cb0\u0cb9\u0cc0\u0ca4\u0cae\u0ccd \u0c85\u0caa\u0cbf \u0cb6\u0cb8\u0ccd\u0ca4\u0ccd\u0cb0 \u0c9c\u0c97\u0ca8 \u0cb0\u0cc1\u0c9a\u0cbf\u0cb0 \u0c95\u0ca8\u0c95\u0cb5\u0cb8\u0ca8\u0cc7 \u0cae\u0ccd\u0cb0\u0ccd\u0ca6\u0cc1 \u0cb5\u0c9a\u0ca8\u0cc7 (\u0cae\u0cbe\u0ca7\u0cb5)"}]}, {"L": "Malayalam", "text": [{"M": "\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d2e\u0d3e\u0d27\u0d35 \u0d39\u0d4d\u0d30\u0d4d\u0d26\u0d3f \u0d17\u0d47\u0d32\u0d3f\u0d28\u0d3f \u0d2e\u0d27\u0d41\u0d30\u0d3f\u0d2a\u0d41 \u0d38\u0d2e\u0d26\u0d28 \u0d35\u0d26\u0d28 \u0d2e\u0d27\u0d41\u0d2a\u0d47 \u0d1c\u0d2f"}, {"M": "\u0d05\u0d28\u0d41\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d35\u0d40\u0d24\u0d4b\u0d2a\u0d2e\u0d3e\u0d28 \u0d35\u0d47\u0d23\u0d41\u0d17\u0d3e\u0d28 \u0d28\u0d3e\u0d26 \u0d38\u0d41\u0d32\u0d2f \u0d30\u0d38\u0d3f\u0d15\u0d47 \u0d30\u0d38\u0d3e\u0d32\u0d2f\u0d47 <br/> \u0d28\u0d3e\u0d28\u0d3e\u0d35\u0d3f\u0d27 \u0d2a\u0d41\u0d37\u0d4d\u0d2a\u0d3f\u0d24\u0d3e\u0d17\u0d4d\u0d30 \u0d38\u0d41\u0d17\u0d28\u0d4d\u0d27 \u0d32\u0d24\u0d3e \u0d28\u0d3f\u0d15\u0d41\u0d1e\u0d4d\u0d1c \u0d2e\u0d28\u0d4d\u0d26\u0d3f\u0d30 \u0d38\u0d26\u0d28\u0d47"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 1", "T": "\u0d30\u0d3e\u0d27\u0d47 \u0d30\u0d38\u0d2f\u0d41\u0d24 \u0d30\u0d3e\u0d38\u0d