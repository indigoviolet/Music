f \u0ba4\u0bcd\u0bb0\u0bc1\u0bb9\u0bbf\u0ba3\u0b9c\u0bbe\u0baf\u0bc7 \u0baa\u0bb0\u0bae\u0bae\u0bc7\u0baf\u0bc6 \u0ba8\u0bbf\u0bb0\u0baa\u0bbe\u0baf\u0bc7 <br/> (\u0ba4\u0bc7\u0bb5\u0bbf)"}, {"M": "\u0b9a\u0bb0\u0ba3\u0bae\u0bcd 2", "T": "\u0b9a\u0b95\u0bb2 \u0b95\u0bb2\u0bbe\u0bb2\u0baf \u0b9a\u0bbe\u0bb0\u0ba4\u0bbe \u0bb9\u0bbf\u0bae\u0b95\u0bb0 \u0b9a\u0ba4\u0bcd\u0bb0\u0bcd\u0b9a \u0bb5\u0bbf\u0bb2\u0bbe\u0b9a\u0baf\u0bc1\u0ba4\u0bbe\u0ba9\u0ba9\u0bc7 <br/> \u0bae\u0b95\u0bb0 \u0bb5\u0bb0\u0bbe\u0b99\u0bcd\u0b95 \u0b9a\u0bb0\u0bbe\u0b9a\u0ba9 \u0bae\u0bcc\u0bb0\u0bcd\u0bb5\u0bbf \u0bae\u0bb9\u0bbf\u0ba4 \u0ba4\u0bb0\u0bbe\u0bb3\u0b95 \u0bae\u0bcb\u0bb9\u0ba9\u0bc7 <br/> \u0b9a\u0bc1\u0b95 \u0b9a\u0ba9\u0b95\u0bbe\u0ba4\u0bbf \u0bae\u0bc1\u0ba9\u0bc0\u0bb6\u0bcd\u0bb5\u0bb0 \u0bb5\u0bbf\u0bb0\u0b9a\u0bbf\u0ba4 \u0b9a\u0bc1\u0ba8\u0bcd\u0ba4\u0bb0 \u0baa\u0ba4\u0baf\u0bc1\u0b95 \u0baa\u0bc2\u0b9c\u0ba9\u0bc7 <br/> \u0bb5\u0bbf\u0b95\u0b9f \u0b9c\u0b9f\u0bbe\u0bb2\u0b9a\u0bbf\u0ba4\u0bc7\u0ba9\u0bcd\u0b9f\u0bc1 \u0b95\u0bb2\u0bbe\u0baa\u0bbf\u0ba9\u0bbf \u0bb5\u0bbf\u0bae\u0bb2 \u0b9a\u0bb0\u0bcb\u0b9c \u0bb5\u0bb0\u0bbe\u0b9a\u0ba9\u0bc7 <br/> \u0bb5\u0bbf\u0b95\u0b9a \u0b95\u0bae\u0bb2\u0baa\u0bbe\u0ba4\u0bc7 \u0bb5\u0bbf\u0bb9\u0ba4\u0baa\u0bc7\u0ba4\u0bc7 \u0ba8\u0bbf\u0bb9\u0ba4\u0b95\u0bc7\u0ba4\u0bc7 <br/> (\u0ba4\u0bc7\u0bb5\u0bbf)"}, {"M": "\u0b9a\u0bb0\u0ba3\u0bae\u0bcd 3", "T": "\u0b9c\u0bcd\u0baf\u0bcb\u0ba4\u0bbf\u0bb0\u0bcd\u0bae\u0baf \u0ba8\u0bb5\u0bb9\u0bbe\u0bb0\u0bbe\u0baf\u0bbf\u0ba4 \u0b9a\u0b9c\u0bcd\u0b9c\u0bcd\u0baf\u0bcb\u0ba4\u0bbf \u0b9a\u0ba4\u0ba8\u0bcd\u0ba4\u0bcd\u0bb0 \u0bb5\u0bbf\u0baa\u0bc2\u0bb7\u0bbf\u0ba4\u0bc7 <br/> \u0bae\u0bbe\u0ba4\u0bb0\u0bcd\u0bae\u0bc7\u0ba4\u0bc1\u0bb0 \u0bae\u0bc0\u0bae\u0bbe\u0bae\u0bcd\u0b9a\u0bbe\u0b95\u0bae\u0bbe\u0bae\u0bae\u0bcd \u0b9a\u0bbf\u0bb2\u0bbf\u0ba4\u0bcb\u0bb0\u0bc1\u0baf\u0bc1\u0b95\u0bbe\u0b9e\u0bcd\u0b9a\u0bbf\u0ba4\u0bc7 <br/> \u0b9a\u0bc7\u0ba4\u0bcb\u0bb9\u0bb0\u0ba4\u0bb0 \u0b9a\u0baa\u0bcd\u0ba4 \u0b95\u0bae\u0b95\u0bbe\u0b9e\u0bcd\u0b9a\u0bbf \u0bb2\u0ba4\u0bbf\u0b95\u0bbe \u0b95\u0bc1\u0ba9\u0b9a\u0bcb\u0baa\u0bbf\u0ba4\u0bc7 <br/> \u0baa\u0bbe\u0ba4\u0b95\u0bb9\u0bb0 \u0baa\u0ba4\u0bcd\u0bae\u0bbe\u0ba4\u0bbf \u0baa\u0bc1\u0bb0\u0bbe\u0ba3\u0baa\u0bbe\u0b9f\u0bbf\u0ba4\u0baa\u0ba3\u0bbf \u0bb5\u0bbf\u0bb0\u0b9c\u0bbf\u0ba4\u0bc7 <br/> \u0bb5\u0bc0\u0ba4\u0b9a\u0bae\u0bb2 \u0bb9\u0bcd\u0bb0\u0bcd\u0ba4\u0bcd\u0baf\u0bc7 \u0b95\u0bb2\u0bbf\u0ba4 \u0bb5\u0bbf\u0ba4\u0bcd\u0baf\u0bc7 \u0ba4\u0bcd\u0bb0\u0bbf\u0b9c\u0b95\u0ba4\u0bbe\u0ba4\u0bcd\u0baf\u0bc7 \u0ba8\u0bc0\u0bb0\u0bb5\u0bbe\u0ba4\u0bcd\u0baf\u0bc7 <br/> (\u0ba4\u0bc7\u0bb5\u0bbf)"}, {"M": "\u0b9a\u0bb0\u0ba3\u0bae\u0bcd 4", "T": "\u0b85\u0bb0\u0bc1\u0ba3 \u0ba4\u0bb0\u0bbe\u0ba4\u0bb0 \u0baa\u0bb0\u0bcd\u0bb2\u0bbf\u0b85 \u0b9a\u0ba4\u0ba4\u0bbf \u0bae\u0bcd\u0bb0\u0bcd\u0ba4\u0bc1\u0bb9 \u0b9a\u0bbf\u0ba4\u0ba4\u0bcd\u0baf\u0bc1\u0ba4\u0bc0\u0baa\u0b9f\u0bb2\u0bcb\u0b9c\u0bcd\u0bb5\u0bb2\u0bc7 <br/> \u0b9a\u0bb0\u0ba3 \u0b9a\u0bae\u0bbe\u0b95\u0ba4 \u0b9c\u0ba9 \u0baa\u0bb0\u0bbf\u0baa\u0bbe\u0bb2\u0ba9 \u0b9a\u0ba4\u0ba4\u0bcb\u0ba4\u0bcd\u0baf\u0ba4 \u0b95\u0bb0\u0bc1\u0ba3\u0bbe\u0b95\u0bc1\u0bb2\u0bc7 <br/> \u0baa\u0bb0\u0bbf\u0b9c\u0ba9 \u0bb5\u0bbf\u0bb0\u0b9a\u0bbf\u0ba4 \u0ba8\u0bb5 \u0bb0\u0ba4\u0bcd\u0bb0\u0bcb\u0ba4\u0bcd\u0b9a\u0bb5 \u0baa\u0bb0\u0bbf\u0ba4\u0bcb\u0bb7\u0bbf\u0ba4 \u0bb9\u0bcd\u0bb0\u0bcd\u0ba4\u0baf\u0bc7\u0bae\u0bb2\u0bc7 <br/> \u0b95\u0bc1\u0bb0\u0bc1 \u0b95\u0bbe\u0bb0\u0bc1\u0ba3\u0bcd\u0baf\u0bae\u0baf\u0bbf \u0baa\u0b9c\u0ba9\u0baa\u0bb0\u0bc7 \u0bae\u0baf\u0bbf \u0b95\u0bc1\u0ba8\u0bcd\u0ba4 \u0bae\u0bc1\u0b95\u0bc1\u0bb3\u0bb0\u0ba4\u0ba9\u0bc7\u0ba4\u0bc1\u0bb2\u0bc7 <br/> \u0baa\u0bc2\u0bb0\u0bbf \u0b9a\u0bc1\u0baa\u0b95\u0bb0\u0bbe\u0ba3\u0bbf \u0b85\u0b95\u0bb9 \u0bb0\u0bbe\u0ba3\u0bbf \u0ba8\u0bc1\u0ba4\u0bbf\u0baa\u0bb0\u0bbe\u0ba3\u0bbf \u0b95\u0bb0\u0bb5\u0bbe\u0ba3\u0bbf <br/> (\u0ba4\u0bc7\u0bb5\u0bbf)"}]}, {"L": "Telugu", "text": [{"M": "\u0c2a\u0c32\u0c4d\u0c32\u0c35\u0c3f", "T": "\u0c26\u0c47\u0c35\u0c3f \u0c1c\u0c17\u0c1c\u0c4d\u0c1c\u0c28\u0c48 \u0c26\u0c47\u0c35\u0c3f \u0c15\u0c4d\u0c30\u0c4d\u0c2a\u0c2f\u0c3e\u0c2e\u0c2e \u0c24\u0c3e\u0c35\u0c15 \u0c1a\u0c30\u0c23 \u0c2d\u0c15\u0c4d\u0c24\u0c3f\u0c2e\u0c4d <br/> (\u0c26\u0c47\u0c35\u0c3f)"}, {"M": "\u0c05\u0c28\u0c41\u0c2a\u0c32\u0c4d\u0c32\u0c35\u0c3f", "T": "\u0c26\u0c47\u0c35 \u0c2e\u0c15\u0c41\u0c1f\u0c2e\u0c23\u0c40 \u0c26\u0c47\u0c26\u0c3f\u0c2a\u0c4d\u0c2f\u0c2e\u0c3e\u0c28\u0c2a\u0c3e\u0c26\u0c47 \u0c15\u0c47\u0c35\u0c32\u0c3e\u0c28\u0c28\u0c4d\u0c26\u0c2a\u0c42\u0c30\u0c4d\u0c23\u0c47 \u0c15\u0c40\u0c30\u0c38\u0c41\u0c35\u0c3e\u0c23\u0c3f \u0c35\u0c3e\u0c23\u0c3f <br/> (\u0c26\u0c47\u0c35\u0c3f)"}, {"M": "\u0c1a\u0c30\u0c23\u0c2e\u0c4d 1", "T": "\u0c35\u0c40\u0c23\u0c3e \u0c2a\u0c41\u0c38\u0c4d\u0c24\u0c15\u0c30\u0c1e\u0c4d\u0c1c\u0c3f\u0c24 \u0c15\u0c30\u0c24\u0c32\u0c35\u0c3f\u0c39 \u0c38\u0c3f\u0c24\u0c15\u0c1e\u0c4d\u0c1c\u0c35\u0c30\u0c47\u0c2e\u0c4d\u0c2c\u0c3f\u0c15\u0c47 <br/> \u0c35\u0c47\u0c23\u0c40 \u0c2e\u0c1e\u0c4d\u0c1c\u0c3f\u0c2e\u0c2a\u0c41\u0c1e\u0c4d\u0c1c\u0c35\u0c3f \u0c28\u0c3f\u0c30\u0c4d\u0c1c\u0c3f\u0c24 \u0c35\u0c3f\u0c24\u0c24\u0c18\u0c28 \u0c18\u0c28\u0c2e\u0c3e\u0c32\u0c3f\u0c15\u0c47 <br/> \u0c0f\u0c23\u0c40\u0c2e\u0c26\u0c35\u0c3f\u0c28\u0c3f \u0c35\u0c3e\u0c30\u0c23 \u0c28\u0c3f\u0c2a\u0c41\u0c23 \u0c24\u0c3e\u0c30\u0c47\u0c15\u0c4d\u0c37\u0c23 \u0c39\u0c4d\u0c30\u0c4d\u0c24\u0c28 \u0c24\u0c2a\u0c3e\u0c24\u0c15\u0c47 <br/> \u0c36\u0c3e\u0c28\u0c4b\u0c32\u0c4d\u0c32\u0c40\u0c21 \u0c2e\u0c39\u0c3e\u0c2e\u0c23\u0c3f \u0c2d\u0c42\u0c37\u0c23 \u0c36\u0c3e\u0c32\u0c3f\u0c28\u0c3f \u0c2c\u0c41\u0c27\u0c1c\u0c28 \u0c2a\u0c3e\u0c32\u0c3f\u0c15\u0c47 <br/> \u0c2a\u0c4d\u0c30\u0c40\u0c23\u0c3f\u0c24\u0c2e\u0c41\u0c28\u0c3f \u0c26\u0c4d\u0c30\u0c41\u0c39\u0c3f\u0c23\u0c1c\u0c3e\u0c2f\u0c47 \u0c2a\u0c30\u0c2e\u0c2e\u0c47\u0c2f\u0c46 \u0c28\u0c3f\u0c30\u0c2a\u0c3e\u0c2f\u0c47 <br/> (\u0c26\u0c47\u0c35\u0c3f)"}, {"M": "\u0c1a\u0c30\u0c23\u0c2e\u0c4d 2", "T": "\u0c38\u0c15\u0c32 \u0c15\u0c32\u0c3e\u0c32\u0c2f \u0c36\u0c3e\u0c30\u0c26\u0c3e \u0c39\u0c3f\u0c2e\u0c15\u0c30 \u0c38\u0c26\u0c4d\u0c30\u0c4d\u0c36 \u0c35\u0c3f\u0c32\u0c3e\u0c38\u0c2f\u0c41\u0c24\u0c3e\u0c28\u0c28\u0c47 <br/> \u0c2e\u0c15\u0c30 \u0c35\u0c30\u0c3e\u0c19\u0c4d\u0c15 \u0c36\u0c30\u0c3e\u0c38\u0c28 \u0c2e\u0c4c\u0c30\u0c4d\u0c35\u0c3f \u0c2e\u0c39\u0c3f\u0c24 \u0c24\u0c30\u0c3e\u0c33\u0c15 \u0c2e\u0c4b\u0c39\u0c28\u0c47 <br/> \u0c36\u0c41\u0c15 \u0c38\u0c28\u0c15\u0c3e\u0c26\u0c3f \u0c2e\u0c41\u0c28\u0c40\u0c36\u0c4d\u0c35\u0c30 \u0c35\u0c3f\u0c30\u0c1a\u0c3f\u0c24 \u0c38\u0c41\u0c28\u0c4d\u0c26\u0c30 \u0c2a\u0c26\u0c2f\u0c41\u0c17 \u0c2a\u0c42\u0c1c\u0c28\u0c47 <br/> \u0c35\u0c3f\u0c15\u0c1f \u0c1c\u0c1f\u0c3e\u0c32\u0c38\u0c3f\u0c24\u0c47\u0c28\u0c4d\u0c21\u0c41 \u0c15\u0c32\u0c3e\u0c2a\u0c3f\u0c28\u0c3f \u0c35\u0c3f\u0c2e\u0c32 \u0c38\u0c30\u0c4b\u0c1c \u0c35\u0c30\u0c3e\u0c38\u0c28\u0c47 <br/> \u0c35\u0c3f\u0c15\u0c1a \u0c15\u0c2e\u0c32\u0c2a\u0c3e\u0c26\u0c47 \u0c35\u0c3f\u0c39\u0c24\u0c2d\u0c47\u0c26\u0c47 \u0c28\u0c3f\u0c39\u0c24\u0c16\u0c47\u0c26\u0c47 <br/> (\u0c26\u0c47\u0c35\u0c3f)"}, {"M": "\u0c1a\u0c30\u0c23\u0c2e\u0c4d 3", "T": "\u0c1c\u0c4d\u0c2f\u0c4b\u0c24\u0c3f\u0c30\u0c4d\u0c2e\u0c2f \u0c28\u0c35\u0c39\u0c3e\u0c30\u0c3e\u0c2f\u0c3f\u0c24 \u0c38\u0c1c\u0c4d\u0c1c\u0c4d\u0c2f\u0c4b\u0c24\u0c3f \u0c36\u0c24\u0c28\u0c4d\u0c24\u0c4d\u0c30 \u0c35\u0c3f\u0c2d\u0c42\u0c37\u0c3f\u0c24\u0c47 <br/> \u0c2e\u0c3e\u0c24\u0c30\u0c4d\u0c2e\u0c47\u0c26\u0c41\u0c30 \u0c2e\u0c40\u0c2e\u0c3e\u0c2e\u0c4d\u0c38\u0c3e\u0c17\u0c2e\u0c3e\u0c2e\u0c2e\u0c4d \u0c38\u0c3f\u0c32\u0c3f\u0c24\u0c4b\u0c30\u0c41\u0c2f\u0c41\u0c17\u0c3e\u0c1e\u0c4d\u0c1a\u0c3f\u0c24\u0c47 <br/> \u0c1a\u0c47\u0c24\u0c4b\u0c39\u0c30\u0c24\u0c30 \u0c36\u0c2c\u0c4d\u0c26 \u0c17\u0c2e\u0c15\u0c3e\u0c1e\u0c4d\u0c1a\u0c3f \u0c32\u0c24\u0c3f\u0c15\u0c3e \u0c17\u0c41\u0c28\u0c36\u0c4b\u0c2d\u0c3f\u0c24\u0c47 <br/> \u0c2a\u0c3e\u0c24\u0c15\u