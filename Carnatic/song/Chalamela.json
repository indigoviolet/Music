u0bb0 \u0b9a\u0bbe\u0bb9\u0bbf\u0ba4\u0bcd\u0baf\u0bae\u0bcd 3", "T": "\u0b85\u0ba9\u0bcd\u0b9f\u0bbe\u0ba3\u0bcd\u0b9f\u0bae\u0bc1\u0bb2\u0b95\u0bc1-\u0ba8\u0ba4\u0bbf\u0baa\u0ba4\u0bbf-\u0bb5\u0ba9\u0bbf \u0ba8\u0bbf\u0ba3\u0bcd\u0b9f \u0bae\u0bca\u0bb0\u0bb2\u0bbf\u0b9f\u0b95, \u0bb5\u0bbf\u0ba9\u0bc0-\u0bb5\u0bbf\u0ba9\u0b95 \u0baf\u0bc1\u0ba3\u0bcd\u0b9f <br/> \u0b9a\u0bb0\u0bbf-\u0baf\u0b95\u0bc1\u0ba9\u0bbe? \u0bae\u0ba4\u0ba9 \u0b9c\u0ba9\u0b95\u0bc1\u0ba3\u0bcd\u0b9f, \u0bb5\u0bb0\u0bae\u0bca\u0b9a\u0b95\u0bbf \u0b85\u0ba9\u0bc1\u0ba4\u0bbf\u0ba9\u0bae\u0bc1"}, {"M": "\u0bb8\u0bcd\u0bb5\u0bb0 \u0b9a\u0bbe\u0bb9\u0bbf\u0ba4\u0bcd\u0baf\u0bae\u0bcd 4", "T": "\u0b86 \u0ba4\u0bcd\u0bb0\u0bcc\u0baa\u0ba4\u0bbf\u0b95\u0bbf \u0bb5\u0bb2\u0bc1\u0bb5 \u0b9a\u0baa\u0bb2\u0bcb \u0ba4\u0bb0\u0bc1\u0b95\u0b95\u0ba9\u0bc1 \u0b92\u0b9a\u0b95\u0bbf\u0ba9 \u0bae\u0bb9\u0bbe \u0ba4\u0baf \u0b95\u0bb2 \u0ba8\u0bbf\u0ba9\u0bc1 <br/> \u0baa\u0bca\u0b95\u0b9f\u0ba9\u0bc1 \u0ba8\u0bbe \u0ba4\u0bb0\u0bae\u0b95\u0bc1\u0ba9\u0bbe, \u0b9a\u0bbf\u0bb0\u0bbf \u0bb5\u0bc6\u0bb2\u0baf\u0bc1 \u0bb0\u0b99\u0bcd\u0b95\u0baa\u0bc1\u0bb0\u0bbf\u0ba9\u0bbf \u0b8f\u0bb2\u0bbf\u0ba9 \u0baa\u0ba4\u0bbf, <br/> \u0ba8\u0bbe\u0b95\u0bb2 \u0bb0\u0b99\u0bcd\u0b95\u0ba9\u0bbf \u0b95\u0bca\u0bae\u0bb0\u0bc1\u0ba3\u0bcd\u0b9f, \u0ba4\u0bc6\u0bb2\u0bbf\u0b9a\u0bbf \u0ba4\u0bc6\u0bb2\u0bbf\u0baf\u0b95 \u0ba8\u0ba4\u0b9a\u0bbf\u0ba9 \u0baa\u0bb2\u0bc1 \u0ba8\u0b9f\u0ba4\u0bb2\u0ba9\u0bc1 <br/> \u0bae\u0ba4\u0bbf \u0ba4\u0bb2\u0bc1\u0baa \u0bb5\u0bb2\u0ba4\u0bc1, \u0bb5\u0bb2\u0ba4\u0bc1, \u0b9a\u0bb0\u0ba3\u0bc1 \u0bae\u0bc1\u0bb0\u0bb9\u0bb0\u0bbf"}]}, {"L": "Telugu", "text": [{"M": "\u0c2a\u0c32\u0c4d\u0c32\u0c35\u0c3f", "T": "\u0c1a\u0c32\u0c2e\u0c47\u0c32 \u0c1c\u0c47\u0c38\u0c47\u0c35\u0c2f\u0c4d\u0c2f \u0c1a\u0c15\u0c4d\u0c15\u0c28\u0c3f \u0c2e\u0c3e \u0c30\u0c19\u0c4d\u0c17\u0c2f\u0c4d\u0c2f |"}, {"M": "\u0c05\u0c28\u0c41\u0c2a\u0c32\u0c4d\u0c32\u0c35\u0c3f", "T": "\u0c15\u0c32 \u0c15\u0c3e\u0c32\u0c2e\u0c41 \u0c28\u0c40 \u0c2a\u0c3e\u0c26\u0c2e\u0c41\u0c32\u0c47 \u0c17\u0c24\u0c3f\u0c2f\u0c28\u0c3f \u0c28\u0c2e\u0c4d\u0c2e\u0c3f \u0c2f\u0c41\u0c23\u0c4d\u0c21\u0c17"}, {"M": "\u0c2e\u0c41\u0c15\u0c4d\u0c24\u0c3e\u0c2f\u0c3f \u0c38\u0c3e\u0c39\u0c3f\u0c24\u0c4d\u0c2f\u0c2e\u0c4d", "T": "\u0c15\u0c30\u0c3f \u0c2e\u0c4a\u0c30\u0c32\u0c3f\u0c21 \u0c35\u0c3f\u0c28\u0c3f, \u0c30\u0c2f\u0c2e\u0c41\u0c28 \u0c35\u0c46\u0c21\u0c32\u0c3f, \u0c2e\u0c15\u0c30\u0c3f\u0c28\u0c3f \u0c26\u0c41\u0c28\u0c3f\u0c2e\u0c3f, <br/> \u0c15\u0c30\u0c3f-\u0c35\u0c30\u0c26\u0c41-\u0c21\u0c28\u0c3f \u0c2c\u0c3f\u0c30\u0c41\u0c26\u0c41 \u0c27\u0c30\u0c28\u0c41 \u0c35\u0c46\u0c32\u0c38\u0c3f, \u0c38\u0c41\u0c30\u0c32\u0c41 \u0c2a\u0c4a\u0c17\u0c21 \u0c17\u0c32\u0c3f\u0c17\u0c3f\u0c28 <br/> \u0c2a\u0c24\u0c3f\u0c24 \u0c2a\u0c3e\u0c35\u0c28\u0c41\u0c21! \u0c28\u0c3e \u0c2e\u0c28\u0c35\u0c3f \u0c35\u0c3f\u0c28  ||\u0c1a||"}, {"M": "\u0c1a\u0c30\u0c23\u0c2e\u0c4d", "T": "\u0c28\u0c28\u0c4d\u0c28\u0c41 \u0c2c\u0c4d\u0c30\u0c4b\u0c35 \u0c28\u0c40\u0c15\u0c41  \u0c2d\u0c3e\u0c30\u0c2e\u0c3e"}, {"M": "\u0c38\u0c4d\u0c35\u0c30 \u0c38\u0c3e\u0c39\u0c3f\u0c24\u0c4d\u0c2f\u0c2e\u0c4d 1", "T": "\u0c07\u0c26\u0c47 \u0c2c\u0c3e\u0c17\u0c28\u0c3f \u0c0a\u0c30\u0c15\u0c3e\u0c28\u0c41 \u0c28\u0c40\u0c35\u0c41\u0c28\u0c4d\u0c28 \u0c28\u0c3f\u0c28\u0c41 \u0c28\u0c47 \u0c35\u0c3f\u0c21\u0c1a\u0c46\u0c26\u0c28\u0c3e"}, {"M": "\u0c38\u0c4d\u0c35\u0c30 \u0c38\u0c3e\u0c39\u0c3f\u0c24\u0c4d\u0c2f\u0c2e\u0c4d 2", "T": "\u0c28\u0c40\u0c26\u0c41 \u0c38\u0c4d\u0c2e\u0c30\u0c23 \u0c17\u0c3e\u0c15 \u0c35\u0c47\u0c30\u0c46 \u0c0e\u0c30\u0c41\u0c17\u0c28\u0c41, \u0c28\u0c40\u0c30\u0c1c \u0c28\u0c2f\u0c28, \u0c26\u0c28\u0c41\u0c1c \u0c2e\u0c26 \u0c39\u0c30\u0c23"}, {"M": "\u0c38\u0c4d\u0c35\u0c30 \u0c38\u0c3e\u0c39\u0c3f\u0c24\u0c4d\u0c2f\u0c2e\u0c4d 3", "T": "\u0c05\u0c28\u0c4d\u0c21\u0c3e\u0c23\u0c4d\u0c21\u0c2e\u0c41\u0c32\u0c15\u0c41-\u0c28\u0c27\u0c3f\u0c2a\u0c24\u0c3f-\u0c35\u0c28\u0c3f \u0c28\u0c3f\u0c23\u0c4d\u0c21 \u0c2e\u0c4a\u0c30\u0c32\u0c3f\u0c21\u0c17, \u0c35\u0c3f\u0c28\u0c40-\u0c35\u0c3f\u0c28\u0c15 \u0c2f\u0c41\u0c23\u0c4d\u0c21 <br/> \u0c38\u0c30\u0c3f-\u0c2f\u0c17\u0c41\u0c28\u0c3e? \u0c2e\u0c26\u0c28 \u0c1c\u0c28\u0c15\u0c41\u0c23\u0c4d\u0c21, \u0c35\u0c30\u0c2e\u0c4a\u0c38\u0c17\u0c3f \u0c05\u0c28\u0c41\u0c26\u0c3f\u0c28\u0c2e\u0c41"}, {"M": "\u0c38\u0c4d\u0c35\u0c30 \u0c38\u0c3e\u0c39\u0c3f\u0c24\u0c4d\u0c2f\u0c2e\u0c4d 4", "T": "\u0c06 \u0c26\u0c4d\u0c30\u0c4c\u0c2a\u0c26\u0c3f\u0c15\u0c3f \u0c35\u0c32\u0c41\u0c35 \u0c38\u0c2d\u0c32\u0c4b \u0c24\u0c30\u0c41\u0c17\u0c15\u0c28\u0c41 \u0c12\u0c38\u0c17\u0c3f\u0c28 \u0c2e\u0c39\u0c3e \u0c26\u0c2f \u0c17\u0c32 \u0c28\u0c3f\u0c28\u0c41 <br/> \u0c2a\u0c4a\u0c17\u0c21\u0c28\u0c41 \u0c28\u0c3e \u0c24\u0c30\u0c2e\u0c17\u0c41\u0c28\u0c3e, \u0c38\u0c3f\u0c30\u0c3f \u0c35\u0c46\u0c32\u0c2f\u0c41 \u0c30\u0c19\u0c4d\u0c17\u0c2a\u0c41\u0c30\u0c3f\u0c28\u0c3f \u0c0f\u0c32\u0c3f\u0c28 \u0c2a\u0c24\u0c3f, <br/> \u0c28\u0c3e\u0c17\u0c32 \u0c30\u0c19\u0c4d\u0c17\u0c28\u0c3f \u0c15\u0c4a\u0c2e\u0c30\u0c41\u0c23\u0c4d\u0c21, \u0c24\u0c46\u0c32\u0c3f\u0c38\u0c3f \u0c24\u0c46\u0c32\u0c3f\u0c2f\u0c15 \u0c28\u0c26\u0c1a\u0c3f\u0c28 \u0c2a\u0c32\u0c41 \u0c28\u0c21\u0c24\u0c32\u0c28\u0c41 <br/> \u0c2e\u0c26\u0c3f \u0c24\u0c32\u0c41\u0c2a \u0c35\u0c32\u0c26\u0c41, \u0c35\u0c32\u0c26\u0c41, \u0c36\u0c30\u0c23\u0c41 \u0c2e\u0c41\u0c30\u0c39\u0c30\u0c3f"}]}, {"L": "Kannada", "text": [{"M": "\u0caa\u0cb2\u0ccd\u0cb2\u0cb5\u0cbf", "T": "\u0c9a\u0cb2\u0cae\u0cc7\u0cb2 \u0c9c\u0cc7\u0cb8\u0cc7\u0cb5\u0caf\u0ccd\u0caf \u0c9a\u0c95\u0ccd\u0c95\u0ca8\u0cbf \u0cae\u0cbe \u0cb0\u0c99\u0ccd\u0c97\u0caf\u0ccd\u0caf |"}, {"M": "\u0c85\u0ca8\u0cc1\u0caa\u0cb2\u0ccd\u0cb2\u0cb5\u0cbf", "T": "\u0c95\u0cb2 \u0c95\u0cbe\u0cb2\u0cae\u0cc1 \u0ca8\u0cc0 \u0caa\u0cbe\u0ca6\u0cae\u0cc1\u0cb2\u0cc7 \u0c97\u0ca4\u0cbf\u0caf\u0ca8\u0cbf \u0ca8\u0cae\u0ccd\u0cae\u0cbf \u0caf\u0cc1\u0ca3\u0ccd\u0ca1\u0c97"}, {"M": "\u0cae\u0cc1\u0c95\u0ccd\u0ca4\u0cbe\u0caf\u0cbf \u0cb8\u0cbe\u0cb9\u0cbf\u0ca4\u0ccd\u0caf\u0cae\u0ccd", "T": "\u0c95\u0cb0\u0cbf \u0cae\u0cca\u0cb0\u0cb2\u0cbf\u0ca1 \u0cb5\u0cbf\u0ca8\u0cbf, \u0cb0\u0caf\u0cae\u0cc1\u0ca8 \u0cb5\u0cc6\u0ca1\u0cb2\u0cbf, \u0cae\u0c95\u0cb0\u0cbf\u0ca8\u0cbf \u0ca6\u0cc1\u0ca8\u0cbf\u0cae\u0cbf, <br/> \u0c95\u0cb0\u0cbf-\u0cb5\u0cb0\u0ca6\u0cc1-\u0ca1\u0ca8\u0cbf \u0cac\u0cbf\u0cb0\u0cc1\u0ca6\u0cc1 \u0ca7\u0cb0\u0ca8\u0cc1 \u0cb5\u0cc6\u0cb2\u0cb8\u0cbf, \u0cb8\u0cc1\u0cb0\u0cb2\u0cc1 \u0caa\u0cca\u0c97\u0ca1 \u0c97\u0cb2\u0cbf\u0c97\u0cbf\u0ca8 <br/> \u0caa\u0ca4\u0cbf\u0ca4 \u0caa\u0cbe\u0cb5\u0ca8\u0cc1\u0ca1! \u0ca8\u0cbe \u0cae\u0ca8\u0cb5\u0cbf \u0cb5\u0cbf\u0ca8  ||\u0c9a||"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd", "T": "\u0ca8\u0ca8\u0ccd\u0ca8\u0cc1 \u0cac\u0ccd\u0cb0\u0ccb\u0cb5 \u0ca8\u0cc0\u0c95\u0cc1  \u0cad\u0cbe\u0cb0\u0cae\u0cbe"}, {"M": "\u0cb8\u0ccd\u0cb5\u0cb0 \u0cb8\u0cbe\u0cb9\u0cbf\u0ca4\u0ccd\u0caf\u0cae\u0ccd 1", "T": "\u0c87\u0ca6\u0cc7 \u0cac\u0cbe\u0c97\u0ca8\u0cbf \u0c8a\u0cb0\u0c95\u0cbe\u0ca8\u0cc1 \u0ca8\u0cc0\u0cb5\u0cc1\u0ca8\u0ccd\u0ca8 \u0ca8\u0cbf\u0ca8\u0cc1 \u0ca8\u0cc7 \u0cb5\u0cbf\u0ca1\u0c9a\u0cc6\u0ca6\u0ca8\u0cbe"}, {"M": "\u0cb8\u0ccd\u0cb5\u0cb0 \u0cb8\u0cbe\u0cb9\u0cbf\u0ca4\u0ccd\u0caf\u0cae\u0ccd 2", "T": "\u0ca8\u0cc0\u0ca6\u0cc1 \u0cb8\u0ccd\u0cae\u0cb0\u0ca3 \u0c97\u0cbe\u0c95 \u0cb5\u0cc7\u0cb0\u0cc6 \u0c8e\u0cb0\u0cc1\u0c97\u0ca8\u0cc1, \u0ca8\u0cc0\u0cb0\u0c9c \u0ca8\u0caf\u0ca8, \u0ca6\u0ca8\u0cc1\u0c9c \u0cae\u0ca6 \u0cb9\u0cb0\u0ca3"}, {"M": "\u0cb8\u0ccd\u0cb5\u0cb0 \u0cb8\u0cbe\u0cb9\u0cbf\u0ca4\u0ccd\u0caf\u0cae\u0ccd 3", "T": "\u0c85\u0ca8\u0ccd\u0ca1\u0cbe\u0ca3\u0ccd\u0ca1\u0cae\u0cc1\u0cb2\u0c95\u0cc1-\u0ca8\u0ca7\u0cbf\u0caa\u0ca4\u0cbf-\u0cb5\u0ca8\u0cbf \u0ca8\u0cbf\u0ca3\u0ccd\u0ca1 \u0cae\u0cca\u0cb0\u0cb2\u0cbf\u0ca1\u0c97, \u0cb5\u0cbf\u0ca8\u0cc0-\u0cb5\u0cbf\u0ca8\u0c95 \u0caf\u0cc1\u0ca3\u0ccd\u0ca1 <br/> \u0cb8\u0cb0\u0cbf-\u0caf\u0c97\u0cc1\u0ca8\u0cbe? \u0cae\u0ca6\u0ca8 \u0c9c\u0ca8\u0c95\u0cc1\u0ca3\u0ccd\u0ca1, \u0cb5\u0cb0\u0cae\u0cca\u0cb8\u0c97\u0cbf \u0c85\u0ca8\u0cc1\u0ca6\u0cbf\u0ca8\u0cae\u0cc1"}, {"M": "\u0cb8\u0ccd\u0cb5\u0cb0 \u0cb8\u0cbe\u0cb9\u0cbf\u0ca4\u0ccd\u0caf\u0cae\u0ccd 4", "T": "\u0c86 \u0ca6\u0ccd\u0cb0\u0ccc\u0caa\u0ca6\u0cbf\u0c95\u0cbf \u0cb5\u0cb2\u0cc1\u0cb5 \u0cb8\u0cad\u0cb2\u0ccb \u0ca4\u0cb0\u0cc1\u0c97\u0c95\u0ca8\u0cc1 \u0c92\u0cb8\u0c97\u0cbf\u0ca8 \u0cae\u0cb9\u0cbe \u0ca6\u0caf \u0c97\u0cb2 \u0ca8\u0cbf\u0ca8\u0cc1 <br/> \u0caa\u0cca\u0c97\u0ca1\u0ca8\u0cc1 \u0ca8\u0cbe \u0ca4\u0cb0\u0cae\u0c97\u0cc1\u0ca8\u0cbe, \u0cb8\u0cbf\u0cb0\u0cbf \u0cb5\u0cc6\u0cb2\u0caf\u0cc1 \u0cb0\u0c99\u0ccd\u0c97\u0caa\u0cc1\u0cb0\u0cbf\u0ca8\u0cbf \u0c8f\u0cb2\u0cbf\u0ca8 \u0caa\u0ca4\u0cbf, <br/> \u0ca8\u0cbe\u0c97\u0cb2 \u0cb0\u0c99\u0ccd\u0c97\u0ca8\u0cbf \u0c95\u0cca\u0cae\u0cb0\u0cc1\u0ca3\u0ccd\u0ca1, \u0ca4\u0cc6\u0cb2\u0cbf\u0cb8\u0cbf \u0ca4\u0cc6\u0cb2\u0cbf\u0caf\u0c95 \u0ca8\u0ca6\u0c9a\u0cbf\u0ca8 \u0caa\u0cb2\u0cc1 \u0ca8\u0ca1\u0ca4\u0cb2\u0ca8\u0cc1 <br/> \u0cae\u0ca6\u0cbf \u0ca4\u0cb2\u0cc1\u0caa \u0cb5\u0cb2\u0ca6\u0cc1, \u0cb5\u0cb2\u0ca6\u0cc1, \u0cb6\u0cb0\u0ca3\u0cc1 \u0cae\u0cc1\u0cb0\u0cb9\u0cb0\u0cbf"}]}, {"L": "Malayalam", "text": [{"M": "\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d1a\u0d32\u0d2e\u0d47\u0d32 \u0d1c\u0d47\u0d38\u0d47\u0d35\u0d2f\u0d4d\u0d2f \u0d1a\u0d15\u0d4d\u0d15\u0d28\u0d3f \u0d2e\u0d3e \u0d30\u0d19\u0d4d\u0d17\u0d2f\u0d4d\u0d2f |"}, {"M": "\u0d05\u0d28\u0d41\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d15\u0d32 \u0d15\u0d3e\u0d32\u0d2e\u0d41 \u0d28\u0d40 \u0d2a\u0d3e\u0d26\u0d2e\u0d41\u0d32\u0d47 \u0d17\u0d24\u0d3f\u0d2f\u0d28\u0d3f \u0d28\u0d2e\u0d4d\u0d2e\u0d3f \u0d2f\u0d41\u0d23\u0d4d\u0d21\u0d17"}, {"M": "\u0d2e\u0d41\u0d15\u0d4d\u0d24\u0d3e\u0d2f\u0d3f \u0d38\u0d3e\u0d39\u0d3f\u0d24\u0d4d\u0d2f\u0d2e\u0d4d", "T": "\u0d15\u0d30\u0d3f \u0d2e\u0d4a\u0d30\u0d32\u0d3f\u0d21 \u0d35\u0d3f\u0d28\u0d3f, \u0d30\u0d2f\u0d2e\u0d41\u0d28 \u0d35\u0d46\u0d21\u0d32\u0d3f, \u0d2e\u0d15\u0d30\u0d3f\u0d28\u0d3f \u0d26\u0d41\u0d28\u0d3f\u0d2e\u0d3f, <br/> \u0d15\u0d30\u0d3f-\u0d35\u0d30\u0d26\u0d41-\u0d21\u0d28\u0d3f \u0d2c\u0d3f\u0d30\u0d41\u0d26\u0d41 \u0d27\u0d30\u0d28\u0d41 \u0d35\u0d46\u0d32\u0d38\u0d3f, \u0d38\u0d41\u0d30\u0d32\u0d41 \u0d2a\u0d4a\u0d17\u0d21 \u0d17\u0d32\u0d3f\u0d17\u0d3f\u0d28 <br/> \u0d2a\u0d24\u0d3f\u0d24 \u0d2a\u0d3e\u0d35\u0d28\u0d41\u0d21! \u0d28\u0d3e \u0d2e\u0d28\u0d35\u0d3f \u0d35\u0d3f\u0d28  ||\u0d1a||"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d", "T": "\u0d28\u0d28\u0d4d\u0d28\u0d41 \u0d2c\u0d4d\u0d30\u0d4b\u0d35 \u0d28\u0d40\u0d15\u0d41  \u0d2d\u0d3e\u0d30\u0d2e\u0d3e"}, {"M": "\u0d38\u0d4d\u0d35\u0d30 \u0d38\u0d3e\u0d39\u0d3f\u0d24\u0d4d\u0d2f\u0d2e\u0d4d 1", "T": "\u0d07\u0d26\u0d47 \u0d2c\u0d3e\u0d17\u0d28\u0d3f \u0d0a\u0d30\u0d15\u0d3e\u0d28\u0d41 \u0d28\u0d40\u0d35\u0d41\u0d28\u0d4d\u0d28 \u0d28\u0d3f\u0d28\u0d41 \u0d28\u0d47 \u0d35\u0d3f\u0d21\u0d1a\u0d46\u0d26\u0d28\u0d3e"}, {"M": "\u0d38\u0d4d\u0d35\u0d30 \u0d38\u0d3e\u0d39\u0d3f\u0d24\u0d4d\u0d2f\u0d2e\u0d4d 2", "T": "\u0d28\u0d40\u0d26\u0d41 \u0d38\u0d4d\u0d2e\u0d30\u0d23 \u0d17\u0d3e\u0d15 \u0d35\u0d47\u0d30\u0d46 \u0d0e\u0d30\u0d41\u0d17\u0d28\u0d41, \u0d28\u0d40\u0d30\u0d1c \u0d28\u0d2f\u0d28, \u0d26\u0d28\u0d41\u0d1c \u0d2e\u0d26 \u0d39\u0d30\u0d23"}, {"M": "\u0d38\u0d4d\u0d35\u0d30 \u0d38\u0d3e\u0d39\u0d3f\u0d24\u0d4d\u0d2f\u0d2e\u0d4d 3", "T": "\u0d05\u0d28\u0d4d\u0d21\u0d3e\u0d23\u0d4d\u0d21\u0d2e\u0d41\u0d32\u0d15\u0d41-\u0d28\u0d27\u0d3f\u0d2a\u0d24\u0d3f-\u0d35\u0d28\u0d3f \u0d28\u0d3f\u0d23\u0d4d\u0d21 \u0d2e\u0d4a\u0d30\u0d32\u0d3f\u0d21\u0d17, \u0d35\u0d3f\u0d28\u0d40-\u0d35\u0d3f\u0d28\u0d15 \u0d2f\u0d41\u0d23\u0d4d\u0d21 <br/> \u0d38\u0d30\u0d3f-\u0d2f\u0d17\u0d41\u0d28\u0d3e? \u0d2e\u0d26\u0d28 \u0d1c\u0d28\u0d15\u0d41\u0d23\u0d4d\u0d21, \u0d35\u0d30\u0d2e\u0d4a\u0d38\u0d17\u0d3f \u0d05\u0d28\u0d41\u0d26\u0d3f\u0d28\u0d2e\u0d41"}, {"M": "\u0d38\u0d4d\u0d35\u0d30 \u0d38\u0d3e\u0d39\u0d3f\u0d24\u0d4d\u0d2f\u0d2e\u0d4d 4", "T": "\u0d06 \u0d26\u0d4d\u0d30\u0d4c\u0d2a\u0d26\u0d3f\u0d15\u0d3f \u0d35\u0d32\u0d41\u0d35 \u0d38\u0d2d\u0d32\u0d4b \u0d24\u0d30\u0d41\u0d17\u0d15\u0d28\u0d41 \u0d12\u0d38\u0d17\u0d3f\u0d28 \u0d2e\u0d39\u0d3e \u0d26\u0d2f \u0d17\u0d32 \u0d28\u0d3f\u0d28\u0d41 <br/> \u0d2a\u0d4a\u0d17\u0d21\u0d28\u0d41 \u0d28\u0d3e \u0d24\u0d30\u0d2e\u0d17\u0d41\u0d28\u0d3e, \u0d38\u0d3f\u0d30\u0d3f \u0d35\u0d46\u0d32\u0d2f\u0d41 \u0d30\u0d19\u0d4d\u0d17\u0d2a\u0d41\u0d30\u0d3f\u0d28\u0d3f \u0d0f\u0d32\u0d3f\u0d28 \u0d2a\u0d24\u0d3f, <br/> \u0d28\u0d3e\u0d17\u0d32 \u0d30\u0d19\u0d4d\u0d17\u0d28\u0d3f \u0d15\u0d4a\u0d2e\u0d30\u0d41\u0d23\u0d4d\u0d21, \u0d24\u0d46\u0d32\u0d3f\u0d38\u0d3f \u0d24\u0d46\u0d32\u0d3f\u0d2f\u0d15 \u0d28\u0d26\u0d1a\u0d3f\u0d28 \u0d2a\u0d32\u0d41 \u0d28\u0d21\u0d24\u0d32\u0d28\u0d41 <br/> \u0d2e\u0d26\u0d3f \u0d24\u0d32\u0d41\u0d2a \u0d35\u0d32\u0d26\u0d41, \u0d35\u0d32\u0d26\u0d41, \u0d36\u0d30\u0d23\u0d41 \u0d2e\u0d41\u0d30\u0d39\u0d30\u0d3f"}]}, {"L": "Sanskrit", "text": [{"M": "\u092a\u0932\u094d\u0932\u0935\u093f", "T": "\u091a\u0932\u092e\u0947\u0932 \u091c\u0947\u0938\u0947\u0935\u092f\u094d\u092f \u091a\u0915\u094d\u0915\u0928\u093f \u092e\u093e \u0930\u0919\u094d\u0917\u092f\u094d\u092f |"}, {"M": "\u0905\u0928\u0941\u092a\u0932\u094d\u0932\u0935\u093f", "T": "\u0915\u0932 \u0915\u093e\u0932\u092e\u0941 \u0928\u0940 \u092a\u093e\u0926\u092e\u0941\u0932\u0947 \u0917\u0924\u093f\u092f\u0928\u093f \u0928\u092e\u094d\u092e\u093f \u092f\u0941\u0923\u094d\u0921\u0917"}, {"M": "\u092e\u0941\u0915\u094d\u0924\u093e\u092f\u093f \u0938\u093e\u0939\u093f\u0924\u094d\u092f\u092e\u094d", "T": "\u0915\u0930\u093f \u092e\u094b\u0930\u0932\u093f\u0921 \u0935\u093f\u0928\u093f, \u0930\u092f\u092e\u0941\u0928 \u0935\u0947\u0921\u0932\u093f, \u092e\u0915\u0930\u093f\u0928\u093f \u0926\u0941\u0928\u093f\u092e\u093f, <br/> \u0915\u0930\u093f-\u0935\u0930\u0926\u0941-\u0921\u0928\u093f \u092c\u093f\u0930\u0941\u0926\u0941 \u0927\u0930\u0928\u0941 \u0935\u0947\u0932\u0938\u093f, \u0938\u0941\u0930\u0932\u0941 \u092a\u094b\u0917\u0921 \u0917\u0932\u093f\u0917\u093f\u0928 <br/> \u092a\u0924\u093f\u0924 \u092a\u093e\u0935\u0928\u0941\u0921! \u0928\u093e \u092e\u0928\u0935\u093f \u0935\u093f\u0928  ||\u091a||"}, {"M": "\u091a\u0930\u0923\u092e\u094d", "T": "\u0928\u0928\u094d\u0928\u0941 \u092c\u094d\u0930\u094b\u0935 \u0928\u0940\u0915\u0941  \u092d\u093e\u0930\u092e\u093e"}, {"M": "\u0938\u094d\u0935\u0930 \u0938\u093e\u0939\u093f\u0924\u094d\u092f\u092e\u094d 1", "T": "\u0907\u0926\u0947 \u092c\u093e\u0917\u0928\u093f \u090a\u0930\u0915\u093e\u0928\u0941 \u0928\u0940\u0935\u0941\u0928\u094d\u0928 \u0928\u093f\u0928\u0941 \u0928\u0947 \u0935\u093f\u0921\u091a\u0947\u0926\u0928\u093e"}, {"M": "\u0938\u094d\u0935\u0930 \u0938\u093e\u0939\u093f\u0924\u094d\u092f\u092e\u094d 2", "T": "\u0928\u0940\u0926\u0941 \u0938\u094d\u092e\u0930\u0923 \u0917\u093e\u0915 \u0935\u0947\u0930\u0947 \u090f\u0930\u0941\u0917\u0928\u0941, \u0928\u0940\u0930\u091c \u0928\u092f\u0928, \u0926\u0928\u0941\u091c \u092e\u0926 \u0939\u0930\u0923"}, {"M": "\u0938\u094d\u0935\u0930 \u0938\u093e\u0939\u093f\u0924\u094d\u092f\u092e\u094d 3", "T": "\u0905\u0928\u094d\u0921\u093e\u0923\u094d\u0921\u092e\u0941\u0932\u0915\u0941-\u0928\u0927\u093f\u092a\u0924\u093f-\u0935\u0928\u093f \u0928\u093f\u0923\u094d\u0921 \u092e\u094b\u0930\u0932\u093f\u0921\u0917, \u0935\u093f\u0928\u0940-\u0935\u093f\u0928\u0915 \u092f\u0941\u0923\u094d\u0921 <br/> \u0938\u0930\u093f-\u092f\u0917\u0941\u0928\u093e? \u092e\u0926\u0928 \u091c\u0928\u0915\u0941\u0923\u094d\u0921, \u0935\u0930\u092e\u094b\u0938\u0917\u093f \u0905\u0928\u0941\u0926\u093f\u0928\u092e\u0941"}, {"M": "\u0938\u094d\u0935\u0930 \u0938\u093e\u0939\u093f\u0924\u094d\u092f\u092e\u094d 4", "T": "\u0906 \u0926\u094d\u0930\u094c\u092a\u0926\u093f\u0915\u093f \u0935\u0932\u0941\u0935 \u0938\u092d\u0932\u094b \u0924\u0930\u0941\u0917\u0915\u0928\u0941 \u0913\u0938\u0917\u093f\u0928 \u092e\u0939\u093e \u0926\u092f \u0917\u0932 \u0928\u093f\u0928\u0941 <br/> \u092a\u094b\u0917