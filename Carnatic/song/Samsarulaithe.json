\u0b89\u0ba3\u0bcd\u0b9f\u0b95"}, {"M": "\u0b85\u0ba9\u0bc1\u0baa\u0bb2\u0bcd\u0bb2\u0bb5\u0bbf", "T": "\u0bb9\u0bbf\u0bae\u0bcd\u0bb8\u0bcd(\u0b86)\u0ba4\u0bc1\u0bb2\u0bcd(\u0b8e)\u0bb2\u0bcd\u0bb2 \u0bb0\u0bcb\u0b9a\u0bbf \u0bb9\u0bae\u0bcd\u0b9a\u0bbe\u0ba4\u0bc1\u0bb2 \u0b95\u0bc2\u0b9f\u0bbf <br/> \u0baa\u0bcd\u0bb0\u0b9a\u0bae\u0bcd\u0b9a \u0b9c\u0bc7\u0baf\u0bc1\u0b9a\u0bc1\u0ba9\u0bcd(\u0b8f) \u0baa\u0bcd\u0bb0\u0bca\u0ba4\u0bcd\u0ba4\u0bc1 <br/> \u0b95\u0bae\u0bcd\u0bb8\u0bcd(\u0b86)\u0bb0\u0bbf\u0ba9\u0bbf \u0ba8\u0bae\u0bcd\u0bae\u0bc1\u0bb5\u0bbe\u0bb0\u0bc1 (\u0b9a\u0bae\u0bcd)"}, {"M": "\u0b9a\u0bb0\u0ba3\u0bae\u0bcd 1", "T": "\u0b9e\u0bbe\u0ba9 \u0bb5\u0bc8\u0bb0\u0bbe\u0b95\u0bcd\u0baf\u0bae\u0bc1\u0bb2\u0bc1 \u0bb9\u0bc0\u0ba9\u0bae\u0bcd(\u0b90)\u0ba8\u0bcd(\u0b85)\u0b9f\u0bcd\u0b9f\u0bbf \u0baa\u0bb5 <br/> \u0b95\u0bbe\u0ba9\u0ba9\u0bae\u0bc1\u0ba9 \u0ba4\u0bbf\u0bb0\u0bc1\u0b95\u0bc1 \u0bae\u0bbe\u0ba9\u0bb5\u0bc1\u0bb2\u0bc1 \u0b9a\u0ba4\u0bbe <br/> \u0ba4\u0bcd\u0baf\u0bbe\u0ba9 \u0baf\u0bcb\u0b95 \u0baf\u0bc1\u0ba4\u0bc1\u0bb2\u0bc8 \u0ba8\u0bc0 \u0ba8\u0bbe\u0bae\u0bae\u0bc1 \u0baa\u0bb2\u0bcd\u0b95\u0bc1\u0b9a\u0bc1 <br/> \u0ba8\u0bbe\u0ba9\u0bbe \u0b95\u0bb0\u0bcd\u0bae \u0baa\u0bb2\u0bae\u0bc1 \u0ba4\u0bbe\u0ba9\u0bae\u0bc1 \u0b9a\u0bc7\u0baf\u0bc1\u0bb5\u0bbe\u0bb0\u0bc1 (\u0b9a\u0bae\u0bcd)"}, {"M": "\u0b9a\u0bb0\u0ba3\u0bae\u0bcd 2", "T": "\u0b95\u0bcd\u0bb0\u0bc2\u0bb0\u0baa\u0bc1 \u0baf\u0bcb\u0b9a\u0ba9\u0bb2\u0bc1 \u0ba4\u0bc2\u0bb0\u0bc1 \u0b9c\u0bc7\u0b9a\u0bbf \u0ba4\u0ba9 <br/> \u0ba4\u0bbe\u0bb0 \u0baa\u0bc1\u0ba4\u0bcd\u0bb0\u0bc1\u0bb2 \u0baa\u0bb0\u0bbf\u0b9a\u0bbe\u0bb0\u0b95\u0bc1\u0bb2 \u0b9c\u0bc7\u0b9a\u0bbf <br/> \u0b9a\u0bbe\u0bb0 \u0bb0\u0bc2\u0baa\u0bc1\u0ba9\u0bbf \u0baa\u0bbe\u0ba4 \u0b9a\u0bbe\u0bb0\u0b9a \u0baf\u0bc1\u0b95\u0bae\u0bc1\u0bb2 <br/> \u0b9a\u0bbe\u0bb0\u0bc6-\u0b9a\u0bbe\u0bb0\u0bc6\u0b95\u0bc1 \u0bae\u0ba9\u0b9a\u0bbe\u0bb0 \u0baa\u0bc2\u0b9c\u0bbf\u0ba9\u0bcd\u0b9a\u0bc1\u0bb5\u0bbe\u0bb0\u0bc1 (\u0b9a\u0bae\u0bcd)"}, {"M": "\u0b9a\u0bb0\u0ba3\u0bae\u0bcd 3", "T": "\u0baa\u0bbe\u0b95\u0bb5\u0ba4\u0bc1\u0bb2 \u0b95\u0bc2\u0b9f\u0bbf \u0baa\u0bcb\u0b95\u0bae\u0bc1\u0bb2\u0bcd(\u0b8e)\u0bb2\u0bcd\u0bb2 <br/> \u0bb9\u0bb0\u0bbf\u0b95\u0bc7 \u0b95\u0bbe\u0bb5\u0bbf\u0bae\u0bcd\u0baa\u0bc1\u0b9a\u0bc1\u0ba9\u0bc1 \u0bb5\u0bc0\u0ba3\u0bbe \u0b95\u0bbe\u0ba9\u0bae\u0bc1\u0bb2\u0ba4\u0bcb <br/> \u0b86\u0b95\u0bae \u0b9a\u0bb0\u0bc1\u0ba9\u0bbf \u0bb6\u0bcd\u0bb0\u0bc0 \u0bb0\u0bbe\u0b95\u0bae\u0bc1\u0ba9 \u0baa\u0bbe\u0b9f\u0bc1\u0b9a\u0bc1 <br/> \u0ba4\u0bcd\u0baf\u0bbe\u0b95\u0bb0\u0bbe\u0b9c \u0ba8\u0bc1\u0ba4\u0bc1\u0ba9\u0bbf \u0baa\u0bbe\u0b95\u0bc1\u0b95 \u0ba8\u0bae\u0bcd\u0bae\u0bc1\u0bb5\u0bbe\u0bb0\u0bc1 (\u0b9a\u0bae\u0bcd)"}]}, {"L": "Telugu", "text": [{"M": "\u0c2a\u0c32\u0c4d\u0c32\u0c35\u0c3f", "T": "\u0c38\u0c02\u0c38\u0c3e\u0c30\u0c41\u0c32\u0c4d(\u0c10)\u0c24\u0c47(\u0c28\u0c4d)\u0c0f\u0c2e\u0c4d(\u0c05)\u0c2f\u0c4d\u0c2f\u0c3e \u0c36\u0c3f\u0c16\u0c3f <br/> \u0c2a\u0c3f\u0c28\u0c4d\u0c1b\u0c4d(\u0c06)\u0c35\u0c24\u0c02\u0c38\u0c41\u0c21\u0c4d(\u0c0e)\u0c26\u0c41\u0c1f(\u0c28\u0c4d)\u0c09\u0c23\u0c4d\u0c21\u0c17"}, {"M": "\u0c05\u0c28\u0c41\u0c2a\u0c32\u0c4d\u0c32\u0c35\u0c3f", "T": "\u0c39\u0c3f\u0c02\u0c38\u0c4d(\u0c06)\u0c26\u0c41\u0c32\u0c4d(\u0c0e)\u0c32\u0c4d\u0c32 \u0c30\u0c4b\u0c38\u0c3f \u0c39\u0c02\u0c38\u0c3e\u0c26\u0c41\u0c32 \u0c15\u0c42\u0c21\u0c3f <br/> \u0c2a\u0c4d\u0c30\u0c36\u0c02\u0c38 \u0c1c\u0c47\u0c2f\u0c41\u0c1a\u0c41\u0c28\u0c4d(\u0c0f) \u0c2a\u0c4d\u0c30\u0c4a\u0c26\u0c4d\u0c26\u0c41 <br/> \u0c15\u0c02\u0c38\u0c4d(\u0c06)\u0c30\u0c3f\u0c28\u0c3f \u0c28\u0c2e\u0c4d\u0c2e\u0c41\u0c35\u0c3e\u0c30\u0c41 (\u0c38\u0c02)"}, {"M": "\u0c1a\u0c30\u0c23\u0c2e\u0c4d 1", "T": "\u0c1c\u0c4d\u0c1e\u0c3e\u0c28 \u0c35\u0c48\u0c30\u0c3e\u0c17\u0c4d\u0c2f\u0c2e\u0c41\u0c32\u0c41 \u0c39\u0c40\u0c28\u0c2e\u0c4d(\u0c10)\u0c28\u0c4d(\u0c05)\u0c1f\u0c4d\u0c1f\u0c3f \u0c2d\u0c35 <br/> \u0c15\u0c3e\u0c28\u0c28\u0c2e\u0c41\u0c28 \u0c24\u0c3f\u0c30\u0c41\u0c17\u0c41 \u0c2e\u0c3e\u0c28\u0c35\u0c41\u0c32\u0c41 \u0c38\u0c26\u0c3e <br/> \u0c27\u0c4d\u0c2f\u0c3e\u0c28 \u0c2f\u0c4b\u0c17 \u0c2f\u0c41\u0c24\u0c41\u0c32\u0c48 \u0c28\u0c40 \u0c28\u0c3e\u0c2e\u0c2e\u0c41 \u0c2a\u0c32\u0c4d\u0c15\u0c41\u0c1a\u0c41 <br/> \u0c28\u0c3e\u0c28\u0c3e \u0c15\u0c30\u0c4d\u0c2e \u0c2b\u0c32\u0c2e\u0c41 \u0c26\u0c3e\u0c28\u0c2e\u0c41 \u0c38\u0c47\u0c2f\u0c41\u0c35\u0c3e\u0c30\u0c41 (\u0c38\u0c02)"}, {"M": "\u0c1a\u0c30\u0c23\u0c2e\u0c4d 2", "T": "\u0c15\u0c4d\u0c30\u0c42\u0c30\u0c2a\u0c41 \u0c2f\u0c4b\u0c1a\u0c28\u0c32\u0c41 \u0c26\u0c42\u0c30\u0c41 \u0c1c\u0c47\u0c38\u0c3f \u0c24\u0c28 <br/> \u0c26\u0c3e\u0c30 \u0c2a\u0c41\u0c24\u0c4d\u0c30\u0c41\u0c32 \u0c2a\u0c30\u0c3f\u0c1a\u0c3e\u0c30\u0c15\u0c41\u0c32 \u0c1c\u0c47\u0c38\u0c3f <br/> \u0c38\u0c3e\u0c30 \u0c30\u0c42\u0c2a\u0c41\u0c28\u0c3f \u0c2a\u0c3e\u0c26 \u0c38\u0c3e\u0c30\u0c38 \u0c2f\u0c41\u0c17\u0c2e\u0c41\u0c32 <br/> \u0c38\u0c3e\u0c30\u0c46-\u0c38\u0c3e\u0c30\u0c46\u0c15\u0c41 \u0c2e\u0c28\u0c38\u0c3e\u0c30 \u0c2a\u0c42\u0c1c\u0c3f\u0c28\u0c4d\u0c1a\u0c41\u0c35\u0c3e\u0c30\u0c41 (\u0c38\u0c02)"}, {"M": "\u0c1a\u0c30\u0c23\u0c2e\u0c4d 3", "T": "\u0c2d\u0c3e\u0c17\u0c35\u0c24\u0c41\u0c32 \u0c15\u0c42\u0c21\u0c3f \u0c2d\u0c4b\u0c17\u0c2e\u0c41\u0c32\u0c4d(\u0c0e)\u0c32\u0c4d\u0c32 <br/> \u0c39\u0c30\u0c3f\u0c15\u0c47 \u0c15\u0c3e\u0c35\u0c3f\u0c2e\u0c4d\u0c2a\u0c41\u0c1a\u0c41\u0c28\u0c41 \u0c35\u0c40\u0c23\u0c3e \u0c17\u0c3e\u0c28\u0c2e\u0c41\u0c32\u0c24\u0c4b <br/> \u0c06\u0c17\u0c2e \u0c1a\u0c30\u0c41\u0c28\u0c3f \u0c36\u0c4d\u0c30\u0c40 \u0c30\u0c3e\u0c17\u0c2e\u0c41\u0c28 \u0c2a\u0c3e\u0c21\u0c41\u0c1a\u0c41 <br/> \u0c24\u0c4d\u0c2f\u0c3e\u0c17\u0c30\u0c3e\u0c1c \u0c28\u0c41\u0c24\u0c41\u0c28\u0c3f \u0c2c\u0c3e\u0c17\u0c41\u0c17 \u0c28\u0c2e\u0c4d\u0c2e\u0c41\u0c35\u0c3e\u0c30\u0c41 (\u0c38\u0c02)"}]}, {"L": "Kannada", "text": [{"M": "\u0caa\u0cb2\u0ccd\u0cb2\u0cb5\u0cbf", "T": "\u0cb8\u0c82\u0cb8\u0cbe\u0cb0\u0cc1\u0cb2\u0ccd(\u0c90)\u0ca4\u0cc7(\u0ca8\u0ccd)\u0c8f\u0cae\u0ccd(\u0c85)\u0caf\u0ccd\u0caf\u0cbe \u0cb6\u0cbf\u0c96\u0cbf <br/> \u0caa\u0cbf\u0ca8\u0ccd\u0c9b\u0ccd(\u0c86)\u0cb5\u0ca4\u0c82\u0cb8\u0cc1\u0ca1\u0ccd(\u0c8e)\u0ca6\u0cc1\u0c9f(\u0ca8\u0ccd)\u0c89\u0ca3\u0ccd\u0ca1\u0c97"}, {"M": "\u0c85\u0ca8\u0cc1\u0caa\u0cb2\u0ccd\u0cb2\u0cb5\u0cbf", "T": "\u0cb9\u0cbf\u0c82\u0cb8\u0ccd(\u0c86)\u0ca6\u0cc1\u0cb2\u0ccd(\u0c8e)\u0cb2\u0ccd\u0cb2 \u0cb0\u0ccb\u0cb8\u0cbf \u0cb9\u0c82\u0cb8\u0cbe\u0ca6\u0cc1\u0cb2 \u0c95\u0cc2\u0ca1\u0cbf <br/> \u0caa\u0ccd\u0cb0\u0cb6\u0c82\u0cb8 \u0c9c\u0cc7\u0caf\u0cc1\u0c9a\u0cc1\u0ca8\u0ccd(\u0c8f) \u0caa\u0ccd\u0cb0\u0cca\u0ca6\u0ccd\u0ca6\u0cc1 <br/> \u0c95\u0c82\u0cb8\u0ccd(\u0c86)\u0cb0\u0cbf\u0ca8\u0cbf \u0ca8\u0cae\u0ccd\u0cae\u0cc1\u0cb5\u0cbe\u0cb0\u0cc1 (\u0cb8\u0c82)"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 1", "T": "\u0c9c\u0ccd\u0c9e\u0cbe\u0ca8 \u0cb5\u0cc8\u0cb0\u0cbe\u0c97\u0ccd\u0caf\u0cae\u0cc1\u0cb2\u0cc1 \u0cb9\u0cc0\u0ca8\u0cae\u0ccd(\u0c90)\u0ca8\u0ccd(\u0c85)\u0c9f\u0ccd\u0c9f\u0cbf \u0cad\u0cb5 <br/> \u0c95\u0cbe\u0ca8\u0ca8\u0cae\u0cc1\u0ca8 \u0ca4\u0cbf\u0cb0\u0cc1\u0c97\u0cc1 \u0cae\u0cbe\u0ca8\u0cb5\u0cc1\u0cb2\u0cc1 \u0cb8\u0ca6\u0cbe <br/> \u0ca7\u0ccd\u0caf\u0cbe\u0ca8 \u0caf\u0ccb\u0c97 \u0caf\u0cc1\u0ca4\u0cc1\u0cb2\u0cc8 \u0ca8\u0cc0 \u0ca8\u0cbe\u0cae\u0cae\u0cc1 \u0caa\u0cb2\u0ccd\u0c95\u0cc1\u0c9a\u0cc1 <br/> \u0ca8\u0cbe\u0ca8\u0cbe \u0c95\u0cb0\u0ccd\u0cae \u0cab\u0cb2\u0cae\u0cc1 \u0ca6\u0cbe\u0ca8\u0cae\u0cc1 \u0cb8\u0cc7\u0caf\u0cc1\u0cb5\u0cbe\u0cb0\u0cc1 (\u0cb8\u0c82)"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 2", "T": "\u0c95\u0ccd\u0cb0\u0cc2\u0cb0\u0caa\u0cc1 \u0caf\u0ccb\u0c9a\u0ca8\u0cb2\u0cc1 \u0ca6\u0cc2\u0cb0\u0cc1 \u0c9c\u0cc7\u0cb8\u0cbf \u0ca4\u0ca8 <br/> \u0ca6\u0cbe\u0cb0 \u0caa\u0cc1\u0ca4\u0ccd\u0cb0\u0cc1\u0cb2 \u0caa\u0cb0\u0cbf\u0c9a\u0cbe\u0cb0\u0c95\u0cc1\u0cb2 \u0c9c\u0cc7\u0cb8\u0cbf <br/> \u0cb8\u0cbe\u0cb0 \u0cb0\u0cc2\u0caa\u0cc1\u0ca8\u0cbf \u0caa\u0cbe\u0ca6 \u0cb8\u0cbe\u0cb0\u0cb8 \u0caf\u0cc1\u0c97\u0cae\u0cc1\u0cb2 <br/> \u0cb8\u0cbe\u0cb0\u0cc6-\u0cb8\u0cbe\u0cb0\u0cc6\u0c95\u0cc1 \u0cae\u0ca8\u0cb8\u0cbe\u0cb0 \u0caa\u0cc2\u0c9c\u0cbf\u0ca8\u0ccd\u0c9a\u0cc1\u0cb5\u0cbe\u0cb0\u0cc1 (\u0cb8\u0c82)"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 3", "T": "\u0cad\u0cbe\u0c97\u0cb5\u0ca4\u0cc1\u0cb2 \u0c95\u0cc2\u0ca1\u0cbf \u0cad\u0ccb\u0c97\u0cae\u0cc1\u0cb2\u0ccd(\u0c8e)\u0cb2\u0ccd\u0cb2 <br/> \u0cb9\u0cb0\u0cbf\u0c95\u0cc7 \u0c95\u0cbe\u0cb5\u0cbf\u0cae\u0ccd\u0caa\u0cc1\u0c9a\u0cc1\u0ca8\u0cc1 \u0cb5\u0cc0\u0ca3\u0cbe \u0c97\u0cbe\u0ca8\u0cae\u0cc1\u0cb2\u0ca4\u0ccb <br/> \u0c86\u0c97\u0cae \u0c9a\u0cb0\u0cc1\u0ca8\u0cbf \u0cb6\u0ccd\u0cb0\u0cc0 \u0cb0\u0cbe\u0c97\u0cae\u0cc1\u0ca8 \u0caa\u0cbe\u0ca1\u0cc1\u0c9a\u0cc1 <br/> \u0ca4\u0ccd\u0caf\u0cbe\u0c97\u0cb0\u0cbe\u0c9c \u0ca8\u0cc1\u0ca4\u0cc1\u0ca8\u0cbf \u0cac\u0cbe\u0c97\u0cc1\u0c97 \u0ca8\u0cae\u0ccd\u0cae\u0cc1\u0cb5\u0cbe\u0cb0\u0cc1 (\u0cb8\u0c82)"}]}, {"L": "Malayalam", "text": [{"M": "\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d38\u0d02\u0d38\u0d3e\u0d30\u0d41\u0d32\u0d4d(\u0d10)\u0d24\u0d47(\u0d28\u0d4d)\u0d0f\u0d2e\u0d4d(\u0d05)\u0d2f\u0d4d\u0d2f\u0d3e \u0d36\u0d3f\u0d16\u0d3f <br/> \u0d2a\u0d3f\u0d28\u0d4d\u0d1b\u0d4d(\u0d06)\u0d35\u0d24\u0d02\u0d38\u0d41\u0d21\u0d4d(\u0d0e)\u0d26\u0d41\u0d1f(\u0d28\u0d4d)\u0d09\u0d23\u0d4d\u0d21\u0d17"}, {"M": "\u0d05\u0d28\u0d41\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d39\u0d3f\u0d02\u0d38\u0d4d(\u0d06)\u0d26\u0d41\u0d32\u0d4d(\u0d0e)\u0d32\u0d4d\u0d32 \u0d30\u0d4b\u0d38\u0d3f \u0d39\u0d02\u0d38\u0d3e\u0d26\u0d41\u0d32 \u0d15\u0d42\u0d21\u0d3f <br/> \u0d2a\u0d4d\u0d30\u0d36\u0d02\u0d38 \u0d1c\u0d47\u0d2f\u0d41\u0d1a\u0d41\u0d28\u0d4d(\u0d0f) \u0d2a\u0d4d\u0d30\u0d4a\u0d26\u0d4d\u0d26\u0d41 <br/> \u0d15\u0d02\u0d38\u0d4d(\u0d06)\u0d30\u0d3f\u0d28\u0d3f \u0d28\u0d2e\u0d4d\u0d2e\u0d41\u0d35\u0d3e\u0d30\u0d41 (\u0d38\u0d02)"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 1", "T": "\u0d1c\u0d4d\u0d1e\u0d3e\u0d28 \u0d35\u0d48\u0d30\u0d3e\u0d17\u0d4d\u0d2f\u0d2e\u0d41\u0d32\u0d41 