u0ca8\u0cae\u0ccd\u0cae//\u0cae\u0ca8\u0ccd\u0ca6//"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 1", "T": "\u0c97\u0ccd\u0c8e\u0ca8\u0ccd\u0ca6\u0cb2\u0cbf\u0cb0\u0cc1\u0ca8\u0c96 \u0cb8\u0cb8\u0cbf\u0cac\u0cbf\u0cae\u0ccd\u0cac \u0caa\u0ca6 \u0caa\u0ca6\u0ccd\u0cae/ <br/> \u0c86\u0ca8\u0ccd\u0ca6\u0cc1\u0c97\u0cc6 \u0c87\u0ca4\u0ccd\u0ca4\u0cb5\u0ca8\u0ccd\u0caf\u0cbe\u0cb0\u0cc6 \u0caa\u0cc6\u0cb2\u0cae\u0ccd\u0cae/ <br/> \u0c86\u0ca8\u0ccd\u0ca6\u0cc1\u0c95\u0cbe\u0cb2\u0cbf\u0c99\u0ccd\u0c97\u0ca8 \u0caa\u0cc6\u0ca6\u0cc6\u0caf\u0ca5\u0cc1\u0cb2\u0cbf\u0ca6 \u0ca6\u0cbf\u0ca4\u0ccd\u0ca4/ <br/> \u0ca3\u0ca8\u0ccd\u0ca6\u0ca8 \u0c95\u0ca8\u0ccd\u0ca6\u0cae\u0cc1\u0c95\u0cc1\u0ca8\u0ccd\u0ca6 \u0c95\u0cbe\u0ca8\u0cae\u0ccd\u0cae//\u0cae\u0ca8\u0ccd\u0ca6//"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 2", "T": "\u0c8a\u0ca6\u0cc1\u0c97\u0cc6 \u0caa\u0cc0\u0ca5\u0cbe\u0cae\u0ccd\u0cac\u0cb0 /\u0ca8\u0ca6\u0cc1\u0c97\u0cc6 \u0cb9\u0cca\u0ca8\u0ccd\u0ca8\u0cc1\u0ca6\u0cc1 \u0ca6\u0cbe\u0cb0/ <br/> \u0c97\u0ccd\u0c85\u0ca6\u0c97 \u0c95\u0c99\u0ccd\u0c95\u0ca8 \u0cb5\u0cbf\u0ca4\u0ccd\u0ca4\u0cb5\u0ca8\u0ccd\u0caf\u0cbe\u0cb0\u0cae\u0cbe/ <br/> \u0c82\u0c85\u0ca6\u0ca6\u0cbf \u0c95\u0cc6\u0cb2\u0ccd \u0cb8\u0c95\u0cb2/\u0cb2\u0cca \u0c95\u0c99\u0ccd\u0c97\u0cb2 \u0c95\u0cc1\u0c95\u0ccd\u0cb6\u0cbf\u0caf\u0cca/ <br/> \u0cb3\u0cca\u0ca6\u0ca8\u0cc6 \u0ca5\u0cca\u0cb0\u0cbf\u0ca6/\u0c9c\u0c97\u0ca6\u0cca\u0ca6\u0cc6\u0caf \u0c95\u0cbe\u0ca8\u0cae\u0ccd\u0cae//\u0cae\u0ca8\u0ccd\u0ca6//"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 3", "T": "\u0ca3\u0cc0\u0cb0\u0ca6 \u0ca8\u0cc0\u0cb2 \u0ca6\u0ca8\u0ccd\u0ca5\u0cc6\u0cb8\u0cc6\u0cb5 \u0cac\u0c95\u0ccd\u0cb6\u0ca6\u0cbf/\u0c95\u0cc6// <br/> Y\u0c89\u0cb0\u0cb5 \u0ca8\u0cbf\u0ca4\u0ccd\u0ca4\u0cb5 \u0ca8\u0ccd\u0caf\u0cbe\u0cb0\u0cae\u0ccd\u0cae/ <br/> \u0ca3\u0cc0\u0cb0\u0cc6\u0c95\u0cc6\u0cb2\u0cc1 \u0ca8\u0cbf\u0cb0\u0ccd\u0c9c\u0cb0 \u0cb0\u0cbe\u0ca6 \u0cb5\u0cb0\u0cbf\u0c97\u0cc6/ <br/> P\u0cb0\u0cc6\u0cb0\u0cbf\u0cb8\u0cbf\u0cab\u0cb2 \u0cb5\u0cbf\u0ca4\u0ccd\u0ca5\u0cc1 \u0ca6\u0cbe\u0cb0\u0cbf \u0c95\u0cbe\u0ca8\u0cae\u0ccd\u0cae//\u0cae\u0ca8\u0ccd\u0ca6//"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 4", "T": "\u0cb7\u0c99\u0ccd\u0c96 \u0c9b\u0c95\u0ccd\u0cb0\u0cb5\u0cc1 /\u0c97\u0ca6\u0cc6 \u0caa\u0ca6\u0ccd\u0cae \u0c95\u0cc8 \u0caf\u0cca\u0cb2\u0c97\u0cbf\u0ca4\u0ccd\u0ca4/ <br/> \u0cb3\u0c99\u0ccd\u0c95\u0cb0\u0cbf\u0cb8\u0cc1\u0cb5\u0ca8\u0cc0\u0ca5 \u0ca8\u0ccd\u0caf\u0cbe\u0cb0\u0cae\u0ccd\u0cae/ <br/> P\u0c85\u0c99\u0ccd\u0c95\u0c9c\u0cae\u0cc1\u0c96\u0cbf \u0cb6\u0ccd\u0cb0\u0cc0 \u0cad\u0cc1\u0ca6\u0cc6\u0cb5\u0cbf \u0caf\u0cb0\u0cb0\u0cb8\u0ca8\u0cc1/ <br/> \u0cb7\u0c99\u0ccd\u0c95\u0cc6\u0caf\u0cbf\u0cb2\u0ccd\u0cb2\u0ca6\u0cc6 \u0c97\u0cca\u0caa\u0cbf \u0ca5\u0ca8\u0caf\u0c95\u0cbe\u0ca8\u0cae\u0ccd\u0cae//\u0cae\u0ca8\u0ccd\u0ca6//"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 5", "T": "\u0c97\u0ccd\u0c86\u0c82B\u0c8a\u0c97\u0ccd\u0c86\u0ca3\u0ccd\u0ca1\u0ccd\u0c83\u0c86\u0cb0\u0cbe/\u0c95\u0cb0\u0ccd\u0ca8\u0cbe\u0cb2\u0cae\u0ccd\u0cac\u0cbf\u0ca5 \u0c95\u0cc1\u0ca8\u0ccd\u0ca6\u0cb2/ <br/> \u0c86\u0cae\u0ccd\u0cac\u0cc1\u0c9c\u0cae\u0cc1\u0c96 \u0ca6\u0cb5 \u0ca8\u0ccd\u0caf\u0cbe\u0cb0\u0cc6 \u0caa\u0cc6\u0cb2\u0cae\u0ccd\u0cae/ <br/> \u0cb0\u0cae\u0ccd\u0cad\u0cc6 \u0c95\u0cc6\u0cb2\u0cbf\u0ca5 \u0caa\u0cc1\u0cb0\u0ca8\u0ccd\u0ca6\u0cb0 \u0cb5\u0cbf\u0ca4\u0cb2/ <br/> \u0ca3\u0cae\u0ccd\u0cac\u0cbf\u0ca6 \u0cad\u0c95\u0ccd\u0ca5 \u0c95\u0cc1\u0ca4\u0cc1\u0cae\u0ccd\u0cac\u0cbf \u0c95\u0cbe\u0ca8\u0cae\u0ccd\u0cae//\u0cae\u0ca8\u0ccd\u0ca6//"}]}, {"L": "Malayalam", "text": [{"M": "\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d02\u0d05\u0d28\u0d4d\u0d26\u0d17\u0d2e\u0d28\u0d46 \u0d07\u0d35\u0d28\u0d4d\u0d2f\u0d3e\u0d30\u0d46 \u0d2a\u0d46\u0d32\u0d2e\u0d4d\u0d2e <br/> \u0d02\u0d05\u0d28\u0d4d\u0d26\u0d30\u0d27\u0d30 /\u0d17\u0d4a\u0d35\u0d3f\u0d28\u0d4d\u0d26 \u0d15\u0d3e\u0d28\u0d2e\u0d4d\u0d2e//\u0d2e\u0d28\u0d4d\u0d26//"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 1", "T": "\u0d17\u0d4d\u0d0e\u0d28\u0d4d\u0d26\u0d32\u0d3f\u0d30\u0d41\u0d28\u0d16 \u0d38\u0d38\u0d3f\u0d2c\u0d3f\u0d2e\u0d4d\u0d2c \u0d2a\u0d26 \u0d2a\u0d26\u0d4d\u0d2e/ <br/> \u0d06\u0d28\u0d4d\u0d26\u0d41\u0d17\u0d46 \u0d07\u0d24\u0d4d\u0d24\u0d35\u0d28\u0d4d\u0d2f\u0d3e\u0d30\u0d46 \u0d2a\u0d46\u0d32\u0d2e\u0d4d\u0d2e/ <br/> \u0d06\u0d28\u0d4d\u0d26\u0d41\u0d15\u0d3e\u0d32\u0d3f\u0d19\u0d4d\u0d17\u0d28 \u0d2a\u0d46\u0d26\u0d46\u0d2f\u0d25\u0d41\u0d32\u0d3f\u0d26 \u0d26\u0d3f\u0d24\u0d4d\u0d24/ <br/> \u0d23\u0d28\u0d4d\u0d26\u0d28 \u0d15\u0d28\u0d4d\u0d26\u0d2e\u0d41\u0d15\u0d41\u0d28\u0d4d\u0d26 \u0d15\u0d3e\u0d28\u0d2e\u0d4d\u0d2e//\u0d2e\u0d28\u0d4d\u0d26//"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 2", "T": "\u0d0a\u0d26\u0d41\u0d17\u0d46 \u0d2a\u0d40\u0d25\u0d3e\u0d2e\u0d4d\u0d2c\u0d30 /\u0d28\u0d26\u0d41\u0d17\u0d46 \u0d39\u0d4a\u0d28\u0d4d\u0d28\u0d41\u0d26\u0d41 \u0d26\u0d3e\u0d30/ <br/> \u0d17\u0d4d\u0d05\u0d26\u0d17 \u0d15\u0d19\u0d4d\u0d15\u0d28 \u0d35\u0d3f\u0d24\u0d4d\u0d24\u0d35\u0d28\u0d4d\u0d2f\u0d3e\u0d30\u0d2e\u0d3e/ <br/> \u0d02\u0d05\u0d26\u0d26\u0d3f \u0d15\u0d46\u0d32\u0d4d \u0d38\u0d15\u0d32/\u0d32\u0d4a \u0d15\u0d19\u0d4d\u0d17\u0d32 \u0d15\u0d41\u0d15\u0d4d\u0d36\u0d3f\u0d2f\u0d4a/ <br/> \u0d33\u0d4a\u0d26\u0d28\u0d46 \u0d25\u0d4a\u0d30\u0d3f\u0d26/\u0d1c\u0d17\u0d26\u0d4a\u0d26\u0d46\u0d2f \u0d15\u0d3e\u0d28\u0d2e\u0d4d\u0d2e//\u0d2e\u0d28\u0d4d\u0d26//"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 3", "T": "\u0d23\u0d40\u0d30\u0d26 \u0d28\u0d40\u0d32 \u0d26\u0d28\u0d4d\u0d25\u0d46\u0d38\u0d46\u0d35 \u0d2c\u0d15\u0d4d\u0d36\u0d26\u0d3f/\u0d15\u0d46// <br/> Y\u0d09\u0d30\u0d35 \u0d28\u0d3f\u0d24\u0d4d\u0d24\u0d35 \u0d28\u0d4d\u0d2f\u0d3e\u0d30\u0d2e\u0d4d\u0d2e/ <br/> \u0d23\u0d40\u0d30\u0d46\u0d15\u0d46\u0d32\u0d41 \u0d28\u0d3f\u0d30\u0d4d\u0d1c\u0d30 \u0d30\u0d3e\u0d26 \u0d35\u0d30\u0d3f\u0d17\u0d46/ <br/> P\u0d30\u0d46\u0d30\u0d3f\u0d38\u0d3f\u0d2b\u0d32 \u0d35\u0d3f\u0d24\u0d4d\u0d25\u0d41 \u0d26\u0d3e\u0d30\u0d3f \u0d15\u0d3e\u0d28\u0d2e\u0d4d\u0d2e//\u0d2e\u0d28\u0d4d\u0d26//"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 4", "T": "\u0d37\u0d19\u0d4d\u0d16 \u0d1b\u0d15\u0d4d\u0d30\u0d35\u0d41 /\u0d17\u0d26\u0d46 \u0d2a\u0d26\u0d4d\u0d2e \u0d15\u0d48 \u0d2f\u0d4a\u0d32\u0d17\u0d3f\u0d24\u0d4d\u0d24/ <br/> \u0d33\u0d19\u0d4d\u0d15\u0d30\u0d3f\u0d38\u0d41\u0d35\u0d28\u0d40\u0d25 \u0d28\u0d4d\u0d2f\u0d3e\u0d30\u0d2e\u0d4d\u0d2e/ <br/> P\u0d05\u0d19\u0d4d\u0d15\u0d1c\u0d2e\u0d41\u0d16\u0d3f \u0d36\u0d4d\u0d30\u0d40 \u0d2d\u0d41\u0d26\u0d46\u0d35\u0d3f \u0d2f\u0d30\u0d30\u0d38\u0d28\u0d41/ <br/> \u0d37\u0d19\u0d4d\u0d15\u0d46\u0d2f\u0d3f\u0d32\u0d4d\u0d32\u0d26\u0d46 \u0d17\u0d4a\u0d2a\u0d3f \u0d25\u0d28\u0d2f\u0d15\u0d3e\u0d28\u0d2e\u0d4d\u0d2e//\u0d2e\u0d28\u0d4d\u0d26//"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 5", "T": "\u0d17\u0d4d\u0d06\u0d02B\u0d0a\u0d17\u0d4d\u0d06\u0d23\u0d4d\u0d21\u0d4d\u0d03\u0d06\u0d30\u0d3e/\u0d15\u0d30\u0d4d\u0d28\u0d3e\u0d32\u0d2e\u0d4d\u0d2c\u0d3f\u0d25 \u0d15\u0d41\u0d28\u0d4d\u0d26\u0d32/ <br/> \u0d06\u0d2e\u0d4d\u0d2c\u0d41\u0d1c\u0d2e\u0d41\u0d16 \u0d26\u0d35 \u0d28\u0d4d\u0d2f\u0d3e\u0d30\u0d46 \u0d2a\u0d46\u0d32\u0d2e\u0d4d\u0d2e/ <br/> \u0d30\u0d2e\u0d4d\u0d2d\u0d46 \u0d15\u0d46\u0d32\u0d3f\u0d25 \u0d2a\u0d41\u0d30\u0d28\u0d4d\u0d26\u0d30 \u0d35\u0d3f\u0d24\u0d32/ <br/> \u0d23\u0d2e\u0d4d\u0d2c\u0d3f\u0d26 \u0d2d\u0d15\u0d4d\u0d25 \u0d15\u0d41\u0d24\u0d41\u0d2e\u0d4d\u0d2c\u0d3f \u0d15\u0d3e\u0d28\u0d2e\u0d4d\u0d2e//\u0d2e\u0d28\u0d4d\u0d26//"}]}, {"L": "Sanskrit", "text": [{"M": "\u092a\u0932\u094d\u0932\u0935\u093f", "T": "\u0902\u0905\u0928\u094d\u0926\u0917\u092e\u0928\u0947 \u0907\u0935\u0928\u094d\u092f\u093e\u0930\u0947 \u092a\u0947\u0932\u092e\u094d\u092e <br/> \u0902\u0905\u0928\u094d\u0926\u0930\u0927\u0930 /\u0917\u094b\u0935\u093f\u0928\u094d\u0926 \u0915\u093e\u0928\u092e\u094d\u092e//\u092e\u0928\u094d\u0926//"}, {"M": "\u091a\u0930\u0923\u092e\u094d 1", "T": "\u0917\u094d\u090f\u0928\u094d\u0926\u0932\u093f\u0930\u0941\u0928\u0916 \u0938\u0938\u093f\u092c\u093f\u092e\u094d\u092c \u092a\u0926 \u092a\u0926\u094d\u092e/ <br/> \u0906\u0928\u094d\u0926\u0941\u0917\u0947 \u0907\u0924\u094d\u0924\u0935\u0928\u094d\u092f\u093e\u0930\u0947 \u092a\u0947\u0932\u092e\u094d\u092e/ <br/> \u0906\u0928\u094d\u0926\u0941\u0915\u093e\u0932\u093f\u0919\u094d\u0917\u0928 \u092a\u0947\u0926\u0947\u092f\u0925\u0941\u0932\u093f\u0926 \u0926\u093f\u0924\u094d\u0924/ <br/> \u0923\u0928\u094d\u0926\u0928 \u0915\u0928\u094d\u0926\u092e\u0941\u0915\u0941\u0928\u094d\u0926 \u0915\u093e\u0928\u092e\u094d\u092e//\u092e\u0928\u094d\u0926//"}, {"M": "\u091a\u0930\u0923\u092e\u094d 2", "T": "\u090a\u0926\u0941\u0917\u0947 \u092a\u0940\u0925\u093e\u092e\u094d\u092c\u0930 /\u0928\u0926\u0941\u0917\u0947 \u0939\u094b\u0928\u094d\u0928\u0941\u0926\u0941 \u0926\u093e\u0930/ <br/> \u0917\u094d\u0905\u0926\u0917 \u0915\u0919\u094d\u0915\u0928 \u0935\u093f\u0924\u094d\u0924\u0935\u0928\u094d\u092f\u093e\u0930\u092e\u093e/ <br/> \u0902\u0905\u0926\u0926\u093f \u0915\u0947\u0932\u094d \u0938\u0915\u0932/\u0932\u094b \u0915\u0919\u094d\u0917\u0932 \u0915\u0941\u0915\u094d\u0936\u093f\u092f\u094b/ <br/> \u0932\u094a\u0926\u0928\u0947 \u0925\u094b\u0930\u093f\u0926/\u091c\u0917\u0926\u094b\u0926\u0947\u092f \u0915\u093e\u0928\u092e\u094d\u092e//\u092e\u0928\u094d\u0926//"}, {"M": "\u091a\u0930\u0923\u092e\u094d 3", "T": "\u0923\u0940\u0930\u0926 \u0928\u0940\u0932 \u0926\u0928\u094d\u0925\u0947\u0938\u0947\u0935 \u092c\u0915\u094d\u0936\u0926\u093f/\u0915\u0947// <br/> Y\u0909\u0930\u0935 \u0928\u093f\u0924\u094d\u0924\u0935 \u0928\u094d\u092f\u093e\u0930\u092e\u094d\u092e/ <br/> \u0923\u0940\u0930\u0947\u0915\u0947\u0932\u0941 \u0928\u093f\u0930\u094d\u091c\u0930 \u0930\u093e\u0926 \u0935\u0930\u093f\u0917\u0947/ <br/> P\u0930\u0947\u0930\u093f\u0938\u093f\u092b\u0932 \u0935\u093f\u0924\u094d\u0925\u0941 \u0926\u093e\u0930\u093f \u0915\u093e\u0928\u092e\u094d\u092e//\u092e\u0928\u094d\u0926//"}, {"M": "\u091a\u0930\u0923\u092e\u094d 4", "T": "\u0937\u0919\u094d\u0916 \u091b\u0915\u