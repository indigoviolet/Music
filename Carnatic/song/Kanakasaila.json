bbf", "T": "\u0bb5\u0ba9\u0b9c \u0baa\u0bb5 \u0bb9\u0bb0\u0bbf \u0ba8\u0bc1\u0ba4\u0bc7 \u0ba4\u0bc7\u0bb5\u0bbf <br/> \u0bb9\u0bbf\u0bae \u0b95\u0bbf\u0bb0\u0bbf\u0b9c\u0bc7 \u0bb2\u0bb2\u0bbf\u0ba4\u0bc7 \u0b9a\u0ba4\u0ba4\u0bae\u0bcd <br/> \u0bb5\u0bbf\u0ba9\u0ba4\u0bae\u0bcd \u0bae\u0bbe\u0bae\u0bcd \u0baa\u0bb0\u0bbf\u0baa\u0bbe\u0bb2\u0baf \u0b9a\u0ba9\u0bcd\u0b95\u0bb0 <br/> \u0bb5\u0ba9\u0bbf\u0ba4\u0bc7 \u0b9a\u0ba4\u0bbf \u0bae\u0bb9\u0bbe \u0ba4\u0bcd\u0bb0\u0bbf\u0baa\u0bc1\u0bb0 \u0b9a\u0bc1\u0ba8\u0bcd\u0ba4\u0bb0\u0bbf (\u0b95\u0ba9\u0b95)"}, {"M": "\u0b9a\u0bb0\u0ba3\u0bae\u0bcd 1", "T": "\u0b95\u0bae\u0bcd\u0baa\u0bc1 \u0b95\u0ba3\u0bcd\u0b9f\u0bbf \u0b95\u0ba9\u0bcd\u0b9c \u0b9a\u0ba4\u0bcd\u0bb0\u0bc1\u0b9a \u0bb5\u0ba4\u0ba9\u0bc7 <br/> \u0b95\u0bb0\u0bbf \u0bb0\u0bbe\u0b9c \u0b95\u0bae\u0ba9\u0bc7 \u0bae\u0ba3\u0bbf \u0b9a\u0ba4\u0ba9\u0bc7 <br/> \u0b9a\u0bae\u0bcd\u0baa\u0bb0 \u0bb5\u0bbf\u0ba4\u0bbe\u0bb0\u0bbf \u0ba4\u0bcb\u0bb7\u0bbf\u0ba3\u0bbf \u0b9a\u0bbf\u0bb5 <br/> \u0b9a\u0ba9\u0bcd\u0b95\u0bb0\u0bbf \u0b9a\u0ba4\u0bbe \u0bae\u0ba4\u0bc1\u0bb0 \u0baa\u0bbe\u0bb7\u0bbf\u0ba3\u0bbf (\u0b95\u0ba9\u0b95)"}, {"M": "\u0b9a\u0bb0\u0ba3\u0bae\u0bcd 2", "T": "\u0b9a\u0ba3\u0bcd\u0b9f \u0bae\u0bc1\u0ba3\u0bcd\u0b9f \u0b95\u0ba3\u0bcd\u0b9f\u0ba9 \u0baa\u0ba3\u0bcd\u0b9f\u0bbf\u0ba4\u0bcd(\u0b8f)\u0b95\u0bcd\u0bb7\u0bc1 <br/> \u0ba4\u0ba3\u0bcd\u0b9f \u0b95\u0bcb\u0ba4\u0ba3\u0bcd\u0b9f \u0bae\u0ba3\u0bcd\u0b9f\u0bbf\u0ba4 \u0baa\u0bbe\u0ba3\u0bbf <br/> \u0baa\u0bc1\u0ba3\u0bcd\u0b9f\u0bb0\u0bc0\u0b95 \u0ba8\u0baf\u0ba9\u0bcd(\u0b86)\u0bb0\u0bcd\u0b9a\u0bbf\u0ba4 \u0baa\u0ba4\u0bc7 \u0ba4\u0bcd\u0bb0\u0bbf- <br/> \u0baa\u0bc1\u0bb0 \u0bb5\u0bbe\u0b9a\u0bbf\u0ba9\u0bbf \u0b9a\u0bbf\u0bb5\u0bc7 \u0bb9\u0bb0 \u0bb5\u0bbf\u0bb2\u0bbe\u0b9a\u0bbf\u0ba9\u0bbf (\u0b95\u0ba9\u0b95)"}, {"M": "\u0b9a\u0bb0\u0ba3\u0bae\u0bcd 3", "T": "\u0bb6\u0bcd\u0baf\u0bbe\u0bae\u0bb3\u0bcd(\u0b86)\u0bae\u0bcd\u0baa\u0bbf\u0b95\u0bc7 \u0baa\u0bb5\u0bcd(\u0b86)\u0baa\u0bcd\u0ba4\u0bbf \u0ba4\u0bb0\u0ba3\u0bc7 <br/> \u0bb6\u0bcd\u0baf\u0bbe\u0bae \u0b95\u0bcd\u0bb0\u0bc1\u0bb7\u0bcd\u0ba3 \u0baa\u0bb0\u0bbf\u0baa\u0bbe\u0bb2\u0bbf\u0ba9\u0bbf \u0b9c\u0ba9\u0ba9\u0bbf <br/> \u0b95\u0bbe\u0bae\u0bbf\u0ba4\u0bcd(\u0b86)\u0bb0\u0bcd\u0ba4 \u0baa\u0bb2 \u0ba4\u0bbe\u0baf\u0bbf\u0b95\u0bc7 <br/> \u0b95\u0bbe\u0bae\u0bbe\u0b95\u0bcd\u0bb7\u0bbf \u0b9a\u0b95\u0bb2 \u0bb2\u0bcb\u0b95 \u0b9a\u0bbe\u0b95\u0bcd\u0bb7\u0bbf (\u0b95\u0ba9\u0b95)"}]}, {"L": "Telugu", "text": [{"M": "\u0c2a\u0c32\u0c4d\u0c32\u0c35\u0c3f", "T": "\u0c15\u0c28\u0c15 \u0c36\u0c48\u0c32 \u0c35\u0c3f\u0c39\u0c3e\u0c30\u0c3f\u0c23\u0c3f \u0c36\u0c4d\u0c30\u0c40 <br/> \u0c15\u0c3e\u0c2e \u0c15\u0c4b\u0c1f\u0c3f \u0c2c\u0c3e\u0c32\u0c47 \u0c38\u0c41\u0c36\u0c40\u0c32\u0c47"}, {"M": "\u0c05\u0c28\u0c41\u0c2a\u0c32\u0c4d\u0c32\u0c35\u0c3f", "T": "\u0c35\u0c28\u0c1c \u0c2d\u0c35 \u0c39\u0c30\u0c3f \u0c28\u0c41\u0c24\u0c47 \u0c26\u0c47\u0c35\u0c3f <br/> \u0c39\u0c3f\u0c2e \u0c17\u0c3f\u0c30\u0c3f\u0c1c\u0c47 \u0c32\u0c32\u0c3f\u0c24\u0c47 \u0c38\u0c24\u0c24\u0c02 <br/> \u0c35\u0c3f\u0c28\u0c24\u0c2e\u0c4d \u0c2e\u0c3e\u0c2e\u0c4d \u0c2a\u0c30\u0c3f\u0c2a\u0c3e\u0c32\u0c2f \u0c36\u0c28\u0c4d\u0c15\u0c30 <br/> \u0c35\u0c28\u0c3f\u0c24\u0c47 \u0c38\u0c24\u0c3f \u0c2e\u0c39\u0c3e \u0c24\u0c4d\u0c30\u0c3f\u0c2a\u0c41\u0c30 \u0c38\u0c41\u0c28\u0c4d\u0c26\u0c30\u0c3f (\u0c15\u0c28\u0c15)"}, {"M": "\u0c1a\u0c30\u0c23\u0c2e\u0c4d 1", "T": "\u0c15\u0c2e\u0c4d\u0c2c\u0c41 \u0c15\u0c23\u0c4d\u0c20\u0c3f \u0c15\u0c28\u0c4d\u0c1c \u0c38\u0c26\u0c43\u0c36 \u0c35\u0c26\u0c28\u0c47 <br/> \u0c15\u0c30\u0c3f \u0c30\u0c3e\u0c1c \u0c17\u0c2e\u0c28\u0c47 \u0c2e\u0c23\u0c3f \u0c38\u0c26\u0c28\u0c47 <br/> \u0c36\u0c2e\u0c4d\u0c2c\u0c30 \u0c35\u0c3f\u0c26\u0c3e\u0c30\u0c3f \u0c24\u0c4b\u0c37\u0c3f\u0c23\u0c3f \u0c36\u0c3f\u0c35 <br/> \u0c36\u0c28\u0c4d\u0c15\u0c30\u0c3f \u0c38\u0c26\u0c3e \u0c2e\u0c27\u0c41\u0c30 \u0c2d\u0c3e\u0c37\u0c3f\u0c23\u0c3f (\u0c15\u0c28\u0c15)"}, {"M": "\u0c1a\u0c30\u0c23\u0c2e\u0c4d 2", "T": "\u0c1a\u0c23\u0c4d\u0c21 \u0c2e\u0c41\u0c23\u0c4d\u0c21 \u0c16\u0c23\u0c4d\u0c21\u0c28 \u0c2a\u0c23\u0c4d\u0c21\u0c3f\u0c24\u0c4d(\u0c0f)\u0c15\u0c4d\u0c37\u0c41 <br/> \u0c26\u0c23\u0c4d\u0c21 \u0c15\u0c4b\u0c26\u0c23\u0c4d\u0c21 \u0c2e\u0c23\u0c4d\u0c21\u0c3f\u0c24 \u0c2a\u0c3e\u0c23\u0c3f <br/> \u0c2a\u0c41\u0c23\u0c4d\u0c21\u0c30\u0c40\u0c15 \u0c28\u0c2f\u0c28\u0c4d(\u0c06)\u0c30\u0c4d\u0c1a\u0c3f\u0c24 \u0c2a\u0c26\u0c47 \u0c24\u0c4d\u0c30\u0c3f- <br/> \u0c2a\u0c41\u0c30 \u0c35\u0c3e\u0c38\u0c3f\u0c28\u0c3f \u0c36\u0c3f\u0c35\u0c47 \u0c39\u0c30 \u0c35\u0c3f\u0c32\u0c3e\u0c38\u0c3f\u0c28\u0c3f (\u0c15\u0c28\u0c15)"}, {"M": "\u0c1a\u0c30\u0c23\u0c2e\u0c4d 3", "T": "\u0c36\u0c4d\u0c2f\u0c3e\u0c2e\u0c33\u0c4d(\u0c06)\u0c2e\u0c4d\u0c2c\u0c3f\u0c15\u0c47 \u0c2d\u0c35\u0c4d(\u0c06)\u0c2c\u0c4d\u0c27\u0c3f \u0c24\u0c30\u0c23\u0c47 <br/> \u0c36\u0c4d\u0c2f\u0c3e\u0c2e \u0c15\u0c43\u0c37\u0c4d\u0c23 \u0c2a\u0c30\u0c3f\u0c2a\u0c3e\u0c32\u0c3f\u0c28\u0c3f \u0c1c\u0c28\u0c28\u0c3f <br/> \u0c15\u0c3e\u0c2e\u0c3f\u0c24\u0c4d(\u0c06)\u0c30\u0c4d\u0c25 \u0c2b\u0c32 \u0c26\u0c3e\u0c2f\u0c3f\u0c15\u0c47 <br/> \u0c15\u0c3e\u0c2e\u0c3e\u0c15\u0c4d\u0c37\u0c3f \u0c38\u0c15\u0c32 \u0c32\u0c4b\u0c15 \u0c38\u0c3e\u0c15\u0c4d\u0c37\u0c3f (\u0c15\u0c28\u0c15)"}]}, {"L": "Kannada", "text": [{"M": "\u0caa\u0cb2\u0ccd\u0cb2\u0cb5\u0cbf", "T": "\u0c95\u0ca8\u0c95 \u0cb6\u0cc8\u0cb2 \u0cb5\u0cbf\u0cb9\u0cbe\u0cb0\u0cbf\u0ca3\u0cbf \u0cb6\u0ccd\u0cb0\u0cc0 <br/> \u0c95\u0cbe\u0cae \u0c95\u0ccb\u0c9f\u0cbf \u0cac\u0cbe\u0cb2\u0cc7 \u0cb8\u0cc1\u0cb6\u0cc0\u0cb2\u0cc7"}, {"M": "\u0c85\u0ca8\u0cc1\u0caa\u0cb2\u0ccd\u0cb2\u0cb5\u0cbf", "T": "\u0cb5\u0ca8\u0c9c \u0cad\u0cb5 \u0cb9\u0cb0\u0cbf \u0ca8\u0cc1\u0ca4\u0cc7 \u0ca6\u0cc7\u0cb5\u0cbf <br/> \u0cb9\u0cbf\u0cae \u0c97\u0cbf\u0cb0\u0cbf\u0c9c\u0cc7 \u0cb2\u0cb2\u0cbf\u0ca4\u0cc7 \u0cb8\u0ca4\u0ca4\u0c82 <br/> \u0cb5\u0cbf\u0ca8\u0ca4\u0cae\u0ccd \u0cae\u0cbe\u0cae\u0ccd \u0caa\u0cb0\u0cbf\u0caa\u0cbe\u0cb2\u0caf \u0cb6\u0ca8\u0ccd\u0c95\u0cb0 <br/> \u0cb5\u0ca8\u0cbf\u0ca4\u0cc7 \u0cb8\u0ca4\u0cbf \u0cae\u0cb9\u0cbe \u0ca4\u0ccd\u0cb0\u0cbf\u0caa\u0cc1\u0cb0 \u0cb8\u0cc1\u0ca8\u0ccd\u0ca6\u0cb0\u0cbf (\u0c95\u0ca8\u0c95)"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 1", "T": "\u0c95\u0cae\u0ccd\u0cac\u0cc1 \u0c95\u0ca3\u0ccd\u0ca0\u0cbf \u0c95\u0ca8\u0ccd\u0c9c \u0cb8\u0ca6\u0cc3\u0cb6 \u0cb5\u0ca6\u0ca8\u0cc7 <br/> \u0c95\u0cb0\u0cbf \u0cb0\u0cbe\u0c9c \u0c97\u0cae\u0ca8\u0cc7 \u0cae\u0ca3\u0cbf \u0cb8\u0ca6\u0ca8\u0cc7 <br/> \u0cb6\u0cae\u0ccd\u0cac\u0cb0 \u0cb5\u0cbf\u0ca6\u0cbe\u0cb0\u0cbf \u0ca4\u0ccb\u0cb7\u0cbf\u0ca3\u0cbf \u0cb6\u0cbf\u0cb5 <br/> \u0cb6\u0ca8\u0ccd\u0c95\u0cb0\u0cbf \u0cb8\u0ca6\u0cbe \u0cae\u0ca7\u0cc1\u0cb0 \u0cad\u0cbe\u0cb7\u0cbf\u0ca3\u0cbf (\u0c95\u0ca8\u0c95)"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 2", "T": "\u0c9a\u0ca3\u0ccd\u0ca1 \u0cae\u0cc1\u0ca3\u0ccd\u0ca1 \u0c96\u0ca3\u0ccd\u0ca1\u0ca8 \u0caa\u0ca3\u0ccd\u0ca1\u0cbf\u0ca4\u0ccd(\u0c8f)\u0c95\u0ccd\u0cb7\u0cc1 <br/> \u0ca6\u0ca3\u0ccd\u0ca1 \u0c95\u0ccb\u0ca6\u0ca3\u0ccd\u0ca1 \u0cae\u0ca3\u0ccd\u0ca1\u0cbf\u0ca4 \u0caa\u0cbe\u0ca3\u0cbf <br/> \u0caa\u0cc1\u0ca3\u0ccd\u0ca1\u0cb0\u0cc0\u0c95 \u0ca8\u0caf\u0ca8\u0ccd(\u0c86)\u0cb0\u0ccd\u0c9a\u0cbf\u0ca4 \u0caa\u0ca6\u0cc7 \u0ca4\u0ccd\u0cb0\u0cbf- <br/> \u0caa\u0cc1\u0cb0 \u0cb5\u0cbe\u0cb8\u0cbf\u0ca8\u0cbf \u0cb6\u0cbf\u0cb5\u0cc7 \u0cb9\u0cb0 \u0cb5\u0cbf\u0cb2\u0cbe\u0cb8\u0cbf\u0ca8\u0cbf (\u0c95\u0ca8\u0c95)"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 3", "T": "\u0cb6\u0ccd\u0caf\u0cbe\u0cae\u0cb3\u0ccd(\u0c86)\u0cae\u0ccd\u0cac\u0cbf\u0c95\u0cc7 \u0cad\u0cb5\u0ccd(\u0c86)\u0cac\u0ccd\u0ca7\u0cbf \u0ca4\u0cb0\u0ca3\u0cc7 <br/> \u0cb6\u0ccd\u0caf\u0cbe\u0cae \u0c95\u0cc3\u0cb7\u0ccd\u0ca3 \u0caa\u0cb0\u0cbf\u0caa\u0cbe\u0cb2\u0cbf\u0ca8\u0cbf \u0c9c\u0ca8\u0ca8\u0cbf <br/> \u0c95\u0cbe\u0cae\u0cbf\u0ca4\u0ccd(\u0c86)\u0cb0\u0ccd\u0ca5 \u0cab\u0cb2 \u0ca6\u0cbe\u0caf\u0cbf\u0c95\u0cc7 <br/> \u0c95\u0cbe\u0cae\u0cbe\u0c95\u0ccd\u0cb7\u0cbf \u0cb8\u0c95\u0cb2 \u0cb2\u0ccb\u0c95 \u0cb8\u0cbe\u0c95\u0ccd\u0cb7\u0cbf (\u0c95\u0ca8\u0c95)"}]}, {"L": "Malayalam", "text": [{"M": "\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d15\u0d28\u0d15 \u0d36\u0d48\u0d32 \u0d35\u0d3f\u0d39\u0d3e\u0d30\u0d3f\u0d23\u0d3f \u0d36\u0d4d\u0d30\u0d40 <br/> \u0d15\u0d3e\u0d2e \u0d15\u0d4b\u0d1f\u0d3f \u0d2c\u0d3e\u0d32\u0d47 \u0d38\u0d41\u0d36\u0d40\u0d32\u0d47"}, {"M": "\u0d05\u0d28\u0d41\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d35\u0d28\u0d1c \u0d2d\u0d35 \u0d39\u0d30\u0d3f \u0d28\u0d41\u0d24\u0d47 \u0d26\u0d47\u0d35\u0d3f <br/> \u0d39\u0d3f\u0d2e \u0d17\u0d3f\u0d30\u0d3f\u0d1c\u0d47 \u0d32\u0d32\u0d3f\u0d24\u0d47 \u0d38\u0d24\u0d24\u0d02 <br/> \u0d35\u0d3f\u0d28\u0d24\u0d2e\u0d4d \u0d2e\u0d3e\u0d2e\u0d4d \u0d2a\u0d30\u0d3f\u0d2a\u0d3e\u0d32\u0d2f \u0d36\u0d28\u0d4d\u0d15\u0d30 <br/> \u0d35\u0d28\u0d3f\u0d24\u0d47 \u0d38\u0d24\u0d3f \u0d2e\u0d39\u0d3e \u0d24\u0d4d\u0d30\u0d3f\u0d2a\u0d41\u0d30 \u0d38\u0d41\u0d28\u0d4d\u0d26\u0d30\u0d3f (\u0d15\u0d28\u0d15)"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 1", "T": "\u0d15\u0d2e\u0d4d\u0d2c\u0d41 \u0d15\u0d23\u0d4d\u0d20\u0d3f \u0d15\u0d28\u0d4d\u0d1c \u0d38\u0d26\u0d43\u0d36 \u0d35\u0d26\u0d28\u0d47 <br/> \u0d15\u0d30\u0d3f \u0d30\u0d3e\u0d1c \u0d17\u0d2e\u0d28\u0d47 \u0d2e\u0d23\u0d3f \u0d38\u0d26\u0d28\u0d47 <br/> \u0d36\u0d2e\u0d4d\u0d2c\u0d30 \u0d35\u0d3f\u0d26\u0d3e\u0d30\u0d3f \u0d24\u0d4b\u0d37\u0d3f\u0d23\u0d3f \u0d36\u0d3f\u0d35 <br/> \u0d36\u0d28\u0d4d\u0d15\u0d30\u0d3f \u0d38\u0d26\u0d3e \u0d2e\u0d27\u0d41\u0d30 \u0d2d\u0d3e\u0d37\u0d3f\u0d23\u0d3f (\u0d15\u0d28\u0d15)"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 2", "T": "\u0d1a\u0d23\u0d4d\u0d21 \u0d2e\u0d41\u0d23\u0d4d\u0d21 \u0d16\u0d23\u0d4d\u0d21\u0d28 \u0d2a\u0d23\u0d4d\u0d21\u0d3f\u0d24\u0d4d(\u0d0f)\u0d15\u0d4d\u0d37\u0d41 <br/> \u0d26\u0d23\u0d4d\u0d21 \u0d15\u0d4b\u0d26\u0d23\u0d4d\u0d21 \u0d2e\u0d23\u0d4d\u0d21\u0d3f\u0d24 \u0d2a\u0d3e\u0d23\u0d3f <br/> \u0d2a\u0d41\u0d23\u0d4d\u0d21\u0d30\u0d40\u0d15 \u0d28\u0d2f\u0d28\u0d4d(\u0d06)\u0d30\u0d4d\u0d1a\u0d3f\u0d24 \u0d2a\u0d26\u0d47 \u0d24\u0d4d\u0d30\u0d3f- <br/> \u0d2a\u0d41\u0d30 \u0d35\u0d3e\u0d38\u0d3f\u0d28\u0d3f \u0d36\u0d3f\u0d35\u0d47 \u0d39\u0d30 \u0d35\u0d3f\u0d32\u0d3e\u0d38\u0d3f\u0d28\u0d3f (\u0d15\u0d28\u0d15)"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 3", "T": "\u0d36\u0d4d\u0d2f\u0d3e\u0d2e\u0d33\u0d4d(\u0d06)\u0d2e\u0d4d\u0d2c\u0d3f\u0d15\u0d47 \u0d2d\u0d35\u0d4d(\u0d06)\u0d2c\u0d4d\u0d27\u0d3f \u0d24\u0d30\u0d23\u0d47 <br/> \u0d36\u0d4d\u0d2f\u0d3e\u0d2e \u0d15\u0d43\u0d37\u0d4d\u0d23 \u0d2a\u0d30\u0d3f\u0d2a\u0d3e\u0d32\u0d3f\u0d28\u0d3f \u0d1c\u0d28\u0d28\u0d3f <br/> \u0d15\u0d3e\u0d2e\u0d3f\u0d24\u0d4d(\u0d06)\u0d30\u0d4d\u0d25 \u0d2b\u0d32 \u0d26\u0d3e\u0d2f\u0d3f\u0d15\u0d47 <br/> \u0d15\u0d3e\u0d2e\u0d3e\u0d15\u0d4d\u0d37\u0d3f \u0d38\u0d15\u0d32 \u0d32\u0d4b\u0d15 \u0d38\u0d3e\u0d15\u0d4d\u0d37\u0d3f (\u0d15\u0d28\u0d15)"}]}, {"L": "Sanskrit", "text": [{"M": "\u092a\u0932\u094d\u0932\u0935\u093f", "T": "\u0915\u0928\u0915 \u0936\u0948\u0932 \u0935\u093f\u0939\u093e\u0930\u093f\u0923\u093f \u0936\u094d\u0930\u0940 <br/> \u0915\u093e\u092e \u0915\u094b\u091f\u093f \u092c\u093e\u0932\u0947 \u0938\u0941\u0936\u0940\u0932\u0947"}, {"M": "\u0905\u0928\u0941\u092a\u0932\u094d\u0932\u0935\u093f", "T": "\u0935\u0928\u091c \u092d\u0935 \u0939\u0930\u093f \u0928\u0941\u0924\u0947 \u0926\u0947\u0935\u093f <br/> \u0939\u093f\u092e \u0917\u093f\u0930\u093f\u091c\u0947 \u0932\u0932\u093f\u0924\u0947 \u0938\u0924\u0924\u0902 <br/> \u0935\u093f\u0928\u0924\u092e\u094d \u092e\u093e\u092e\u094d \u092a\u0930\u093f\u092a\u093e\u0932\u092f \u0936\u0928\u094d\u0915\u0930 <br/> \u0935\u0928\u093f\u0924\u0947 \u0938\u0924\u093f \u092e\u0939\u093e \u0924\u094d\u0930\u093f\u092a\u0941\u0930 \u0938\u0941\u0928\u094d\u0926\u0930\u093f (\u0915\u0928\u0915)"}, {"M": "\u091a\u0930\u0923\u092e\u094d 1", "T": "\u0915\u092e\u094d\u092c\u0941 \u0915\u0923\u094d\u0920\u093f \u0915\u0928\u094d\u091c \u0938\u0926\u0943\u0936 \u0935\u0926\u0928\u0947 <br/> \u0915\u0930\u093f \u0930\u093e\u091c \u0917\u092e\u0928\u0947 \u092e\u0923\u093f \u0938\u0926\u0928\u0947 <br/> \u0936\u092e\u094d\u092c\u0930 \u0935\u093f\u0926\u093e\u0930\u093f \u0924\u094b\u0937\u093f\u0923\u093f \u0936\u093f\u0935 <br/> \u0936\u0928\u094d\u0915\u0930\u093f \u0938\u0926\u093e \u092e\u0927\u0941\u0930 \u092d\u093e\u0937\u093f\u0923\u093f (\u0915\u0928\u0915)"}, {"M": "\u091a\u0930\u0923\u092e\u094d 2", "T": "\u091a\u0923\u094d\u0921 \u092e\u0941\u0923\u094d\u0921 \u0916\u0923\u094d\u0921\u0928 \u092a\u0923\u094d\u0921\u093f\u0924\u094d(\u090f)\u0915\u094d\u0937\u0941 <br/> \u0926\u0923\u094d\u0921 \u0915\u094b\u0926\u0923\u094d\u0921 \u092e\u0923\u094d\u0921\u093f\u0924 \u092a\u093e\u0923\u093f <br/> \u092a\u0941\u0923\u094d\u0921\u0930\u0940\u0915 \u0928\u092f\u0928\u094d(\u0906)\u0930\u094d\u091a\u093f\u0924 \u092a\u0926\u0947 \u0924\u094d\u0930\u093f- <br/> \u092a\u0941\u0930 \u0935\u093e\u0938\u093f\u0928\u093f \u0936\u093f\u0935\u0947 \u0939\u0930 \u0935\u093f\u0932\u093e\u0938\u093f\u0928\u093f (\u0915\u0928\u0915)"}, {"M": "\u091a\u0930\u0923\u092e\u094d 3", "T": "\u0936\u094d\u092f\u093e\u092e\u0932\u094d(\u0906)\u092e\u094d\u092c\u093f\u0915\u0947 \u092d\u0935\u094d(\u0906)\u092c\u094d\u0927\u093f \u0924\u0930\u0923\u0947 <br/> \u0936\u094d\u092f\u093e\u092e \u0915\u0943\u0937\u094d\u0923 \u092a\u0930\u093f\u092a\u093e\u0932\u093f\u0928\u093f \u091c\u0928\u0928\u093f <br/> \u0915\u093e\u092e\u093f\u0924\u094d(\u0906)\u0930\u094d\u0925 \u092b\u0932 \u0926\u093e\u092f\u093f\u0915\u0947 <br/> \u0915\u093e\u092e\u093e\u0915\u094d\u0937\u093f \u0938\u0915\u0932 \u0932\u094b\u0915 \u0938\u093e\u0915\u094d\u0937\u093f (\u0915\u0928\u0915)"}]}], "lyricsref": [{"links": [{"N": "syamakrishnavaibhavam.blogspot.com", "L": "http://syamakrishnavaibhavam.blogspot.com/2011/04/syama-sastry-kriti-kanaka-saila-raga.html"}, {"N": "www.sangeetasudha.org", "L": "http://www.sangeetasudha.org/othercomposers/syamasastry2.html#MARK_KANAKASAILA"}, {"N": 