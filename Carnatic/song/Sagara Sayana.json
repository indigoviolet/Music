\u0c2e\u0c4d 1", "T": "\u0c24\u0c3e\u0c2a\u0c36\u0c2e\u0c28\u0c15\u0c30\u0c3e! \u0c24\u0c3e\u0c2e\u0c38\u0c3e\u0c26\u0c3f \u0c26\u0c42\u0c30\u0c3e <br/> \u0c2a\u0c3e\u0c2a\u0c35\u0c3f\u0c2e\u0c4b\u0c1a\u0c28 \u0c2a\u0c3e\u0c35\u0c28 \u0c28\u0c3e\u0c2e\u0c3e <br/> \u0c36\u0c4d\u0c30\u0c40 \u0c2a\u0c26\u0c4d\u0c2e\u0c28\u0c3e\u0c2d\u0c3e \u0c26\u0c47\u0c35\u0c15 \u0c36\u0c4d\u0c30\u0c3f\u0c24 \u0c1c\u0c28 <br/> \u0c36\u0c4d\u0c30\u0c40\u0c15\u0c30 \u0c36\u0c41\u0c2d\u0c15\u0c30 \u0c35\u0c30\u0c26\u0c26\u0c3e\u0c38\u0c28\u0c41\u0c24\u0c2a\u0c3e\u0c26\u0c3e \u0c38\u0c3e\u0c17\u0c30"}, {"M": "\u0c1a\u0c30\u0c23\u0c2e\u0c4d 2", "T": "\u0c36\u0c4d\u0c30\u0c3e\u0c35\u0c23\u0c2e\u0c3e\u0c38 \u0c36\u0c4d\u0c30\u0c3e\u0c35\u0c23 \u0c26\u0c4d\u0c35\u0c3e\u0c26\u0c36\u0c40 \u0c2f\u0c41\u0c24 <br/> \u0c36\u0c4d\u0c30\u0c47\u0c36\u0c4d\u0c1f\u0c2e\u0c48\u0c28 \u0c26\u0c3f\u0c28\u0c2e\u0c28\u0c4d\u0c26\u0c41 \u0c2d\u0c15\u0c4d\u0c24\u0c1c\u0c28 <br/> \u0c36\u0c4d\u0c30\u0c47\u0c36\u0c4d\u0c24\u0c3e\u0c26\u0c3f \u0c2a\u0c42\u0c1c\u0c3f\u0c24 \u0c36\u0c4d\u0c30\u0c40 \u0c2a\u0c26\u0c4d\u0c2e\u0c28\u0c3e\u0c2d\u0c3e <br/> \u0c36\u0c4d\u0c30\u0c40\u0c30\u0c2e\u0c23\u0c40 \u0c2a\u0c4d\u0c30\u0c3f\u0c2f \u0c36\u0c4d\u0c30\u0c41\u0c24\u0c1c\u0c28 \u0c2a\u0c3e\u0c32\u0c3e \u0c38\u0c3e\u0c17\u0c30"}, {"M": "\u0c1a\u0c30\u0c23\u0c2e\u0c4d 3", "T": "\u0c35\u0c47\u0c26 \u0c38\u0c3e\u0c30\u0c3e ! \u0c28\u0c40\u0c35\u0c41 \u0c35\u0c47\u0c35\u0c47\u0c17\u0c2e\u0c47 \u0c35\u0c1a\u0c4d\u0c1a\u0c3f <br/> \u0c35\u0c46\u0c26\u0c26\u0c40\u0c30\u0c4d\u0c2a\u0c30\u0c3e\u0c26\u0c3e? \u0c28\u0c3e\u0c24\u0c4b \u0c35\u0c3e\u0c26\u0c3e ? <br/> \u0c38\u0c26\u0c30\u0c2e\u0c41 \u0c28\u0c3f\u0c28\u0c4d\u0c28\u0c41 \u0c38\u0c3e\u0c2f\u0c3f\u0c28\u0c4d\u0c39\u0c2e\u0c28\u0c4d\u0c26\u0c41 <br/> \u0c38\u0c47\u0c35\u0c3f\u0c1e\u0c4d\u0c1a\u0c3f\u0c28\u0c3e\u0c16\u0c3f\u0c32 \u0c38\u0c47\u0c35\u0c15\u0c4b\u0c26\u0c4d\u0c27\u0c3e\u0c30\u0c15 \u0c38\u0c3e\u0c17\u0c30"}, {"M": "\u0c1a\u0c30\u0c23\u0c2e\u0c4d 4", "T": "\u0c2a\u0c3e\u0c26\u0c2e\u0c47 \u0c17\u0c24\u0c40 \u0c17\u0c26\u0c3e ? \u0c2a\u0c3e\u0c2e\u0c30 \u0c26\u0c42\u0c30\u0c3e ! <br/> \u0c2a\u0c3e\u0c32\u0c3f\u0c2e\u0c4d\u0c2a\u0c30\u0c3e\u0c26\u0c3e \u0c2a\u0c3e\u0c2a\u0c36\u0c2e\u0c28 \u0c2a\u0c3e\u0c26! <br/> \u0c28\u0c40 \u0c26\u0c2f \u0c15\u0c4b\u0c30\u0c3f\u0c2f\u0c41\u0c28\u0c4d\u0c28 \u0c28\u0c28\u0c4d\u0c28\u0c41 \u0c2c\u0c4d\u0c30\u0c4b\u0c35 <br/> \u0c28\u0c40 \u0c2e\u0c28\u0c38\u0c41 \u0c15\u0c30\u0c17\u0c26\u0c47\u0c2e\u0c4b ? \u0c28\u0c40\u0c24\u0c4d\u0c2f\u0c30\u0c42\u0c2a\u0c3e \u0c38\u0c3e\u0c17\u0c30"}]}, {"L": "Kannada", "text": [{"M": "\u0caa\u0cb2\u0ccd\u0cb2\u0cb5\u0cbf", "T": "\u0cb8\u0cbe\u0c97\u0cb0 \u0cb6\u0caf\u0ca8 \u0cb5\u0cbf\u0cad\u0ccb ! \u0caa\u0ccd\u0cb0\u0cad\u0ccb ! <br/> \u0cb8\u0cbe\u0cae\u0c97\u0cbe\u0ca8\u0cb2\u0ccb\u0cb2\u0cbe !\u0cb8\u0cbe\u0cb0\u0cb8 \u0cb2\u0ccb\u0c9a\u0ca8 ! \u0c95\u0ccd\u0cb6\u0cc0\u0cb0 \u0cb8\u0cbe\u0c97\u0cb0"}, {"M": "\u0c85\u0ca8\u0cc1\u0caa\u0cb2\u0ccd\u0cb2\u0cb5\u0cbf", "T": "\u0cad\u0cbe\u0c97\u0cb5\u0ca4\u0cbe \u0caa\u0ccd\u0cb0\u0cbf\u0caf ! \u0cac\u0cbe\u0c97 \u0cac\u0ccd\u0cb0\u0ccb\u0cb5\u0cc1\u0cae\u0caf\u0ccd\u0caf\u0cbe ! <br/> \u0cac\u0cbe\u0c97 \u0cb6\u0ccd\u0cb0\u0cc0 \u0cad\u0cc2\u0cae\u0cbf \u0cb8\u0cb9\u0cbf\u0ca4\u0cc1\u0ca1\u0cc8 \u0cb5\u0cc6\u0cb2\u0caf\u0cc1 \u0c95\u0ccd\u0cb6\u0cc0\u0cb0 \u0cb8\u0cbe\u0c97\u0cb0"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 1", "T": "\u0ca4\u0cbe\u0caa\u0cb6\u0cae\u0ca8\u0c95\u0cb0\u0cbe! \u0ca4\u0cbe\u0cae\u0cb8\u0cbe\u0ca6\u0cbf \u0ca6\u0cc2\u0cb0\u0cbe <br/> \u0caa\u0cbe\u0caa\u0cb5\u0cbf\u0cae\u0ccb\u0c9a\u0ca8 \u0caa\u0cbe\u0cb5\u0ca8 \u0ca8\u0cbe\u0cae\u0cbe <br/> \u0cb6\u0ccd\u0cb0\u0cc0 \u0caa\u0ca6\u0ccd\u0cae\u0ca8\u0cbe\u0cad\u0cbe \u0ca6\u0cc7\u0cb5\u0c95 \u0cb6\u0ccd\u0cb0\u0cbf\u0ca4 \u0c9c\u0ca8 <br/> \u0cb6\u0ccd\u0cb0\u0cc0\u0c95\u0cb0 \u0cb6\u0cc1\u0cad\u0c95\u0cb0 \u0cb5\u0cb0\u0ca6\u0ca6\u0cbe\u0cb8\u0ca8\u0cc1\u0ca4\u0caa\u0cbe\u0ca6\u0cbe \u0cb8\u0cbe\u0c97\u0cb0"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 2", "T": "\u0cb6\u0ccd\u0cb0\u0cbe\u0cb5\u0ca3\u0cae\u0cbe\u0cb8 \u0cb6\u0ccd\u0cb0\u0cbe\u0cb5\u0ca3 \u0ca6\u0ccd\u0cb5\u0cbe\u0ca6\u0cb6\u0cc0 \u0caf\u0cc1\u0ca4 <br/> \u0cb6\u0ccd\u0cb0\u0cc7\u0cb6\u0ccd\u0c9f\u0cae\u0cc8\u0ca8 \u0ca6\u0cbf\u0ca8\u0cae\u0ca8\u0ccd\u0ca6\u0cc1 \u0cad\u0c95\u0ccd\u0ca4\u0c9c\u0ca8 <br/> \u0cb6\u0ccd\u0cb0\u0cc7\u0cb6\u0ccd\u0ca4\u0cbe\u0ca6\u0cbf \u0caa\u0cc2\u0c9c\u0cbf\u0ca4 \u0cb6\u0ccd\u0cb0\u0cc0 \u0caa\u0ca6\u0ccd\u0cae\u0ca8\u0cbe\u0cad\u0cbe <br/> \u0cb6\u0ccd\u0cb0\u0cc0\u0cb0\u0cae\u0ca3\u0cc0 \u0caa\u0ccd\u0cb0\u0cbf\u0caf \u0cb6\u0ccd\u0cb0\u0cc1\u0ca4\u0c9c\u0ca8 \u0caa\u0cbe\u0cb2\u0cbe \u0cb8\u0cbe\u0c97\u0cb0"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 3", "T": "\u0cb5\u0cc7\u0ca6 \u0cb8\u0cbe\u0cb0\u0cbe ! \u0ca8\u0cc0\u0cb5\u0cc1 \u0cb5\u0cc7\u0cb5\u0cc7\u0c97\u0cae\u0cc7 \u0cb5\u0c9a\u0ccd\u0c9a\u0cbf <br/> \u0cb5\u0cc6\u0ca6\u0ca6\u0cc0\u0cb0\u0ccd\u0caa\u0cb0\u0cbe\u0ca6\u0cbe? \u0ca8\u0cbe\u0ca4\u0ccb \u0cb5\u0cbe\u0ca6\u0cbe ? <br/> \u0cb8\u0ca6\u0cb0\u0cae\u0cc1 \u0ca8\u0cbf\u0ca8\u0ccd\u0ca8\u0cc1 \u0cb8\u0cbe\u0caf\u0cbf\u0ca8\u0ccd\u0cb9\u0cae\u0ca8\u0ccd\u0ca6\u0cc1 <br/> \u0cb8\u0cc7\u0cb5\u0cbf\u0c9e\u0ccd\u0c9a\u0cbf\u0ca8\u0cbe\u0c96\u0cbf\u0cb2 \u0cb8\u0cc7\u0cb5\u0c95\u0ccb\u0ca6\u0ccd\u0ca7\u0cbe\u0cb0\u0c95 \u0cb8\u0cbe\u0c97\u0cb0"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 4", "T": "\u0caa\u0cbe\u0ca6\u0cae\u0cc7 \u0c97\u0ca4\u0cc0 \u0c97\u0ca6\u0cbe ? \u0caa\u0cbe\u0cae\u0cb0 \u0ca6\u0cc2\u0cb0\u0cbe ! <br/> \u0caa\u0cbe\u0cb2\u0cbf\u0cae\u0ccd\u0caa\u0cb0\u0cbe\u0ca6\u0cbe \u0caa\u0cbe\u0caa\u0cb6\u0cae\u0ca8 \u0caa\u0cbe\u0ca6! <br/> \u0ca8\u0cc0 \u0ca6\u0caf \u0c95\u0ccb\u0cb0\u0cbf\u0caf\u0cc1\u0ca8\u0ccd\u0ca8 \u0ca8\u0ca8\u0ccd\u0ca8\u0cc1 \u0cac\u0ccd\u0cb0\u0ccb\u0cb5 <br/> \u0ca8\u0cc0 \u0cae\u0ca8\u0cb8\u0cc1 \u0c95\u0cb0\u0c97\u0ca6\u0cc7\u0cae\u0ccb ? \u0ca8\u0cc0\u0ca4\u0ccd\u0caf\u0cb0\u0cc2\u0caa\u0cbe \u0cb8\u0cbe\u0c97\u0cb0"}]}, {"L": "Malayalam", "text": [{"M"