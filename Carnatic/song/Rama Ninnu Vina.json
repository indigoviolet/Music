bbf <br/> \u0b89\u0bb2\u0bcd\u0bb2\u0bae\u0bc1\u0ba9\u0ba9\u0bc1(\u0baf\u0bcd)\u0b85\u0ba9\u0bcd\u0ba9\u0bbf\u0b9f \u0ba8\u0bbf\u0ba9\u0bc1 <br/> \u0b9a\u0bb2\u0bcd\u0bb2\u0b95 \u0baa\u0bbe\u0b95\u0bc1\u0b95 \u0b9c\u0bc2\u0b9a\u0bc6\u0ba4 (\u0bb0\u0bbe\u0bae)"}, {"M": "\u0b9a\u0bb0\u0ba3\u0bae\u0bcd 2", "T": "\u0baa\u0bbe\u0bb5\u0ba9 \u0ba8\u0bc0 \u0baa\u0b95\u0bcd\u0ba4\u0bbf \u0b9a\u0ba4\u0bbe <br/> \u0baa\u0bbe\u0bb2\u0bbf\u0ba9\u0bcd\u0b9a\u0bc1\u0ba9\u0bc1 \u0bae\u0bcb\u0b95\u0bcd\u0bb7\u0bae\u0bcd(\u0b92)\u0b9a\u0b95\u0bc1 <br/> \u0bae\u0bbe-\u0bb5\u0bb0 \u0ba8\u0bc0 \u0b9a\u0ba9\u0bcd\u0ba9\u0bbf\u0ba4\u0bbf\u0ba9\u0bbf <br/> \u0baa\u0bcd\u0bb0\u0bae\u0bbe\u0ba3\u0bae\u0bc1 \u0b9c\u0bc7\u0b9a\u0bbf \u0baa\u0bb2\u0bcd\u0b95\u0bc6\u0ba4 (\u0bb0\u0bbe\u0bae)"}, {"M": "\u0b9a\u0bb0\u0ba3\u0bae\u0bcd 3", "T": "\u0b9a\u0ba4\u0bcd\u0bb5 \u0b95\u0bc1\u0ba3\u0bae\u0bcd\u0baa\u0bc1\u0ba9\u0ba9\u0bc1(\u0baf\u0bcd)- <br/> \u0b89\u0baa\u0bbe\u0b9a\u0ba9\u0bae\u0bcd(\u0b92)\u0ba8\u0bb0\u0bbf\u0ba9\u0bcd\u0b9a\u0bbf\u0bb0\u0bbf \u0baa\u0bc6\u0ba4\u0bcd\u0ba4\u0bb2\u0bc1 <br/> \u0ba4\u0ba4\u0bcd\u0bb5\u0bae\u0bc1 \u0ba4\u0bc6\u0bb2\u0bbf\u0b9a\u0bc6\u0ba9\u0bc1 \u0b87\u0b95 \u0baa\u0bb5 <br/> \u0ba4\u0bb0\u0ba3\u0bcd(\u0b93)\u0baa\u0bbe\u0baf\u0bae\u0bc1 \u0ba8\u0bc0\u0bb5\u0bcd(\u0b85)\u0ba8\u0bbf (\u0bb0\u0bbe\u0bae)"}, {"M": "\u0b9a\u0bb0\u0ba3\u0bae\u0bcd 4", "T": "\u0bb2\u0bcb\u0b95\u0bc1\u0bb2\u0bc1 \u0ba8\u0bbf\u0b9c \u0ba4\u0bbe\u0b9a\u0bc1\u0bb2 \u0b95\u0ba9\u0bbf <br/> \u0bb2\u0bcb\u0baa\u0b9f\u0bc1 \u0ba4\u0bc1\u0bb0\u0bcd-\u0b85\u0b9a\u0bc2\u0baf\u0bb2\u0ba4\u0bcb <br/> \u0bb6\u0bcd\u0bb0\u0bc0 \u0b95\u0bb0 \u0ba8\u0bbf\u0ba9\u0bc1 \u0ba4\u0bc2\u0bb7\u0bbf\u0ba9\u0bcd\u0b9a\u0bbf\u0ba9 <br/> \u0b9a\u0bc6\u0b9f\u0bbf \u0baa\u0bcb\u0bb2\u0bc7\u0bb0\u0bc1 \u0b95\u0bbe\u0ba9\u0bbf (\u0bb0\u0bbe\u0bae)"}, {"M": "\u0b9a\u0bb0\u0ba3\u0bae\u0bcd 5", "T": "\u0b95\u0bcb\u0bb0\u0bbf\u0ba9 \u0b95\u0bcb\u0bb0\u0bbf\u0b95\u0bb2\u0bcd(\u0b92)\u0b9a\u0b95\u0bc7 <br/> \u0bb6\u0bc