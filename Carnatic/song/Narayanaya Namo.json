u0ca3\u0cbe\u0c97\u0cc6\u0ca8\u0ccd\u0ca6\u0ccd\u0cb0 \u0cb7\u0caf\u0ca8\u0cbe\u0caf \u0ca3\u0ca8\u0ccd\u0ca7\u0cbf\u0ca5\u0cbe\u0c95\u0cbf\u0cb2 \u0cb3\u0cca\u0c96\u0caf"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 1", "T": "\u0ca3\u0cbe\u0cad\u0cbf \u0c95\u0cae\u0cb2 \u0c9c\u0ca8\u0cbf\u0ca5 \u0cb5\u0cbf\u0ca7\u0cbf \u0cae\u0cc1\u0c96\u0cbe\u0caf <br/> \u0ca3\u0cc2\u0ca5\u0ca8 \u0c9e\u0cb2 \u0ca5\u0cbe\u0cb0\u0cbe\u0caa\u0caf <br/> \u0ca3\u0cbe\u0ca8\u0cbe \u0cb8\u0cb0\u0cbe\u0cb8\u0cb0 \u0ca8\u0cbe\u0cb0\u0cc0 \u0ca8\u0cb0 \u0cb0\u0cc2\u0caa\u0cbe\u0caf <br/> \u0ca3\u0cbe\u0cb0\u0ca7\u0cbe\u0ca7\u0cbf \u0cae\u0cc1\u0ca8\u0cbf \u0cb5\u0ca8\u0ccd\u0ca7\u0ccd\u0caf\u0cbe\u0ca8\u0ca8\u0ccd\u0ca5 \u0c97\u0ca8 \u0c82\u0c85\u0caf\u0cbe\u0caf"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 2", "T": "\u0c86\u0c95\u0ca8\u0cbf\u0ca5 \u0c99\u0cc1\u0ca8 \u0c99\u0ca8\u0cbe\u0caf \u0c86\u0c97\u0cae \u0c97\u0ccd\u0cb9\u0cc6\u0caf\u0cbe\u0caf <br/> V\u0c87\u0c96\u0cb2\u0cbf\u0ca5 B\u0cb9\u0caf\u0ccd \u0cb8\u0cca\u0c95\u0cbe\u0caf <br/> \u0c99\u0c97\u0ca8 \u0ca8\u0cbf\u0cb0\u0ccd\u0cae\u0cb2 \u0cac\u0cca\u0ca7 \u0c97\u0cae\u0ccd\u0cad\u0cc0\u0cb0 \u0caa\u0cbe\u0cb5\u0cbe\u0caf <br/> B\u0cb9\u0c97\u0cb5\u0ca5 \u0cad\u0c95\u0ccd\u0ca5 \u0cb9\u0ccd\u0cb0\u0cbf\u0ca6\u0caf \u0caa\u0ca6\u0ccd\u0cae \u0cad\u0ca8\u0cc1 \u0cb0\u0cc2\u0caa\u0cbe\u0caf"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 3", "T": "\u0c97\u0ccd\u0c85\u0cb0\u0ca7\u0ccd\u0cb0\u0cbf\u0ca5 \u0cb0\u0ca5 \u0c9b\u0cb0\u0ca8 \u0c95\u0cae\u0ccd\u0cac\u0cc1 \u0caa\u0c99\u0ccd\u0c95\u0c9c <br/> \u0c97\u0ccd\u0c94 \u0cae\u0cca\u0ca7\u0c95\u0cc0 \u0cb8\u0cbe\u0cb0\u0ccd\u0c99\u0ccd\u0c96\u0cbe\u0caf <br/> \u0ca3\u0cbf\u0cb0\u0cb5\u0ca5\u0cbf \u0c95\u0cb0 \u0cb8\u0cca\u0cad\u0cbf \u0ca8\u0cbf\u0c9c \u0c95\u0ccc\u0cb8\u0ccd\u0ca5\u0cc1\u0cac \u0cad\u0cc2\u0cb6\u0cbe\u0caf <br/> \u0caa\u0cb0\u0cbf \u0cb9\u0ccd\u0cb0\u0cbf\u0ca5 \u0caa\u0cb0 \u0cad\u0c95\u0ccd\u0cb6 \u0caa\u0c95\u0ccd\u0cb6\u0cbf\u0cb0\u0c9c \u0cb5\u0cbe\u0cb9\u0cbe\u0caf"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 4", "T": "\u0c86\u0cae\u0caf\u0cbe\u0ca5\u0cbf \u0cb5\u0cbf\u0cb0\u0cb9\u0cbf\u0ca5\u0cbe\u0c95\u0cbf\u0cb2 \u0cae\u0caf\u0cbe\u0caf <br/> \u0c95\u0cbe\u0cae \u0c95\u0cca\u0ca4\u0cbf \u0cb8\u0cc1\u0ca8\u0ccd\u0ca7\u0cb0\u0caf <br/> \u0ca3\u0cae \u0cb0\u0cc2\u0c85 \u0cb0\u0cb9\u0cbf\u0ca5 \u0cad\u0cc2\u0cae\u0cc8\u0c95 \u0cb0\u0cc2\u0caa\u0cbe\u0caf <br/> \u0cb7\u0cca\u0cae \u0cb8\u0cc2\u0cb0\u0ccd\u0caf\u0ccd \u0ca8\u0caf\u0ca8 \u0cb8\u0cbe\u0cae \u0c97\u0cbe\u0ca8 \u0c96\u0cc6\u0caf\u0cbe\u0caf"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 5", "T": "\u0c9e\u0c97\u0ca7\u0cc6\u0c97 \u0ca8\u0cbe\u0ca7\u0cbe\u0caf \u0c9c\u0cbe\u0ca5 \u0cae\u0ca7\u0ca8\u0cbe\u0caf <br/> \u0c9e\u0cbf\u0ca5 \u0cb5\u0cc8\u0cb0\u0cbf \u0cae\u0ca8\u0ccd\u0ca6\u0cb2\u0cbe\u0caf <br/> \u0ca8\u0cbf\u0c97\u0cae \u0ca8\u0cbf\u0cb6\u0ccd\u0cb5\u0cbe\u0cb8\u0cbe\u0caf \u0ca8\u0cbf\u0ca5\u0ccd\u0caf\u0cbe\u0ca8\u0ca8\u0ccd\u0ca7 \u0cb0\u0cc2\u0caa\u0cbe\u0caf <br/> \u0ca8\u0cbf\u0cb0\u0cc1\u0caa\u0cae \u0c97\u0cca\u0c95\u0cc1\u0cb2 \u0ca8\u0cc0\u0ca5 \u0ca8\u0cb5 \u0ca8\u0cc0\u0ca5\u0cbe\u0caf"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 6", "T": "V\u0c86\u0cb0\u0cbf\u0c9c \u0cb2\u0cca\u0c9b\u0ca8\u0cbe\u0caf \u0cb5\u0cbe\u0cb8\u0cc1\u0ca6\u0cc6\u0cb5\u0cbe\u0caf <br/> \u0cb5\u0cb0 \u0cae\u0cc1\u0ca8\u0cbf \u0caa\u0cbe\u0cb0\u0cbf\u0c9c\u0cbe\u0ca5\u0cbe\u0caf <br/> \u0c95\u0cb0\u0cc1\u0ca8\u0cbe\u0c95\u0cb0 \u0cad\u0c95\u0ccd\u0ca5 \u0c95\u0cbe\u0cb0\u0cc1\u0ca8\u0ccd\u0caf \u0cb8\u0cbe\u0cb0\u0cbe\u0caf <br/> \u0ca3\u0cbe\u0cb0\u0caf\u0ca8 \u0ca0\u0cc0\u0cb0\u0ccd\u0ca5 \u0ca8\u0ca5 \u0c9b\u0cb0\u0ca8\u0cbe\u0cac\u0ccd\u0c9c\u0cbe\u0caf"}]}, {"L": "Malayalam", "text": [{"M": "\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d23\u0d30\u0d2f\u0d28\u0d3e\u0d2f \u0d23\u0d2e\u0d4a \u0d02\u0d06\u0d27\u0d35\u0d2f\u0d28\u0d2e\u0d4a"}, {"M": "\u0d05\u0d28\u0d41\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d23\u0d3e\u0d17\u0d46\u0d28\u0d4d\u0d26\u0d4d\u0d30 \u0d37\u0d2f\u0d28\u0d3e\u0d2f \u0d23\u0d28\u0d4d\u0d27\u0d3f\u0d25\u0d3e\u0d15\u0d3f\u0d32 \u0d33\u0d4a\u0d16\u0d2f"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 1", "T": "\u0d23\u0d3e\u0d2d\u0d3f \u0d15\u0d2e\u0d32 \u0d1c\u0d28\u0d3f\u0d25 \u0d35\u0d3f\u0d27\u0d3f \u0d2e\u0d41\u0d16\u0d3e\u0d2f <br/> \u0d23\u0d42\u0d25\u0d28 \u0d1e\u0d32 \u0d25\u0d3e\u0d30\u0d3e\u0d2a\u0d2f <br/> \u0d23\u0d3e\u0d28\u0d3e \u0d38\u0d30\u0d3e\u0d38\u0d30 \u0d28\u0d3e\u0d30\u0d40 \u0d28\u0d30 \u0d30\u0d42\u0d2a\u0d3e\u0d2f <br/> \u0d23\u0d3e\u0d30\u0d27\u0d3e\u0d27\u0d3f \u0d2e\u0d41\u0d28\u0d3f \u0d35\u0d28\u0d4d\u0d27\u0d4d\u0d2f\u0d3e\u0d28\u0d28\u0d4d\u0d25 \u0d17\u0d28 \u0d02\u0d05\u0d2f\u0d3e\u0d2f"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 2", "T": "\u0d06\u0d15\u0d28\u0d3f\u0d25 \u0d19\u0d41\u0d28 \u0d19\u0d28\u0d3e\u0d2f \u0d06\u0d17\u0d2e \u0d17\u0d4d\u0d39\u0d46\u0d2f\u0d3e\u0d2f <br/> V\u0d07\u0d16\u0d32\u0d3f\u0d25 B\u0d39\u0d2f\u0d4d \u0d38\u0d4a\u0d15\u0d3e\u0d2f <br/> \u0d19\u0d17\u0d28 \u0d28\u0d3f\u0d30\u0d4d\u0d2e\u0d32 \u0d2c\u0d4a\u0d27 \u0d17\u0d2e\u0d4d\u0d2d\u0d40\u0d30 \u0d2a\u0d3e\u0d35\u0d3e\u0d2f <br/> B\u0d39\u0d17\u0d35\u0d25 \u0d2d\u0d15\u0d4d\u0d25 \u0d39\u0d4d\u0d30\u0d3f\u0d26\u0d2f \u0d2a\u0d26\u0d4d\u0d2e \u0d2d\u0d28\u0d41 \u0d30\u0d42\u0d2a\u0d3e\u0d2f"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 3", "T": "\u0d17\u0d4d\u0d05\u0d30\u0d27\u0d4d\u0d30\u0d3f\u0d25 \u0d30\u0d25 \u0d1b\u0d30\u0d28 \u0d15\u0d2e\u0d4d\u0d2c\u0d41 \u0d2a\u0d19\u0d4d\u0d15\u0d1c <br/> \u0d17\u0d4d\u0d14 \u0d2e\u0d4a\u0d27\u0d15\u0d40 \u0d38\u0d3e\u0d30\u0d4d\u0d19\u0d4d\u0d16\u0d3e\u0d2f <br/> \u0d23\u0d3f\u0d30\u0d35\u0d25\u0d3f \u0d15\u0d30 \u0d38\u0d4a\u0d2d\u0d3f \u0d28\u0d3f\u0d1c \u0d15\u0d4c\u0d38\u0d4d\u0d25\u0d41\u0d2c \u0d2d\u0d42\u0d36\u0d3e\u0d2f <br/> \u0d2a\u0d30\u0d3f \u0d39\u0d4d\u0d30\u0d3f\u0d25 \u0d2a\u0d30 \u0d2d\u0d15\u0d4d\u0d36 \u0d2a\u0d15\u0d4d\u0d36\u0d3f\u0d30\u0d1c \u0d35\u0d3e\u0d39\u0d3e\u0d2f"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 4", "T": "\u0d06\u0d2e\u0d2f\u0d3e\u0d25\u0d3f \u0d35\u0d3f\u0d30\u0d39\u0d3f\u0d25\u0d3e\u0d15\u0d3f\u0d32 \u0d2e\u0d2f\u0d3e\u0d2f <br/> \u0d15\u0d3e\u0d2e \u0d15\u0d4a\u0d24\u0d3f \u0d38\u0d41\u0d28\u0d4d\u0d27\u0d30\u0d2f <br/> \u0d23\u0d2e \u0d30\u0d42\u0d05 \u0d30\u0d39\u0d3f\u0d25 \u0d2d\u0d42\u0d2e\u0d48\u0d15 \u0d30\u0d42\u0d2a\u0d3e\u0d2f <br/> \u0d37\u0d4a\u0d2e \u0d38\u0d42\u0d30\u0d4d\u0d2f\u0d4d \u0d28\u0d2f\u0d28 \u0d38\u0d3e\u0d2e \u0d17\u0d3e\u0d28 \u0d16\u0d46\u0d2f\u0d3e\u0d2f"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 5", "T": "\u0d1e\u0d17\u0d27\u0d46\u0d17 \u0d28\u0d3e\u0d27\u0d3e\u0d2f \u0d1c\u0d3e\u0d25 \u0d2e\u0d27\u0d28\u0d3e\u0d2f <br/> \u0d1e\u0d3f\u0d25 \u0d35\u0d48\u0d30\u0d3f \u0d2e\u0d28\u0d4d\u0d26\u0d32\u0d3e\u0d2f <br/> \u0d28\u0d3f\u0d17\u0d2e \u0d28\u0d3f\u0d36\u0d4d\u0d35\u0d3e\u0d38\u0d3e\u0d2f \u0d28\u0d3f\u0d25\u0d4d\u0d2f\u0d3e\u0d28\u0d28\u0d4d\u0d27 \u0d30\u0d42\u0d2a\u0d3e\u0d2f <br/> \u0d28\u0d3f\u0d30\u0d41\u0d2a\u0d2e \u0d17\u0d4a\u0d15\u0d41\u0d32 \u0d28\u0d40\u0d25 \u0d28\u0d35 \u0d28\u0d40\u0d25\u0d3e\u0d2f"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 6", "T": "V\u0d06\u0d30\u0d3f\u0d1c \u0d32\u0d4a\u0d1b\u0d28\u0d3e\u0d2f \u0d35\u0d3e\u0d38\u0d41\u0d26\u0d46\u0d35\u0d3e\u0d2f <br/> \u0d35\u0d30 \u0d2e\u0d41\u0d28\u0d3f \u0d2a\u0d3e\u0d30\u0d3f\u0d1c\u0d3e\u0d25\u0d3e\u0d2f <br/> \u0d15\u0d30\u0d41\u0d28\u0d3e\u0d15\u0d30 \u0d2d\u0d15\u0d4d\u0d25 \u0d15\u0d3e\u0d30\u0d41\u0d28\u0d4d\u0d2f \u0d38\u0d3e\u0d30\u0d3e\u0d2f <br/> \u0d23\u0d3e\u0d30\u0d2f\u0d28 \u0d20\u0d40\u0d30\u0d4d\u0d25 \u0d28\u0d25 \u0d1b\u0d30\u0d28\u0d3e\u0d2c\u0d4d\u0d1c\u0d3e\u0d2f"}]}, {"L": "Sanskrit", "text": [{"M": "\u092a\u0932\u094d\u0932\u0935\u093f", "T": "\u0923\u0930\u092f\u0928\u093e\u092f \u0923\u092e\u094b \u0902\u0906\u0927\u0935\u092f\u0928\u092e\u094b"}, {"M": "\u0905\u0928\u0941\u092a\u0932\u094d\u0932\u0935\u093f", "T": "\u0923\u093e\u0917\u0947\u0928\u094d\u0926\u094d\u0930 \u0937\u092f\u0928\u093e\u092f \u0923\u0928\u094d\u0927\u093f\u0925\u093e\u0915\u093f\u0932 \u0932\u094a\