e \u0cb9\u0cc7 \u0cb8\u0cc1\u0c97\u0cc1\u0ca3 \u0cb8\u0cbe\u0ca8\u0ccd\u0ca6\u0ccd\u0cb0"}, {"M": "\u0c85\u0ca8\u0cc1\u0caa\u0cb2\u0ccd\u0cb2\u0cb5\u0cbf", "T": "\u0caa\u0c9a\u0ccd\u0c9a \u0cb5\u0cbf\u0cb2\u0ccd\u0ca4\u0cc1\u0ca8\u0cbf\u0c95\u0ccd(\u0c85)\u0ca8\u0ccd\u0ca8 \u0caa\u0cbe\u0cb2\u0cbf\u0ca4 \u0cb8\u0cc1\u0cb0\u0ccd(\u0c8f)\u0ca8\u0ccd\u0ca6\u0ccd\u0cb0 (\u0cb9\u0cc6\u0c9a\u0ccd\u0c9a\u0cb0\u0cbf\u0c95)"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 1", "T": "\u0c95\u0ca8\u0c95 \u0cae\u0caf\u0cae\u0ccc \u0cae\u0c95\u0cc1\u0c9f \u0c95\u0cbe\u0ca8\u0ccd\u0ca4\u0cbf \u0cae\u0cc6\u0cb0\u0caf\u0c97\u0ca8\u0cc1 <br/> \u0c98\u0ca8\u0cae\u0cc8\u0ca8 \u0c95\u0cc1\u0ca3\u0ccd\u0ca1\u0cb2 \u0caf\u0cc1\u0c97\u0cae\u0ccd\u0cac\u0cc1 \u0c95\u0ca6\u0cb2\u0c97\u0ca8\u0cc1 <br/> \u0c98\u0ca8\u0cae\u0cc8\u0ca8 \u0ca8\u0cc2\u0caa\u0cc1\u0cb0 \u0caf\u0cc1\u0c97\u0cae\u0ccd\u0cac\u0cc1 \u0c98\u0cb2\u0ccd\u0cb2\u0ccd(\u0c85)\u0ca8\u0ca8\u0cc1 <br/> \u0cb8\u0ca8\u0c95\u0ccd(\u0c86)\u0ca6\u0cc1\u0cb2\u0ccd(\u0c8e)\u0cb2\u0ccd\u0cb2 \u0c95\u0ca8\u0cbf \u0cb8\u0ca8\u0ccd\u0ca4\u0cb8\u0cbf\u0cb2\u0ccd\u0cb2\u0c97\u0ca8\u0cc1 (\u0cb9\u0cc6\u0c9a\u0ccd\u0c9a\u0cb0\u0cbf\u0c95)"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 2", "T": "\u0c86\u0ca3\u0cbf \u0cae\u0cc1\u0ca4\u0ccd\u0caf\u0cbe\u0cb2 \u0cb8\u0cb0\u0cc1\u0cb2\u0ccd(\u0c85)\u0cb2\u0ccd\u0cb2\u0cb2\u0cbe\u0ca1\u0c97\u0ca8\u0cc1 <br/> \u0cb5\u0cbe\u0ca3\u0cbf \u0caa\u0ca4\u0ccd(\u0c88)\u0ca8\u0ccd\u0ca6\u0ccd\u0cb0\u0cc1\u0cb2\u0ccd(\u0c87)\u0cb0\u0cc1 \u0cb5\u0cb0\u0cc1\u0cb8 \u0caa\u0cca\u0c97\u0ca1\u0c97\u0ca8\u0cc1 <br/> \u0cae\u0cbe\u0ca3\u0cbf\u0c95\u0ccd\u0caf \u0cb8\u0ccb\u0caa\u0cbe\u0ca8\u0cae\u0ccd(\u0c85)\u0ca8\u0ccd\u0ca6\u0cc1 \u0cae\u0cc6\u0cb2\u0ccd\u0cb2\u0c97\u0ca8\u0cc1 <br/> \u0cb5\u0cc0\u0ca3 \u0caa\u0cb2\u0ccd\u0c95\u0cc1\u0cb2 \u0cb5\u0cbf\u0ca8\u0cc1\u0c9a\u0cc1 \u0cb5\u0cc7\u0ca1\u0ccd\u0c95 \u0c9a\u0cc6\u0cb2\u0ccd\u0cb2\u0c97\u0ca8\u0cc1 (\u0cb9\u0cc6\u0c9a\u0ccd\u0c9a\u0cb0\u0cbf\u0c95)"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 3", "T": "\u0ca8\u0cbf\u0ca8\u0cc1 \u0c9c\u0cc2\u0ca1 \u0cb5\u0c9a\u0ccd\u0c9a\u0cc1 \u0cad\u0c97\u0cbf\u0ca8\u0cbf \u0c95\u0cb0\u0cae\u0ccd\u0cac\u0cc1 \u0c9a\u0cbf\u0cb2\u0cc1\u0c95 <br/> \u0cae\u0ca8\u0cb8\u0cc1 \u0cb0\u0ca8\u0ccd\u0c9c\u0cbf\u0cb2\u0ccd\u0cb2 \u0ca8\u0cc0 \u0cae\u0cb9\u0cbf\u0cae\u0cb2\u0ca8\u0cc1 \u0caa\u0cb2\u0cc1\u0c95 <br/> \u0cae\u0cbf\u0ca8\u0cc1 \u0cb5\u0cbe\u0cb8\u0cc1\u0cb2\u0ccd(\u0c8e)\u0cb2\u0ccd\u0cb2 \u0cb5\u0cbf\u0cb0\u0cc1\u0cb2\u0ca8\u0cc1 \u0c9a\u0cbe\u0cb2 \u0c9c\u0cbf\u0cb2\u0cc1\u0c95 <br/> \u0c98\u0ca8 \u0ca4\u0ccd\u0caf\u0cbe\u0c97\u0cb0\u0cbe\u0c9c\u0cc1 \u0c95\u0ca8\u0cc1\u0c97\u0cca\u0ca8 \u0cae\u0cc1\u0ca6\u0ccd\u0ca6\u0cc1 \u0c97\u0cc1\u0cb2\u0cc1\u0c95 (\u0cb9\u0cc6\u0c9a\u0ccd\u0c9a\u0cb0\u0cbf\u0c95)"}]}, {"L": "Malayalam", "text": [{"M": "\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d39\u0d46\u0d1a\u0d4d\u0d1a\u0d30\u0d3f\u0d15\u0d17\u0d3e \u0d30\u0d3e\u0d30\u0d3e \u0d39\u0d47 \u0d30\u0d3e\u0d2e \u0d1a\u0d28\u0d4d\u0d26\u0d4d\u0d30 <br/> \u0d39\u0d46\u0d1a\u0d4d\u0d1a\u0d30\u0d3f\u0d15\u0d17\u0d3e \u0d30\u0d3e\u0d30\u0d3e \u0d39\u0d47 \u0d38\u0d41\u0d17\u0d41\u0d23 \u0d38\u0d3e\u0d28\u0d4d\u0d26\u0d4d\u0d30"}, {"M": "\u0d05\u0d28\u0d41\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d2a\u0d1a\u0d4d\u0d1a \u0d35\u0d3f\u0d32\u0d4d\u0d24\u0d41\u0d28\u0d3f\u0d15\u0d4d(\u0d05)\u0d28\u0d4d\u0d28 \u0d2a\u0d3e\u0d32\u0d3f\u0d24 \u0d38\u0d41\u0d30\u0d4d(\u0d0f)\u0d28\u0d4d\u0d26\u0d4d\u0d30 (\u0d39\u0d46\u0d1a\u0d4d\u0d1a\u0d30\u0d3f\u0d15)"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 1", "T": "\u0d15\u0d28\u0d15 \u0d2e\u0d2f\u0d2e\u0d4c \u0d2e\u0d15\u0d41\u0d1f \u0d15\u0d3e\u0d28\u0d4d\u0d24\u0d3f \u0d2e\u0d46\u0d30\u0d2f\u0d17\u0d28\u0d41 <br/> \u0d18\u0d28\u0d2e\u0d48\u0d28 \u0d15\u0d41\u0d23\u0d4d\u0d21\u0d32 \u0d2f\u0d41\u0d17\u0d2e\u0d4d\u0d2c\u0d41 \u0d15\u0d26\u0d32\u0d17\u0d28\u0d41 <br/> \u0d18\u0d28\u0d2e\u0d48\u0d28 \u0d28\u0d42\u0d2a\u0d41\u0d30 \u0d2f\u0d41\u0d17\u0d2e\u0d4d\u0d2c\u0d41 \u0d18\