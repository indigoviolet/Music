d\u0ba3\u0bc1\u0ba9\u0bbf\u0ba4\u0bcb\u0ba9\u0bc1"}, {"M": "\u0b85\u0ba9\u0bc1\u0baa\u0bb2\u0bcd\u0bb2\u0bb5\u0bbf", "T": "\u0b85\u0b99\u0bcd\u0b95-\u0bb0\u0b99\u0bcd\u0b95 \u0bb5\u0bc8\u0baa\u0bcb\u0b95\u0bae\u0bc1\u0ba4\u0bcb <br/> \u0b95\u0bcb\u0baa\u0bcd(\u0b86)\u0b99\u0bcd\u0b95\u0ba9\u0bbe\u0bae\u0ba3\u0bc1\u0bb2\u0bcd(\u0b8e)\u0ba8\u0bcd\u0ba4\u0bcb \u0b9a\u0bca\u0b95\u0b9a\u0bc1\u0b95 (\u0b9a\u0bcd\u0bb0\u0bc1)"}, {"M": "\u0b9a\u0bb0\u0ba3\u0bae\u0bcd 1", "T": "\u0ba8\u0bb5\u0bcd\u0bb5\u0bc1\u0b9a\u0bc1 \u0b95\u0bc1\u0bb2\u0bc1\u0b95\u0bc1\u0b9a\u0bc1\u0ba9\u0bcd(\u0b92)\u0b95\u0ba4\u0bc6 \u0b95\u0bca\u0baa\u0bcd\u0baa\u0bc1\u0ba9 <br/> \u0baa\u0bc1\u0bb5\u0bcd\u0bb5\u0bc1\u0bb2 \u0bae\u0bc1\u0b9f\u0bc1\u0b9a\u0bc1\u0b9a\u0bc1\u0ba9\u0bcd(\u0b92)\u0b95\u0ba4\u0bc6 <br/> \u0ba4\u0bc1\u0bb5\u0bcd\u0bb5\u0bc1\u0b9a\u0bc1 \u0b95\u0bc1\u0bb0\u0bc1\u0bb2\u0ba9\u0bc1(\u0ba8\u0bcd)\u0b92\u0b95\u0ba4\u0bc6 \u0b95\u0bcd\u0bb0\u0bc1\u0bb7\u0bcd\u0ba3\u0bc1\u0ba9\u0bbf <br/> \u0bb0\u0bb5\u0bcd\u0bb5 \u0b9c\u0bc7\u0baf\u0bc1\u0b9a\u0bc1\u0ba9\u0bcd(\u0b92)\u0b95\u0ba4\u0bc6 \u0bb5\u0bc7\u0b9f\u0bcd\u0b95\u0b95 (\u0b9a\u0bcd\u0bb0\u0bc1)"}, {"M": "\u0b9a\u0bb0\u0ba3\u0bae\u0bcd 2", "T": "\u0bae\u0b95\u0b9f\u0bc1 \u0bb5\u0bc0\u0b9f\u0bcd(\u0b85)\u0ba8\u0bc1\u0b9a\u0bc1\u0ba9\u0bcd(\u0b92)\u0b95\u0ba4\u0bc6 \u0bb0\u0bb5\u0bbf\u0b95\u0baf\u0bc1 <br/> \u0baa\u0bbf\u0b95\u0bc1\u0bb5\u0bc1\u0ba9 \u0b9c\u0bc7\u0bb0\u0bcd\u0b9a\u0bc1\u0b9a\u0bc1\u0ba9\u0bcd(\u0b92)\u0b95\u0ba4\u0bc6 <br/> \u0ba4\u0b95\u0bc1\u0ba9\u0bc1 \u0ba4\u0ba9\u0b95\u0bcd(\u0b85)\u0ba8\u0bc1\u0b9a\u0bc1\u0ba9\u0bcd(\u0b92)\u0b95\u0ba4\u0bc6 \u0baa\u0bbe\u0ba4 <br/> \u0baf\u0bc1\u0b95\u0bae\u0bc1\u0bb2\u0ba9\u0bcd(\u0b92)\u0ba4\u0bcd\u0ba4\u0bc1\u0b9a\u0bc1\u0ba9\u0bcd(\u0b92)\u0b95\u0ba4\u0bc6 \u0bb5\u0bc7\u0b9f\u0bcd\u0b95\u0b95 (\u0b9a\u0bcd\u0bb0\u0bc1)"}, {"M": "\u0b9a\u0bb0\u0ba3\u0bae\u0bcd 3", "T": "\u0b9a\u0bca\u0b95\u0bcd\u0b95\u0bc1\u0b9a\u0bc1 \u0b9a\u0bcb\u0bb2\u0bc1\u0b9a\u0bc1\u0ba9\u0bcd(\u0b92)\u0b95\u0ba4\u0bc6 \u0b95\u0bcd\u0bb0\u0bc1\u0bb7\u0bcd\u0ba3\u0bc1\u0ba9\u0bbf <br/> \u0b95\u0bcd\u0bb0\u0b95\u0bcd\u0b95\u0bc1\u0ba9 \u0bae\u0bc1\u0ba4\u0bcd\u0ba4\u0bcd(\u0b87)\u0b9f\u0bc1(\u0ba8\u0bcd)\u0b92\u0b95\u0ba4\u0bc6 <br/> \u0baa\u0b95\u0bcd\u0b95\u0b95\u0bc1 \u0bb0\u0bae\u0bcd\u0bae\u0bcd(\u0b85)\u0ba8\u0bc1\u0b9a\u0bc1\u0ba9\u0bcd(\u0b92)\u0b95\u0ba4\u0bc6 \u0bae\u0b9f\u0bc1\u0baa\u0bc1\u0bb2\u0ba9\u0bcd- <br/> (\u0b85)\u0b95\u0bcd\u0b95\u0bb0\u0ba9\u0bcd(\u0b92)\u0b9a\u0b95\u0bc1\u0b9a\u0bc1\u0ba9\u0bcd(\u0b92)\u0b95\u0ba4\u0bc6 \u0bb5\u0bc7\u0b9f\u0bcd\u0b95\u0b95 (\u0b9a\u0bcd\u0bb0\u0bc1)"}, {"M": "\u0b9a\u0bb0\u0ba3\u0bae\u0bcd 4", "T": "\u0baa\u0bb0\u0bbf\u0bae\u0bb3\u0bae\u0bc1\u0bb2\u0bcd(\u0b85)\u0ba8\u0bcd\u0ba4\u0bc1\u0b9a\u0bc1\u0ba9\u0bcd(\u0b92)\u0b95\u0ba4\u0bc6 \u0bb6\u0bcd\u0bb0\u0bc0 <br/> \u0bb9\u0bb0\u0bbf \u0bb9\u0bb0\u0bbf(\u0baf\u0bcd)\u0b85\u0ba9\u0bc1\u0b9a\u0bc1\u0ba9\u0bc1(\u0ba8\u0bcd)\u0b92\u0b95\u0ba4\u0bc6 <br/> \u0b89\u0bb0\u0bae\u0bc1\u0ba9 \u0b9c\u0bc7\u0bb0\u0bcd\u0b9a\u0bc1\u0b9a\u0bc1\u0ba9\u0bcd(\u0b92)\u0b95\u0ba4\u0bc6 \u0baa\u0baf\u0bcd\u0baf\u0bc6\u0ba4 <br/> \u0b9c\u0bb0\u0bbf\u0baa\u0bbf \u0bb5\u0bc7\u0b9f\u0bc1\u0b95\u0bca\u0ba9\u0bc1\u0b9a\u0bc1\u0ba9\u0bcd(\u0b92)\u0b95\u0ba4\u0bc6 \u0bb5\u0bc7\u0b9f\u0bcd\u0b95\u0b95 (\u0b9a\u0bcd\u0bb0\u0bc1)"}, {"M": "\u0b9a\u0bb0\u0ba3\u0bae\u0bcd 5", "T": "\u0b9a\u0bbe\u0bb0\u0bb8\u0bcd(\u0b86)\u0b95\u0bcd\u0bb7(\u0baf\u0bcd)\u0b85\u0ba9\u0bc1\u0b9a\u0bc1\u0ba9\u0bcd(\u0b92)\u0b95\u0ba4\u0bc6 \u0b95\u0ba9\u0bc1 <br/> \u0b9a\u0bc8\u0b95\u0ba9\u0bc1 \u0baa\u0bbf\u0bb2\u0bc1\u0b9a\u0bc1\u0b9a\u0bc1\u0ba9\u0bcd(\u0b92)\u0b95\u0ba4\u0bc6 <br/> \u0bb0\u0bbe\u0bb0\u0bbe(\u0baf\u0bcd)\u0b85\u0ba9\u0bc1\u0b9a\u0bc1\u0ba9\u0bc1(\u0ba8\u0bcd)\u0b92\u0b95\u0ba4\u0bc6 \u0ba4\u0bcd\u0baf\u0bbe\u0b95- <br/> \u0bb0\u0bbe\u0b9c \u0b9a\u0b95\u0bc1\u0b9f\u0bcd(\u0b85)\u0ba8\u0bc1\u0b9a\u0bc1\u0ba9\u0bcd(\u0b92)\u0b95\u0ba4\u0bc6 \u0bb5\u0bc7\u0b9f\u0bcd\u0b95\u0b95 (\u0b9a\u0bcd\u0bb0\u0bc1)"}]}, {"L": "Telugu", "text": [{"M": "\u0c2a\u0c32\u0c4d\u0c32\u0c35\u0c3f", "T": "\u0c36\u0c43\u0c19\u0c4d\u0c17\u0c3e\u0c30\u0c3f\u0c28\u0c4d\u0c1a\u0c41\u0c15\u0c4a\u0c28\u0c3f \u0c35\u0c46\u0c21\u0c32\u0c3f\u0c30\u0c3f \u0c36\u0c4d\u0c30\u0c40 \u0c15\u0c43\u0c37\u0c4d\u0c23\u0c41\u0c28\u0c3f\u0c24\u0c4b\u0c28\u0c41"}, {"M": "\u0c05\u0c28\u0c41\u0c2a\u0c32\u0c4d\u0c32\u0c35\u0c3f", "T": "\u0c05\u0c19\u0c4d\u0c17-\u0c30\u0c19\u0c4d\u0c17 \u0c35\u0c48\u0c2d\u0c4b\u0c17\u0c2e\u0c41\u0c24\u0c4b <br/> \u0c17\u0c4b\u0c2a\u0c4d(\u0c06)\u0c19\u0c4d\u0c17\u0c28\u0c3e\u0c2e\u0c23\u0c41\u0c32\u0c4d(\u0c0e)\u0c28\u0c4d\u0c24\u0c4b \u0c38\u0c4a\u0c17\u0c38\u0c41\u0c17 (\u0c36\u0c43)"}, {"M": "\u0c1a\u0c30\u0c23\u0c2e\u0c4d 1", "T": "\u0c28\u0c35\u0c4d\u0c35\u0c41\u0c1a\u0c41 \u0c15\u0c41\u0c32\u0c41\u0c15\u0c41\u0c1a\u0c41\u0c28\u0c4d(\u0c12)\u0c15\u0c24\u0c46 \u0c15\u0c4a\u0c2a\u0c4d\u0c2a\u0c41\u0c28 <br/> \u0c2a\u0c41\u0c35\u0c4d\u0c35\u0c41\u0c32 \u0c2e\u0c41\u0c21\u0c41\u0c1a\u0c41\u0c1a\u0c41\u0c28\u0c4d(\u0c12)\u0c15\u0c24\u0c46 <br/> \u0c26\u0c41\u0c35\u0c4d\u0c35\u0c41\u0c1a\u0c41 \u0c15\u0c41\u0c30\u0c41\u0c32\u0c28\u0c41(\u0c28\u0c4d)\u0c12\u0c15\u0c24\u0c46 \u0c15\u0c43\u0c37\u0c4d\u0c23\u0c41\u0c28\u0c3f <br/> \u0c30\u0c35\u0c4d\u0c35 \u0c1c\u0c47\u0c2f\u0c41\u0c1a\u0c41\u0c28\u0c4d(\u0c12)\u0c15\u0c24\u0c46 \u0c35\u0c47\u0c21\u0c4d\u0c15\u0c17 (\u0c36\u0c43)"}, {"M": "\u0c1a\u0c30\u0c23\u0c2e\u0c4d 2", "T": "\u0c2e\u0c17\u0c21\u0c41 \u0c35\u0c40\u0c21\u0c4d(\u0c05)\u0c28\u0c41\u0c1a\u0c41\u0c28\u0c4d(\u0c12)\u0c15\u0c24\u0c46 \u0c30\u0c35\u0c3f\u0c15\u0c2f\u0c41 <br/> \u0c2c\u0c3f\u0c17\u0c41\u0c35\u0c41\u0c28 \u0c1c\u0c47\u0c30\u0c4d\u0c1a\u0c41\u0c1a\u0c41\u0c28\u0c4d(\u0c12)\u0c15\u0c24\u0c46 <br/> \u0c24\u0c17\u0c41\u0c28\u0c41 \u0c24\u0c28\u0c15\u0c4d(\u0c05)\u0c28\u0c41\u0c1a\u0c41\u0c28\u0c4d(\u0c12)\u0c15\u0c24\u0c46 \u0c2a\u0c3e\u0c26 <br/> \u0c2f\u0c41\u0c17\u0c2e\u0c41\u0c32\u0c28\u0c4d(\u0c12)\u0c24\u0c4d\u0c24\u0c41\u0c1a\u0c41\u0c28\u0c4d(\u0c12)\u0c15\u0c24\u0c46 \u0c35\u0c47\u0c21\u0c4d\u0c15\u0c17 (\u0c36\u0c43)"}, {"M": "\u0c1a\u0c30\u0c23\u0c2e\u0c4d 3", "T": "\u0c38\u0c4a\u0c15\u0c4d\u0c15\u0c41\u0c1a\u0c41 \u0c38\u0c4b\u0c32\u0c41\u0c1a\u0c41\u0c28\u0c4d(\u0c12)\u0c15\u0c24\u0c46 \u0c15\u0c43\u0c37\u0c4d\u0c23\u0c41\u0c28\u0c3f <br/> \u0c17\u0c4d\u0c30\u0c15\u0c4d\u0c15\u0c41\u0c28 \u0c2e\u0c41\u0c26\u0c4d\u0c26\u0c4d(\u0c07)\u0c21\u0c41(\u0c28\u0c4d)\u0c12\u0c15\u0c24\u0c46 <br/> \u0c2a\u0c15\u0c4d\u0c15\u0c17\u0c41 \u0c30\u0c2e\u0c4d\u0c2e\u0c4d(\u0c05)\u0c28\u0c41\u0c1a\u0c41\u0c28\u0c4d(\u0c12)\u0c15\u0c24\u0c46 \u0c2e\u0c21\u0c41\u0c2a\u0c41\u0c32\u0c28\u0c4d- <br/> (\u0c05)\u0c15\u0c4d\u0c15\u0c30\u0c28\u0c4d(\u0c12)\u0c38\u0c17\u0c41\u0c1a\u0c41\u0c28\u0c4d(\u0c12)\u0c15\u0c24\u0c46 \u0c35\u0c47\u0c21\u0c4d\u0c15\u0c17 (\u0c36\u0c43)"}, {"M": "\u0c1a\u0c30\u0c23\u0c2e\u0c4d 4", "T": "\u0c2a\u0c30\u0c3f\u0c2e\u0c33\u0c2e\u0c41\u0c32\u0c4d(\u0c05)\u0c28\u0c4d\u0c26\u0c41\u0c1a\u0c41\u0c28\u0c4d(\u0c12)\u0c15\u0c24\u0c46 \u0c36\u0c4d\u0c30\u0c40 <br/> \u0c39\u0c30\u0c3f \u0c39\u0c30\u0c3f(\u0c2f\u0c4d)\u0c05\u0c28\u0c41\u0c1a\u0c41\u0c28\u0c41(\u0c28\u0c4d)\u0c12\u0c15\u0c24\u0c46 <br/> \u0c09\u0c30\u0c2e\u0c41\u0c28 \u0c1c\u0c47\u0c30\u0c4d\u0c1a\u0c41\u0c1a\u0c41\u0c28\u0c4d(\u0c12)\u0c15\u0c24\u0c46 \u0c2a\u0c2f\u0c4d\u0c2f\u0c46\u0c26 <br/> \u0c1c\u0c30\u0c3f\u0c2a\u0c3f \u0c35\u0c47\u0c21\u0c41\u0c15\u0c4a\u0c28\u0c41\u0c1a\u0c41\u0c28\u0c4d(\u0c12)\u0c15\u0c24\u0c46 \u0c35\u0c47\u0c21\u0c4d\u0c15\u0c17 (\u0c36\u0c43)"}, {"M": "\u0c1a\u0c30\u0c23\u0c2e\u0c4d 5", "T": "\u0c38\u0c3e\u0c30\u0c38\u0c4d(\u0c06)\u0c15\u0c4d\u0c37(\u0c2f\u0c4d)\u0c05\u0c28\u0c41\u0c1a\u0c41\u0c28\u0c4d(\u0c12)\u0c15\u0c24\u0c46 \u0c15\u0c28\u0c41 <br/> \u0c38\u0c48\u0c17\u0c28\u0c41 \u0c2a\u0c3f\u0c32\u0c41\u0c1a\u0c41\u0c1a\u0c41\u0c28\u0c4d(\u0c12)\u0c15\u0c24\u0c46 <br/> \u0c30\u0c3e\u0c30\u0c3e(\u0c2f\u0c4d)\u0c05\u0c28\u0c41\u0c1a\u0c41\u0c28\u0c41(\u0c28\u0c4d)\u0c12\u0c15\u0c24\u0c46 \u0c24\u0c4d\u0c2f\u0c3e\u0c17- <br/> \u0c30\u0c3e\u0c1c \u0c38\u0c16\u0c41\u0c21\u0c4d(\u0c05)\u0c28\u0c41\u0c1a\u0c41\u0c28\u0c4d(\u0c12)\u0c15\u0c24\u0c46 \u0c35\u0c47\u0c21\u0c4d\u0c15\u0c17 (\u0c36\u0c43)"}]}, {"L": "Kannada", "text": [{"M": "\u0caa\u0cb2\u0ccd\u0cb2\u0cb5\u0cbf", "T": "\u0cb6\u0cc3\u0c99\u0ccd\u0c97\u0cbe\u0cb0\u0cbf\u0ca8\u0ccd\u0c9a\u0cc1\u0c95\u0cca\u0ca8\u0cbf \u0cb5\u0cc6\u0ca1\u0cb2\u0cbf\u0cb0\u0cbf \u0cb6\u0ccd\u0cb0\u0cc0 \u0c95\u0cc3\u0cb7\u0ccd\u0ca3\u0cc1\u0ca8\u0cbf\u0ca4\u0ccb\u0ca8\u0cc1"}, {"M": "\u0c85\u0ca8\u0cc1\u0caa\u0cb2\u0ccd\u0cb2\u0cb5\u0cbf", "T": "\u0c85\u0c99\u0ccd\u0c97-\u0cb0\u0c99\u0ccd\u0c97 \u0cb5\u0cc8\u0cad\u0ccb\u0c97\u0cae\u0cc1\u0ca4\u0ccb <br/> \u0c97\u0ccb\u0caa\u0ccd(\u0c86)\u0c99\u0ccd\u0c97\u0ca8\u0cbe\u0cae\u0ca3\u0cc1\u0cb2\u0ccd(\u0c8e)\u0ca8\u0ccd\u0ca4\u0ccb \u0cb8\u0cca\u0c97\u0cb8\u0cc1\u0c97 (\u0cb6\u0cc3)"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 1", "T": "\u0ca8\u0cb5\u0ccd\u0cb5\u0cc1\u0c9a\u0cc1 \u0c95\u0cc1\u0cb2\u0cc1\u0c95\u0cc1\u0c9a\u0cc1\u0ca8\u0ccd(\u0c92)\u0c95\u0ca4\u0cc6 \u0c95\u0cca\u0caa\u0ccd\u0caa\u0cc1\u0ca8 <br/> \u0caa\u0cc1\u0cb5\u0ccd\u0cb5\u0cc1\u0cb2 \u0cae\u0cc1\u0ca1\u0cc1\u0c9a\u0cc1\u0c9a\u0cc1\u0ca8\u0ccd(\u0c92)\u0c95\u0ca4\u0cc6 <br/> \u0ca6\u0cc1\u0cb5\u0ccd\u0cb5\u0cc1\u0c9a\u0cc1 \u0c95\u0cc1\u0cb0\u0cc1\u0cb2\u0ca8\u0cc1(\u0ca8\u0ccd)\u0c92\u0c95\u0ca4\u0cc6 \u0c95\u0cc3\u0cb7\u0ccd\u0ca3\u0cc1\u0ca8\u0cbf <br/> \u0cb0\u0cb5\u0ccd\u0cb5 \u0c9c\u0cc7\u0caf\u0cc1\u0c9a\u0cc1\u0ca8\u0ccd(\u0c92)\u0c95\u0ca4\u0cc6 \u0cb5\u0cc7\u0ca1\u0ccd\u0c95\u0c97 (\u0cb6\u0cc3)"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 2", "T": "\u0cae\u0c97\u0ca1\u0cc1 \u0cb5\u0cc0\u0ca1\u0ccd(\u0c85)\u0ca8\u0cc1\u0c9a\u0cc1\u0ca8\u0ccd(\u0c92)\u0c95\u0ca4\u0cc6 \u0cb0\u0cb5\u0cbf\u0c95\u0caf\u0cc1 <br/> \u0cac\u0cbf\u0c97\u0cc1\u0cb5\u0cc1\u0ca8 \u0c9c\u0cc7\u0cb0\u0ccd\u0c9a\u0cc1\u0c9a\u0cc1\u0ca8\u0ccd(\u0c92)\u0c95\u0ca4\u0cc6 <br/> \u0ca4\u0c97\u0cc1\u0ca8\u0cc1 \u0ca4\u0ca8\u0c95\u0ccd(\u0c85)\u0ca8\u0cc1\u0c9a\u0cc1\u0ca8\u0ccd(\u0c92)\u0c95\u0ca4\u0cc6 \u0caa\u0cbe\u0ca6 <br/> \u0caf\u0cc1\u0c97\u0cae\u0cc1\u0cb2\u0ca8\u0ccd(\u0c92)\u0ca4\u0ccd\u0ca4\u0cc1\u0c9a\u0cc1\u0ca8\u0ccd(\u0c92)\u0c95\u0ca4\u0cc6 \u0cb5\u0cc7\u0ca1\u0ccd\u0c95\u0c97 (\u0cb6\u0cc3)"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 3", "T": "\u0cb8\u0cca\u0c95\u0ccd\u0c95\u0cc1\u0c9a\u0cc1 \u0cb8\u0ccb\u0cb2\u0cc1\u0c9a\u0cc1\u0ca8\u0ccd(\u0c92)\u0c95\u0ca4\u0cc6 \u0c95\u0cc3\u0cb7\u0ccd\u0ca3\u0cc1\u0ca8\u0cbf <br/> \u0c97\u0ccd\u0cb0\u0c95\u0ccd\u0c95\u0cc1\u0ca8 \u0cae\u0cc1\u0ca6\u0ccd\u0ca6\u0ccd(\u0c87)\u0ca1\u0cc1(\u0ca8\u0ccd)\u0c92\u0c95\u0ca4\u0cc6 <br/> \u0caa\u0c95\u0ccd\u0c95\u0c97\u0cc1 \u0cb0\u0cae\u0ccd\u0cae\u0ccd(\u0c85)\u0ca8\u0cc1\u0c9a\u0cc1\u0ca8\u0ccd(\u0c92)\u0c95\u0ca4\u0cc6 \u0cae\u0ca1\u0cc1\u0caa\u0cc1\u0cb2\u0ca8\u0ccd- <br/> (\u0c85)\u0c95\u0ccd\u0c95\u0cb0\u0ca8\u0ccd(\u0c92)\u0cb8\u0c97\u0cc1\u0c9a\u0cc1\u0ca8\u0ccd(\u0c92)\u0c95\u0ca4\u0cc6 \u0cb5\u0cc7\u0ca1\u0ccd\u0c95\u0c97 (\u0cb6\u0cc3)"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 4", "T": "\u0caa\u0cb0\u0cbf\u0cae\u0cb3\u0cae\u0cc1\u0cb2\u0ccd(\u0c85)\u0ca8\u0ccd\u0ca6\u0cc1\u0c9a\u0cc1\u0ca8\u0ccd(\u0c92)\u0c95\u0ca4\u0cc6 \u0cb6\u0ccd\u0cb0\u0cc0 <br/> \u0cb9\u0cb0\u0cbf \u0cb9\u0cb0\u0cbf(\u0caf\u0ccd)\u0c85\u0ca8\u0cc1\u0c9a\u0cc1\u0ca8\u0cc1(\u0ca8\u0ccd)\u0c92\u0c95\u0ca4\u0cc6 <br/> \u0c89\u0cb0\u0cae\u0cc1\u0ca8 \u0c9c\u0cc7\u0cb0\u0ccd\u0c9a\u0cc1\u0c9a\u0cc1\u0ca8\u0ccd(\u0c92)\u0c95\u0ca4\u0cc6 \u0caa\u0caf\u0ccd\u0caf\u0cc6\u0ca6 <br/> \u0c9c\u0cb0\u0cbf\u0caa\u0cbf \u0cb5\u0cc7\u0ca1\u0cc1\u0c95\u0cca\u0ca8\u0cc1\u0c9a\u0cc1\u0ca8\u0ccd(\u0c92)\u0c95\u0ca4\u0cc6 \u0cb5\u0cc7\u0ca1\u0ccd\u0c95\u0c97 (\u0cb6\u0cc3)"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 5", "T": "\u0cb8\u0cbe\u0cb0\u0cb8\u0ccd(\u0c86)\u0c95\u0ccd\u0cb7(\u0caf\u0ccd)\u0c85\u0ca8\u0cc1\u0c9a\u0cc1\u0ca8\u0ccd(\u0c92)\u0c95\u0ca4\u0cc6 \u0c95\u0ca8\u0cc1 <br/> \u0cb8\u0cc8\u0c97\u0ca8\u0cc1 \u0caa\u0cbf\u0cb2\u0cc1\u0c9a\u0cc1\u0c9a\u0cc1\u0ca8\u0ccd(\u0c92)\u0c95\u0ca4\u0cc6 <br/> \u0cb0\u0cbe\u0cb0\u0cbe(\u0caf\u0ccd)\u0c85\u0ca8\u0cc1\u0c9a\u0cc1\u0ca8\u0cc1(\u0ca8\u0ccd)\u0c92\u0c95\u0ca4\u0cc6 \u0ca4\u0ccd\u0caf\u0cbe\u0c97- <br/> \u0cb0\u0cbe\u0c9c \u0cb8\u0c96\u0cc1\u0ca1\u0ccd(\u0c85)\u0ca8\u0cc1\u0c9a\u0cc1\u0ca8\u0ccd(\u0c92)\u0c95\u0ca4\u0cc6 \u0cb5\u0cc7\u0ca1\u0ccd\u0c95\u0c97 (\u0cb6\u0cc3)"}]}, {"L": "Malayalam", "text": [{"M": "\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d36\u0d43\u0d19\u0d4d\u0d17\u0d3e\u0d30\u0d3f\u0d28\u0d4d\u0d1a\u0d41\u0d15\u0d4a\u0d28\u0d3f \u0d35\u0d46\u0d21\u0d32\u0d3f\u0d30\u0d3f \u0d36\u0d4d\u0d30\u0d40 \u0d15\u0d43\u0d37\u0d4d\u0d23\u0d41\u0d28\u0d3f\u0d24\u0d4b\u0d28\u0d41"}, {"M": "\u0d05\u0d28\u0d41\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d05\u0d19\u0d4d\u0d17-\u0d30\u0d19\u0d4d\u0d17 \u0d35\u0d48\u0d2d\u0d4b\u0d17\u0d2e\u0d41\u0d24\u0d4b <br/> \u0d17\u0d4b\u0d2a\u0d4d(\u0d06)\u0d19\u0d4d\u0d17\u0d28\u0d3e\u0d2e\u0d23\u0d41\u0d32\u0d4d(\u0d0e)\u0d28\u0d4d\u0d24\u0d4b \u0d38\u0d4a\u0d17\u0d38\u0d41\u0d17 (\u0d36\u0d43)"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 1", "T": "\u0d28\u0d35\u0d4d\u0d35\u0d41\u0d1a\u0d41 \u0d15\u0d41\u0d32\u0d41\u0d15\u0d41\u0d1a\u0d41\u0d28\u0d4d(\u0d12)\u0d15\u0d24\u0d46 \u0d15\u0d4a\u0d2a\u0d4d\u0d2a\u0d41\u0d28 <br/> \u0d2a\u0d41\u0d35\u0d4d\u0d35\u0d41\u0d32 \u0d2e\u0d41\u0d21\u0d41\u0d1a\u0d41\u0d1a\u0d41\u0d28\u0d4d(\u0d12)\u0d15\u0d24\u0d46 <br/> \u0d26\u0d41\u0d35\u0d4d\u0d35\u0d41\u0d1a\u0d41 \u0d15\u0d41\u0d30\u0d41\u0d32\u0d28\u0d41(\u0d28\u0d4d)\u0d12\u0d15\u0d24\u0d46 \u0d15\u0d43\u0d37\u0d4d\u0d23\u0d41\u0d28\u0d3f <br/> \u0d30\u0d35\u0d4d\u0d35 \u0d1c\u0d47\u0d2f\u0d41\u0d1a\u0d41\u0d28\u0d4d(\u0d12)\u0d15\u0d24\u0d46 \u0d35\u0d47\u0d21\u0d4d\u0d15\u0d17 (\u0d36\u0d43)"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 2", "T": "\u0d2e\u0d17\u0d21\u0d41 \u0d35\u0d40\u0d21\u0d4d(\u0d05)\u0d28\u0d41\u0d1a\u0d41\u0d28\u0d4d(\u0d12)\u0d15\u0d24\u0d46 \u0d30\u0d35\u0d3f\u0d15\u0d2f\u0d41 <br/> \u0d2c\u0d3f\u0d17\u0d41\u0d35\u0d41\u0d28 \u0d1c\u0d47\u0d30\u0d4d\u0d1a\u0d41\u0d1a\u0d41\u0d28\u0d4d(\u0d12)\u0d15\u0d24\u0d46 <br/> \u0d24\u0d17\u0d41\u0d28\u0d41 \u0d24\u0d28\u0d15\u0d4d(\u0d05)\u0d28\u0d41\u0d1a\u0d41\u0d28\u0d4d(\u0d12)\u0d15\u0d24\u0d46 \u0d2a\u0d3e\u0d26 <br/> \u0d2f\u0d41\u0d17\u0d2e\u0d41\u0d32\u0d28\u0d4d(\u0d12)\u0d24\u0d4d\u0d24\u0d41\u0d1a\u0d41\u0d28\u0d4d(\u0d12)\u0d15\u0d24\u0d46 \u0d35\u0d47\u0d21\u0d4d\u0d15\u0d17 (\u0d36\u0d43)"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 3", "T": "\u0d38\u0d4a\u0d15\u0d4d\u0d15\u0d41\u0d1a\u0d41 \u0d38\u0d4b\u0d32\u0d41\u0d1a\u0d41\u0d28\u0d4d(\u0d12)\u0d15\u0d24\u0d46 \u0d15\u0d43\u0d37\u0d4d\u0d23\u0d41\u0d28\u0d3f <br/> \u0d17\u0d4d\u0d30\u0d15\u0d4d\u0d15\u0d41\u0d28 \u0d2e\u0d41\u0d26\u0d4d\u0d26\u0d4d(\u0d07)\u0d21\u0d41(\u0d28\u0d4d)\u0d12\u0d15\u0d24\u0d46 <br/> \u0d2a\u0d15\u0d4d\u0d15\u0d17\u0d41 \u0d30\u0d2e\u0d4d\u0d2e\u0d4d(\u0d05)\u0d28\u0d41\u0d1a\u0d41\u0d28\u0d4d(\u0d12)\u0d15\u0d24\u0d46 \u0d2e\u0d21\u0d41\u0d2a\u0d41\u0d32\u0d28\u0d4d- <br/> (\u0d05)\u0d15\u0d4d\u0d15\u0d30\u0d28\u0d4d(\u0d12)\u0d38\u0d17\u0d41\u0d1a\u0d41\u0d28\u0d4d(\u0d12)\u0d15\u0d24\u0d46 \u0d35\u0d47\u0d21\u0d4d\u0d15\u0d17 (\u0d36\u0d43)"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 4", "T": "\u0d2a\u0d30\u0d3f\u0d2e\u0d33\u0d2e\u0d41\u0d32\u0d4d(\u0d05)\u0d28\u0d4d\u0d26\u0d41\u0d1a\u0d41\u0d28\u0d4d(\u0d12)\u0d15\u0d24\u0d46 \u0d36\u0d4d\u0d30\u0d40 <br/> \u0d39\u0d30\u0d3f \u0d39\u0d30\u0d3f(\u0d2f\u0d4d)\u0d05\u0d28\u0d41\u0d1a\u0d41\u0d28\u0d41(\u0d28\u0d4d)\u0d12\u0d15\u0d24\u0d46 <br/> \u0d09\u0d30\u0d2e\u0d41\u0d28 \u0d1c\u0d47\u0d30\u0d4d\u0d1a\u0d41\u0d1a\u0d41\u0d28\u0d4d(\u0d12)\u0d15\u0d24\u0d46 \u0d2a\u0d2f\u0d4d\u0d2f\u0d46\u0d26 <br/> \u0d1c\u0d30\u0d3f\u0d2a\u0d3f \u0d35\u0d47\u0d21\u0d41\u0d15\u0d4a\u0d28\u0d41\u0d1a\u0d41\u0d28\u0d4d(\u0d12)\u0d15\u0d24\u0d46 \u0d35\u0d47\u0d21\u0d4d\u0d15\u0d17 (\u0d36\u0d43)"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 5", "T": "\u0d38\u0d3e\u0d30\u0d38\u0d4d(\u0d06)\u0d15\u0d4d\u0d37(\u0d2f\u0d4d)\u0d05\u0d28\u0d41\u0d1a\u0d41\u0d28\u0d4d(\u0d12)\u0d15\u0d24\u0d46 \u0d15\u0d28\u0d41 <br/> \u0d38\u0d48\u0d17\u0d28\u0d41 \u0d2a\u0d3f\u0d32\u0d41\u0d1a\u0d41\u0d1a\u0d41\u0d28\u0d4d(\u0d12)\u0d15\u0d24\u0d46 <br/> \u0d30\u0d3e\u0d30\u0d3e(\u0d2f\u0d4d)\u0d05\u0d28\u0d41\u0d1a\u0d41\u0d28\u0d41(\u0d28\u0d4d)\u0d12\u0d15\u0d24\u0d46 \u0d24\u0d4d\u0d2f\u0d3e\u0d17- <br/> \u0d30\u0d3e\u0d1c \u0d38\u0d16\u0d41\u0d21\u0d4d(\u0d05)\u0d28\u0d41\u0d1a\u0d41\u0d28\u0d4d(\u0d12)\u0d15\u0d24\u0d46 \u0d35\u0d47\u0d21\u0d4d\u0d15\u0d17 (\u0d36\u0d43)"}]}, {"L": "Sanskrit", "text": [{"M": "\u092a\u0932\u094d\u0932\u0935\u093f", "T": "\u0936\u0943\u0919\u094d\u0917\u093e\u0930\u093f\u0928\u094d\u091a\u0941\u0915\u094b\u0928\u093f \u0935\u0947\u0921\u0932\u093f\u0930\u093f \u0936\u094d\u0930\u0940 \u0915\u0943\u0937\u094d\u0923\u0941\u0928\u093f\u0924\u094b\u0928\u0941"}, {"M": "\u0905\u0928\u0941\u092a\u0932\u094d\u0932\u0935\u093f", "T": "\u0905\u0919\u094d\u0917-\u0930\u0919\u094d\u0917 \u0935\u0948\u092d\u094b\u0917\u092e\u0941\u0924\u094b <br/> \u0917\u094b\u092a\u094d(\u0906)\u0919\u094d\u0917\u0928\u093e\u092e\u0923\u0941\u0932\u094d(\u090f)\u0928\u094d\u0924\u094b \u0938\u094b\u0917\u0938\u0941\u0917 (\u0936\u0943)"}, {"M": "\u091a\u0930\u0923\u092e\u094d 1", "T": "\u0928\u0935\u094d\u0935\u0941\u091a\u0941 \u0915\u0941\u0932\u0941\u0915\u0941\u091a\u0941\u0928\u094d(\u0913)\u0915\u0924\u0947 \u0915\u094b\u092a\u094d\u092a\u0941\u0928 <br/> \u092a\u0941\u0935\u094d\u0935\u0941\u0932 \u092e\u0941\u0921\u0941\u091a\u0941\u091a\u0941\u0928\u094d(\u0913)\u0915\u0924\u0947 <br/> \u0926\u0941\u0935\u094d\u0935\u0941\u091a\u0941 \u0915\u0941\u0930\u0941\u0932\u0928\u0941(\u0928\u094d)\u0913\u0915\u0924\u0947 \u0915\u0943\u0937\u094d\u0923\u0941\u0928\u093f <br/> \u0930\u0935\u094d\u0935 \u091c\u0947\u092f\u0941\u091a\u0941\u0928\u094d(\u0913)\u0915\u0924\u0947 \u0935\u0947\u0921\u094d\u0915\u0917 (\u0936\u0943)"}, {"M": "\u091a\u0930\u0923\u092e\u094d 2", "T": "\u092e\u0917\u0921\u0941 \u0935\u0940\u0921\u094d(\u0905)\u0928\u0941\u091a\u0941\u0928\u094d(\u0913)\u0915\u0924\u0947 \u0930\u0935\u093f\u0915\u092f\u0941 <br/> \u092c\u093f\u0917\u0941\u0935\u0941\u0928 \u091c\u0947\u0930\u094d\u091a\u0941\u091a\u0941\u0928\u094d(\u0913)\u0915\u0924\u0947 <br/> \u0924\u0917\u0941\u0928\u0941 \u0924\u0928\u0915\u094d(\u0905)\u0928\u0941\u091a\u0941\u0928\u094d(\u0913)\u0915\u0924\u0947 \u092a\u093e\u0926 <br/> \u092f\u0941\u0917\u092e\u0941\u0932\u0928\u094d(\u0913)\u0924\u094d\u0924\u0941\u091a\u0941\u0928\u094d(\u0913)\u0915\u0924\u0947 \u0935\u0947\u0921\u094d\u0915\u0917 (\u0936\u0943)"}, {"M": "\u091a\u0930\u0923\u092e\u094d 3", "T": "\u0938\u094b\u0915\u094d\u0915\u0941\u091a\u0941 \u0938\u094b\u0932\u0941\u091a\u0941\u0928\u094d(\u0913)\u0915\u0924\u0947 \u0915\u0943\u0937\u094d\u0923\u0941\u0928\u093f <br/> \u0917\u094d\u0930\u0915\u094d\u0915\u0941\u0928 \u092e\u0941\u0926\u094d\u0926\u094d(\u0907)\u0921\u0941(\u0928\u094d)\u0913\u0915\u0924\u0947 <br/> \u092a\u0915\u094d\u0915\u0917\u0941 \u0930\u092e\u094d\u092e\u094d(\u0905)\u0928\u0941\u091a\u0941\u0928\u094d(\u0913)\u0915\u0924\u0947 \u092e\u0921\u0941\u092a\u0941\u0932\u0928\u094d- <br/> (\u0905)\u0915\u094d\u0915\u0930\u0928\u094d(\u0913)\u0938\u0917\u0941\u091a\u0941\u0928\u094d(\u0913)\u0915\u0924\u0947 \u0935\u0947\u0921\u094d\u0915\u0917 (\u0936\u0943)"}, {"M": "\u091a\u0930\u0923\u092e\u094d 4", "T": "\u092a\u0930\u093f\u092e\u0932\u092e\u0941\u0932\u094d(\u0905)\u0928\u094d\u0926\u0941\u091a\u0941\u0928\u094d(\u0913)\u0915\u0924\u0947 \u0936\u094d\u0930\u0940 <br/> \u0939\u0930\u093f \u0939\u0930\u093f(\u092f\u094d)\u0905\u0928\u0941\u091a\u0941\u0928\u0941(\u0928\u094d)\u0913\u0915\u0924\u0947 <br/> \u0909\u0930\u092e\u0941\u0928 \u091c\u0947\u0930\u094d\u091a\u0941\u091a\u0941\u0928\u094d(\u0913)\u0915\u0924\u0947 \u092a\u092f\u094d\u092f\u0947\u0926 <br/> \u091c\u0930\u093f\u092a\u093f \u0935\u0947\u0921\u0941\u0915\u094b\u0928\u0941\u091a\u0941\u0928\u094d