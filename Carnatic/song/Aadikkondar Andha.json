u0bc1\u0bae\u0bcd \u0baa\u0ba4\u0b9e\u0bcd\u0b9a\u0bb2\u0bbf \u0b86\u0b9f \u0bb5\u0bcd\u0baf\u0bbe\u0b95\u0bcd\u0bb0 \u0baa\u0bbe\u0ba4\u0bb0\u0bc1\u0bae\u0bcd \u0ba8\u0ba8\u0bcd\u0ba4\u0bbf\u0baf\u0bc1\u0bae\u0bbe\u0b9f <br/> \u0b92\u0baa\u0bcd\u0baa\u0b9f\u0bcd\u0bb1 \u0b9a\u0bbf\u0bb5\u0b95\u0bbe\u0bae\u0bbf \u0b85\u0bae\u0bcd\u0bae\u0bc8\u0baf\u0bc1\u0bae\u0bcd \u0b95\u0bc2\u0b9f\u0bb5\u0bc7 \u0ba8\u0bbf\u0ba9\u0bcd\u0bb1\u0bbe\u0b9f      (\u0b86\u0b9f\u0bbf\u0b95\u0bcd\u0b95\u0bca\u0ba3\u0bcd\u0b9f\u0bbe\u0bb0\u0bcd)"}]}, {"L": "Telugu", "text": [{"M": "\u0c2a\u0c32\u0c4d\u0c32\u0c35\u0c3f", "T": "\u0c06\u0c21\u0c3f\u0c15\u0c4d\u0c15\u0c4a\u0c23\u0c4d\u0c21\u0c3e\u0c30\u0c4d \u0c05\u0c28\u0c4d\u0c26 \u0c35\u0c47\u0c21\u0c3f\u0c15\u0c4d\u0c15\u0c48\u0c15\u0c4d \u0c15\u0c3e\u0c23\u0c15\u0c4d <br/> \u0c15\u0c23\u0c4d \u0c06\u0c2f\u0c3f\u0c30\u0c2e\u0c4d \u0c35\u0c47\u0c23\u0c4d\u0c21\u0c3e\u0c2e\u0c4b"}, {"M": "\u0c05\u0c28\u0c41\u0c2a\u0c32\u0c4d\u0c32\u0c35\u0c3f", "T": "\u0c28\u0c3e\u0c21\u0c3f\u0c24\u0c4d \u0c24\u0c41\u0c24\u0c3f\u0c2a\u0c4d\u0c2a\u0c35\u0c30\u0c4d \u0c2a\u0c19\u0c4d\u0c15\u0c3f\u0c32\u0c4d \u0c09\u0c31\u0c48\u0c2a\u0c35\u0c30\u0c4d \u0c28\u0c2e\u0c4d\u0c2a\u0c30\u0c4d <br/> \u0c24\u0c3f\u0c30\u0c41\u0c1a\u0c4d\u0c1a\u0c46\u0c2e\u0c4d\u0c2a\u0c4a\u0c28\u0c4d  \u0c05\u0c2e\u0c4d\u0c2a\u0c32\u0c35\u0c3e\u0c23\u0c30\u0c4d (\u0c06\u0c21\u0c3f\u0c15\u0c4d\u0c15\u0c4a\u0c23\u0c4d\u0c21\u0c3e\u0c30\u0c4d)"}, {"M": "\u0c1a\u0c30\u0c23\u0c2e\u0c4d 1", "T": "\u0c2a\u0c19\u0c4d\u0c15\u0c2f \u0c1a\u0c3f\u0c32\u0c2e\u0c4d\u0c2a\u0c48\u0c28\u0c4d\u0c24\u0c3e\u0c21\u0c2a\u0c4d \u0c2a\u0c3e\u0c24\u0c1a\u0c4d <br/> \u0c1a\u0c24\u0c19\u0c4d\u0c15\u0c48\u0c15\u0c33\u0c4d \u0c15\u0c3f\u0c23\u0c4d\u0c15\u0c3f\u0c23\u0c4d\u0c23\u0c46\u0c28\u0c4d\u0c31\u0c3e\u0c21 <br/> \u0c2a\u0c4a\u0c19\u0c4d\u0c15\u0c2e\u0c41\u0c21\u0c28\u0c47 \u0c09\u0c30\u0c3f\u0c24\u0c4d\u0c24\u0c41 \u0c09\u0c21\u0c41\u0c24\u0c4d\u0c24 \u0c2a\u0c41\u0c32\u0c3f\u0c24\u0c4d\u0c24\u0c4b\u0c32\u0c4d \u0c05\u0c1a\u0c48\u0c28\u0c4d\u0c24\u0c3e\u0c21 <br/> \u0c1a\u0c46\u0c19\u0c4d\u0c15\u0c48\u0c2f\u0c3f\u0c32\u0c4d \u0c0f\u0c28\u0c4d\u0c24\u0c3f\u0c2f \u0c2e\u0c3e\u0c28\u0c4d \u0c2e\u0c34\u0c41\u0c35\u0c3e\u0c21 <br/> \u0c1a\u0c46\u0c2e\u0c4d\u0c2a\u0c4a\u0c31\u0c4d\u0c15\u0c4d\u0c15\u0c34\u0c31\u0c4d \u0c15\u0c23\u0c4d \u0c2e\u0c41\u0c2f\u0c32\u0c15\u0c28\u0c3e\u0c21 <br/> \u0c15\u0c19\u0c4d\u0c15\u0c48 \u0c07\u0c33\u0c2e\u0c4d \u0c2a\u0c3f\u0c31\u0c48 \u0c1a\u0c46\u0c1e\u0c4d\u0c1a\u0c21\u0c48\u0c2f\u0c3e\u0c21\u0c15\u0c4d <br/> \u0c15\u0c28\u0c15 \u0c1a\u0c2a\u0c48 \u0c24\u0c28\u0c3f\u0c32\u0c47 (\u0c06\u0c21\u0c3f\u0c15\u0c4d\u0c15\u0c4a\u0c23\u0c4d\u0c21\u0c3e\u0c30\u0c4d)"}, {"M": "\u0c1a\u0c30\u0c23\u0c2e\u0c4d 2", "T": "\u0c06\u0c30 \u0c28\u0c35\u0c2e\u0c23\u0c3f \u0c2e\u0c3e\u0c32\u0c48\u0c15\u0c33\u0c3e\u0c21 \u0c06\u0c21\u0c41\u0c2e\u0c4d \u0c05\u0c30\u0c35\u0c2e\u0c4d \u0c2a\u0c21\u0c2e\u0c4d \u0c35\u0c3f\u0c30\u0c3f\u0c24\u0c4d\u0c24\u0c3e\u0c21 <br/> \u0c1a\u0c40\u0c30\u0c23\u0c3f\u0c15\u0c4d \u0c15\u0c4a\u0c28\u0c4d\u0c31\u0c48 \u0c2e\u0c32\u0c30\u0c4d\u0c24\u0c4d \u0c24\u0c4a\u0c21\u0c48\u0c2f\u0c3e\u0c21\u0c1a\u0c4d \u0c1a\u0c3f\u0c24\u0c2e\u0c4d\u0c2a\u0c30\u0c24\u0c4d\u0c24\u0c47\u0c30\u0c4d \u0c06\u0c21 <br/> \u0c2a\u0c47\u0c30\u0c23\u0c3f \u0c35\u0c47\u0c24\u0c3f\u0c2f\u0c30\u0c4d \u0c24\u0c3f\u0c32\u0c4d\u0c32\u0c48 \u0c2e\u0c42\u0c35\u0c3e\u0c2f\u0c3f\u0c30\u0c2e\u0c4d \u0c2a\u0c47\u0c30\u0c4d\u0c15\u0c33\u0c41\u0c2e\u0c4d \u0c2a\u0c42\u0c1c\u0c3f\u0c24\u0c4d\u0c24\u0c41\u0c15\u0c4d \u0c15\u0c4a\u0c23\u0c4d\u0c21\u0c41 \u0c28\u0c3f\u0c28\u0c4d\u0c31\u0c3e\u0c21 <br/> \u0c15\u0c3e\u0c30\u0c23\u0c3f  \u0c15\u0c3e\u0c33\u0c3f \u0c0e\u0c24\u0c3f\u0c30\u0c4d\u0c24\u0c4d\u0c24\u0c41   \u0c28\u0c3f\u0c28\u0c4d\u0c31\u0c3e\u0c21\u0c15\u0c4d \u0c15\u0c28\u0c15 \u0c1a\u0c2a\u0c48 \u0c24\u0c28\u0c3f\u0c32\u0c47 (\u0c06\u0c21\u0c3f\u0c15\u0c4d\u0c15\u0c4a\u0c23\u0c4d\u0c21\u0c3e\u0c30\u0c4d)"}, {"M": "\u0c1a\u0c30\u0c23\u0c2e\u0c4d 3", "T": "\u0c28\u0c3f\u0c30\u0c3f\u0c24\u0c4d\u0c24 \u0c17\u0c23\u0c2a\u0c24\u0c3f \u0c35\u0c47\u0c32\u0c30\u0c4d \u0c28\u0c3f\u0c28\u0c4d\u0c31\u0c3e\u0c21 \u0c28\u0c3f\u0c28\u0c4d\u0c31\u0c2f\u0c28\u0c4d \u0c2e\u0c3e\u0c32\u0c41\u0c21\u0c28\u0c4d \u0c07\u0c28\u0c4d\u0c24\u0c3f\u0c30\u0c28\u0c4d \u0c06\u0c21 <br/> \u0c2e\u0c41\u0c2a\u0c4d\u0c2a\u0c24\u0c4d\u0c24\u0c41 \u0c2e\u0c41\u0c15\u0c4d\u0c15\u0c4b\u0c21\u0c3f \u0c24\u0c47\u0c35\u0c30\u0c41\u0c21\u0c28\u0c47 \u0c2e\u0c41\u0c28\u0c3f\u0c35\u0c30\u0c41\u0c2e\u0c4d \u0c28\u0c3f\u0c28\u0c4d\u0c31\u0c3e\u0c21 <br/> \u0c2e\u0c46\u0c2f\u0c4d\u0c2a\u0c4d\u0c2a\u0c24\u0c3f \u0c2e\u0c47\u0c35\u0c41\u0c2e\u0c4d \u0c2a\u0c24\u0c1e\u0c4d\u0c1a\u0c32\u0c3f \u0c06\u0c21 \u0c35\u0c4d\u0c2f\u0c3e\u0c15\u0c4d\u0c30 \u0c2a\u0c3e\u0c24\u0c30\u0c41\u0c2e\u0c4d \u0c28\u0c28\u0c4d\u0c24\u0c3f\u0c2f\u0c41\u0c2e\u0c3e\u0c21 <br/> \u0c12\u0c2a\u0c4d\u0c2a\u0c21\u0c4d\u0c31 \u0c1a\u0c3f\u0c35\u0c15\u0c3e\u0c2e\u0c3f \u0c05\u0c2e\u0c4d\u0c2e\u0c48\u0c2f\u0c41\u0c2e\u0c4d \u0c15\u0c42\u0c21\u0c35\u0c47 \u0c28\u0c3f\u0c28\u0c4d\u0c31\u0c3e\u0c21      (\u0c06\u0c21\u0c3f\u0c15\u0c4d\u0c15\u0c4a\u0c23\u0c4d\u0c21\u0c3e\u0c30\u0c4d)"}]}, {"L": "Kannada", "text": [{"M": "\u0caa\u0cb2\u0ccd\u0cb2\u0cb5\u0cbf", "T": "\u0c86\u0ca1\u0cbf\u0c95\u0ccd\u0c95\u0cca\u0ca3\u0ccd\u0ca1\u0cbe\u0cb0\u0ccd \u0c85\u0ca8\u0ccd\u0ca6 \u0cb5\u0cc7\u0ca1\u0cbf\u0c95\u0ccd\u0c95\u0cc8\u0c95\u0ccd \u0c95\u0cbe\u0ca3\u0c95\u0ccd <br/> \u0c95\u0ca3\u0ccd \u0c86\u0caf\u0cbf\u0cb0\u0cae\u0ccd \u0cb5\u0cc7\u0ca3\u0ccd\u0ca1\u0cbe\u0cae\u0ccb"}, {"M": "\u0c85\u0ca8\u0cc1\u0caa\u0cb2\u0ccd\u0cb2\u0cb5\u0cbf", "T": "\u0ca8\u0cbe\u0ca1\u0cbf\u0ca4\u0ccd \u0ca4\u0cc1\u0ca4\u0cbf\u0caa\u0ccd\u0caa\u0cb5\u0cb0\u0ccd \u0caa\u0c99\u0ccd\u0c95\u0cbf\u0cb2\u0ccd \u0c89\u0cb1\u0cc8\u0caa\u0cb5\u0cb0\u0ccd \u0ca8\u0cae\u0ccd\u0caa\u0cb0\u0ccd <br/> \u0ca4\u0cbf\u0cb0\u0cc1\u0c9a\u0ccd\u0c9a\u0cc6\u0cae\u0ccd\u0caa\u0cca\u0ca8\u0ccd  \u0c85\u0cae\u0ccd\u0caa\u0cb2\u0cb5\u0cbe\u0ca3\u0cb0\u0ccd (\u0c86\u0ca1\u0cbf\u0c95\u0ccd\u0c95\u0cca\u0ca3\u0ccd\u0ca1\u0cbe\u0cb0\u0ccd)"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 1", "T": "\u0caa\u0c99\u0ccd\u0c95\u0caf \u0c9a\u0cbf\u0cb2\u0cae\u0ccd\u0caa\u0cc8\u0ca8\u0ccd\u0ca4\u0cbe\u0ca1\u0caa\u0ccd \u0caa\u0cbe\u0ca4\u0c9a\u0ccd <br/> \u0c9a\u0ca4\u0c99\u0ccd\u0c95\u0cc8\u0c95\u0cb3\u0ccd \u0c95\u0cbf\u0ca3\u0ccd\u0c95\u0cbf\u0ca3\u0ccd\u0ca3\u0cc6\u0ca8\u0ccd\u0cb1\u0cbe\u0ca1 <br/> \u0caa\u0cca\u0c99\u0ccd\u0c95\u0cae\u0cc1\u0ca1\u0ca8\u0cc7 \u0c89\u0cb0\u0cbf\u0ca4\u0ccd\u0ca4\u0cc1 \u0c89\u0ca1\u0cc1\u0ca4\u0ccd\u0ca4 \u0caa\u0cc1\u0cb2\u0cbf\u0ca4\u0ccd\u0ca4\u0ccb\u0cb2\u0ccd \u0c85\u0c9a\u0cc8\u0ca8\u0ccd\u0ca4\u0cbe\u0ca1 <br/> \u0c9a\u0cc6\u0c99\u0ccd\u0c95\u0cc8\u0caf\u0cbf\u0cb2\u0ccd \u0c8f\u0ca8\u0ccd\u0ca4\u0cbf\u0caf \u0cae\u0cbe\u0ca8\u0ccd \u0cae\u0cb3\u0cc1\u0cb5\u0cbe\u0ca1 <br/> \u0c9a\u0cc6\u0cae\u0ccd\u0caa\u0cca\u0cb1\u0ccd\u0c95\u0ccd\u0c95\u0cb3\u0cb1\u0ccd \u0c95\u0ca3\u0ccd \u0cae\u0cc1\u0caf\u0cb2\u0c95\u0ca8\u0cbe\u0ca1 <br/> \u0c95\u0c99\u0ccd\u0c95\u0cc8 \u0c87\u0cb3\u0cae\u0ccd \u0caa\u0cbf\u0cb1\u0cc8 \u0c9a\u0cc6\u0c9e\u0ccd\u0c9a\u0ca1\u0cc8\u0caf\u0cbe\u0ca1\u0c95\u0ccd <br/> \u0c95\u0ca8\u0c95 \u0c9a\u0caa\u0cc8 \u0ca4\u0ca8\u0cbf\u0cb2\u0cc7 (\u0c86\u0ca1\u0cbf\u0c95\u0ccd\u0c95\u0cca\u0ca3\u0ccd\u0ca1\u0cbe\u0cb0\u0ccd)"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 2", "T": "\u0c86\u0cb0 \u0ca8\u0cb5\u0cae\u0ca3\u0cbf \u0cae\u0cbe\u0cb2\u0cc8\u0c95\u0cb3\u0cbe\u0ca1 \u0c86\u0ca1\u0cc1\u0cae\u0ccd \u0c85\u0cb0\u0cb5\u0cae\u0ccd \u0caa\u0ca1\u0cae\u0ccd \u0cb5\u0cbf\u0cb0\u0cbf\u0ca4\u0ccd\u0ca4\u0cbe\u0ca1 <br/> \u0c9a\u0cc0\u0cb0\u0ca3\u0cbf\u0c95\u0ccd \u0c95\u0cca\u0ca8\u0ccd\u0cb1\u0cc8 \u0cae\u0cb2\u0cb0\u0ccd\u0ca4\u0ccd \u0ca4\u0cca\u0ca1\u0cc8\u0caf\u0cbe\u0ca1\u0c9a\u0ccd \u0c9a\u0cbf\u0ca4\u0cae\u0ccd\u0caa\u0cb0\u0ca4\u0ccd\u0ca4\u0cc7\u0cb0\u0ccd \u0c86\u0ca1 <br/> \u0caa\u0cc7\u0cb0\u0ca3\u0cbf \u0cb5\u0cc7\u0ca4\u0cbf\u0caf\u0cb0\u0ccd \u0ca4\u0cbf\u0cb2\u0ccd\u0cb2\u0cc8 \u0cae\u0cc2\u0cb5\u0cbe\u0caf\u0cbf\u0cb0\u0cae\u0ccd \u0caa\u0cc7\u0cb0\u0ccd\u0c95\u0cb3\u0cc1\u0cae\u0ccd \u0caa\u0cc2\u0c9c\u0cbf\u0ca4\u0ccd\u0ca4\u0cc1\u0c95\u0ccd \u0c95\u0cca\u0ca3\u0ccd\u0ca1\u0cc1 \u0ca8\u0cbf\u0ca8\u0ccd\u0cb1\u0cbe\u0ca1 <br/> \u0c95\u0cbe\u0cb0\u0ca3\u0cbf  \u0c95\u0cbe\u0cb3\u0cbf \u0c8e\u0ca4\u0cbf\u0cb0\u0ccd\u0ca4\u0ccd\u0ca4\u0cc1   \u0ca8\u0cbf\u0ca8\u0ccd\u0cb1\u0cbe\u0ca1\u0c95\u0ccd \u0c95\u0ca8\u0c95 \u0c9a\u0caa\u0cc8 \u0ca4\u0ca8\u0cbf\u0cb2\u0cc7 (\u0c86\u0ca1\u0cbf\u0c95\u0ccd\u0c95\u0cca\u0ca3\u0ccd\u0ca1\u0cbe\u0cb0\u0ccd)"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 3", "T": "\u0ca8\u0cbf\u0cb0\u0cbf\u0ca4\u0ccd\u0ca4 \u0c97\u0ca3\u0caa\u0ca4\u0cbf \u0cb5\u0cc7\u0cb2\u0cb0\u0ccd \u0ca8\u0cbf\u0ca8\u0ccd\u0cb1\u0cbe\u0ca1 \u0ca8\u0cbf\u0ca8\u0ccd\u0cb1\u0caf\u0ca8\u0ccd \u0cae\u0cbe\u0cb2\u0cc1\u0ca1\u0ca8\u0ccd \u0c87\u0ca8\u0ccd\u0ca4\u0cbf\u0cb0\u0ca8\u0ccd \u0c86\u0ca1 <br/> \u0cae\u0cc1\u0caa\u0ccd\u0caa\u0ca4\u0ccd\u0ca4\u0cc1 \u0cae\u0cc1\u0c95\u0ccd\u0c95\u0ccb\u0ca1\u0cbf \u0ca4\u0cc7\u0cb5\u0cb0\u0cc1\u0ca1\u0ca8\u0cc7 \u0cae\u0cc1\u0ca8\u0cbf\u0cb5\u0cb0\u0cc1\u0cae\u0ccd \u0ca8\u0cbf\u0ca8\u0ccd\u0cb1\u0cbe\u0ca1 <br/> \u0cae\u0cc6\u0caf\u0ccd\u0caa\u0ccd\u0caa\u0ca4\u0cbf \u0cae\u0cc7\u0cb5\u0cc1\u0cae\u0ccd \u0caa\u0ca4\u0c9e\u0ccd\u0c9a\u0cb2\u0cbf \u0c86\u0ca1 \u0cb5\u0ccd\u0caf\u0cbe\u0c95\u0ccd\u0cb0 \u0caa\u0cbe\u0ca4\u0cb0\u0cc1\u0cae\u0ccd \u0ca8\u0ca8\u0ccd\u0ca4\u0cbf\u0caf\u0cc1\u0cae\u0cbe\u0ca1 <br/> \u0c92\u0caa\u0ccd\u0caa\u0ca1\u0ccd\u0cb1 \u0c9a\u0cbf\u0cb5\u0c95\u0cbe\u0cae\u0cbf \u0c85\u0cae\u0ccd\u0cae\u0cc8\u0caf\u0cc1\u0cae\u0ccd \u0c95\u0cc2\u0ca1\u0cb5\u0cc7 \u0ca8\u0cbf\u0ca8\u0ccd\u0cb1\u0cbe\u0ca1      (\u0c86\u0ca1\u0cbf\u0c95\u0ccd\u0c95\u0cca\u0ca3\u0ccd\u0ca1\u0cbe\u0cb0\u0ccd)"}]}, {"L": "Malayalam", "text": [{"M": "\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d06\u0d21\u0d3f\u0d15\u0d4d\u0d15\u0d4a\u0d23\u0d4d\u0d21\u0d3e\u0d30\u0d4d \u0d05\u0d28\u0d4d\u0d26 \u0d35\u0d47\u0d21\u0d3f\u0d15\u0d4d\u0d15\u0d48\u0d15\u0d4d \u0d15\u0d3e\u0d23\u0d15\u0d4d <br/> \u0d15\u0d23\u0d4d \u0d06\u0d2f\u0d3f\u0d30\u0d2e\u0d4d \u0d35\u0d47\u0d23\u0d4d\u0d21\u0d3e\u0d2e\u0d4b"}, {"M": "\u0d05\u0d28\u0d41\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d28\u0d3e\u0d21\u0d3f\u0d24\u0d4d \u0d24\u0d41\u0d24\u0d3f\u0d2a\u0d4d\u0d2a\u0d35\u0d30\u0d4d \u0d2a\u0d19\u0d4d\u0d15\u0d3f\u0d32\u0d4d \u0d09\u0d31\u0d48\u0d2a\u0d35\u0d30\u0d4d \u0d28\u0d2e\u0d4d\u0d2a\u0d30\u0d4d <br/> \u0d24\u0d3f\u0d30\u0d41\u0d1a\u0d4d\u0d1a\u0d46\u0d2e\u0d4d\u0d2a\u0d4a\u0d28\u0d4d  \u0d05\u0d2e\u0d4d\u0d2a\u0d32\u0d35\u0d3e\u0d23\u0d30\u0d4d (\u0d06\u0d21\u0d3f\u0d15\u0d4d\u0d15\u0d4a\u0d23\u0d4d\u0d21\u0d3e\u0d30\u0d4d)"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 1", "T": "\u0d2a\u0d19\u0d4d\u0d15\u0d2f \u0d1a\u0d3f\u0d32\u0d2e\u0d4d\u0d2a\u0d48\u0d28\u0d4d\u0d24\u0d3e\u0d21\u0d2a\u0d4d \u0d2a\u0d3e\u0d24\u0d1a\u0d4d <br/> \u0d1a\u0d24\u0d19\u0d4d\u0d15\u0d48\u0d15\u0d33\u0d4d \u0d15\u0d3f\u0d23\u0d4d\u0d15\u0d3f\u0d23\u0d4d\u0d23\u0d46\u0d28\u0d4d\u0d31\u0d3e\u0d21 <br/> \u0d2a\u0d4a\u0d19\u0d4d\u0d15\u0d2e\u0d41\u0d21\u0d28\u0d47 \u0d09\u0d30\u0d3f\u0d24\u0d4d\u0d24\u0d41 \u0d09\u0d21\u0d41\u0d24\u0d4d\u0d24 \u0d2a\u0d41\u0d32\u0d3f\u0d24\u0d4d\u0d24\u0d4b\u0d32\u0d4d \u0d05\u0d1a\u0d48\u0d28\u0d4d\u0d24\u0d3e\u0d21 <br/> \u0d1a\u0d46\u0d19\u0d4d\u0d15\u0d48\u0d2f\u0d3f\u0d32\u0d4d \u0d0f\u0d28\u0d4d\u0d24\u0d3f\u0d2f \u0d2e\u0d3e\u0d28\u0d4d \u0d2e\u0d34\u0d41\u0d35\u0d3e\u0d21 <br/> \u0d1a\u0d46\u0d2e\u0d4d\u0d2a\u0d4a\u0d31\u0d4d\u0d15\u0d4d\u0d15\u0d34\u0d31\u0d4d \u0d15\u0d23\u0d4d \u0d2e\u0d41\u0d2f\u0d32\u0d15\u0d28\u0d3e\u0d21 <br/> \u0d15\u0d19\u0d4d\u0d15\u0d48 \u0d07\u0d33\u0d2e\u0d4d \u0d2a\u0d3f\u0d31\u0d48 \u0d1a\u0d46\u0d1e\u0d4d\u0d1a\u0d21\u0d48\u0d2f\u0d3e\u0d21\u0d15\u0d4d <br/> \u0d15\u0d28\u0d15 \u0d1a\u0d2a\u0d48 \u0d24\u0d28\u0d3f\u0d32\u0d47 (\u0d06\u0d21\u0d3f\u0d15\u0d4d\u0d15\u0d4a\u0d23\u0d4d\u0d21\u0d3e\u0d30\u0d4d)"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 2", "T": "\u0d06\u0d30 \u0d28\u0d35\u0d2e\u0d23\u0d3f \u0d2e\u0d3e\u0d32\u0d48\u0d15\u0d33\u0d3e\u0d21 \u0d06\u0d21\u0d41\u0d2e\u0d4d \u0d05\u0d30\u0d35\u0d2e\u0d4d \u0d2a\u0d21\u0d2e\u0d4d \u0d35\u0d3f\u0d30\u0d3f\u0d24\u0d4d\u0d24\u0d3e\u0d21 <br/> \u0d1a\u0d40\u0d30\u0d23\u0d3f\u0d15\u0d4d \u0d15\u0d4a\u0d28\u0d4d\u0d31\u0d48 \u0d2e\u0d32\u0d30\u0d4d\u0d24\u0d4d \u0d24\u0d4a\u0d21\u0d48\u0d2f\u0d3e\u0d21\u0d1a\u0d4d \u0d1a\u0d3f\u0d24\u0d2e\u0d4d\u0d2a\u0d30\u0d24\u0d4d\u0d24\u0d47\u0d30\u0d4d \u0d06\u0d21 <br/> \u0d2a\u0d47\u0d30\u0d23\u0d3f \u0d35\u0d47\u0d24\u0d3f\u0d2f\u0d30\u0d4d \u0d24\u0d3f\u0d32\u0d4d\u0d32\u0d48 \u0d2e\u0d42\u0d35\u0d3e\u0d2f\u0d3f\u0d30\u0d2e\u0d4d \u0d2a\u0d47\u0d30\u0d4d\u0d15\u0d33\u0d41\u0d2e\u0d4d \u0d2a\u0d42\u0d1c\u0d3f\u0d24\u0d4d\u0d24\u0d41\u0d15\u0d4d \u0d15\u0d4a\u0d23\u0d4d\u0d21\u0d41 \u0d28\u0d3f\u0d28\u0d4d\u0d31\u0d3e\u0d21 <br/> \u0d15\u0d3e\u0d30\u0d23\u0d3f  \u0d15\u0d3e\u0d33\u0d3f \u0d0e\u0d24\u0d3f\u0d30\u0d4d\u0d24\u0d4d\u0d24\u0d41   \u0d28\u0d3f\u0d28\u0d4d\u0d31\u0d3e\u0d21\u0d15\u0d4d \u0d15\u0d28\u0d15 \u0d1a\u0d2a\u0d48 \u0d24\u0d28\u0d3f\u0d32\u0d47 (\u0d06\u0d21\u0d3f\u0d15\u0d4d\u0d15\u0d4a\u0d23\u0d4d\u0d21\u0d3e\u0d30\u0d4d)"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 3", "T": "\u0d28\u0d3f\u0d30\u0d3f\u0d24\u0d4d\u0d24 \u0d17\u0d23\u0d2a\u0d24\u0d3f \u0d35\u0d47\u0d32\u0d30\u0d4d \u0d28\u0d3f\u0d28\u0d4d\u0d31\u0d3e\u0d21 \u0d28\u0d3f\u0d28\u0d4d\u0d31\u0d2f\u0d28\u0d4d \u0d2e\u0d3e\u0d32\u0d41\u0d21\u0d28\u0d4d \u0d07\u0d28\u0d4d\u0d24\u0d3f\u0d30\u0d28\u0d4d \u0d06\u0d21 <br/> \u0d2e\u0d41\u0d2a\u0d4d\u0d2a\u0d24\u0d4d\u0d24\u0d41 \u0d2e\u0d41\u0d15\u0d4d\u0d15\u0d4b\u0d21\u0d3f \u0d24\u0d47\u0d35\u0d30\u0d41\u0d21\u0d28\u0d47 \u0d2e\u0d41\u0d28\u0d3f\u0d35\u0d30\u0d41\u0d2e\u0d4d \u0d28\u0d3f\u0d28\u0d4d\u0d31\u0d3e\u0d21 <br/> \u0d2e\u0d46\u0d2f\u0d4d\u0d2a\u0d4d\u0d2a\u0d24\u0d3f \u0d2e\u0d47\u0d35\u0d41\u0d2e\u0d4d \u0d2a\u0d24\u0d1e\u0d4d\u0d1a\u0d32\u0d3f \u0d06\u0d21 \u0d35\u0d4d\u0d2f\u0d3e\u0d15\u0d4d\u0d30 \u0d2a\u0d3e\u0d24\u0d30\u0d41\u0d2e\u0d4d \u0d28\u0d28\u0d4d\u0d24\u0d3f\u0d2f\u0d41\u0d2e\u0d3e\u0d21 <br/> \u0d12\u0d2a\u0d4d\u0d2a\u0d21\u0d4d\u0d31 \u0d1a\u0d3f\u0d35\u0d15\u0d3e\u0d2e\u0d3f \u0d05\u0d2e\u0d4d\u0d2e\u0d48\u0d2f\u0d41\u0d2e\u0d4d \u0d15\u0d42\u0d21\u0d35\u0d47 \u0d28\u0d3f\u0d28\u0d4d\u0d31\u0d3e\u0d21      (\u0d06\u0d21\u0d3f\u0d15\u0d4d\u0d15\u0d4a\u0d23\u0d4d\u0d21\u0d3e\u0d30\u0d4d)"}]}, {"L": "Sanskrit", "text": [{"M": "\u092a\u0932\u094d\u0932\u0935\u093f", "T": "\u0906\u0921\u093f\u0915\u094d\u0915\u094b\u0923\u094d\u0921\u093e\u0930\u094d \u0905\u0928\u094d\u0926 \u0935\u0947\u0921\u093f\u0915\u094d\u0915\u0948\u0915\u094d \u0915\u093e\u0923\u0915\u094d <br/> \u0915\u0923\u094d \u0906\u092f\u093f\u0930\u092e\u094d \u0935\u0947\u0923\u094d\u0921\u093e\u092e\u094b"}, {"M": "\u0905\u0928\u0941\u092a\u0932\u094d\u0932\u0935\u093f", "T": "\u0928\u093e\u0921\u093f\u0924\u094d \u0924\u0941\u0924\u093f\u092a\u094d\u092a\u0935\u0930\u094d \u092a\u0919\u094d\u0915\u093f\u0932\u094d \u0909\u0930\u0948\u092a\u0935\u0930\u094d \u0928\u092e\u094d\u092a\u0930\u094d <br/> \u0924\u093f\u0930\u0941\u091a\u094d\u091a\u0947\u092e\u094d\u092a\u094b\u0928\u094d  \u0905\u092e\u094d\u092a\u0932\u0935\u093e\u0923\u0930\u094d (\u0906\u0921\u093f\u0915\u094d\u0915\u094b\u0923\u094d\u0921\u093e\u0930\u094d)"}, {"M": "\u091a\u0930\u0923\u092e\u094d 1", "T": "\u092a\u0919\u094d\u0915\u092f \u091a\u093f\u0932\u092e\u094d\u092a\u0948\u0928\u094d\u0924\u093e\u0921\u092a\u094d \u092a\u093e\u0924\u091a\u094d <br/> \u091a\u0924\u0919\u094d\u0915\u0948\u0915\u0932\u094d \u0915\u093f\u0923\u094d\u0915\u093f\u0923\u094d\u0923\u0947\u0928\u094d\u0930\u093e\u0921 <br/> \u092a\u094b\u0919\u094d\u0915\u092e\u0941\u0921\u0928\u0947 \u0909\u0930\u093f\u0924\u094d\u0924\u0941 \u0909\u0921\u0941\u0924\u094d\u0924 \u092a\u0941\u0932\u093f\u0924\u094d\u0924\u094b\u0932\u094d \u0905\u091a\u0948\u0928\u094d\u0924\u093e\u0921 <br/> \u091a\u0947\u0919\u094d\u0915\u0948\u092f\u093f\u0932\u094d \u090f\u0928\u094d\u0924\u093f\u092f \u092e\u093e\u0928\u094d \u092e\u0932\u0941\u0935\u093e\u0921 <br/> \u091a\u0947\u092e\u094d\u092a\u094b\u0930\u094d\u0915\u094d\u0915\u0932\u0930\u094d \u0915\u0923\u094d \u092e\u0941\u092f\u0932\u0915\u0928\u093e\u0921 <br/> \u0915\u0919\u094d\u0915\u0948 \u0907\u0932\u092e\u094d \u092a\u093f\u0930\u0948 \u091a\u0947\u091e\u094d\u091a\u0921\u0948\u092f\u093e\u0921\u0915\u094d <br/> \u0915\u0928\u0915 \u091a\u092a\u0948 \u0924\u0928\u093f\u0932\u0947 (\u0906\u0921\u093f\u0915\u094d\u0915\u094b\u0923\u094d\u0921\u093e\u0930\u094d)"}, {"M": "\u091a\u0930\u0923\u092e\u094d 2", "T": "\u0906\u0930 \u0928\u0935\u092e\u0923\u093f \u092e\u093e\u0932\u0948\u0915\u0932\u093e\u0921 \u0906\u0921\u0941\u092e\u094d \u0905\u0930\u0935\u092e\u094d \u092a\u0921\u092e\u094d \u0935\u093f\u0930\u093f\u0924\u094d\u0924\u093e\u0921 <br/> \u091a\u0940\u0930\u0923\u093f\u0915\u094d \u0915\u094b\u0928\u094d\u0930\u0948 \u092e\u0932\u0930\u094d\u0924\u094d \u0924\u094b\u0921\u0948\u092f\u093e\u0921\u091a\u094d \u091a\u093f\u0924\u092e\u094d\u092a\u0930\u0924\u094d\u0924\u0947\u0930\u094d \u0906\u0921 <br/> \u092a\u0947\u0930\u0923\u093f \u0935\u0947\u0924\u093f\u092f\u0930\u094d \u0924\u093f\u0932\u094d\u0932\u0948 \u092e\u0942\u0935\u093e\u092f\u093f\u0930\u092e\u094d \u092a\u0947\u0930\u094d\u0915\u0932\u0941\u092e\u094d \u092a\u0942\u091c\u093f\u0924\u094d\u0924\u0941\u0915\u094d \u0915\u094b\u0923\u094d\u0921\u0941 \u0928\u093f\u0928\u094d\u0930\u093e\u0921 <br/> \u0915\u093e\u0930\u0923\u093f  \u0915\u093e\u0932\u093f \u090f\u0924\u093f\u0930\u094d\u0924\u094d\u0924\u0941   \u0928\u093f\u0928\u094d\u0930\u093e\u0921\u0915\u094d \u0915\u0928\u0915 \u091a\u092a\u0948 \u0924\u0928\u093f\u0932\u0947 (\u0906\u0921\u093f\u0915\u094d\u0915\u094b\u0923\u094d\u0921\u093e\u0930\u094d)"}, {"M": "\u091a\u0930\u0923\u092e\u094d 3", "T": "\u0928\u093f\u0930\u093f\u0924\u094d\u0924 \u0917\u0923\u092a\u0924\u093f \u0935\u0947\u0932\u0930\u094d 