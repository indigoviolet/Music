u0b85\u0ba9\u0bc1\u0baa\u0bb2\u0bcd\u0bb2\u0bb5\u0bbf", "T": "\u0b9a\u0bae\u0bcd\u0b95\u0bbe\u0b8f\u0ba4\u0bc6\u0b95 \u0ba4\u0bcd\u0bb0\u0bc6B\u0b89\u0bb5\u0ba9 \u0bb7\u0b95\u0bcd\u0ba4\u0bc6 \u0bae\u0bc1\u0bb0\u0bb2\u0bc6 \u0b95\u0bbe\u0ba9 \u0bb8\u0bcd\u0bb5\u0bb0\u0bc2\u0baa\u0bc6 ||"}, {"M": "\u0bae\u0bc1\u0b95\u0bcd\u0ba4\u0bbe\u0baf\u0bbf \u0b9a\u0bbe\u0bb9\u0bbf\u0ba4\u0bcd\u0baf\u0bae\u0bcd", "T": "\u0baa\u0bcd\u0bb0\u0bb3\u0baf \u0b9c\u0bb2 \u0bb5\u0bb2\u0baf \u0baa\u0bc1\u0ba4\u0bb2\u0bb2\u0bb2\u0bca \u0ba4\u0bbe\u0b8f\u0bb2\u0ba9 \u0b9f\u0bbe\u0b8f \u0bb5\u0ba4 \u0baa\u0ba4\u0bcd\u0bb0\u0bae\u0bcd | <br/> \u0bb7\u0baf\u0ba9\u0bc6\u0bae\u0bcd\u0b9a\u0bc1 \u0bb5\u0bb0\u0ba4\u0bc1\u0ba9\u0bc6 \u0ba8\u0bbeB\u0b8e\u0ba9\u0bc6\u0bb5\u0ba4\u0bb2\u0bc6\u0ba9 \u0b95\u0bae\u0bb2 \u0b9a\u0bae\u0bcd\u0b9c\u0ba9\u0bc6\u0ba4 \u0bb5\u0bc6\u0b9f\u0bc6 \u0bae\u0bc1\u0b83\u0b85 \u0b9c\u0ba9\u0bc6\u0ba4 ||"}, {"M": "\u0b9a\u0bb0\u0ba3\u0bae\u0bcd", "T": "\u0baa\u0bbe\u0ba4 \u0ba8\u0bc7\u0bb0\u0b9c\u0bae\u0bc1\u0bb2\u0bbe\u0b8f \u0ba8\u0bae\u0bcd\u0bae\u0bc6\u0ba4\u0bc6 ||"}, {"M": "\u0bb8\u0bcd\u0bb5\u0bb0 \u0b9a\u0bbe\u0bb9\u0bbf\u0ba4\u0bcd\u0baf\u0bae\u0bcd 1", "T": "\u0bb7\u0bbe\u0bb8\u0bcd\u0bb5\u0ba4\u0bae\u0bc0 \u0b9a\u0bc1\u0b83\u0b85\u0bae\u0bc0\u0ba9\u0ba4\u0bc6 | <br/> (\u0baa\u0bbe\u0ba4)"}, {"M": "\u0bb8\u0bcd\u0bb5\u0bb0 \u0b9a\u0bbe\u0bb9\u0bbf\u0ba4\u0bcd\u0baf\u0bae\u0bcd 2", "T": "\u0b95\u0bbe\u0b8a\u0bb0\u0bc7\u0bb7 \u0bb8\u0bcd\u0bb0\u0bc7\u0bb7 \u0bb5\u0bc6\u0b9f\u0bc7\u0ba8\u0bcd\u0ba4\u0bcd\u0bb0 \u0ba4\u0bcd\u0baf\u0bbe\u0b95\u0bb0\u0bbe\u0b9c\u0bbe\u0ba4\u0bc6 \u0bb5\u0bb0\u0bcd\u0baf \u0b9a\u0baa\u0bb0\u0bcd\u0baf\u0bb2\u0ba8\u0bcd\u0ba4\u0bc1 | <br/> (\u0baa\u0bbe\u0ba4)"}, {"M": "\u0bb8\u0bcd\u0bb5\u0bb0 \u0b9a\u0bbe\u0bb9\u0bbf\u0ba4\u0bcd\u0baf\u0bae\u0bcd 3", "T": "\u0baa\u0bcd\u0bb0B\u0b85\u0bb5\u0bae\u0bc1 \u0bb5\u0bc6B\u0b85\u0bb5\u0bae\u0bc1 \u0bb2\u0bc7\u0ba9\u0bae\u0bc1\u0bb2\u0bc1 \u0ba4\u0bb7 \u0ba4\u0bc6\u0bb7\u0bb2\u0bc1 B\u0b8a\u0bae\u0bc6 \u0b95\u0b95\u0ba9 \u0baa\u0bb5\u0ba9\u0bae\u0bc1 | <br/> \u0b9a\u0bb2\u0bc6\u0bb2\u0bae\u0b95\u0bcd\u0ba9\u0bc6 \u0ba8\u0b95 \u0b83\u0b85\u0b95 \u0ba4\u0bb0\u0bc1 \u0bae\u0bcd\u0bb0\u0bcd\u0b95\u0bbe\u0ba4\u0bc6 \u0b9a\u0b95\u0bb2\u0bae\u0bc1 \u0b95\u0bb2\u0bc1\u0b95\u0bc1 \u0ba8\u0bc7 \u0ba4\u0baf\u0bb5\u0bb2\u0ba9 | <br/> (\u0baa\u0bbe\u0ba4)"}, {"M": "\u0bb8\u0bcd\u0bb5\u0bb0 \u0b9a\u0bbe\u0bb9\u0bbf\u0ba4\u0bcd\u0baf\u0bae\u0bcd 4", "T": "\u0ba8\u0bc7 \u0ba8\u0bbe \u0baf\u0b8e\u0ba9\u0ba4\u0bc1 \u0ba4\u0bbe\u0bb0\u0ba4\u0bae\u0bcd\u0baf\u0bae\u0bc1\u0bb2\u0bc1 \u0bae\u0ba9\u0b95\u0bbe \u0b9a\u0bc1\u0b95\u0bc1\u0ba9\u0bae\u0bc6\u0b9a\u0bcd\u0b9a\u0bc6 | <br/> \u0b9a\u0bb9\u0ba9\u0bae\u0bc1 \u0bb5\u0bc6\u0bb0\u0bbe\u0bb2 \u0bb5\u0bc6\u0ba4\u0bb0\u0ba9\u0bae\u0bc1 \u0ba8\u0bc6\u0b9c\u0bae\u0bc1\u0ba9\u0bc1 \u0baa\u0bb2\u0bcd\u0b95\u0bc1\u0ba4\u0bb2\u0bc1 \u0b9a\u0ba4\u0bbe \u0ba4\u0b8e-\u0bb2\u0bc6\u0baf\u0b95 | <br/> B\u0b86\u0bb0\u0ba4 B\u0b89\u0bb5\u0bc6\u0ba9\u0bc6 \u0b9c\u0ba9\u0bc1\u0bb2\u0bc1 \u0b95\u0bb2\u0bc6\u0b9a\u0bc6 \u0bae\u0b8e\u0bb2\u0bc6\u0b9a\u0bc6 \u0b9a\u0bc1\u0b83\u0b85\u0bae\u0bc1\u0b95 \u0bb5\u0ba4\u0bb2\u0bc1\u0ba4\u0bbe\u0b8f | <br/> \u0bae\u0bbe \u0b9f\u0bcd\u0baf\u0bbe\u0b8f\u0baf\u0bae\u0bc1\u0b95 \u0b9a\u0ba4\u0bcd\u0b95\u0bb0\u0bcd\u0bae\u0bbe\u0b9a\u0bb0\u0ba3\u0bae\u0bc1 B\u0b85\u0b95\u0bcd\u0ba4\u0bcd\u0baf\u0bbe\u0ba9\u0ba8\u0bcd\u0ba4 \u0bb5\u0bb0\u0bae\u0bc6\u0ba4\u0bc1 | <br/> (\u0baa\u0bbe\u0ba4) ||"}]}, {"L": "Telugu", "text": [{"M": "\u0c2a\u0c32\u0c4d\u0c32\u0c35\u0c3f", "T": "\u0c13\u0c2e\u0c4d\u0c15\u0c3e\u0c30 \u0c2a\u0c4d\u0c30\u0c23\u0c35 \u0c28\u0c3e\u0c27\u0c4b\u0c27\u0c4dB\u0c05\u0c35 \u0c37\u0c4d\u0c30\u0c41\u0c25\u0c46 \u0c32\u0c2f \u0c38\u0c4d\u0c35\u0c30 ||"}, {"M": "\u0c05\u0c28\u0c41\u0c2a\u0c32\u0c4d\u0c32\u0c35\u0c3f", "T": "\u0c38\u0c01\u0c15\u0c3e\u0c0f\u0c25\u0c46\u0c15 \u0c25\u0c4d\u0c30\u0c46B\u0c09\u0c35\u0c28 \u0c37\u0c15\u0c4d\u0c25\u0c46 \u0c2e\u0c41\u0c30\u0c32\u0c46 \u0c17\u0c3e\u0c28 \u0c38\u0c4d\u0c35\u0c30\u0c42\u0c2a\u0c46 ||"}, {"M": "\u0c2e\u0c41\u0c15\u0c4d\u0c24\u0c3e\u0c2f\u0c3f \u0c38\u0c3e\u0c39\u0c3f\u0c24\u0c4d\u0c2f\u0c2e\u0c4d", "T": "\u0c2a\u0c4d\u0c30\u0c33\u0c2f \u0c1c\u0c32 \u0c35\u0c32\u0c2f \u0c2a\u0c41\u0c24\u0c32\u0c32\u0c32\u0c4a \u0c25\u0c3e\u0c0f\u0c32\u0c28 \u0c22\u0c3e\u0c0f \u0c35\u0c24 \u0c2a\u0c25\u0c4d\u0c30\u0c2e\u0c4d | <br/> \u0c37\u0c2f\u0c28\u0c46\u0c01\u0c1a\u0c41 \u0c35\u0c30\u0c27\u0c41\u0c28\u0c46 \u0c28\u0c3eB\u0c0e\u0c28\u0c46\u0c35\u0c27\u0c32\u0c46\u0c28 \u0c15\u0c2e\u0c32 \u0c38\u0c01\u0c1c\u0c28\u0c46\u0c25 \u0c35\u0c46\u0c22\u0c46 \u0c2e\u0c41\u0c17\u0c4d\u0c05 \u0c1c\u0c28\u0c46\u0c25 ||"}, {"M": "\u0c1a\u0c30\u0c23\u0c2e\u0c4d", "T": "\u0c2a\u0c3e\u0c27 \u0c28\u0c47\u0c30\u0c1c\u0c2e\u0c41\u0c32\u0c3e\u0c0f \u0c28\u0c2e\u0c4d\u0c2e\u0c46\u0c25\u0c46 ||"}, {"M": "\u0c38\u0c4d\u0c35\u0c30 \u0c38\u0c3e\u0c39\u0c3f\u0c24\u0c4d\u0c2f\u0c2e\u0c4d 1", "T": "\u0c37\u0c3e\u0c38\u0c4d\u0c35\u0c25\u0c2e\u0c40 \u0c38\u0c41\u0c17\u0c4d\u0c05\u0c2e\u0c40\u0c28\u0c27\u0c46 | <br/> (\u0c2a\u0c3e\u0c27)"}, {"M": "\u0c38\u0c4d\u0c35\u0c30 \u0c38\u0c3e\u0c39\u0c3f\u0c24\u0c4d\u0c2f\u0c2e\u0c4d 2", "T": "\u0c17\u0c3e\u0c0a\u0c30\u0c47\u0c37 \u0c38\u0c4d\u0c30\u0c47\u0c37 \u0c35\u0c46\u0c22\u0c47\u0c28\u0c4d\u0c27\u0c4d\u0c30 \u0c25\u0c4d\u0c2f\u0c3e\u0c17\u0c30\u0c3e\u0c1c\u0c3e\u0c27\u0c46 \u0c35\u0c30\u0c4d\u0c2f \u0c38\u0c2a\u0c30\u0c4d\u0c2f\u0c32\u0c28\u0c4d\u0c27\u0c41 | <br/> (\u0c2a\u0c3e\u0c27)"}, {"M": "\u0c38\u0c4d\u0c35\u0c30 \u0c38\u0c3e\u0c39\u0c3f\u0c24\u0c4d\u0c2f\u0c2e\u0c4d 3", "T": "\u0c2a\u0c4d\u0c30B\u0c05\u0c35\u0c2e\u0c41 \u0c35\u0c46B\u0c05\u0c35\u0c2e\u0c41 \u0c32\u0c47\u0c28\u0c2e\u0c41\u0c32\u0c41 \u0c27\u0c37 \u0c27\u0c46\u0c37\u0c32\u0c41 B\u0c0a\u0c2e\u0c46 \u0c17\u0c17\u0c28 \u0c2a\u0c35\u0c28\u0c2e\u0c41 | <br/> \u0c38\u0c32\u0c46\u0c32\u0c2e\u0c17\u0c4d\u0c28\u0c46 \u0c28\u0c17 \u0c17\u0c4d\u0c05\u0c17 \u0c25\u0c30\u0c41 \u0c2e\u0c4d\u0c30\u0c4d\u0c17\u0c3e\u0c27\u0c46 \u0c38\u0c15\u0c32\u0c2e\u0c41 \u0c15\u0c32\u0c41\u0c17\u0c41 \u0c28\u0c47 \u0c27\u0c2f\u0c35\u0c32\u0c28 | <br/> (\u0c2a\u0c3e\u0c27)"}, {"M": "\u0c38\u0c4d\u0c35\u0c30 \u0c38\u0c3e\u0c39\u0c3f\u0c24\u0c4d\u0c2f\u0c2e\u0c4d 4", "T": "\u0c28\u0c47 \u0c28\u0c3e \u0c2f\u0c0e\u0c28\u0c27\u0c41 \u0c25\u0c3e\u0c30\u0c25\u0c2e\u0c4d\u0c2f\u0c2e\u0c41\u0c32\u0c41 \u0c2e\u0c28\u0c17\u0c3e \u0c38\u0c41\u0c17\u0c41\u0c28\u0c2e\u0c46\u0c1a\u0c4d\u0c1a\u0c46 | <br/> \u0c38\u0c39\u0c28\u0c2e\u0c41 \u0c35\u0c46\u0c30\u0c3e\u0c32 \u0c35\u0c46\u0c25\u0c30\u0c28\u0c2e\u0c41 \u0c28\u0c46\u0c1c\u0c2e\u0c41\u0c28\u0c41 \u0c2a\u0c32\u0c4d\u0c15\u0c41\u0c24\u0c32\u0c41 \u0c38\u0c27\u0c3e \u0c25\u0c0e-\u0c32\u0c46\u0c2f\u0c17 | <br/> B\u0c06\u0c30\u0c25 B\u0c09\u0c35\u0c46\u0c28\u0c46 \u0c1c\u0c28\u0c41\u0c32\u0c41 \u0c15\u0c32\u0c46\u0c38\u0c46 \u0c2e\u0c0e\u0c32\u0c46\u0c38\u0c46 \u0c38\u0c41\u0c17\u0c4d\u0c05\u0c2e\u0c41\u0c17 \u0c35\u0c27\u0c32\u0c41\u0c25\u0c3e\u0c0f | <br/> \u0c2e\u0c3e \u0c22\u0c4d\u0c2f\u0c3e\u0c0f\u0c2f\u0c2e\u0c41\u0c17 \u0c38\u0c25\u0c4d\u0c15\u0c30\u0c4d\u0c2e\u0c3e\u0c1a\u0c30\u0c23\u0c2e\u0c41 B\u0c05\u0c15\u0c4d\u0c25\u0c4d\u0c2f\u0c3e\u0c28\u0c28\u0c4d\u0c27 \u0c35\u0c30\u0c2e\u0c46\u0c26\u0c41 | <br/> (\u0c2a\u0c3e\u0c27) ||"}]}, {"L": "Kannada", "text": [{"M": "\u0caa\u0cb2\u0ccd\u0cb2\u0cb5\u0cbf", "T": "\u0c93\u0cae\u0ccd\u0c95\u0cbe\u0cb0 \u0caa\u0ccd\u0cb0\u0ca3\u0cb5 \u0ca8\u0cbe\u0ca7\u0ccb\u0ca7\u0ccdB\u0c85\u0cb5 \u0cb7\u0ccd\u0cb0\u0cc1\u0ca5\u0cc6 \u0cb2\u0caf \u0cb8\u0ccd\u0cb5\u0cb0 ||"}, {"M": "\u0c85\u0ca8\u0cc1\u0caa\u0cb2\u0ccd\u0cb2\u0cb5\u0cbf", "T": "\u0cb8\u0c82\u0c95\u0cbe\u0c8f\u0ca5\u0cc6\u0c95 \u0ca5\u0ccd\u0cb0\u0cc6B\u0c89\u0cb5\u0ca8 \u0cb7\u0c95\u0ccd\u0ca5\u0cc6 \u0cae\u0cc1\u0cb0\u0cb2\u0cc6 \u0c97\u0cbe\u0ca8 \u0cb8\u0ccd\u0cb5\u0cb0\u0cc2\u0caa\u0cc6 ||"}, {"M": "\u0cae\u0cc1\u0c95\u0ccd\u0ca4\u0cbe\u0caf\u0cbf \u0cb8\u0cbe\u0cb9\u0cbf\u0ca4\u0ccd\u0caf\u0cae\u0ccd", "T": "\u0caa\u0ccd\u0cb0\u0cb3\u0caf \u0c9c\u0cb2 \u0cb5\u0cb2\u0caf \u0caa\u0cc1\u0ca4\u0cb2\u0cb2\u0cb2\u0cca \u0ca5\u0cbe\u0c8f\u0cb2\u0ca8 \u0ca2\u0cbe\u0c8f \u0cb5\u0ca4 \u0caa\u0ca5\u0ccd\u0cb0\u0cae\u0ccd | <br/> \u0cb7\u0caf\u0ca8\u0cc6\u0c82\u0c9a\u0cc1 \u0cb5\u0cb0\u0ca7\u0cc1\u0ca8\u0cc6 \u0ca8\u0cbeB\u0c8e\u0ca8\u0cc6\u0cb5\u0ca7\u0cb2\u0cc6\u0ca8 \u0c95\u0cae\u0cb2 \u0cb8\u0c82\u0c9c\u0ca8\u0cc6\u0ca5 \u0cb5\u0cc6\u0ca2\u0cc6 \u0cae\u0cc1\u0c97\u0ccd\u0c85 \u0c9c\u0ca8\u0cc6\u0ca5 ||"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd", "T": "\u0caa\u0cbe\u0ca7 \u0ca8\u0cc7\u0cb0\u0c9c\u0cae\u0cc1\u0cb2\u0cbe\u0c8f \u0ca8\u0cae\u0ccd\u0cae\u0cc6\u0ca5\u0cc6 ||"}, {"M": "\u0cb8\u0ccd\u0cb5\u0cb0 \u0cb8\u0cbe\u0cb9\u0cbf\u0ca4\u0ccd\u0caf\u0cae\u0ccd 1", "T": "\u0cb7\u0cbe\u0cb8\u0ccd\u0cb5\u0ca5\u0cae\u0cc0 \u0cb8\u0cc1\u0c97\u0ccd\u0c85\u0cae\u0cc0\u0ca8\u0ca7\u0cc6 | <br/> (\u0caa\u0cbe\u0ca7)"}, {"M": "\u0cb8\u0ccd\u0cb5\u0cb0 \u0cb8\u0cbe\u0cb9\u0cbf\u0ca4\u0ccd\u0caf\u0cae\u0ccd 2", "T": "\u0c97\u0cbe\u0c8a\u0cb0\u0cc7\u0cb7 \u0cb8\u0ccd\u0cb0\u0cc7\u0cb7 \u0cb5\u0cc6\u0ca2\u0cc7\u0ca8\u0ccd\u0ca7\u0ccd\u0cb0 \u0ca5\u0ccd\u0caf\u0cbe\u0c97\u0cb0\u0cbe\u0c9c\u0cbe\u0ca7\u0cc6 \u0cb5\u0cb0\u0ccd\u0caf \u0cb8\u0caa\u0cb0\u0ccd\u0caf\u0cb2\u0ca8\u0ccd\u0ca7\u0cc1 | <br/> (\u0caa\u0cbe\u0ca7)"}, {"M": "\u0cb8\u0ccd\u0cb5\u0cb0 \u0cb8\u0cbe\u0cb9\u0cbf\u0ca4\u0ccd\u0caf\u0cae\u0ccd 3", "T": "\u0caa\u0ccd\u0cb0B\u0c85\u0cb5\u0cae\u0cc1 \u0cb5\u0cc6B\u0c85\u0cb5\u0cae\u0cc1 \u0cb2\u0cc7\u0ca8\u0cae\u0cc1\u0cb2\u0cc1 \u0ca7\u0cb7 \u0ca7\u0cc6\u0cb7\u0cb2\u0cc1 B\u0c8a\u0cae\u0cc6 \u0c97\u0c97\u0ca8 \u0caa\u0cb5\u0ca8\u0cae\u0cc1 | <br/> \u0cb8\u0cb2\u0cc6\u0cb2\u0cae\u0c97\u0ccd\u0ca8\u0cc6 \u0ca8\u0c97 \u0c97\u0ccd\u0c85\u0c97 \u0ca5\u0cb0\u0cc1 \u0cae\u0ccd\u0cb0\u0ccd\u0c97\u0cbe\u0ca7\u0cc6 \u0cb8\u0c95\u0cb2\u0cae\u0cc1 \u0c95\u0cb2\u0cc1\u0c97\u0cc1 \u0ca8\u0cc7 \u0ca7\u0caf\u0cb5\u0cb2\u0ca8 | <br/> (\u0caa\u0cbe\u0ca7)"}, {"M": "\u0cb8\u0ccd\u0cb5\u0cb0 \u0cb8\u0cbe\u0cb9\u0cbf\u0ca4\u0ccd\u0caf\u0cae\u0ccd 4", "T": "\u0ca8\u0cc7 \u0ca8\u0cbe \u0caf\u0c8e\u0ca8\u0ca7\u0cc1 \u0ca5\u0cbe\u0cb0\u0ca5\u0cae\u0ccd\u0caf\u0cae\u0cc1\u0cb2\u0cc1 \u0cae\u0ca8\u0c97\u0cbe \u0cb8\u0cc1\u0c97\u0cc1\u0ca8\u0cae\u0cc6\u0c9a\u0ccd\u0c9a\u0cc6 | <br/> \u0cb8\u0cb9\u0ca8\u0cae\u0cc1 \u0cb5\u0cc6\u0cb0\u0cbe\u0cb2 \u0cb5\u0cc6\u0ca5\u0cb0\u0ca8\u0cae\u0cc1 \u0ca8\u0cc6\u0c9c\u0cae\u0cc1\u0ca8\u0cc1 \u0caa\u0cb2\u0ccd\u0c95\u0cc1\u0ca4\u0cb2\u0cc1 \u0cb8\u0ca7\u0cbe \u0ca5\u0c8e-\u0cb2\u0cc6\u0caf\u0c97 | <br/> B\u0c86\u0cb0\u0ca5 B\u0c89\u0cb5\u0cc6\u0ca8\u0cc6 \u0c9c\u0ca8\u0cc1\u0cb2\u0cc1 \u0c95\u0cb2\u0cc6\u0cb8\u0cc6 \u0cae\u0c8e\u0cb2\u0cc6\u0cb8\u0cc6 \u0cb8\u0cc1\u0c97\u0ccd\u0c85\u0cae\u0cc1\u0c97 \u0cb5\u0ca7\u0cb2\u0cc1\u0ca5\u0cbe\u0c8f | <br/> \u0cae\u0cbe \u0ca2\u0ccd\u0caf\u0cbe\u0c8f\u0caf\u0cae\u0cc1\u0c97 \u0cb8\u0ca5\u0ccd\u0c95\u0cb0\u0ccd\u0cae\u0cbe\u0c9a\u0cb0\u0ca3\u0cae\u0cc1 B\u0c85\u0c95\u0ccd\u0ca5\u0ccd\u0caf\u0cbe\u0ca8\u0ca8\u0ccd\u0ca7 \u0cb5\u0cb0\u0cae\u0cc6\u0ca6\u0cc1 | <br/> (\u0caa\u0cbe\u0ca7) ||"}]}, {"L": "Malayalam", "text": [{"M": "\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d13\u0d2e\u0d4d\u0d15\u0d3e\u0d30 \u0d2a\u0d4d\u0d30\u0d23\u0d35 \u0d28\u0d3e\u0d27\u0d4b\u0d27\u0d4dB\u0d05\u0d35 \u0d37\u0d4d\u0d30\u0d41\u0d25\u0d46 \u0d32\u0d2f \u0d38\u0d4d\u0d35\u0d30 ||"}, {"M": "\u0d05\u0d28\u0d41\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d38\u0d02\u0d15\u0d3e\u0d0f\u0d25\u0d46\u0d15 \u0d25\u0d4d\u0d30\u0d46B\u0d09\u0d35\u0d28 \u0d37\u0d15\u0d4d\u0d25\u0d46 \u0d2e\u0d41\u0d30\u0d32\u0d46 \u0d17\u0d3e\u0d28 \u0d38\u0d4d\u0d35\u0d30\u0d42\u0d2a\u0d46 ||"}, {"M": "\u0d2e\u0d41\u0d15\u0d4d\u0d24\u0d3e\u0d2f\u0d3f \u0d38\u0d3e\u0d39\u0d3f\u0d24\u0d4d\u0d2f\u0d2e\u0d4d", "T": "\u0d2a\u0d4d\u0d30\u0d33\u0d2f \u0d1c\u0d32 \u0d35\u0d32\u0d2f \u0d2a\u0d41\u0d24\u0d32\u0d32\u0d32\u0d4a \u0d25\u0d3e\u0d0f\u0d32\u0d28 \u0d22\u0d3e\u0d0f \u0d35\u0d24 \u0d2a\u0d25\u0d4d\u0d30\u0d2e\u0d4d | <br/> \u0d37\u0d2f\u0d28\u0d46\u0d02\u0d1a\u0d41 \u0d35\u0d30\u0d27\u0d41\u0d28\u0d46 \u0d28\u0d3eB\u0d0e\u0d28\u0d46\u0d35\u0d27\u0d32\u0d46\u0d28 \u0d15\u0d2e\u0d32 \u0d38\u0d02\u0d1c\u0d28\u0d46\u0d25 \u0d35\u0d46\u0d22\u0d46 \u0d2e\u0d41\u0d17\u0d4d\u0d05 \u0d1c\u0d28\u0d46\u0d25 ||"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d", "T": "\u0d2a\u0d3e\u0d27 \u0d28\u0d47\u0d30\u0d1c\u0d2e\u0d41\u0d32\u0d3e\u0d0f \u0d28\u0d2e\u0d4d\u0d2e\u0d46\u0d25\u0d46 ||"}, {"M": "\u0d38\u0d4d\u0d35\u0d30 \u0d38\u0d3e\u0d39\u0d3f\u0d24\u0d4d\u0d2f\u0d2e\u0d4d 1", "T": "\u0d37\u0d3e\u0d38\u0d4d\u0d35\u0d25\u0d2e\u0d40 \u0d38\u0d41\u0d17\u0d4d\u0d05\u0d2e\u0d40\u0d28\u0d27\u0d46 | <br/> (\u0d2a\u0d3e\u0d27)"}, {"M": "\u0d38\u0d4d\u0d35\u0d30 \u0d38\u0d3e\u0d39\u0d3f\u0d24\u0d4d\u0d2f\u0d2e\u0d4d 2", "T": "\u0d17\u0d3e\u0d0a\u0d30\u0d47\u0d37 \u0d38\u0d4d\u0d30\u0d47\u0d37 \u0d35\u0d46\u0d22\u0d47\u0d28\u0d4d\u0d27\u0d4d\u0d30 \u0d25\u0d4d\u0d2f\u0d3e\u0d17\u0d30\u0d3e\u0d1c\u0d3e\u0d27\u0d46 \u0d35\u0d30\u0d4d\u0d2f \u0d38\u0d2a\u0d30\u0d4d\u0d2f\u0d32\u0d28\u0d4d\u0d27\u0d41 | <br/> (\u0d2a\u0d3e\u0d27)"}, {"M": "\u0d38\u0d4d\u0d35\u0d30 \u0d38\u0d3e\u0d39\u0d3f\u0d24\u0d4d\u0d2f\u0d2e\u0d4d 3", "T": "\u0d2a\u0d4d\u0d30B\u0d05\u0d35\u0d2e\u0d41 \u0d35\u0d46B\u0d05\u0d35\u0d2e\u0d41 \u0d32\u0d47\u0d28\u0d2e\u0d41\u0d32\u0d41 \u0d27\u0d37 \u0d27\u0d46\u0d37\u0d32\u0d41 B\u0d0a\u0d2e\u0d46 \u0d17\u0d17\u0d28 \u0d2a\u0d35\u0d28\u0d2e\u0d41 | <br/> \u0d38\u0d32\u0d46\u0d32\u0d2e\u0d17\u0d4d\u0d28\u0d46 \u0d28\u0d17 \u0d17\u0d4d\u0d05\u0d17 \u0d25\u0d30\u0d41 \u0d2e\u0d4d\u0d30\u0d4d\u0d17\u0d3e\u0d27\u0d46 \u0d38\u0d15\u0d32\u0d2e\u0d41 \u0d15\u0d32\u0d41\u0d17\u0d41 \u0d28\u0d47 \u0d27\u0d2f\u0d35\u0d32\u0d28 | <br/> (\u0d2a\u0d3e\u0d27)"}, {"M": "\u0d38\u0d4d\u0d35\u0d30 \u0d38\u0d3e\u0d39\u0d3f\u0d24\u0d4d\u0d2f\u0d2e\u0d4d 4", "T": "\u0d28\u0d47 \u0d28\u0d3e \u0d2f\u0d0e\u0d28\u0d27\u0d41 \u0d25\u0d3e\u0d30\u0d25\u0d2e\u0d4d\u0d2f\u0d2e\u0d41\u0d32\u0d41 \u0d2e\u0d28\u0d17\u0d3e \u0d38\u0d41\u0d17\u0d41\u0d28\u0d2e\u0d46\u0d1a\u0d4d\u0d1a\u0d46 | <br/> \u0d38\u0d39\u0d28\u0d2e