ae\u0ba3\u0bbf\u0baf\u0bc7          (\u0bae\u0bbe\u0b9f\u0bc1 \u0bae\u0bc7\u0baf\u0bcd\u0b95\u0bcd\u0b95\u0bc1\u0bae\u0bcd) <br/> \u0b95\u0bb3\u0bcd\u0bb3\u0ba9\u0bc1\u0b95\u0bcd\u0b95\u0bcb\u0bb0\u0bcd \u0b95\u0bb3\u0bcd\u0bb3\u0ba9\u0bcd \u0b89\u0ba3\u0bcd\u0b9f\u0bcb? \u0b95\u0ba3\u0bcd\u0b9f\u0ba4\u0bc1\u0ba3\u0bcd\u0b9f\u0bcb \u0b9a\u0bca\u0bb2\u0bcd\u0bb2\u0bc1\u0bae\u0bcd \u0b85\u0bae\u0bcd\u0bae\u0bbe? <br/> \u0b95\u0bb3\u0bcd\u0bb5\u0bb0\u0bcd \u0bb5\u0ba8\u0bcd\u0ba4\u0bc1 \u0b8e\u0ba9\u0bc8 \u0b85\u0b9f\u0bbf\u0ba4\u0bcd\u0ba4\u0bbe\u0bb2\u0bcd \u0b95\u0ba3\u0bcd\u0b9f \u0ba4\u0bc1\u0ba3\u0bcd\u0b9f\u0bae\u0bcd \u0b9a\u0bc6\u0baf\u0bcd\u0ba4\u0bbf\u0b9f\u0bc1\u0bb5\u0bc7\u0ba9\u0bcd      (\u0baa\u0bcb\u0b95 \u0bb5\u0bc7\u0ba3\u0bc1\u0bae\u0bcd)"}, {"M": "\u0b9a\u0bb0\u0ba3\u0bae\u0bcd 2", "T": "\u0b95\u0bcb\u0bb5\u0bb0\u0bcd\u0ba4\u0bcd\u0ba4\u0ba9 \u0b95\u0bbf\u0bb0\u0bbf\u0baf\u0bbf\u0bb2\u0bcd \u0b95\u0bcb\u0bb0\u0bae\u0bbe\u0ba9 \u0bae\u0bbf\u0bb0\u0bc1\u0b95\u0b99\u0bcd\u0b95\u0bb3\u0bcd \u0b89\u0ba3\u0bcd\u0b9f\u0bc1 <br/> \u0b95\u0bb0\u0b9f\u0bbf \u0baa\u0bc1\u0bb2\u0bbf\u0baf\u0bc8\u0b95\u0bcd \u0b95\u0ba3\u0bcd\u0b9f\u0bbe\u0bb2\u0bcd \u0b95\u0bb2\u0b99\u0bcd\u0b95\u0bbf\u0b9f\u0bc1\u0bb5\u0bbe\u0baf\u0bcd \u0b95\u0ba3\u0bcd\u0bae\u0ba3\u0bbf\u0baf\u0bc7                     (\u0bae\u0bbe\u0b9f\u0bc1 \u0bae\u0bc7\u0baf\u0bcd\u0b95\u0bcd\u0b95\u0bc1\u0bae\u0bcd) <br/> \u0b95\u0bbe\u0b9f\u0bcd\u0b9f\u0bc1 \u0bae\u0bbf\u0bb0\u0bc1\u0b95\u0bae\u0bc6\u0bb2\u0bcd\u0bb2\u0bbe\u0bae\u0bcd \u0b8e\u0ba9\u0bcd\u0ba9\u0bc8\u0b95\u0bcd \u0b95\u0ba3\u0bcd\u0b9f\u0bbe\u0bb2\u0bcd \u0b93\u0b9f\u0bbf \u0bb5\u0bb0\u0bc1\u0bae\u0bcd <br/> \u0b95\u0bc2\u0b9f\u0bcd\u0b9f\u0b99\u0bcd \u0b95\u0bc2\u0b9f\u0bcd\u0b9f\u0bae\u0bbe\u0b95 \u0bb5\u0ba8\u0bcd\u0ba4\u0bbe\u0bb2\u0bcd \u0bb5\u0bc7\u0b9f\u0bcd\u0b9f\u0bc8 \u0b86\u0b9f\u0bbf \u0b9c\u0bc6\u0baf\u0bbf\u0ba4\u0bcd\u0ba4\u0bbf\u0b9f\u0bc1\u0bb5\u0bc7\u0ba9\u0bcd       (\u0baa\u0bcb\u0b95 \u0bb5\u0bc7\u0ba3\u0bc1\u0bae\u0bcd)"}, {"M": "\u0b9a\u0bb0\u0ba3\u0bae\u0bcd 3", "T": "\u0baa\u0bbe\u0b9a\u0bae\u0bc1\u0bb3\u0bcd\u0bb3 \u0ba8\u0ba8\u0bcd\u0ba4\u0b95\u0bcb\u0baa\u0bb0\u0bcd \u0baa\u0bbe\u0bb2\u0ba9\u0bcd \u0b8e\u0b99\u0bcd\u0b95\u0bc7 \u0b8e\u0ba9\u0bcd\u0bb1\u0bc1 \u0b95\u0bc7\u0b9f\u0bcd\u0b9f\u0bbe\u0bb2\u0bcd <br/> \u0b8e\u0ba9\u0bcd\u0ba9 \u0baa\u0ba4\u0bbf\u0bb2\u0bcd \u0b9a\u0bca\u0bb2\u0bcd\u0bb5\u0bc7\u0ba9\u0b9f\u0bbe \u0b8e\u0ba9\u0bcd\u0ba9\u0bc1\u0b9f\u0bc8\u0baf \u0b95\u0ba3\u0bcd\u0bae\u0ba3\u0bbf\u0baf\u0bc7                    (\u0bae\u0bbe\u0b9f\u0bc1 \u0bae\u0bc7\u0baf\u0bcd\u0b95\u0bcd\u0b95\u0bc1\u0bae\u0bcd) <br/> \u0baa\u0bbe\u0bb2\u0bb0\u0bc1\u0b9f\u0ba9\u0bcd \u0bb5\u0bc0\u0ba4\u0bbf\u0baf\u0bbf\u0bb2\u0bc7 \u0baa\u0ba8\u0bcd\u0ba4\u0bbe\u0b9f\u0bc1\u0bb1\u0bbe\u0ba9\u0bcd \u0b8e\u0ba9\u0bcd\u0bb1\u0bc1 \u0b9a\u0bca\u0bb2\u0bcd\u0bb2\u0bc7\u0ba9\u0bcd <br/> \u0ba4\u0bc7\u0b9f\u0bbf \u0b8e\u0ba9\u0bcd\u0ba9\u0bc8 \u0bb5\u0bb0\u0bc1\u0b95\u0bc8\u0baf\u0bbf\u0bb2\u0bc7 \u0b93\u0b9f\u0bbf \u0bb5\u0ba8\u0bcd\u0ba4\u0bc1 \u0ba8\u0bbf\u0ba9\u0bcd\u0bb1\u0bbf\u0b9f\u0bc1\u0bb5\u0bc7\u0ba9\u0bcd                     (\u0baa\u0bcb\u0b95 \u0bb5\u0bc7\u0ba3\u0bc1\u0bae\u0bcd) <br/> \u0bae\u0bbe\u0b9f\u0bc1 \u0bae\u0bc7\u0baf\u0bcd\u0b95\u0bcd\u0b95\u0bc1\u0bae\u0bcd \u0b95\u0ba3\u0bcd\u0ba3\u0bc7 \u0ba8\u0bc0 <br/> \u0baa\u0bcb\u0b95 \u0bb5\u0bc7\u0ba3\u0bcd\u0b9f\u0bbe\u0bae\u0bcd \u0b9a\u0bca\u0ba9\u0bcd\u0ba9\u0bc7\u0ba9\u0bcd \u0baa\u0bcb\u0b95 \u0bb5\u0bc7\u0ba3\u0bc1\u0bae\u0bcd \u0ba4\u0bbe\u0baf\u0bc7 <br/> \u0ba4\u0b9f\u0bc8 \u0b9a\u0bca\u0bb2\u0bcd\u0bb2\u0bbe\u0ba4\u0bc7 \u0ba8\u0bc0\u0baf\u0bc7"}]}, {"L": "Telugu", "text": [{"M": "\u0c2a\u0c32\u0c4d\u0c32\u0c35\u0c3f", "T": "\u0c2e\u0c3e\u0c21\u0c41 \u0c2e\u0c47\u0c2f\u0c4d\u0c15\u0c4d\u0c15\u0c41\u0c2e\u0c4d \u0c15\u0c23\u0c4d\u0c23\u0c47 \u0c28\u0c40 <br/> \u0c2a\u0c4b\u0c17 \u0c35\u0c47\u0c23\u0c4d\u0c21\u0c3e\u0c2e\u0c4d \u0c38\u0c4a\u0c28\u0c4d\u0c28\u0c47\u0c28\u0c4d <br/> \u0c2a\u0c4b\u0c17 \u0c35\u0c47\u0c23\u0c41\u0c2e\u0c4d \u0c24\u0c3e\u0c2f\u0c47 <br/> \u0c24\u0c21\u0c48 \u0c38\u0c4a\u0c32\u0c4d\u0c32\u0c3e\u0c26\u0c47 \u0c28\u0c40\u0c2f\u0c47 <br/> \u0c15\u0c3e\u0c2f\u0c4d\u0c1a\u0c4d\u0c1a\u0c3f\u0c28 \u0c2a\u0c3e\u0c32\u0c41 \u0c24\u0c30\u0c47\u0c28\u0c4d; \u0c15\u0c32\u0c4d\u0c15\u0c23\u0c4d\u0c21\u0c41\u0c1a\u0c4d \u0c1a\u0c40\u0c28\u0c3f \u0c24\u0c30\u0c47\u0c28\u0c4d <br/> \u0c15\u0c48 \u0c28\u0c3f\u0c31\u0c48\u0c2f \u0c35\u0c46\u0c23\u0c4d\u0c23\u0c46\u0c2f\u0c4d  \u0c24\u0c30\u0c47\u0c28\u0c4d; \u0c35\u0c46\u0c2f\u0c4d\u0c2f\u0c3f\u0c32\u0c3f\u0c32\u0c47 \u0c2a\u0c4b\u0c17 \u0c35\u0c47\u0c23\u0c4d\u0c21\u0c3e\u0c2e\u0c4d        (\u0c2e\u0c3e\u0c21\u0c41 \u0c2e\u0c47\u0c2f\u0c4d\u0c15\u0c4d\u0c15\u0c41\u0c2e\u0c4d) <br/> \u0c15\u0c3e\u0c2f\u0c4d\u0c1a\u0c4d\u0c1a\u0c3f\u0c28 \u0c2a\u0c3e\u0c32\u0c41\u0c2e\u0c4d \u0c35\u0c47\u0c23\u0c4d\u0c21\u0c3e\u0c2e\u0c4d; \u0c15\u0c32\u0c4d\u0c15\u0c23\u0c4d\u0c21\u0c41\u0c1a\u0c4d \u0c1a\u0c40\u0c28\u0c3f \u0c35\u0c47\u0c23\u0c4d\u0c21\u0c3e\u0c2e\u0c4d <br/> \u0c09\u0c32\u0c4d\u0c32\u0c3e\u0c38\u0c2e\u0c3e\u0c2f\u0c4d \u0c2e\u0c3e\u0c21\u0c41 \u0c2e\u0c47\u0c2f\u0c4d\u0c24\u0c4d\u0c24\u0c41, \u0c12\u0c30\u0c41 \u0c28\u0c4a\u0c21\u0c3f\u0c2f\u0c3f\u0c32\u0c4d \u0c24\u0c3f\u0c30\u0c41\u0c2e\u0c4d\u0c2c\u0c3f\u0c21\u0c41\u0c35\u0c47\u0c28\u0c4d          (\u0c2a\u0c4b\u0c17 \u0c35\u0c47\u0c23\u0c41\u0c2e\u0c4d)"}, {"M": "\u0c1a\u0c30\u0c23\u0c2e\u0c4d 1", "T": "\u0c2f\u0c2e\u0c41\u0c28\u0c3e \u0c28\u0c26\u0c3f\u0c15\u0c4d \u0c15\u0c30\u0c48\u0c2f\u0c3f\u0c32\u0c4d \u0c0e\u0c2a\u0c4d\u0c2a\u0c4a\u0c34\u0c41\u0c26\u0c41\u0c2e\u0c4d \u0c15\u0c33\u0c4d\u0c35\u0c30\u0c4d \u0c2a\u0c2f\u0c2e\u0c4d <br/> \u0c15\u0c33\u0c4d\u0c35\u0c30\u0c4d \u0c35\u0c28\u0c4d\u0c26\u0c41 \u0c09\u0c28\u0c48 \u0c05\u0c21\u0c3f\u0c24\u0c4d\u0c24\u0c3e\u0c32\u0c4d \u0c15\u0c32\u0c19\u0c4d\u0c17\u0c3f\u0c21\u0c41\u0c35\u0c3e\u0c2f\u0c4d \u0c15\u0c23\u0c4d\u0c2e\u0c23\u0c3f\u0c2f\u0c47          (\u0c2e\u0c3e\u0c21\u0c41 \u0c2e\u0c47\u0c2f\u0c4d\u0c15\u0c4d\u0c15\u0c41\u0c2e\u0c4d) <br/> \u0c15\u0c33\u0c4d\u0c33\u0c28\u0c41\u0c15\u0c4d\u0c15\u0c4b\u0c30\u0c4d \u0c15\u0c33\u0c4d\u0c33\u0c28\u0c4d \u0c09\u0c23\u0c4d\u0c21\u0c4b? \u0c15\u0c23\u0c4d\u0c21\u0c26\u0c41\u0c23\u0c4d\u0c21\u0c4b \u0c38\u0c4a\u0c32\u0c4d\u0c32\u0c41\u0c2e\u0c4d \u0c05\u0c2e\u0c4d\u0c2e\u0c3e? <br/> \u0c15\u0c33\u0c4d\u0c35\u0c30\u0c4d \u0c35\u0c28\u0c4d\u0c26\u0c41 \u0c0e\u0c28\u0c48 \u0c05\u0c21\u0c3f\u0c24\u0c4d\u0c24\u0c3e\u0c32\u0c4d \u0c15\u0c23\u0c4d\u0c21 \u0c24\u0c41\u0c23\u0c4d\u0c21\u0c2e\u0c4d \u0c38\u0c46\u0c2f\u0c4d\u0c24\u0c3f\u0c21\u0c41\u0c35\u0c47\u0c28\u0c4d      (\u0c2a\u0c4b\u0c17 \u0c35\u0c47\u0c23\u0c41\u0c2e\u0c4d)"}, {"M": "\u0c1a\u0c30\u0c23\u0c2e\u0c4d 2", "T": "\u0c15\u0c4b\u0c35\u0c30\u0c4d\u0c24\u0c4d\u0c24\u0c28 \u0c15\u0c3f\u0c30\u0c3f\u0c2f\u0c3f\u0c32\u0c4d \u0c15\u0c4b\u0c30\u0c2e\u0c3e\u0c28 \u0c2e\u0c3f\u0c30\u0c41\u0c17\u0c19\u0c4d\u0c17\u0c33\u0c4d \u0c09\u0c23\u0c4d\u0c21\u0c41 <br/> \u0c15\u0c30\u0c21\u0c3f \u0c2a\u0c41\u0c32\u0c3f\u0c2f\u0c48\u0c15\u0c4d \u0c15\u0c23\u0c4d\u0c21\u0c3e\u0c32\u0c4d \u0c15\u0c32\u0c19\u0c4d\u0c17\u0c3f\u0c21\u0c41\u0c35\u0c3e\u0c2f\u0c4d \u0c15\u0c23\u0c4d\u0c2e\u0c23\u0c3f\u0c2f\u0c47                     (\u0c2e\u0c3e\u0c21\u0c41 \u0c2e\u0c47\u0c2f\u0c4d\u0c15\u0c4d\u0c15\u0c41\u0c2e\u0c4d) <br/> \u0c15\u0c3e\u0c1f\u0c4d\u0c1f\u0c41 \u0c2e\u0c3f\u0c30\u0c41\u0c17\u0c2e\u0c46\u0c32\u0c4d\u0c32\u0c3e\u0c2e\u0c4d \u0c0e\u0c28\u0c4d\u0c28\u0c48\u0c15\u0c4d \u0c15\u0c23\u0c4d\u0c21\u0c3e\u0c32\u0c4d \u0c13\u0c21\u0c3f \u0c35\u0c30\u0c41\u0c2e\u0c4d <br/> \u0c15\u0c42\u0c1f\u0c4d\u0c1f\u0c19\u0c4d \u0c15\u0c42\u0c1f\u0c4d\u0c1f\u0c2e\u0c3e\u0c17 \u0c35\u0c28\u0c4d\u0c26\u0c3e\u0c32\u0c4d \u0c35\u0c47\u0c1f\u0c4d\u0c1f\u0c48 \u0c06\u0c21\u0c3f \u0c1c\u0c46\u0c2f\u0c3f\u0c24\u0c4d\u0c24\u0c3f\u0c21\u0c41\u0c35\u0c47\u0c28\u0c4d       (\u0c2a\u0c4b\u0c17 \u0c35\u0c47\u0c23\u0c41\u0c2e\u0c4d)"}, {"M": "\u0c1a\u0c30\u0c23\u0c2e\u0c4d 3", "T": "\u0c2a\u0c3e\u0c38\u0c2e\u0c41\u0c33\u0c4d\u0c33 \u0c28\u0c28\u0c4d\u0c26\u0c17\u0c4b\u0c2c\u0c30\u0c4d \u0c2a\u0c3e\u0c32\u0c28\u0c4d \u0c0e\u0c19\u0c4d\u0c17\u0c47 \u0c0e\u0c28\u0c4d\u0c31\u0c41 \u0c15\u0c47\u0c1f\u0c4d\u0c1f\u0c3e\u0c32\u0c4d <br/> \u0c0e\u0c28\u0c4d\u0c28 \u0c2a\u0c26\u0c3f\u0c32\u0c4d \u0c38\u0c4a\u0c32\u0c4d\u0c35\u0c47\u0c28\u0c21\u0c3e \u0c0e\u0c28\u0c4d\u0c28\u0c41\u0c21\u0c48\u0c2f \u0c15\u0c23\u0c4d\u0c2e\u0c23\u0c3f\u0c2f\u0c47                    (\u0c2e\u0c3e\u0c21\u0c41 \u0c2e\u0c47\u0c2f\u0c4d\u0c15\u0c4d\u0c15\u0c41\u0c2e\u0c4d) <br/> \u0c2a\u0c3e\u0c32\u0c30\u0c41\u0c21\u0c28\u0c4d \u0c35\u0c40\u0c26\u0c3f\u0c2f\u0c3f\u0c32\u0c47 \u0c2a\u0c28\u0c4d\u0c26\u0c3e\u0c21\u0c41\u0c31\u0c3e\u0c28\u0c4d \u0c0e\u0c28\u0c4d\u0c31\u0c41 \u0c38\u0c4a\u0c32\u0c4d\u0c32\u0c47\u0c28\u0c4d <br/> \u0c24\u0c47\u0c21\u0c3f \u0c0e\u0c28\u0c4d\u0c28\u0c48 \u0c35\u0c30\u0c41\u0c17\u0c48\u0c2f\u0c3f\u0c32\u0c47 \u0c13\u0c21\u0c3f \u0c35\u0c28\u0c4d\u0c26\u0c41 \u0c28\u0c3f\u0c28\u0c4d\u0c31\u0c3f\u0c21\u0c41\u0c35\u0c47\u0c28\u0c4d                     (\u0c2a\u0c4b\u0c17 \u0c35\u0c47\u0c23\u0c41\u0c2e\u0c4d) <br/> \u0c2e\u0c3e\u0c21\u0c41 \u0c2e\u0c47\u0c2f\u0c4d\u0c15\u0c4d\u0c15\u0c41\u0c2e\u0c4d \u0c15\u0c23\u0c4d\u0c23\u0c47 \u0c28\u0c40 <br/> \u0c2a\u0c4b\u0c17 \u0c35\u0c47\u0c23\u0c4d\u0c21\u0c3e\u0c2e\u0c4d \u0c38\u0c4a\u0c28\u0c4d\u0c28\u0c47\u0c28\u0c4d \u0c2a\u0c4b\u0c17 \u0c35\u0c47\u0c23\u0c41\u0c2e\u0c4d \u0c24\u0c3e\u0c2f\u0c47 <br/> \u0c24\u0c21\u0c48 \u0c38\u0c4a\u0c32\u0c4d\u0c32\u0c3e\u0c26\u0c47 \u0c28\u0c40\u0c2f\u0c47"}]}, {"L": "Kannada", "text": [{"M": "\u0caa\u0cb2\u0ccd\u0cb2\u0cb5\u0cbf", "T": "\u0cae\u0cbe\u0ca1\u0cc1 \u0cae\u0cc7\u0caf\u0ccd\u0c95\u0ccd\u0c95\u0cc1\u0cae\u0ccd \u0c95\u0ca3\u0ccd\u0ca3\u0cc7 \u0ca8\u0cc0 <br/> \u0caa\u0ccb\u0c97 \u0cb5\u0cc7\u0ca3\u0ccd\u0ca1\u0cbe\u0cae\u0ccd \u0cb8\u0cca\u0ca8\u0ccd\u0ca8\u0cc7\u0ca8\u0ccd <br/> \u0caa\u0ccb\u0c97 \u0cb5\u0cc7\u0ca3\u0cc1\u0cae\u0ccd \u0ca4\u0cbe\u0caf\u0cc7 <br/> \u0ca4\u0ca1\u0cc8 \u0cb8\u0cca\u0cb2\u0ccd\u0cb2\u0cbe\u0ca6\u0cc7 \u0ca8\u0cc0\u0caf\u0cc7 <br/> \u0c95\u0cbe\u0caf\u0ccd\u0c9a\u0ccd\u0c9a\u0cbf\u0ca8 \u0caa\u0cbe\u0cb2\u0cc1 \u0ca4\u0cb0\u0cc7\u0ca8\u0ccd; \u0c95\u0cb2\u0ccd\u0c95\u0ca3\u0ccd\u0ca1\u0cc1\u0c9a\u0ccd \u0c9a\u0cc0\u0ca8\u0cbf \u0ca4\u0cb0\u0cc7\u0ca8\u0ccd <br/> \u0c95\u0cc8 \u0ca8\u0cbf\u0cb1\u0cc8\u0caf \u0cb5\u0cc6\u0ca3\u0ccd\u0ca3\u0cc6\u0caf\u0ccd  \u0ca4\u0cb0\u0cc7\u0ca8\u0ccd; \u0cb5\u0cc6\u0caf\u0ccd\u0caf\u0cbf\u0cb2\u0cbf\u0cb2\u0cc7 \u0caa\u0ccb\u0c97 \u0cb5\u0cc7\u0ca3\u0ccd\u0ca1\u0cbe\u0cae\u0ccd        (\u0cae\u0cbe\u0ca1\u0cc1 \u0cae\u0cc7\u0caf\u0ccd\u0c95\u0ccd\u0c95\u0cc1\u0cae\u0ccd) <br/> \u0c95\u0cbe\u0caf\u0ccd\u0c9a\u0ccd\u0c9a\u0cbf\u0ca8 \u0caa\u0cbe\u0cb2\u0cc1\u0cae\u0ccd \u0cb5\u0cc7\u0ca3\u0ccd\u0ca1\u0cbe\u0cae\u0ccd; \u0c95\u0cb2\u0ccd\u0c95\u0ca3\u0ccd\u0ca1\u0cc1\u0c9a\u0ccd \u0c9a\u0cc0\u0ca8\u0cbf \u0cb5\u0cc7\u0ca3\u0ccd\u0ca1\u0cbe\u0cae\u0ccd <br/> \u0c89\u0cb2\u0ccd\u0cb2\u0cbe\u0cb8\u0cae\u0cbe\u0caf\u0ccd \u0cae\u0cbe\u0ca1\u0cc1 \u0cae\u0cc7\u0caf\u0ccd\u0ca4\u0ccd\u0ca4\u0cc1, \u0c92\u0cb0\u0cc1 \u0ca8\u0cca\u0ca1\u0cbf\u0caf\u0cbf\u0cb2\u0ccd \u0ca4\u0cbf\u0cb0\u0cc1\u0cae\u0ccd\u0cac\u0cbf\u0ca1\u0cc1\u0cb5\u0cc7\u0ca8\u0ccd          (\u0caa\u0ccb\u0c97 \u0cb5\u0cc7\u0ca3\u0cc1\u0cae\u0ccd)"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 1", "T": "\u0caf\u0cae\u0cc1\u0ca8\u0cbe \u0ca8\u0ca6\u0cbf\u0c95\u0ccd \u0c95\u0cb0\u0cc8\u0caf\u0cbf\u0cb2\u0ccd \u0c8e\u0caa\u0ccd\u0caa\u0cca\u0cb3\u0cc1\u0ca6\u0cc1\u0cae\u0ccd \u0c95\u0cb3\u0ccd\u0cb5\u0cb0\u0ccd \u0caa\u0caf\u0cae\u0ccd <br/> \u0c95\u0cb3\u0ccd\u0cb5\u0cb0\u0ccd \u0cb5\u0ca8\u0ccd\u0ca6\u0cc1 \u0c89\u0ca8\u0cc8 \u0c85\u0ca1\u0cbf\u0ca4\u0ccd\u0ca4\u0cbe\u0cb2\u0ccd \u0c95\u0cb2\u0c99\u0ccd\u0c97\u0cbf\u0ca1\u0cc1\u0cb5\u0cbe\u0caf\u0ccd \u0c95\u0ca3\u0ccd\u0cae\u0ca3\u0cbf\u0caf\u0cc7          (\u0cae\u0cbe\u0ca1\u0cc1 \u0cae\u0cc7\u0caf\u0ccd\u0c95\u0ccd\u0c95\u0cc1\u0cae\u0ccd) <br/> \u0c95\u0cb3\u0ccd\u0cb3\u0ca8\u0cc1\u0c95\u0ccd\u0c95\u0ccb\u0cb0\u0ccd \u0c95\u0cb3\u0ccd\u0cb3\u0ca8\u0ccd \u0c89\u0ca3\u0ccd\u0ca1\u0ccb? \u0c95\u0ca3\u0ccd\u0ca1\u0ca6\u0cc1\u0ca3\u0ccd\u0ca1\u0ccb \u0cb8\u0cca\u0cb2\u0ccd\u0cb2\u0cc1\u0cae\u0ccd \u0c85\u0cae\u0ccd\u0cae\u0cbe? <br/> \u0c95\u0cb3\u0ccd\u0cb5\u0cb0\u0ccd \u0cb5\u0ca8\u0ccd\u0ca6\u0cc1 \u0c8e\u0ca8\u0cc8 \u0c85\u0ca1\u0cbf\u0ca4\u0ccd\u0ca4\u0cbe\u0cb2\u0ccd \u0c95\u0ca3\u0ccd\u0ca1 \u0ca4\u0cc1\u0ca3\u0ccd\u0ca1\u0cae\u0ccd \u0cb8\u0cc6\u0caf\u0ccd\u0ca4\u0cbf\u0ca1\u0cc1\u0cb5\u0cc7\u0ca8\u0ccd      (\u0caa\u0ccb\u0c97 \u0cb5\u0cc7\u0ca3\u0cc1\u0cae\u0ccd)"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 2", "T": "\u0c95\u0ccb\u0cb5\u0cb0\u0ccd\u0ca4\u0ccd\u0ca4\u0ca8 \u0c95\u0cbf\u0cb0\u0cbf\u0caf\u0cbf\u0cb2\u0ccd \u0c95\u0ccb\u0cb0\u0cae\u0cbe\u0ca8 \u0cae\u0cbf\u0cb0\u0cc1\u0c97\u0c99\u0ccd\u0c97\u0cb3\u0ccd \u0c89\u0ca3\u0ccd\u0ca1\u0cc1 <br/> \u0c95\u0cb0\u0ca1\u0cbf \u0caa\u0cc1\u0cb2\u0cbf\u0caf\u0cc8\u0c95\u0ccd \u0c95\u0ca3\u0ccd\u0ca1\u0cbe\u0cb2\u0ccd \u0c95\u0cb2\u0c99\u0ccd\u0c97\u0cbf\u0ca1\u0cc1\u0cb5\u0cbe\u0caf\u0ccd \u0c95\u0ca3\u0ccd\u0cae\u0ca3\u0cbf\u0caf\u0cc7                     (\u0cae\u0cbe\u0ca1\u0cc1 \u0cae\u0cc7\u0caf\u0ccd\u0c95\u0ccd\u0c95\u0cc1\u0cae\u0ccd) <br/> \u0c95\u0cbe\u0c9f\u0ccd\u0c9f\u0cc1 \u0cae\u0cbf\u0cb0\u0cc1\u0c97\u0cae\u0cc6\u0cb2\u0ccd\u0cb2\u0cbe\u0cae\u0ccd \u0c8e\u0ca8\u0ccd\u0ca8\u0cc8\u0c95\u0ccd \u0c95\u0ca3\u0ccd\u0ca1\u0cbe\u0cb2\u0ccd \u0c93\u0ca1\u0cbf \u0cb5\u0cb0\u0cc1\u0cae\u0ccd <br/> \u0c95\u0cc2\u0c9f\u0ccd\u0c9f\u0c99\u0ccd \u0c95\u0cc2\u0c9f\u0ccd\u0c9f\u0cae\u0cbe\u0c97 \u0cb5\u0ca8\u0ccd\u0ca6\u0cbe\u0cb2\u0ccd \u0cb5\u0cc7\u0c9f\u0ccd\u0c9f\u0cc8 \u0c86\u0ca1\u0cbf \u0c9c\u0cc6\u0caf\u0cbf\u0ca4\u0ccd\u0ca4\u0cbf\u0ca1\u0cc1\u0cb5\u0cc7\u0ca8\u0ccd       (\u0caa\u0ccb\u0c97 \u0cb5\u0cc7\u0ca3\u0cc1\u0cae\u0ccd)"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 3", "T": "\u0caa\u0cbe\u0cb8\u0cae\u0cc1\u0cb3\u0ccd\u0cb3 \u0ca8\u0ca8\u0ccd\u0ca6\u0c97\u0ccb\u0cac\u0cb0\u0ccd \u0caa\u0cbe\u0cb2\u0ca8\u0ccd \u0c8e\u0c99\u0ccd\u0c97\u0cc7 \u0c8e\u0ca8\u0ccd\u0cb1\u0cc1 \u0c95\u0cc7\u0c9f\u0ccd\u0c9f\u0cbe\u0cb2\u0ccd <br/> \u0c8e\u0ca8\u0ccd\u0ca8 \u0caa\u0ca6\u0cbf\u0cb2\u0ccd \u0cb8\u0cca\u0cb2\u0ccd\u0cb5\u0cc7\u0ca8\u0ca1\u0cbe \u0c8e\u0ca8\u0ccd\u0ca8\u0cc1\u0ca1\u0cc8\u0caf \u0c95\u0ca3\u0ccd\u0cae\u0ca3\u0cbf\u0caf\u0cc7                    (\u0cae\u0cbe\u0ca1\u0cc1 \u0cae\u0cc7\u0caf\u0ccd\u0c95\u0ccd\u0c95\u0cc1\u0cae\u0ccd) <br/> \u0caa\u0cbe\u0cb2\u0cb0\u0cc1\u0ca1\u0ca8\u0ccd \u0cb5\u0cc0\u0ca6\u0cbf\u0caf\u0cbf\u0cb2\u0cc7 \u0caa\u0ca8\u0ccd\u0ca6\u0cbe\u0ca1\u0cc1\u0cb1\u0cbe\u0ca8\u0ccd \u0c8e\u0ca8\u0ccd\u0cb1\u0cc1 \u0cb8\u0cca\u0cb2\u0ccd\u0cb2\u0cc7\u0ca8\u0ccd <br/> \u0ca4\u0cc7\u0ca1\u0cbf \u0c8e\u0ca8\u0ccd\u0ca8\u0cc8 \u0cb5\u0cb0\u0cc1\u0c97\u0cc8\u0caf\u0cbf\u0cb2\u0cc7 \u0c93\u0ca1\u0cbf \u0cb5\u0ca8\u0ccd\u0ca6\u0cc1 \u0ca8\u0cbf\u0ca8\u0ccd\u0cb1\u0cbf\u0ca1\u0cc1\u0cb5\u0cc7\u0ca8\u0ccd                     (\u0caa\u0ccb\u0c97 \u0cb5\u0cc7\u0ca3\u0cc1\u0cae\u0ccd) <br/> \u0cae\u0cbe\u0ca1\u0cc1 \u0cae\u0cc7\u0caf\u0ccd\u0c95\u0ccd\u0c95\u0cc1\u0cae\u0ccd \u0c95\u0ca3\u0ccd\u0ca3\u0cc7 \u0ca8\u0cc0 <br/> \u0caa\u0ccb\u0c97 \u0cb5\u0cc7\u0ca3\u0ccd\u0ca1\u0cbe\u0cae\u0ccd \u0cb8\u0cca\u0ca8\u0ccd\u0ca8\u0cc7\u0ca8\u0ccd \u0caa\u0ccb\u0c97 \u0cb5\u0cc7\u0ca3\u0cc1\u0cae\u0ccd \u0ca4\u0cbe\u0caf\u0cc7 <br/> \u0ca4\u0ca1\u0cc8 \u0cb8\u0cca\u0cb2\u0ccd\u0cb2\u0cbe\u0ca6\u0cc7 \u0ca8\u0cc0\u0caf\u0cc7"}]}, {"L": "Malayalam", "text": [{"M": "\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d2e\u0d3e\u0d21\u0d41 \u0d2e\u0d47\u0d2f\u0d4d\u0d15\u0d4d\u0d15\u0d41\u0d2e\u0d4d \u0d15\u0d23\u0d4d\u0d23\u0d47 \u0d28\u0d40 <br/> \u0d2a\u0d4b\u0d17 \u0d35\u0d47\u0d23\u0d4d\u0d21\u0d3e\u0d2e\u0d4d \u0d38\u0d4a\u0d28\u0d4d\u0d28\u0d47\u0d28\u0d4d <br/> \u0d2a\u0d4b\u0d17 \u0d35\u0d47\u0d23\u0d41\u0d2e\u0d4d \u0d24\u0d3e\u0d2f\u0d47 <br/> \u0d24\u0d21\u0d48 \u0d38\u0d4a\u0d32\u0d4d\u0d32\u0d3e\u0d26\u0d47 \u0d28\u0d40\u0d2f\u0d47 <br/> \u0d15\u0d3e\u0d2f\u0d4d\u0d1a\u0d4d\u0d1a\u0d3f\u0d28 \u0d2a\u0d3e\u0d32\u0d41 \u0d24\u0d30\u0d47\u0d28\u0d4d; \u0d15\u0d32\u0d4d\u0d15\u0d23\u0d4d\u0d21\u0d41\u0d1a\u0d4d \u0d1a\u0d40\u0d28\u0d3f \u0d24\u0d30\u0d47\u0d28\u0d4d <br/> \u0d15\u0d48 \u0d28\u0d3f\u0d31\u0d48\u0d2f \u0d35\u0d46\u0d23\u0d4d\u0d23\u0d46\u0d2f\u0d4d  \u0d24\u0d30\u0d47\u0d28\u0d4d; \u0d35\u0d46\u0d2f\u0d4d\u0d2f\u0d3f\u0d32\u0d3f\u0d32\u0d47 \u0d2a\u0d4b\u0d17 \u0d35\u0d47\u0d23\u0d4d\u0d21\u0d3e\u0d2e\u0d4d        (\u0d2e\u0d3e\u0d21\u0d41 \u0d2e\u0d47\u0d2f\u0d4d\u0d15\u0d4d\u0d15\u0d41\u0d2e\u0d4d) <br/> \u0d15\u0d3e\u0d2f\u0d4d\u0d1a\u0d4d\u0d1a\u0d3f\u0d28 \u0d2a\u0d3e\u0d32\u0d41\u0d2e\u0d4d \u0d35\u0d47\u0d23\u0d4d\u0d21\u0d3e\u0d2e\u0d4d; \u0d15\u0d32\u0d4d\u0d15\u0d23\u0d4d\u0d21\u0d41\u0d1a\u0d4d \u0d1a\u0d40\u0d28\u0d3f \u0d35\u0d47\u0d23\u0d4d\u0d21\u0d3e\u0d2e\u0d4d <br/> \u0d09\u0d32\u0d4d\u0d32\u0d3e\u0d38\u0d2e\u0d3e\u0d2f\u0d4d \u0d2e\u0d3e\u0d21\u0d41 \u0d2e\u0d47\u0d2f\u0d4d\u0d24\u0d4d\u0d24\u0d41, \u0d12\u0d30\u0d41 \u0d28\u0d4a\u0d21\u0d3f\u0d2f\u0d3f\u0d32\u0d4d \u0d24\u0d3f\u0d30\u0d41\u0d2e\u0d4d\u0d2c\u0d3f\u0d21\u0d41\u0d35\u0d47\u0d28\u0d4d          (\u0d2a\u0d4b\u0d17 \u0d35\u0d47\u0d23\u0d41\u0d2e\u0d4d)"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 1", "T": "\u0d2f\u0d2e\u0d41\u0d28\u0d3e \u0d28\u0d26\u0d3f\u0d15\u0d4d \u0d15\u0d30\u0d48\u0d2f\u0d3f\u0d32\u0d4d \u0d0e\u0d2a\u0d4d\u0d2a\u0d4a\u0d34\u0d41\u0d26\u0d41\u0d2e\u0d4d \u0d15\u0d33\u0d4d\u0d35\u0d30\u0d4d \u0d2a\u0d2f\u0d2e\u0d4d <br/> \u0d15\u0d33\u0d4d\u0d35\u0d30\u0d4d \u0d35\u0d28\u0d4d\u0d26\u0d41 \u0d09\u0d28\u0d48 \u0d05\u0d21\u0d3f\u0d24\u0d4d\u0d24\u0d3e\u0d32\u0d4d \u0d15\u0d32\u0d19\u0d4d\u0d17\u0d3f\u0d21\u0d41\u0d35\u0d3e\u0d2f\u0d4d \u0d15\u0d23\u0d4d\u0d2e\u0d23\u0d3f\u0d2f\u0d47          (\u0d2e\u0d3e\u0d21\u0d41 \u0d2e\u0d47\u0d2f\u0d4d\u0d15\u0d4d\u0d15\u0d41\u0d2e\u0d4d) <br/> \u0d15\u0d33\u0d4d\u0d33\u0d28\u0d41\u0d15\u0d4d\u0d15\u0d4b\u0d30\u0d4d \u0d15\u0d33\u0d4d\u0d33\u0d28\u0d4d \u0d09\u0d23\u0d4d\u0d21\u0d4b? \u0d15\u0d23\u0d4d\u0d21\u0d26\u0d41\u0d23\u0d4d\u0d21\u0d4b \u0d38\u0d4a\u0d32\u0d4d\u0d32\u0d41\u0d2e\u0d4d \u0d05\u0d2e\u0d4d\u0d2e\u0d3e? <br/> \u0d15\u0d33\u0d4d\u0d35\u0d30\u0d4d \u0d35\u0d28\u0d4d\u0d26\u0d41 \u0d0e\u0d28\u0d48 \u0d05\u0d21\u0d3f\u0d24\u0d4d\u0d24\u0d3e\u0d32\u0d4d \u0d15\u0d23\u0d4d\u0d21 \u0d24\u0d41\u0d23\u0d4d\u0d21\u0d2e\u0d4d \u0d38\u0d46\u0d2f\u0d4d\u0d24\u0d3f\u0d21\u0d41\u0d35\u0d47\u0d28\u0d4d      (\u0d2a\u0d4b\u0d17 \u0d35\u0d47\u0d23\u0d41\u0d2e\u0d4d)"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 2", "T": "\u0d15\u0d4b\u0d35\u0d30\u0d4d\u0d24\u0d4d\u0d24\u0d28 \u0d15\u0d3f\u0d30\u0d3f\u0d2f\u0d3f\u0d32\u0d4d \u0d15\u0d4b\u0d30\u0d2e\u0d3e\u0d28 \u0d2e\u0d3f\u0d30\u0d41\u0d17\u0d19\u0d4d\u0d17\u0d33\u0d4d \u0d09\u0d23\u0d4d\u0d21\u0d41 <br/> \u0d15\u0d30\u0d21\u0d3f \u0d2a\u0d41\u0d32\u0d3f\u0d2f\u0d48\u0d15\u0d4d \u0d15\u0d23\u0d4d\u0d21\u0d3e\u0d32\u0d4d \u0d15\u0d32\u0d19\u0d4d\u0d17\u0d3f\u0d21\u0d41\u0d35\u0d3e\u0d2f\u0d4d \u0d15\u0d23\u0d4d\u0d2e\u0d23\u0d3f\u0d2f\u0d47                     (\u0d2e\u0d3e\u0d21\u0d41 \u0d2e\u0d47\u0d2f\u0d4d\u0d15\u0d4d\u0d15\u0d41\u0d2e\u0d4d) <br/> \u0d15\u0d3e\u0d1f\u0d4d\u0d1f\u0d41 \u0d2e\u0d3f\u0d30\u0d41\u0d17\u0d2e\u0d46\u0d32\u0d4d\u0d32\u0d3e\u0d2e\u0d4d \u0d0e\u0d28\u0d4d\u0d28\u0d48\u0d15\u0d4d \u0d15\u0d23\u0d4d\u0d21\u0d3e\u0d32\u0d4d \u0d13\u0d21\u0d3f \u0d35\u0d30\u0d41\u0d2e\u0d4d <br/> \u0d15\u0d42\u0d1f\u0d4d\u0d1f\u0d19\u0d4d \u0d15\u0d42\u0d1f\u0d4d\u0d1f\u0d2e\u0d3e\u0d17 \u0d35\u0d28\u0d4d\u0d26\u0d3e\u0d32\u0d4d \u0d35\u0d47\u0d1f\u0d4d\u0d1f\u0d48 \u0d06\u0d21\u0d3f \u0d1c\u0d46\u0d2f\u0d3f\u0d24\u0d4d\u0d24\u0d3f\u0d21\u0d41\u0d35\u0d47\u0d28\u0d4d       (\u0d2a\u0d4b\u0d17 \u0d35\u0d47\u0d23\u0d41\u0d2e\u0d4d)"}, {"M": "\u0d1a\u0d30\u0d23\u0d2e\u0d4d 3", "T": "\u0d2a\u0d3e\u0d38\u0d2e\u0d41\u0d33\u0d4d\u0d33 \u0d28\u0d28\u0d4d\u0d26\u0d17\u0d4b\u0d2c\u0d30\u0d4d \u0d2a\u0d3e\u0d32\u0d28\u0d4d \u0d0e\u0d19\u0d4d\u0d17\u0d47 \u0d0e\u0d28\u0d4d\u0d31\u0d41 \u0d15\u0d47\u0d1f\u0d4d\u0d1f\u0d3e\u0d32\u0d4d <br/> \u0d0e\u0d28\u0d4d\u0d28 \u0d2a\u0d26\u0d3f\u0d32\u0d4d \u0d38\u0d4a\u0d32\u0d4d\u0d35\u0d47\u0d28\u0d21\u0d3e \u0d0e\u0d28\u0d4d\u0d28\u0d41\u0d21\u0d48\u0d2f \u0d15\u0d23\u0d4d\u0d2e\u0d23\u0d3f\u0d2f\u0d47                    (\u0d2e\u0d3e\u0d21\u0d41 \u0d2e\u0d47\u0d2f\u0d4d\u0d15\u0d4d\u0d15\u0d41\u0d2e\u0d4d) <br/> \u0d2a\u0d3e\u0d32\u0d30\u0d41\u0d21\u0d28\u0d4d \u0d35\u0d40\u0d26\u0d3f\u0d2f\u0d3f\u0d32\u0d47 \u0d2a\u0d28\u0d4d\u0d26\u0d3e\u0d21\u0d41\u0d31\u0d3e\u0d28\u0d4d \u0d0e\u0d28\u0d4d\u0d31\u0d41 \u0d38\u0d4a\u0d32\u0d4d\u0d32\u0d47\u0d28\u0d4d <br/> \u0d24\u0d47\u0d21\u0d3f \u0d0e\u0d28\u0d4d\u0d28\u0d48 \u0d35\u0d30\u0d41\u0d17\u0d48\u0d2f\u0d3f\u0d32\u0d47 \u0d13\u0d21\u0d3f \u0d35\u0d28\u0d4d\u0d26\u0d41 \u0d28\u0d3f\u0d28\u0d4d\u0d31\u0d3f\u0d21\u0d41\u0d35\u0d47\u0d28\u0d4d                     (\u0d2a\u0d4b\u0d17 \u0d35\u0d47\u0d23\u0d41\u0d2e\u0d4d) <br/> \u0d2e\u0d3e\u0d21\u0d41 \u0d2e\u0d47\u0d2f\u0d4d\u0d15\u0d4d\u0d15\u0d41\u0d2e\u0d4d \u0d15\u0d23\u0d4d\u0d23\u0d47 \u0d28\u0d40 <br/> \u0d2a\u0d4b\u0d17 \u0d35\u0d47\u0d23\u0d4d\u0d21\u0d3e\u0d2e\u0d4d \u0d38\u0d4a\u0d28\u0d4d\u0d28\u0d47\u0d28\u0d4d \u0d2a\u0d4b\u0d17 \u0d35\u0d47\u0d23\u0d41\u0d2e\u0d4d \u0d24\u0d3e\u0d2f\u0d47 <br/> \u0d24\u0d21\u0d48 \u0d38\u0d4a\u0d32\u0d4d\u0d32\u0d3e\u0d26\u0d47 \u0d28\u0d40\u0d2f\u0d47"}]}, {"L": "Sanskrit", "text": [{"M": "\u092a\u0932\u094d\u0932\u0935\u093f", "T": "\u092e\u093e\u0921\u0941 \u092e\u0947\u092f\u094d\u0915\u094d\u0915\u0941\u092e\u094d \u0915\u0923\u094d\u0923\u0947 \u0928\u0940 <br/> \u092a\u094b\u0917 \u0935\u0947\u0923\u094d\u0921\u093e\u092e\u094d \u0938\u094b\u0928\u094d\u0928\u0947\u0928\u094d <br/> \u092a\u094b\u0917 \u0935\u0947\u0923\u0941\u092e\u094d \u0924\u093e\u092f\u0947 <br/> \u0924\u0921\u0948 \u0938\u094b\u0932\u094d\u0932\u093e\u0926\u0947 \u0928\u0940\u092f\u0947 <br/> \u0915\u093e\u092f\u094d\u091a\u094d\u091a\u093f\u0928 \u092a\u093e\u0932\u0941 \u0924\u0930\u0947\u0928\u094d; \u0915\u0932\u094d\u0915\u0923\u094d\u0921\u0941\u091a\u094d \u091a\u0940\u0928\u093f \u0924\u0930\u0947\u0928\u094d <br/> \u0915\u0948 \u0928\u093f\u0930\u0948\u092f \u0935\u0947\u0923\u094d\u0923\u0947\u092f\u094d  \u0924\u0930\u0947\u0928\u094d; \u0935\u0947\u092f\u094d\u092f\u093f\u0932\u093f\u0932\u0947 \u092a\u094b\u0917 \u0935\u0947\u0923\u094d\u0921\u093e\u092e\u094d        (\u092e\u093e\u0921\u0941 \u092e\u0947\u092f\u094d\u0915\u094d\u0915\u0941\u092e\u094d) <br/> \u0915\u093e\u092f\u094d\u091a\u094d\u091a\u093f\u0928 \u092a\u093e\u0932\u0941\u092e\u094d \u0935\u0947\u0923\u094d\u0921\u093e\u092e\u094d; \u0915\u0932\u094d\u0915\u0923\u094d\u0921\u0941\u091a\u094d \u091a\u0940\u0928\u093f \u0935\u0947\u0923\u094d\u0921\u093e\u092e\u094d <br/> \u0909\u0932\u094d\u0932\u093e\u0938\u092e\u093e\u092f\u094d \u092e\u093e\u0921\u0941 \u092e\u0947\u092f\u094d\u0924\u094d\u0924\u0941, \u0913\u0930\u0941 \u0928\u094b\u0921\u093f\u092f\u093f\u0932\u094d \u0924\u093f\u0930\u0941\u092e\u094d\u092c\u093f\u0921\u0941\u0935\u0947\u0928\u094d          (\u092a\u094b\u0917 \u0935\u0947\u0923\u0941\u092e\u094d)"}, {"M": "\u091a\u0930\u0923\u092e\u094d 1", "T": "\u092f\u092e\u0941\u0928\u093e \u0928\u0926\u093f\u0915\u094d \u0915\u0930\u0948\u092f\u093f\u0932\u094d \u090f\u092a\u094d\u092a\u094b\u0932\u0941\u0926\u0941\u092e\u094d \u0915\u0932\u094d\u0935\u0930\u094d \u092a\u092f\u092e\u094d <br/> \u0915\u0932\u094d\u0935\u0930\u094d \u0935\u0928\u094d\u0926\u0941 \u0909\u0928\u0948 \u0905\u0921\u093f\u0924\u094d\u0924\u093e\u0932\u094d \u0915\u0932\u0919\u094d\u0917\u093f\u0921\u0941\u0935\u093e\u092f\u094d \u0915\u0923\u094d\u092e\u0923\u093f\u092f\u0947          (\u092e\u093e\u0921\u0941 \u092e\u0947\u092f\u094d\u0915\u094d\u0915\u0941\u092e\u094d) <br/> \u0915\u0932\u094d\u0932\u0928\u0941\u0915\u094d\u0915\u094b\u0930\u094d \u0915\u0932\u094d\u0932\u0928\u094d \u0909\u0923\u094d\u0921\u094b? \u0915\u0923\u094d\u0921\u0926\u0941\u0923\u094d\u0921\u094b \u0938\u094b\u0932\u094d\u0932\u0941\u092e\u094d \u0905\u092e\u094d\u092e\u093e? <br/> \u0915\u0932\u094d\u0935\u0930\u094d \u0935\u0928\u094d\u0926\u0941 \u090f\u0928\u0948 \u0905\u0921\u093f\u0924\u094d\u0924\u093e\u0932\u094d \u0915\u0923\u094d\u0921 \u0924\u0941\u0923\u094d\u0921\u092e\u094d \u0938\u0947\u092f\u094d\u0924\u093f\u0921\u0941\u0935\u0947\u0928\u094d      (\u092a\u094b\u0917 \u0935\u0947\u0923\u0941\u092e\u094d)"}, {"M": "\u091a\u0930\u0923\u092e\u094d 2", "T": "\u0915\u094b\u0935\u0930\u094d\u0924\u094d\u0924\u0928 \u0915\u093f\u0930\u093f\u092f\u093f\u0932\u094d \u0915\u094b\u0930\u092e\u093e\u0928 \u092e\u093f\u0930\u0941\u0917\u0919\u094d\u0917\u0932\u094d \u0909\u0923\u094d\u0921\u0941 <br/> \u0915\u0930\u0921\u093f \u092a\u0941\u0932\u093f\u092f\u0948\u0915\u094d \u0915\u0923\u094d\u0921\u093e\u0932\u094d \u0915\u0932\u0919\u094d\u0917\u093f\u0921\u0941\u0935\u093e\u092f\u094d \u0915\u0923\u094d\u092e\u0923\u093f\u092f\u0947                     (\u092e\u093e\u0921\u0941 \u092e\u0947\u092f\u094d\u0915\u094d\u0915\u0941\u092e\u094d) <br/> \u0915\u093e\u091f\u094d\u091f\u0941 \u092e\u093f\u0930\u0941\u0917\u092e\u0947\u0932\u094d\u0932\u093e\u092e\u094d \u090f\u0928\u094d\u0928\u0948\u0915\u094d \u0915\u0923\u094d\u0921\u093e\u0932\u094d \u0913\u0921\u093f \u0935\u0930\u0941\u092e\u094d <br/> \u0915\u0942\u091f\u094d\u091f\u0919\u094d \u0915\u0942\u091f\u094d\u091f\u092e\u093e\u0917 \u0935\u0928\u094d\u0926\u093e\u0932\u094d \u0935\u0947\u091f\u094d\u091f\u0948 \u0906\u0921\u093f \u091c\u0947\u092f\u093f\u0924\u094d\u0924\u093f\u0921\u0941\u0935\u0947\u0928\u094d       (\u092a\u094b\u0917 \u0935\u0947\u0923\u0941\u092e\u094d)"}, {"M": "\u091a\u0930\u0923\u092e\u094d 3", "T": "\u092a\u093e\u0938\u092e\u0941\u0932\u094d\u0932 \u0928\u0928\u094d\u0926\u0917\u094b\u092c\u0930\u094d \u092a\u093e\u0932\u0928\u094d \u090f\u0919\u094d\u0917\u0947 \u090f\u0928\u094d\u0930\u0941 \u0915\u0947\u091f\u094d\u091f\u093e\u0932\u094d <br/> \u090f\u