\u0b8e)\u0bb5\u0bcd\u0bb5\u0bb0\u0bc1 \u0bb2\u0bc7\u0bb0\u0bbe (\u0b8f)"}, {"M": "\u0b9a\u0bb0\u0ba3\u0bae\u0bcd 3", "T": "\u0b8e\u0ba8\u0bcd\u0ba4\u0bc1\u0b95\u0bcd(\u0b85)\u0ba8\u0bc1\u0b9a\u0bc1 \u0b9a\u0bc8\u0bb0\u0bbf\u0ba8\u0bcd\u0ba4\u0bc1\u0ba9\u0bc1\u0bb0\u0bbe \u0bb0\u0bbe\u0bae <br/> \u0bae\u0bc1\u0ba8\u0bcd\u0ba4\u0bc1 \u0bb5\u0bc6\u0ba9\u0bc1\u0b95 \u0ba4\u0bcb\u0b9a\u0ba4\u0bc1\u0bb0\u0bbe <br/> \u0bae\u0bc1\u0ba8\u0bcd\u0ba4\u0bb0 \u0ba8\u0bbf\u0bb2\u0bbf\u0b9a\u0bbf \u0baa\u0bb2\u0bc1\u0b95\u0bb0\u0bbe <br/> \u0ba8\u0bbe(\u0baf\u0bcd)\u0b85\u0ba8\u0bcd\u0ba4\u0bc1 \u0ba8\u0bc0\u0b95\u0bc1 \u0b88\u0bb5\u0bb0\u0b95\u0bc1 \u0bae\u0bb0\u0baa\u0bc1\u0bb0\u0bbe (\u0b8f)"}, {"M": "\u0b9a\u0bb0\u0ba3\u0bae\u0bcd 4", "T": "\u0b95\u0ba9\u0bcd\u0ba9 \u0bb5\u0bbe\u0bb0\u0bbf\u0ba9\u0bbf \u0bb5\u0bc7\u0b9f\u0bbf\u0ba9\u0bbe\u0ba9\u0bbe <br/> \u0ba8\u0bbe(\u0baf\u0bcd)\u0b85\u0ba9\u0bcd\u0ba9 \u0ba8\u0bbf\u0ba9\u0bcd\u0ba9\u0bcd(\u0b86)\u0b9f\u0bc1\u0b95\u0bca\u0ba9\u0bcd\u0ba9\u0bbe\u0ba9\u0bbe <br/> \u0ba8\u0bbf\u0ba9\u0bcd\u0ba9\u0bc1 \u0ba8\u0bae\u0bcd\u0bae\u0bbf\u0ba9-\u0bb5\u0bbe\u0b9f\u0ba9\u0bc1 \u0b95\u0bbe\u0ba9\u0bbe \u0bae\u0bc1\u0ba9\u0bbf <br/> \u0b9a\u0ba9\u0bcd\u0ba9\u0bc1\u0ba4 \u0b95\u0baa\u0b9f\u0bae\u0bc1\u0bb2\u0bcd(\u0b85)\u0ba8\u0bcd\u0ba9\u0bbf \u0ba8\u0bbe\u0ba4\u0bcb\u0ba9\u0bbe (\u0b8f)"}, {"M": "\u0b9a\u0bb0\u0ba3\u0bae\u0bcd 5", "T": "\u0bb5\u0bbf\u0ba9\u0bb5\u0bcd(\u0b85)\u0baf\u0bcd\u0baf \u0b87\u0ba9 \u0b95\u0bc1\u0bb2 \u0ba4\u0ba9\u0bae\u0bbe \u0bb0\u0bbe\u0bae <br/> \u0ba8\u0bbf\u0ba9\u0bcd\u0ba9\u0bc1 \u0ba8\u0bae\u0bcd\u0bae\u0bbf(\u0baf\u0bcd)\u0b87\u0ba9\u0bcd\u0b95 \u0ba4\u0bc1\u0bb0\u0bbf\u0ba4\u0bae\u0bbe <br/> \u0baa\u0bc1\u0bb5\u0ba9\u0bae\u0bc1\u0ba9 \u0ba8\u0bc0\u0b95\u0bcd(\u0b87)\u0ba4\u0bbf \u0b95\u0ba9\u0bae\u0bbe \u0ba8\u0bc0 <br/> \u0bae\u0ba9\u0bb8\u0bcd(\u0b87)\u0b9f\u0bc1\u0bb5\u0ba3\u0bcd\u0b9f\u0bbf\u0ba4\u0bcd(\u0b85)\u0ba8\u0bc1\u0b9a\u0bc1\u0ba9\u0bc1 \u0ba4\u0bc6\u0bb2\u0bc1\u0baa\u0bc1\u0bae\u0bbe (\u0b8f)"}, {"M": "\u0b9a\u0bb0\u0ba3\u0bae\u0bcd 6", "T": "\u0b9a\u0ba4\u0baf\u0bc1\u0b9f\u0bc8 \u0b87\u0ba9\u0bcd\u0b95 \u0baa\u0bb0\u0bbe\u0b95\u0bbe \u0ba8\u0bbe <br/> \u0bb9\u0bcd\u0bb0\u0bc1\u0ba4\u0baf \u0bb5\u0bbe\u0b9a\u0bc1\u0b9f\u0bc1 \u0ba8\u0bc0\u0bb5\u0bc7 \u0b95\u0bbe\u0b95 <br/> \u0b87\u0ba4\u0bbf \u0baa\u0bc1\u0ba4\u0bcd\u0ba4\u0bbf(\u0baf\u0bcd)\u0b85\u0ba9\u0bc1\u0b9a\u0bc1 \u0ba4\u0bc6\u0bb2\u0bcd\u0baa \u0bb2\u0bc7\u0b95 \u0ba8\u0bbe\u0b9f\u0bc1 <br/> \u0bae\u0bca\u0ba4\u0bb2\u0bc1\u0b95\u0bbe\u0ba9\u0bc1 \u0b8a\u0bb0\u0b95\u0bcd(\u0b89)\u0ba8\u0bcd\u0ba9\u0bbe\u0bb5\u0bc1 \u0b95\u0bbe\u0b95 (\u0b8f)"}, {"M": "\u0b9a\u0bb0\u0ba3\u0bae\u0bcd 7", "T": "\u0bb0\u0bbe\u0b9c \u0b9a\u0bc7\u0b95\u0bb0 \u0b9a\u0ba9\u0bcd\u0ba9\u0bc1\u0ba4\u0bcd(\u0b86)\u0b99\u0bcd\u0b95 <br/> \u0ba4\u0bcd\u0baf\u0bbe\u0b95\u0bb0\u0bbe\u0b9c \u0bb9\u0bcd\u0bb0\u0bc1\u0ba4\u0bcd-\u0b85\u0baa\u0bcd\u0b9c \u0b9a\u0bc1-\u0baa\u0bcd\u0bb0\u0bc1\u0b99\u0bcd\u0b95 <br/> \u0bb0\u0bbe\u0b9c\u0bbf\u0ba4 \u0b95\u0bb0\u0bc1\u0ba3\u0bcd(\u0b86)\u0baa\u0bbe\u0b99\u0bcd\u0b95 \u0bb0\u0ba4\u0bbf <br/> \u0bb0\u0bbe\u0b9c \u0b9c\u0ba9\u0b95 \u0baa\u0bbe\u0baa \u0ba4\u0bcd\u0bb5\u0bbe\u0ba8\u0bcd\u0ba4 \u0baa\u0ba4\u0b99\u0bcd\u0b95 \u0ba8\u0bc7\u0ba9\u0bcd(\u0b8f)"}]}, {"L": "Telugu", "text": [{"M": "\u0c2a\u0c32\u0c4d\u0c32\u0c35\u0c3f", "T": "\u0c0f \u0c2a\u0c3e\u0c2a\u0c2e\u0c41 \u0c1c\u0c47\u0c38\u0c3f\u0c24\u0c3f\u0c30\u0c3e \u0c30\u0c3e\u0c2e <br/> \u0c28\u0c40\u0c15\u0c4d(\u0c0f) \u0c2a\u0c3e\u0c1f\u0c48\u0c28 \u0c26\u0c2f \u0c30\u0c3e\u0c26\u0c41 \u0c28\u0c47\u0c28\u0c4d(\u0c0f)"}, {"M": "\u0c05\u0c28\u0c41\u0c2a\u0c32\u0c4d\u0c32\u0c35\u0c3f", "T": "\u0c28\u0c40 \u0c2a\u0c3e\u0c26\u0c2e\u0c41\u0c32\u0c28\u0c41 \u0c15\u0c28 \u0c2e\u0c4a\u0c30\u0c32\u0c4d(\u0c07)\u0c21\u0c3f\u0c24\u0c47 <br/> \u0c28\u0c40 \u0c2a\u0c3e\u0c1f\u0c41\u0c28 \u0c35\u0c3f\u0c28\u0c3f \u0c35\u0c3f\u0c28\u0c28\u0c1f\u0c4d\u0c1f\u0c4d(\u0c09)\u0c23\u0c4d\u0c21\u0c41\u0c1f\u0c15\u0c41 \u0c28\u0c47\u0c28\u0c4d(\u0c0f)"}, {"M": "\u0c1a\u0c30\u0c23\u0c2e\u0c4d 1", "T": "\u0c28\u0c3e\u0c26 \u0c30\u0c42\u0c2a\u0c41\u0c21\u0c35\u0c4d(\u0c05)\u0c28\u0c3f \u0c35\u0c3f\u0c28\u0c3f \u0c28\u0c47 \u0c36\u0c4d\u0c30\u0c40 <br/> \u0c28\u0c3e\u0c25 \u0c28\u0c3f\u0c28\u0c4d\u0c28\u0c41 \u0c28\u0c2e\u0c4d\u0c2e\u0c3f\u0c24\u0c3f\u0c28\u0c3f <br/> \u0c28\u0c3e\u0c26 \u0c2a\u0c41\u0c30\u0c2e\u0c41\u0c28(\u0c28\u0c4d)\u0c09\u0c23\u0c4d\u0c21\u0c3f\u0c2f\u0c41 <br/> \u0c28\u0c28\u0c4d\u0c28\u0c4d(\u0c06)\u0c26\u0c30\u0c3f\u0c28\u0c4d\u0c1a\u0c15 \u0c0a\u0c30\u0c15\u0c4d(\u0c09)\u0c23\u0c4d\u0c21\u0c41\u0c1f\u0c15\u0c41 \u0c28\u0c47\u0c28\u0c4d(\u0c0f)"}, {"M": "\u0c1a\u0c30\u0c23\u0c2e\u0c4d 2", "T": "\u0c17\u0c3e\u0c30\u0c35\u0c3f\u0c2e\u0c4d\u0c2a \u0c26\u0c2f \u0c30\u0c3e\u0c26\u0c3e \u0c2a\u0c3e\u0c32\u0c41 <br/> \u0c15\u0c3e\u0c30\u0c41 \u0c2e\u0c4b\u0c2e\u0c41 \u0c1c\u0c42\u0c2a \u0c30\u0c3e\u0c26\u0c3e <br/> \u0c0a\u0c30\u0c15\u0c4d(\u0c09)\u0c23\u0c4d\u0c21\u0c41\u0c1f \u0c2e\u0c30\u0c3f\u0c2f\u0c3e\u0c26\u0c3e \u0c28\u0c3e <br/> \u0c26\u0c41\u0c30\u0c3e \u0c24\u0c46\u0c32\u0c4d\u0c2a\u0c41-\u0c35\u0c3e\u0c30\u0c4d(\u0c0e)\u0c35\u0c4d\u0c35\u0c30\u0c41 \u0c32\u0c47\u0c30\u0c3e (\u0c0f)"}, {"M": "\u0c1a\u0c30\u0c23\u0c2e\u0c4d 3", "T": "\u0c0e\u0c28\u0c4d\u0c26\u0c41\u0c15\u0c4d(\u0c05)\u0c28\u0c41\u0c1a\u0c41 \u0c38\u0c48\u0c30\u0c3f\u0c28\u0c4d\u0c24\u0c41\u0c28\u0c41\u0c30\u0c3e \u0c30\u0c3e\u0c2e <br/> \u0c2e\u0c41\u0c28\u0c4d\u0c26\u0c41 \u0c35\u0c46\u0c28\u0c41\u0c15 \u0c24\u0c4b\u0c1a\u0c26\u0c41\u0c30\u0c3e <br/> \u0c2e\u0c41\u0c28\u0c4d\u0c26\u0c30 \u0c28\u0c3f\u0c32\u0c3f\u0c1a\u0c3f \u0c2a\u0c32\u0c41\u0c15\u0c30\u0c3e <br/> \u0c28\u0c3e(\u0c2f\u0c4d)\u0c05\u0c28\u0c4d\u0c26\u0c41 \u0c28\u0c40\u0c15\u0c41 \u0c08\u0c35\u0c30\u0c15\u0c41 \u0c2e\u0c30\u0c2a\u0c41\u0c30\u0c3e (\u0c0f)"}, {"M": "\u0c1a\u0c30\u0c23\u0c2e\u0c4d 4", "T": "\u0c15\u0c28\u0c4d\u0c28 \u0c35\u0c3e\u0c30\u0c3f\u0c28\u0c3f \u0c35\u0c47\u0c21\u0c3f\u0c28\u0c3e\u0c28\u0c3e <br/> \u0c28\u0c3e(\u0c2f\u0c4d)\u0c05\u0c28\u0c4d\u0c28 \u0c28\u0c3f\u0c28\u0c4d\u0c28\u0c4d(\u0c06)\u0c21\u0c41\u0c15\u0c4a\u0c28\u0c4d\u0c28\u0c3e\u0c28\u0c3e <br/> \u0c28\u0c3f\u0c28\u0c4d\u0c28\u0c41 \u0c28\u0c2e\u0c4d\u0c2e\u0c3f\u0c28-\u0c35\u0c3e\u0c21\u0c28\u0c41 \u0c15\u0c3e\u0c28\u0c3e \u0c2e\u0c41\u0c28\u0c3f <br/> \u0c38\u0c28\u0c4d\u0c28\u0c41\u0c24 \u0c15\u0c2a\u0c1f\u0c2e\u0c41\u0c32\u0c4d(\u0c05)\u0c28\u0c4d\u0c28\u0c3f \u0c28\u0c3e\u0c24\u0c4b\u0c28\u0c3e (\u0c0f)"}, {"M": "\u0c1a\u0c30\u0c23\u0c2e\u0c4d 5", "T": "\u0c35\u0c3f\u0c28\u0c35\u0c4d(\u0c05)\u0c2f\u0c4d\u0c2f \u0c07\u0c28 \u0c15\u0c41\u0c32 \u0c27\u0c28\u0c2e\u0c3e \u0c30\u0c3e\u0c2e <br/> \u0c28\u0c3f\u0c28\u0c4d\u0c28\u0c41 \u0c28\u0c2e\u0c4d\u0c2e\u0c3f(\u0c2f\u0c4d)\u0c07\u0c28\u0c4d\u0c15 \u0c26\u0c41\u0c30\u0c3f\u0c24\u0c2e\u0c3e <br/> \u0c2d\u0c41\u0c35\u0c28\u0c2e\u0c41\u0c28 \u0c28\u0c40\u0c15\u0c4d(\u0c07)\u0c26\u0c3f \u0c18\u0c28\u0c2e\u0c3e \u0c28\u0c40 <br/> \u0c2e\u0c28\u0c38\u0c4d(\u0c07)\u0c1f\u0c41\u0c35\u0c23\u0c4d\u0c1f\u0c3f\u0c26\u0c4d(\u0c05)\u0c28\u0c41\u0c1a\u0c41\u0c28\u0c41 \u0c24\u0c46\u0c32\u0c41\u0c2a\u0c41\u0c2e\u0c3e (\u0c0f)"}, {"M": "\u0c1a\u0c30\u0c23\u0c2e\u0c4d 6", "T": "\u0c38\u0c26\u0c2f\u0c41\u0c21\u0c48 \u0c07\u0c28\u0c4d\u0c15 \u0c2a\u0c30\u0c3e\u0c15\u0c3e \u0c28\u0c3e <br/> \u0c39\u0c43\u0c26\u0c2f \u0c35\u0c3e\u0c38\u0c41\u0c21\u0c41 \u0c28\u0c40\u0c35\u0c47 \u0c17\u0c3e\u0c15 <br/> \u0c07\u0c26\u0c3f \u0c2c\u0c41\u0c26\u0c4d\u0c27\u0c3f(\u0c2f\u0c4d)\u0c05\u0c28\u0c41\u0c1a\u0c41 \u0c24\u0c46\u0c32\u0c4d\u0c2a \u0c32\u0c47\u0c15 \u0c28\u0c3e\u0c21\u0c41 <br/> \u0c2e\u0c4a\u0c26\u0c32\u0c41\u0c17\u0c3e\u0c28\u0c41 \u0c0a\u0c30\u0c15\u0c4d(\u0c09)\u0c28\u0c4d\u0c28\u0c3e\u0c35\u0c41 \u0c17\u0c3e\u0c15 (\u0c0f)"}, {"M": "\u0c1a\u0c30\u0c23\u0c2e\u0c4d 7", "T": "\u0c30\u0c3e\u0c1c \u0c36\u0c47\u0c16\u0c30 \u0c38\u0c28\u0c4d\u0c28\u0c41\u0c24\u0c4d(\u0c06)\u0c19\u0c4d\u0c17 <br/> \u0c24\u0c4d\u0c2f\u0c3e\u0c17\u0c30\u0c3e\u0c1c \u0c39\u0c43\u0c26\u0c4d-\u0c05\u0c2c\u0c4d\u0c1c \u0c38\u0c41-\u0c2d\u0c43\u0c19\u0c4d\u0c17 <br/> \u0c30\u0c3e\u0c1c\u0c3f\u0c24 \u0c15\u0c30\u0c41\u0c23\u0c4d(\u0c06)\u0c2a\u0c3e\u0c19\u0c4d\u0c17 \u0c30\u0c24\u0c3f <br/> \u0c30\u0c3e\u0c1c \u0c1c\u0c28\u0c15 \u0c2a\u0c3e\u0c2a \u0c27\u0c4d\u0c35\u0c3e\u0c28\u0c4d\u0c24 \u0c2a\u0c24\u0c19\u0c4d\u0c17 \u0c28\u0c47\u0c28\u0c4d(\u0c0f)"}]}, {"L": "Kannada", "text": [{"M": "\u0caa\u0cb2\u0ccd\u0cb2\u0cb5\u0cbf", "T": "\u0c8f \u0caa\u0cbe\u0caa\u0cae\u0cc1 \u0c9c\u0cc7\u0cb8\u0cbf\u0ca4\u0cbf\u0cb0\u0cbe \u0cb0\u0cbe\u0cae <br/> \u0ca8\u0cc0\u0c95\u0ccd(\u0c8f) \u0caa\u0cbe\u0c9f\u0cc8\u0ca8 \u0ca6\u0caf \u0cb0\u0cbe\u0ca6\u0cc1 \u0ca8\u0cc7\u0ca8\u0ccd(\u0c8f)"}, {"M": "\u0c85\u0ca8\u0cc1\u0caa\u0cb2\u0ccd\u0cb2\u0cb5\u0cbf", "T": "\u0ca8\u0cc0 \u0caa\u0cbe\u0ca6\u0cae\u0cc1\u0cb2\u0ca8\u0cc1 \u0c95\u0ca8 \u0cae\u0cca\u0cb0\u0cb2\u0ccd(\u0c87)\u0ca1\u0cbf\u0ca4\u0cc7 <br/> \u0ca8\u0cc0 \u0caa\u0cbe\u0c9f\u0cc1\u0ca8 \u0cb5\u0cbf\u0ca8\u0cbf \u0cb5\u0cbf\u0ca8\u0ca8\u0c9f\u0ccd\u0c9f\u0ccd(\u0c89)\u0ca3\u0ccd\u0ca1\u0cc1\u0c9f\u0c95\u0cc1 \u0ca8\u0cc7\u0ca8\u0ccd(\u0c8f)"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 1", "T": "\u0ca8\u0cbe\u0ca6 \u0cb0\u0cc2\u0caa\u0cc1\u0ca1\u0cb5\u0ccd(\u0c85)\u0ca8\u0cbf \u0cb5\u0cbf\u0ca8\u0cbf \u0ca8\u0cc7 \u0cb6\u0ccd\u0cb0\u0cc0 <br/> \u0ca8\u0cbe\u0ca5 \u0ca8\u0cbf\u0ca8\u0ccd\u0ca8\u0cc1 \u0ca8\u0cae\u0ccd\u0cae\u0cbf\u0ca4\u0cbf\u0ca8\u0cbf <br/> \u0ca8\u0cbe\u0ca6 \u0caa\u0cc1\u0cb0\u0cae\u0cc1\u0ca8(\u0ca8\u0ccd)\u0c89\u0ca3\u0ccd\u0ca1\u0cbf\u0caf\u0cc1 <br/> \u0ca8\u0ca8\u0ccd\u0ca8\u0ccd(\u0c86)\u0ca6\u0cb0\u0cbf\u0ca8\u0ccd\u0c9a\u0c95 \u0c8a\u0cb0\u0c95\u0ccd(\u0c89)\u0ca3\u0ccd\u0ca1\u0cc1\u0c9f\u0c95\u0cc1 \u0ca8\u0cc7\u0ca8\u0ccd(\u0c8f)"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 2", "T": "\u0c97\u0cbe\u0cb0\u0cb5\u0cbf\u0cae\u0ccd\u0caa \u0ca6\u0caf \u0cb0\u0cbe\u0ca6\u0cbe \u0caa\u0cbe\u0cb2\u0cc1 <br/> \u0c95\u0cbe\u0cb0\u0cc1 \u0cae\u0ccb\u0cae\u0cc1 \u0c9c\u0cc2\u0caa \u0cb0\u0cbe\u0ca6\u0cbe <br/> \u0c8a\u0cb0\u0c95\u0ccd(\u0c89)\u0ca3\u0ccd\u0ca1\u0cc1\u0c9f \u0cae\u0cb0\u0cbf\u0caf\u0cbe\u0ca6\u0cbe \u0ca8\u0cbe <br/> \u0ca6\u0cc1\u0cb0\u0cbe \u0ca4\u0cc6\u0cb2\u0ccd\u0caa\u0cc1-\u0cb5\u0cbe\u0cb0\u0ccd(\u0c8e)\u0cb5\u0ccd\u0cb5\u0cb0\u0cc1 \u0cb2\u0cc7\u0cb0\u0cbe (\u0c8f)"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 3", "T": "\u0c8e\u0ca8\u0ccd\u0ca6\u0cc1\u0c95\u0ccd(\u0c85)\u0ca8\u0cc1\u0c9a\u0cc1 \u0cb8\u0cc8\u0cb0\u0cbf\u0ca8\u0ccd\u0ca4\u0cc1\u0ca8\u0cc1\u0cb0\u0cbe \u0cb0\u0cbe\u0cae <br/> \u0cae\u0cc1\u0ca8\u0ccd\u0ca6\u0cc1 \u0cb5\u0cc6\u0ca8\u0cc1\u0c95 \u0ca4\u0ccb\u0c9a\u0ca6\u0cc1\u0cb0\u0cbe <br/> \u0cae\u0cc1\u0ca8\u0ccd\u0ca6\u0cb0 \u0ca8\u0cbf\u0cb2\u0cbf\u0c9a\u0cbf \u0caa\u0cb2\u0cc1\u0c95\u0cb0\u0cbe <br/> \u0ca8\u0cbe(\u0caf\u0ccd)\u0c85\u0ca8\u0ccd\u0ca6\u0cc1 \u0ca8\u0cc0\u0c95\u0cc1 \u0c88\u0cb5\u0cb0\u0c95\u0cc1 \u0cae\u0cb0\u0caa\u0cc1\u0cb0\u0cbe (\u0c8f)"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 4", "T": "\u0c95\u0ca8\u0ccd\u0ca8 \u0cb5\u0cbe\u0cb0\u0cbf\u0ca8\u0cbf \u0cb5\u0cc7\u0ca1\u0cbf\u0ca8\u0cbe\u0ca8\u0cbe <br/> \u0ca8\u0cbe(\u0caf\u0ccd)\u0c85\u0ca8\u0ccd\u0ca8 \u0ca8\u0cbf\u0ca8\u0ccd\u0ca8\u0ccd(\u0c86)\u0ca1\u0cc1\u0c95\u0cca\u0ca8\u0ccd\u0ca8\u0cbe\u0ca8\u0cbe <br/> \u0ca8\u0cbf\u0ca8\u0ccd\u0ca8\u0cc1 \u0ca8\u0cae\u0ccd\u0cae\u0cbf\u0ca8-\u0cb5\u0cbe\u0ca1\u0ca8\u0cc1 \u0c95\u0cbe\u0ca8\u0cbe \u0cae\u0cc1\u0ca8\u0cbf <br/> \u0cb8\u0ca8\u0ccd\u0ca8\u0cc1\u0ca4 \u0c95\u0caa\u0c9f\u0cae\u0cc1\u0cb2\u0ccd(\u0c85)\u0ca8\u0ccd\u0ca8\u0cbf \u0ca8\u0cbe\u0ca4\u0ccb\u0ca8\u0cbe (\u0c8f)"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 5", "T": "\u0cb5\u0cbf\u0ca8\u0cb5\u0ccd(\u0c85)\u0caf\u0ccd\u0caf \u0c87\u0ca8 \u0c95\u0cc1\u0cb2 \u0ca7\u0ca8\u0cae\u0cbe \u0cb0\u0cbe\u0cae <br/> \u0ca8\u0cbf\u0ca8\u0ccd\u0ca8\u0cc1 \u0ca8\u0cae\u0ccd\u0cae\u0cbf(\u0caf\u0ccd)\u0c87\u0ca8\u0ccd\u0c95 \u0ca6\u0cc1\u0cb0\u0cbf\u0ca4\u0cae\u0cbe <br/> \u0cad\u0cc1\u0cb5\u0ca8\u0cae\u0cc1\u0ca8 \u0ca8\u0cc0\u0c95\u0ccd(\u0c87)\u0ca6\u0cbf \u0c98\u0ca8\u0cae\u0cbe \u0ca8\u0cc0 <br/> \u0cae\u0ca8\u0cb8\u0ccd(\u0c87)\u0c9f\u0cc1\u0cb5\u0ca3\u0ccd\u0c9f\u0cbf\u0ca6\u0ccd(\u0c85)\u0ca8\u0cc1\u0c9a\u0cc1\u0ca8\u0cc1 \u0ca4\u0cc6\u0cb2\u0cc1\u0caa\u0cc1\u0cae\u0cbe (\u0c8f)"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 6", "T": "\u0cb8\u0ca6\u0caf\u0cc1\u0ca1\u0cc8 \u0c87\u0ca8\u0ccd\u0c95 \u0caa\u0cb0\u0cbe\u0c95\u0cbe \u0ca8\u0cbe <br/> \u0cb9\u0cc3\u0ca6\u0caf \u0cb5\u0cbe\u0cb8\u0cc1\u0ca1\u0cc1 \u0ca8\u0cc0\u0cb5\u0cc7 \u0c97\u0cbe\u0c95 <br/> \u0c87\u0ca6\u0cbf \u0cac\u0cc1\u0ca6\u0ccd\u0ca7\u0cbf(\u0caf\u0ccd)\u0c85\u0ca8\u0cc1\u0c9a\u0cc1 \u0ca4\u0cc6\u0cb2\u0ccd\u0caa \u0cb2\u0cc7\u0c95 \u0ca8\u0cbe\u0ca1\u0cc1 <br/> \u0cae\u0cca\u0ca6\u0cb2\u0cc1\u0c97\u0cbe\u0ca8\u0cc1 \u0c8a\u0cb0\u0c95\u0ccd(\u0c89)\u0ca8\u0ccd\u0ca8\u0cbe\u0cb5\u0cc1 \u0c97\u0cbe\u0c95 (\u0c8f)"}, {"M": "\u0c9a\u0cb0\u0ca3\u0cae\u0ccd 7", "T": "\u0cb0\u0cbe\u0c9c \u0cb6\u0cc7\u0c96\u0cb0 \u0cb8\u0ca8\u0ccd\u0ca8\u0cc1\u0ca4\u0ccd(\u0c86)\u0c99\u0ccd\u0c97 <br/> \u0ca4\u0ccd\u0caf\u0cbe\u0c97\u0cb0\u0cbe\u0c9c \u0cb9\u0cc3\u0ca6\u0ccd-\u0c85\u0cac\u0ccd\u0c9c \u0cb8\u0cc1-\u0cad\u0cc3\u0c99\u0ccd\u0c97 <br/> \u0cb0\u0cbe\u0c9c\u0cbf\u0ca4 \u0c95\u0cb0\u0cc1\u0ca3\u0ccd(\u0c86)\u0caa\u0cbe\u0c99\u0ccd\u0c97 \u0cb0\u0ca4\u0cbf <br/> \u0cb0\u0cbe\u0c9c \u0c9c\u0ca8\u0c95 \u0caa\u0cbe\u0caa \u0ca7\u0ccd\u0cb5\u0cbe\u0ca8\u0ccd\u0ca4 \u0caa\u0ca4\u0c99\u0ccd\u0c97 \u0ca8\u0cc7\u0ca8\u0ccd(\u0c8f)"}]}, {"L": "Malayalam", "text": [{"M": "\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d0f \u0d2a\u0d3e\u0d2a\u0d2e\u0d41 \u0d1c\u0d47\u0d38\u0d3f\u0d24\u0d3f\u0d30\u0d3e \u0d30\u0d3e\u0d2e <br/> \u0d28\u0d40\u0d15\u0d4d(\u0d0f) \u0d2a\u0d3e\u0d1f\u0d48\u0d28 \u0d26\u0d2f \u0d30\u0d3e\u0d26\u0d41 \u0d28\u0d47\u0d28\u0d4d(\u0d0f)"}, {"M": "\u0d05\u0d28\u0d41\u0d2a\u0d32\u0d4d\u0d32\u0d35\u0d3f", "T": "\u0d28\u0d40 \u0d2a\u0d3e\u0d26\u0d2e\u0d41\u0d32\u0d28\u0d41 \u0d15\u0d28 \u0d2e\u0d4a\u0d30\u0d32\u0d4d(\u0d07)\u0d21\u0d3f\u0d24\u0d47 <br/> \u0d28\u0d40 \u0d2a\u0d3e\u0d1f\u0d41\u0d28 \u